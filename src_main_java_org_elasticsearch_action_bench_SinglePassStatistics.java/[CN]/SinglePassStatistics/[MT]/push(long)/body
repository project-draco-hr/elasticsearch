{
  count++;
  if (count == 1) {
    runningMean=value;
    runningVariance=0.0;
  }
 else {
    double newMean=runningMean + ((value - runningMean) / count);
    double newVariance=runningVariance + ((value - runningMean) * (value - newMean));
    runningMean=newMean;
    runningVariance=newVariance;
  }
  runningSum+=value;
  tdigest.add(value);
}

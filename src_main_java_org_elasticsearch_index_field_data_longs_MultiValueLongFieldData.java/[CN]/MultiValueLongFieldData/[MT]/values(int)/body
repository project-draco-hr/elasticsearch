{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] == 0) {
      break;
    }
    length++;
  }
  if (length == 0) {
    return EMPTY_LONG_ARRAY;
  }
  long[] longs;
  if (length < VALUE_CACHE_SIZE) {
    longs=valuesCache.get().get()[length];
  }
 else {
    longs=new long[length];
  }
  for (int i=0; i < length; i++) {
    longs[i]=values[ordinals[i][docId]];
  }
  return longs;
}

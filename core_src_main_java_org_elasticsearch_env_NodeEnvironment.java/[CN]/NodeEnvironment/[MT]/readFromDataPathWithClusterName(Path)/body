{
  if (Files.exists(dataPathWithClusterName) == false || Files.isDirectory(dataPathWithClusterName) == false || dirEmpty(dataPathWithClusterName)) {
    return false;
  }
  Path nodesPath=dataPathWithClusterName.resolve(NODES_FOLDER);
  if (Files.isDirectory(nodesPath)) {
    return true;
  }
  return false;
}

{
  if (closed.compareAndSet(false,true) && locks != null) {
    for (    Lock lock : locks) {
      try {
        logger.trace("releasing lock [{}]",lock);
        lock.close();
      }
 catch (      IOException e) {
        logger.trace(new ParameterizedMessage("failed to release lock [{}]",lock),e);
      }
    }
  }
}

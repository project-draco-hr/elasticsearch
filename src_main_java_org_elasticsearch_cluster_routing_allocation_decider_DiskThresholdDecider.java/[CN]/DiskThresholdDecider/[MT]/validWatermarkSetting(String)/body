{
  try {
    RatioValue.parseRatioValue(watermark);
    return true;
  }
 catch (  ElasticsearchParseException e) {
    try {
      ByteSizeValue.parseBytesSizeValue(watermark,"DiskThresholdDecider watermark");
      return true;
    }
 catch (    ElasticsearchParseException ex) {
      return false;
    }
  }
}

{
  if (this == obj) {
    return true;
  }
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  BaseTermQueryBuilder other=(BaseTermQueryBuilder)obj;
  return Objects.equals(fieldName,other.fieldName) && Objects.equals(value,other.value) && Objects.equals(boost,other.boost)&& Objects.equals(queryName,other.queryName);
}

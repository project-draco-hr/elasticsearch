{
  XContentBuilder expected=XContentFactory.jsonBuilder().startObject().field("type","MultiPoint").startArray("coordinates").startArray().value(100.0).value(0.0).endArray().startArray().value(101.0).value(1.0).endArray().endArray().endObject();
  List<Coordinate> multiPointCoordinates=new ArrayList<Coordinate>();
  multiPointCoordinates.add(new Coordinate(100,0));
  multiPointCoordinates.add(new Coordinate(101,1));
  MultiPoint multiPoint=GEOMETRY_FACTORY.createMultiPoint(multiPointCoordinates.toArray(new Coordinate[multiPointCoordinates.size()]));
  assertSerializationEquals(expected,new JtsGeometry(multiPoint,GeoShapeConstants.SPATIAL_CONTEXT,false));
}

{
  Settings merged=ImmutableSettings.builder().put("tribe.t1.cluster.name",cluster().getClusterName()).put("tribe.t2.cluster.name",cluster2.getClusterName()).put("tribe.blocks.write",false).put("tribe.blocks.read",false).put(settings).build();
  tribeNode=NodeBuilder.nodeBuilder().settings(merged).node();
  tribeClient=tribeNode.client();
}

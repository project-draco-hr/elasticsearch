{
  cluster2=new TestCluster(randomLong(),2,2,cluster().getClusterName() + "-2");
  cluster2.beforeTest(getRandom(),getPerTestTransportClientRatio());
  cluster2.ensureAtLeastNumNodes(2);
  Settings settings=ImmutableSettings.builder().put("tribe.t1.cluster.name",cluster().getClusterName()).put("tribe.t2.cluster.name",cluster2.getClusterName()).build();
  tribeNode=NodeBuilder.nodeBuilder().settings(settings).node();
  tribeClient=tribeNode.client();
}

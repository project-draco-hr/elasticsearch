{
  BreakerSettings newInFlightRequestsSettings=new BreakerSettings(CircuitBreaker.IN_FLIGHT_REQUESTS,newInFlightRequestsMax.bytes(),newInFlightRequestsOverhead,HierarchyCircuitBreakerService.this.inFlightRequestsSettings.getType());
  registerBreaker(newInFlightRequestsSettings);
  HierarchyCircuitBreakerService.this.inFlightRequestsSettings=newInFlightRequestsSettings;
  logger.info("Updated breaker settings for in-flight requests: {}",newInFlightRequestsSettings);
}

{
  BreakerSettings newRequestSettings=new BreakerSettings(CircuitBreaker.REQUEST,newRequestMax.bytes(),newRequestOverhead,HierarchyCircuitBreakerService.this.requestSettings.getType());
  registerBreaker(newRequestSettings);
  HierarchyCircuitBreakerService.this.requestSettings=newRequestSettings;
  logger.info("Updated breaker settings request: {}",newRequestSettings);
}

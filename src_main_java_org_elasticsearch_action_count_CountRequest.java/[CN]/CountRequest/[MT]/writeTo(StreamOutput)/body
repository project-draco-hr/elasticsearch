{
  super.writeTo(out);
  out.writeFloat(minScore);
  if (queryHint == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(queryHint);
  }
  if (routing == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(routing);
  }
  out.writeBytesReference(querySource);
  out.writeVInt(types.length);
  for (  String type : types) {
    out.writeUTF(type);
  }
}

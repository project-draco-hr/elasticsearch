{
  super.readFrom(in);
  minScore=in.readFloat();
  if (in.readBoolean()) {
    queryHint=in.readUTF();
  }
  if (in.readBoolean()) {
    routing=in.readUTF();
  }
  querySourceUnsafe=false;
  querySource=in.readBytesReference();
  int typesSize=in.readVInt();
  if (typesSize > 0) {
    types=new String[typesSize];
    for (int i=0; i < typesSize; i++) {
      types[i]=in.readUTF();
    }
  }
}

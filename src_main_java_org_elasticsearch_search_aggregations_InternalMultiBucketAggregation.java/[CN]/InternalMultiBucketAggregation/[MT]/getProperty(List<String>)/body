{
  if (path.isEmpty()) {
    return this;
  }
 else {
    List<? extends Bucket> buckets=getBuckets();
    Object[] propertyArray=new Object[buckets.size()];
    for (int i=0; i < buckets.size(); i++) {
      propertyArray[i]=buckets.get(i).getProperty(getName(),path);
    }
    return propertyArray;
  }
}

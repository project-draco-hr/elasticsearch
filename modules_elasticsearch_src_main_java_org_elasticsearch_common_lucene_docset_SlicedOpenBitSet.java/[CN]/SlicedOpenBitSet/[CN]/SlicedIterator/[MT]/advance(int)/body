{
  indexArray=0;
  i=target >> 6;
  if (i >= words) {
    word=0;
    return curDocId=NO_MORE_DOCS;
  }
  wordShift=target & 0x3f;
  word=arr[from + i] >>> wordShift;
  if (word != 0) {
    wordShift--;
  }
 else {
    while (word == 0) {
      if (++i >= words) {
        return curDocId=NO_MORE_DOCS;
      }
      word=arr[from + i];
    }
    wordShift=-1;
  }
  shift();
  int bitIndex=(indexArray & 0x0f) + wordShift;
  indexArray>>>=4;
  return curDocId=(i << 6) + bitIndex;
}

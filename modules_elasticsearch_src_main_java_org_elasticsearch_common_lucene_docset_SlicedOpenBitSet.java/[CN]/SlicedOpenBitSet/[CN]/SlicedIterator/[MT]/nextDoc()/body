{
  if (indexArray == 0) {
    if (word != 0) {
      word>>>=8;
      wordShift+=8;
    }
    while (word == 0) {
      if (++i >= words) {
        return curDocId=NO_MORE_DOCS;
      }
      word=arr[from + i];
      wordShift=-1;
    }
    shift();
  }
  int bitIndex=(indexArray & 0x0f) + wordShift;
  indexArray>>>=4;
  return curDocId=(i << 6) + bitIndex;
}

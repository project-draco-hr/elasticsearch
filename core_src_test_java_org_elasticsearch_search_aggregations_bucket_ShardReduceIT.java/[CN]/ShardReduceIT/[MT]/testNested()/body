{
  SearchResponse response=client().prepareSearch("idx").setQuery(QueryBuilders.matchAllQuery()).addAggregation(nested("nested").path("nested").subAggregation(dateHistogram("histo").field("nested.date").interval(DateHistogramInterval.DAY).minDocCount(0))).execute().actionGet();
  assertSearchResponse(response);
  Nested nested=response.getAggregations().get("nested");
  Histogram histo=nested.getAggregations().get("histo");
  assertThat(histo.getBuckets().size(),equalTo(4));
}

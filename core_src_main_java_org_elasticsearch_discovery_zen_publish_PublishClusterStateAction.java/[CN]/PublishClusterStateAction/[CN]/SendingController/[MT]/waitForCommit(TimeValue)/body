{
  boolean timedout=false;
  try {
    timedout=committedOrFailedLatch.await(commitTimeout.millis(),TimeUnit.MILLISECONDS) == false;
  }
 catch (  InterruptedException e) {
  }
  if (timedout) {
    markAsFailed("timed out waiting for commit (commit timeout [" + commitTimeout + "]");
  }
  if (isCommitted() == false) {
    throw new FailedToCommitException("{} enough masters to ack sent cluster state. [{}] left",timedout ? "timed out while waiting for" : "failed to get",neededMastersToCommit);
  }
}

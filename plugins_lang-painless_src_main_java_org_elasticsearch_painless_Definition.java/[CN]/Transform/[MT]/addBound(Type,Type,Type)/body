{
  final Pair pair0=new Pair(type0,type1);
  final Pair pair1=new Pair(type1,type0);
  if (bounds.containsKey(pair0)) {
    throw new IllegalArgumentException("Bound already defined for types [" + type0.name + "] and ["+ type1.name+ "].");
  }
  if (bounds.containsKey(pair1)) {
    throw new IllegalArgumentException("Bound already defined for types [" + type1.name + "] and ["+ type0.name+ "].");
  }
  bounds.put(pair0,bound);
  bounds.put(pair1,bound);
}

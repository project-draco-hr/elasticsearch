{
  try {
    Map<String,Object> allVars=new HashMap<>();
    if (vars != null) {
      allVars.putAll(vars);
    }
    return new GroovyScript(createScript(compiledScript,allVars),this.logger);
  }
 catch (  Exception e) {
    throw new ScriptException("failed to build executable script",e);
  }
}

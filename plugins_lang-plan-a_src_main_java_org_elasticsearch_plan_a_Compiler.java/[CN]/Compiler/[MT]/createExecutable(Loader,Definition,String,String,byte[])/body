{
  try {
    final Class<? extends Executable> clazz=loader.define(Writer.CLASS_NAME,bytes);
    final java.lang.reflect.Constructor<? extends Executable> constructor=clazz.getConstructor(Definition.class,String.class,String.class);
    return constructor.newInstance(definition,name,source);
  }
 catch (  Exception exception) {
    throw new IllegalStateException("An internal error occurred attempting to define the script [" + name + "].",exception);
  }
}

{
  ArrayList<ClusterState> states=new ArrayList<>(count);
  ClusterState[] lastClusterStatePerMaster=new ClusterState[masters.length];
  for (; count > 0; count--) {
    int masterIndex=randomInt(masters.length - 1);
    ClusterState state=lastClusterStatePerMaster[masterIndex];
    if (state == null) {
      state=ClusterState.builder(ClusterName.CLUSTER_NAME_SETTING.getDefault(Settings.EMPTY)).nodes(DiscoveryNodes.builder().put(new DiscoveryNode(masters[masterIndex],LocalTransportAddress.buildUnique(),emptyMap(),emptySet(),Version.CURRENT)).masterNodeId(masters[masterIndex]).build()).build();
    }
 else {
      state=ClusterState.builder(state).incrementVersion().build();
    }
    states.add(state);
    lastClusterStatePerMaster[masterIndex]=state;
  }
  return states;
}

{
  NettyTransport.NodeConnections nodeConnections=connectedNodes.get(node.id());
  if (nodeConnections == null) {
    throw new NodeNotConnectedException(node,"Node not connected");
  }
  Channel channel=nodeConnections.channel();
  if (channel == null) {
    throw new NodeNotConnectedException(node,"Node not connected");
  }
  return channel;
}

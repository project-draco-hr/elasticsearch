{
  if (!lifecycle.started()) {
    throw new ElasticSearchIllegalStateException("Can't add nodes to a stopped transport");
  }
  for (  DiscoveryNode node : nodes) {
    try {
      nodeChannel(node);
    }
 catch (    Exception e) {
      logger.warn("Failed to connect to discovered node [" + node + "]",e);
    }
  }
}

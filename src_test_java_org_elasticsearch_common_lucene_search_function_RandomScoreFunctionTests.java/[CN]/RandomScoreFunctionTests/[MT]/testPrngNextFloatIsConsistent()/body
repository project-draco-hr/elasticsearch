{
  long seed=randomLong();
  RandomScoreFunction.PRNG prng=new RandomScoreFunction.PRNG(seed);
  RandomScoreFunction.PRNG prng2=new RandomScoreFunction.PRNG(seed);
  assertThat(prng.nextFloat(),equalTo(prng2.nextFloat()));
  assertThat(prng.nextFloat(),equalTo(prng2.nextFloat()));
  assertThat(prng.nextFloat(),equalTo(prng2.nextFloat()));
  assertThat(prng.nextFloat(),equalTo(prng2.nextFloat()));
}

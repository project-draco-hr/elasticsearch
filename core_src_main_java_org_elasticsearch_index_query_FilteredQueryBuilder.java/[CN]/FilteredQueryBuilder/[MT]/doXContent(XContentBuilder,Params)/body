{
  builder.startObject(NAME);
  if (queryBuilder != null) {
    builder.field("query");
    queryBuilder.toXContent(builder,params);
  }
  if (filterBuilder != null) {
    builder.field("filter");
    filterBuilder.toXContent(builder,params);
  }
  printBoostAndQueryName(builder);
  builder.endObject();
}

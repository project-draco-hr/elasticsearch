{
  @SuppressWarnings("unchecked") TermSuggestionContext oldContext=(TermSuggestionContext)oldSuggestion;
  @SuppressWarnings("unchecked") TermSuggestionContext newContext=(TermSuggestionContext)newSuggestion;
  assertSpellcheckerSettings(oldContext.getDirectSpellCheckerSettings(),newContext.getDirectSpellCheckerSettings());
}

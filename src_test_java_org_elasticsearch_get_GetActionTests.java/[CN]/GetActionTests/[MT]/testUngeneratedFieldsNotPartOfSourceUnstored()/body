{
  indexSingleDocumentWithUngeneratedFieldsThatAreNeverPartOf_source(false,randomBoolean());
  String[] fieldsList={"_timestamp","_size","_routing"};
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList,"1");
  refresh();
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList,"1");
  flush();
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList,"1");
}

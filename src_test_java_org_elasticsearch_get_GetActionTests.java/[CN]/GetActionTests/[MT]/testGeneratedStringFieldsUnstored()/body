{
  indexSingleDocumentWithStringFieldsGeneratedFromText(false,randomBoolean());
  String[] fieldsList={"_all","_field_names"};
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList);
  refresh();
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList);
  flush();
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList);
}

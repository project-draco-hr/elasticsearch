{
  indexSingleDocumentWithNumericFieldsGeneratedFromText(false,randomBoolean());
  String[] fieldsList={"token_count","text.token_count","murmur","text.murmur"};
  assertGetFieldsAlwaysNull(indexOrAlias(),"doc","1",fieldsList);
  refresh();
  assertGetFieldsAlwaysNull(indexOrAlias(),"doc","1",fieldsList);
  flush();
  assertGetFieldsAlwaysNull(indexOrAlias(),"doc","1",fieldsList);
}

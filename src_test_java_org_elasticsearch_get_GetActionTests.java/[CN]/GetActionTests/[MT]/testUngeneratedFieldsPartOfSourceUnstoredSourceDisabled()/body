{
  indexSingleDocumentWithUngeneratedFieldsThatArePartOf_source(false,false);
  String[] fieldsList={"my_boost"};
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList);
  refresh();
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList);
  flush();
  assertGetFieldsAlwaysNull("testidx","doc","1",fieldsList);
}

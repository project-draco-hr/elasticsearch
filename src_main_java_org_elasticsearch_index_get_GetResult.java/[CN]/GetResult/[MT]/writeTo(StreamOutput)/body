{
  out.writeUTF(index);
  out.writeOptionalUTF(type);
  out.writeUTF(id);
  out.writeLong(version);
  out.writeBoolean(exists);
  if (exists) {
    out.writeBytesReference(source);
    if (fields == null) {
      out.writeVInt(0);
    }
 else {
      out.writeVInt(fields.size());
      for (      GetField field : fields.values()) {
        field.writeTo(out);
      }
    }
  }
}

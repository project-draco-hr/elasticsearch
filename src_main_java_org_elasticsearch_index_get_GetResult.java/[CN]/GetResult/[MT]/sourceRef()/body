{
  if (LZF.isCompressed(source.bytes(),source.offset(),source.length())) {
    try {
      this.source=new BytesHolder(LZFDecoder.decode(source.copyBytes()));
    }
 catch (    IOException e) {
      throw new ElasticSearchParseException("failed to decompress source",e);
    }
  }
  return this.source;
}

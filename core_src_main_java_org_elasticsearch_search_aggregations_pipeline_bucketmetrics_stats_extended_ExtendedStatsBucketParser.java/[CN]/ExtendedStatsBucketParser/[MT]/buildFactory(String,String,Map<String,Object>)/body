{
  Double sigma=null;
  Object param=unparsedParams.get(SIGMA.getPreferredName());
  if (param != null) {
    if (param instanceof Double) {
      sigma=(Double)param;
      unparsedParams.remove(SIGMA.getPreferredName());
    }
 else {
      throw new ParseException("Parameter [" + SIGMA.getPreferredName() + "] must be a Double, type `"+ param.getClass().getSimpleName()+ "` provided instead",0);
    }
  }
  ExtendedStatsBucketPipelineAggregatorBuilder factory=new ExtendedStatsBucketPipelineAggregatorBuilder(pipelineAggregatorName,bucketsPath);
  if (sigma != null) {
    factory.sigma(sigma);
  }
  return factory;
}

{
  Settings settings=Settings.settingsBuilder().put(Environment.PATH_HOME_SETTING.getKey(),createTempDir()).put("index.analysis.filter.myCollator.type","icu_collation").put("index.analysis.filter.myCollator.language","en").put("index.analysis.filter.myCollator.numeric","true").build();
  AnalysisService analysisService=createAnalysisService(settings);
  TokenFilterFactory filterFactory=analysisService.tokenFilter("myCollator");
  assertCollation(filterFactory,"foobar-9","foobar-10",-1);
}

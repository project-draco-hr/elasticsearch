{
  if (this == obj) {
    return true;
  }
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  QueryRescorerBuilder other=(QueryRescorerBuilder)obj;
  return super.equals(obj) && Objects.equals(scoreMode,other.scoreMode) && Objects.equals(queryWeight,other.queryWeight)&& Objects.equals(rescoreQueryWeight,other.rescoreQueryWeight)&& Objects.equals(queryBuilder,other.queryBuilder);
}

{
  super.writeTo(out);
  if (scrollId == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeString(scrollId);
  }
  if (scroll == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    scroll.writeTo(out);
  }
  out.writeBytesReference(source);
}

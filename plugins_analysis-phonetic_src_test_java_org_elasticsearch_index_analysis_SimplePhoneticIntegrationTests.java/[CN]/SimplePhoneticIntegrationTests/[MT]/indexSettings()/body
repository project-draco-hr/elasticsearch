{
  Settings settings=Settings.builder().put(super.indexSettings()).put("index.analysis.analyzer.my_analyzer.tokenizer","standard").putArray("index.analysis.analyzer.my_analyzer.filter","standard","lowercase","my_metaphone").put("index.analysis.filter.my_metaphone.type","phonetic").put("index.analysis.filter.my_metaphone.encoder","metaphone").put("index.analysis.filter.my_metaphone.replace",false).build();
  return settings;
}

{
  if (facetFilter != null) {
    builder.field("facet_filter");
    facetFilter.toXContent(builder,params);
  }
  if (nested != null) {
    builder.field("nested",nested);
  }
  if (global != null) {
    builder.field("global",global);
  }
  if (mode != null) {
    builder.field("mode",mode.toString());
  }
}

{
  if (in.readBoolean()) {
    preference=in.readUTF();
  }
  refresh=in.readBoolean();
  byte realtime=in.readByte();
  if (realtime == 0) {
    this.realtime=false;
  }
 else   if (realtime == 1) {
    this.realtime=true;
  }
  int size=in.readVInt();
  items=new ArrayList<Item>(size);
  for (int i=0; i < size; i++) {
    items.add(Item.readItem(in));
  }
}

{
  out.writeString(index);
  if (type == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeString(type);
  }
  out.writeString(id);
  if (routing == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeString(routing);
  }
  if (fields == null) {
    out.writeVInt(0);
  }
 else {
    out.writeVInt(fields.length);
    for (    String field : fields) {
      out.writeString(field);
    }
  }
}

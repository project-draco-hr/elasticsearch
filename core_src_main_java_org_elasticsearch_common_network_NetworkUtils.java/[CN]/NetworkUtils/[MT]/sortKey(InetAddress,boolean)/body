{
  int key=address.getAddress().length;
  if (prefer_v4 == false) {
    key=-key;
  }
  if (address.isAnyLocalAddress()) {
    key+=5;
  }
  if (address.isMulticastAddress()) {
    key+=4;
  }
  if (address.isLoopbackAddress()) {
    key+=3;
  }
  if (address.isLinkLocalAddress()) {
    key+=2;
  }
  if (address.isSiteLocalAddress()) {
    key+=1;
  }
  return key;
}

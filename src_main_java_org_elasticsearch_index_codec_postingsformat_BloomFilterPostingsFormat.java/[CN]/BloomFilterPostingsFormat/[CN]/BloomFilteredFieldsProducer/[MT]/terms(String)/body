{
  LazyBloomLoader filter=bloomsByFieldName.get(field);
  if (filter == null) {
    return delegateFieldsProducer.terms(field);
  }
 else {
    Terms result=delegateFieldsProducer.terms(field);
    if (result == null) {
      return null;
    }
    return new BloomFilteredTerms(result,filter.get());
  }
}

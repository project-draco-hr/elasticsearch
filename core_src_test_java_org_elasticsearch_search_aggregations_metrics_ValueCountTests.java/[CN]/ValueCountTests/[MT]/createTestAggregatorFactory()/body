{
  ValueCountAggregator.Factory<ValuesSource> factory=new ValueCountAggregator.Factory<ValuesSource>("foo",ValuesSourceType.ANY,null);
  String field=randomNumericField();
  int randomFieldBranch=randomInt(3);
switch (randomFieldBranch) {
case 0:
    factory.field(field);
  break;
case 1:
factory.field(field);
factory.script(new Script("_value + 1"));
break;
case 2:
factory.script(new Script("doc[" + field + "] + 1"));
break;
}
if (randomBoolean()) {
factory.missing("MISSING");
}
return factory;
}

{
switch (field) {
case METHOD:
    if (value == null) {
      unsetMethod();
    }
 else {
      setMethod((Method)value);
    }
  break;
case URI:
if (value == null) {
  unsetUri();
}
 else {
  setUri((String)value);
}
break;
case PARAMS:
if (value == null) {
unsetParams();
}
 else {
setParams((Map<String,String>)value);
}
break;
case HEADERS:
if (value == null) {
unsetHeaders();
}
 else {
setHeaders((Map<String,String>)value);
}
break;
case BODY:
if (value == null) {
unsetBody();
}
 else {
setBody((ByteBuffer)value);
}
break;
}
}

{
  super.readFrom(in);
  int size=in.readVInt();
  items=new ArrayList<Item>(size);
  for (int i=0; i < size; i++) {
    Item item=new Item();
    item.slot=in.readVInt();
    if (in.readBoolean()) {
      item.response=new PercolateShardResponse();
      item.response.readFrom(in);
    }
 else {
      item.error=in.readText();
    }
    items.add(item);
  }
}

{
  setupFieldType(context);
  Murmur3FieldMapper fieldMapper=new Murmur3FieldMapper(fieldType,docValues,null,ignoreMalformed(context),coerce(context),fieldDataSettings,context.indexSettings(),multiFieldsBuilder.build(this,context),copyTo);
  fieldMapper.includeInAll(includeInAll);
  return fieldMapper;
}

{
  setupFieldType(context);
  Murmur3FieldMapper fieldMapper=new Murmur3FieldMapper(name,fieldType,docValues,ignoreMalformed(context),coerce(context),fieldDataSettings,context.indexSettings(),multiFieldsBuilder.build(this,context),copyTo);
  fieldMapper.includeInAll(includeInAll);
  return fieldMapper;
}

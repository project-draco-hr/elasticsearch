{
  buildScriptService(Settings.EMPTY);
  CompiledScript compiledScript1=scriptService.compile(new Script("1+1",ScriptType.INLINE,"test",null),randomFrom(scriptContexts));
  CompiledScript compiledScript2=scriptService.compile(new Script("1+1",ScriptType.INLINE,"test",null),randomFrom(scriptContexts));
  assertThat(compiledScript1,sameInstance(compiledScript2));
}

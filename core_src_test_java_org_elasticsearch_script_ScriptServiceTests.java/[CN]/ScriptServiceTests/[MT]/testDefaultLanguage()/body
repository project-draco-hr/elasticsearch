{
  ContextAndHeaderHolder contextAndHeaderHolder=new ContextAndHeaderHolder();
  Settings.Builder builder=Settings.builder();
  builder.put("script.default_lang","test");
  buildScriptService(builder.build());
  CompiledScript script=scriptService.compile(new Script("1 + 1",ScriptType.INLINE,null,null),randomFrom(scriptContexts),contextAndHeaderHolder,Collections.emptyMap());
  assertEquals(script.lang(),"test");
}

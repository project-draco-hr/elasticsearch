{
  int idx=(int)(pos / uncompressed.length);
  if (idx >= offsets.length) {
    currentOffsetIdx=idx;
    position=0;
    valid=0;
    return;
  }
  long pointer=offsets[idx];
  if (pointer != currentOffset) {
    in.seek(pointer);
    position=0;
    valid=0;
    currentOffsetIdx=idx - 1;
    readyBuffer();
  }
  position=(int)(pos % uncompressed.length);
}

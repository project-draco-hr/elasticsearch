{
  int idx=(int)(pos / uncompressedLength);
  if (idx >= offsets.size()) {
    currentOffsetIdx=idx;
    position=0;
    valid=0;
    return;
  }
  if (idx != currentOffsetIdx) {
    long pointer=offsets.get(idx);
    in.seek(pointer);
    position=0;
    valid=0;
    currentOffsetIdx=idx - 1;
    readyBuffer();
  }
  position=(int)(pos % uncompressedLength);
}

{
  super(new Names(name,name,name,name),Defaults.BOOST,new FieldType(Defaults.SOURCE_FIELD_TYPE),Lucene.KEYWORD_ANALYZER,Lucene.KEYWORD_ANALYZER,null);
  this.enabled=enabled;
  this.compress=compress;
  this.compressThreshold=compressThreshold;
  this.includes=includes;
  this.excludes=excludes;
  this.format=format;
  this.formatContentType=format == null ? null : XContentType.fromRestContentType(format);
}

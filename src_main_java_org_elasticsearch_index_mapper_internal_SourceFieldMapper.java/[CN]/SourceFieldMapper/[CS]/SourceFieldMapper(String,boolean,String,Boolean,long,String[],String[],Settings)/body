{
  super(new Names(name,name,name,name),Defaults.BOOST,new FieldType(Defaults.FIELD_TYPE),false,Lucene.KEYWORD_ANALYZER,Lucene.KEYWORD_ANALYZER,null,null,null,indexSettings);
  this.enabled=enabled;
  this.compress=compress;
  this.compressThreshold=compressThreshold;
  this.includes=includes == null ? Strings.EMPTY_ARRAY : includes;
  this.excludes=excludes == null ? Strings.EMPTY_ARRAY : excludes;
  this.format=format;
  this.formatContentType=format == null ? null : XContentType.fromRestContentType(format);
  this.complete=enabled && includes == null && excludes == null;
}

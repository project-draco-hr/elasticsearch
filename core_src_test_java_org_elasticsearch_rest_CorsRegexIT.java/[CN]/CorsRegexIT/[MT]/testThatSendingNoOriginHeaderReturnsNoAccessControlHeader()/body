{
  try (RestClient client=restClient()){
    try (ElasticsearchResponse response=client.performRequest("GET","/",Collections.emptyMap(),null,new BasicHeader("User-Agent","Mozilla Bar"))){
      assertThat(response.getStatusLine().getStatusCode(),is(200));
      assertThat(response.getFirstHeader("Access-Control-Allow-Origin"),nullValue());
    }
   }
 }

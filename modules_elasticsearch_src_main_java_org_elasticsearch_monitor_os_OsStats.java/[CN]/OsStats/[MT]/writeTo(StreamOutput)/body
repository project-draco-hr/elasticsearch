{
  out.writeVLong(timestamp);
  out.writeVInt(loadAverage.length);
  for (  double val : loadAverage) {
    out.writeDouble(val);
  }
  out.writeLong(uptime);
  if (cpu == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    cpu.writeTo(out);
  }
  if (mem == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    mem.writeTo(out);
  }
  if (swap == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    swap.writeTo(out);
  }
}

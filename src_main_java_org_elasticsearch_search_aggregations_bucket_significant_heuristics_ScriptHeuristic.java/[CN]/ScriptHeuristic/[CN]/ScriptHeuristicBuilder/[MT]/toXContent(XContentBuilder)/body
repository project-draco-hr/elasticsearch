{
  builder.startObject(STREAM.getName());
  if (script != null) {
    builder.field("script",script);
  }
  if (lang != null) {
    builder.field("lang",lang);
  }
  if (params != null) {
    builder.field("params",params);
  }
  if (scriptId != null) {
    builder.field("script_id",scriptId);
  }
  builder.endObject();
}

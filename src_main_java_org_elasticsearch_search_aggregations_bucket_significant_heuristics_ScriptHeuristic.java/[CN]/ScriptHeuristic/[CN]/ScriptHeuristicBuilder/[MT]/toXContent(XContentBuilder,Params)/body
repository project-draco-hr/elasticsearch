{
  builder.startObject(STREAM.getName());
  builder.field(ScriptField.SCRIPT.getPreferredName());
  if (script == null) {
    new Script(scriptString,type,lang,params).toXContent(builder,builderParams);
  }
 else {
    script.toXContent(builder,builderParams);
  }
  builder.endObject();
  return builder;
}

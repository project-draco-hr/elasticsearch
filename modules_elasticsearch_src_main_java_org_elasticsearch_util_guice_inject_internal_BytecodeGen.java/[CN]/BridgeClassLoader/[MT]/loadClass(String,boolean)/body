{
  if (name.startsWith(GUICE_INTERNAL_PACKAGE) || name.startsWith(CGLIB_PACKAGE)) {
    try {
      Class<?> clazz=GUICE_CLASS_LOADER.loadClass(name);
      if (resolve) {
        resolveClass(clazz);
      }
      return clazz;
    }
 catch (    Exception e) {
    }
  }
  return super.loadClass(name,resolve);
}

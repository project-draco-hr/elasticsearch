{
  Map<String,Object> vars=new HashMap<>();
  vars.put("template","all");
  TemplateQueryBuilder builder=new TemplateQueryBuilder(new Template("storedTemplate",ScriptService.ScriptType.FILE,null,null,vars));
  SearchResponse sr=client().prepareSearch().setQuery(builder).execute().actionGet();
  assertHitCount(sr,2);
}

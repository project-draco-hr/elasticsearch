{
  Map<String,Object> params=new HashMap<>();
  params.put("template","all");
  SearchResponse sr=client().prepareSearch().setSource(new SearchSourceBuilder().size(0).query(new TemplateQueryBuilder("{ \"match_{{template}}\": {} }",ScriptType.INLINE,params))).get();
  assertNoFailures(sr);
  assertThat(sr.getHits().hits().length,equalTo(0));
}

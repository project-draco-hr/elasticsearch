{
  List<List<Integer>> list=new ArrayList<>();
  Iterable<Integer> allInts=Iterables.flatten(list);
  int count=0;
  for (  int x : allInts) {
    count++;
  }
  assertEquals(0,count);
  list.add(new ArrayList<>());
  for (  int x : allInts) {
    count++;
  }
  assertEquals(0,count);
  list.get(0).add(0);
  for (  int x : allInts) {
    count++;
  }
  assertEquals(1,count);
}

{
  builder.startObject(NAME);
  builder.field("field",name);
  if (nullValue != null) {
    builder.field("null_value",nullValue);
  }
  if (existence != null) {
    builder.field("existence",existence);
  }
  if (queryName != null) {
    builder.field("_name",queryName);
  }
  builder.endObject();
}

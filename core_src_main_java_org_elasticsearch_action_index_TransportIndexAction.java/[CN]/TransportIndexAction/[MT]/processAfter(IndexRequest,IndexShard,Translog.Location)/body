{
  if (request.refresh()) {
    try {
      indexShard.refresh("refresh_flag_index");
    }
 catch (    Throwable e) {
    }
  }
  if (indexShard.getTranslogDurability() == Translog.Durabilty.REQUEST && location != null) {
    try {
      indexShard.sync(location);
    }
 catch (    EngineClosedException e) {
    }
  }
}

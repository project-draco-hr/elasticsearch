{
  Cell[] as=cells;
  double max=Double.longBitsToDouble(base);
  base=MIN_AS_LONG;
  if (as != null) {
    int n=as.length;
    for (int i=0; i < n; ++i) {
      Cell a=as[i];
      if (a != null) {
        double v=Double.longBitsToDouble(a.value);
        a.value=MIN_AS_LONG;
        if (v > max)         max=v;
      }
    }
  }
  return max;
}

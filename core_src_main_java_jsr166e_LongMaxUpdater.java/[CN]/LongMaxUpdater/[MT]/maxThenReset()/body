{
  Cell[] as=cells;
  long max=base;
  base=Long.MIN_VALUE;
  if (as != null) {
    int n=as.length;
    for (int i=0; i < n; ++i) {
      Cell a=as[i];
      if (a != null) {
        long v=a.value;
        a.value=Long.MIN_VALUE;
        if (v > max)         max=v;
      }
    }
  }
  return max;
}

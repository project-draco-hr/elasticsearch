{
  builder.field("rescore_query",queryBuilder);
  if (queryWeight != null) {
    builder.field("query_weight",queryWeight);
  }
  if (rescoreQueryWeight != null) {
    builder.field("rescore_query_weight",rescoreQueryWeight);
  }
  if (scoreMode != null) {
    builder.field("score_mode",scoreMode);
  }
  return builder;
}

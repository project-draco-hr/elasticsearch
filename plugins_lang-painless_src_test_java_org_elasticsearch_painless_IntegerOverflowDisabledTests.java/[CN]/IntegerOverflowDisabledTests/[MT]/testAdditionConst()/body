{
  try {
    exec("return 2147483647 + 2147483647;");
    fail("should have hit exception");
  }
 catch (  ArithmeticException expected) {
  }
  try {
    exec("return 9223372036854775807L + 9223372036854775807L;");
    fail("should have hit exception");
  }
 catch (  ArithmeticException expected) {
  }
}

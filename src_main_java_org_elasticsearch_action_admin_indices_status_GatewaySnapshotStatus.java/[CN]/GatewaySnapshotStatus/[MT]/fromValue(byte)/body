{
  if (value == 0) {
    return Stage.NONE;
  }
 else   if (value == 1) {
    return Stage.INDEX;
  }
 else   if (value == 2) {
    return Stage.TRANSLOG;
  }
 else   if (value == 3) {
    return Stage.FINALIZE;
  }
 else   if (value == 4) {
    return Stage.DONE;
  }
 else   if (value == 5) {
    return Stage.FAILURE;
  }
  throw new ElasticSearchIllegalArgumentException("No stage found for [" + value + "]");
}

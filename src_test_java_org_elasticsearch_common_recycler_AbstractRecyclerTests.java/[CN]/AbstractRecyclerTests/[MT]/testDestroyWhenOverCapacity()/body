{
  Recycler<byte[]> r=newRecycler(limit);
  Recycler.V<byte[]> o=r.obtain();
  byte[] data=o.v();
  assertFresh(data);
  List<V<byte[]>> vals=new ArrayList<V<byte[]>>(limit);
  for (int i=0; i < limit; ++i) {
    vals.add(r.obtain());
  }
  for (  V<byte[]> v : vals) {
    v.release();
  }
  o.release();
  assertDead(data);
  r.close();
}

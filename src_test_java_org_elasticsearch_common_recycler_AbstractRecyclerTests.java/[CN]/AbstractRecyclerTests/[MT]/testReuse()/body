{
  Recycler<byte[]> r=newRecycler(limit);
  Recycler.V<byte[]> o=r.obtain();
  assertFalse(o.isRecycled());
  final byte[] b1=o.v();
  assertFresh(b1);
  o.release();
  assertRecycled(b1);
  o=r.obtain();
  final byte[] b2=o.v();
  if (o.isRecycled()) {
    assertRecycled(b2);
    assertSame(b1,b2);
  }
 else {
    assertFresh(b2);
    assertNotSame(b1,b2);
  }
  o.release();
  r.close();
}

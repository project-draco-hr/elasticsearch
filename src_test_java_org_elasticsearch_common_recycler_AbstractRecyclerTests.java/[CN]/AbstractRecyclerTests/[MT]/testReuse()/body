{
  Recycler<byte[]> r=newRecycler();
  Recycler.V<byte[]> o=r.obtain();
  assertFalse(o.isRecycled());
  final byte[] b1=o.v();
  o.release();
  o=r.obtain();
  final byte[] b2=o.v();
  if (o.isRecycled()) {
    assertSame(b1,b2);
  }
 else {
    assertNotSame(b1,b2);
  }
  o.release();
  r.close();
}

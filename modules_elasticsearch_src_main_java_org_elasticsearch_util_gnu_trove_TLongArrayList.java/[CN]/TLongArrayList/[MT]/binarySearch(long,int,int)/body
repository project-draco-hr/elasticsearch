{
  if (fromIndex < 0) {
    throw new ArrayIndexOutOfBoundsException(fromIndex);
  }
  if (toIndex > _pos) {
    throw new ArrayIndexOutOfBoundsException(toIndex);
  }
  int low=fromIndex;
  int high=toIndex - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    long midVal=_data[mid];
    if (midVal < value) {
      low=mid + 1;
    }
 else     if (midVal > value) {
      high=mid - 1;
    }
 else {
      return mid;
    }
  }
  return -(low + 1);
}

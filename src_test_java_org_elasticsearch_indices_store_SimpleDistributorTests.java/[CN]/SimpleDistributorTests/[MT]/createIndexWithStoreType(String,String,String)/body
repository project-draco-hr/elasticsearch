{
  wipeIndices(index);
  client().admin().indices().prepareCreate(index).setSettings(settingsBuilder().put("index.store.distributor",distributor).put("index.store.type",storeType).put("index.number_of_replicas",0).put("index.number_of_shards",1)).execute().actionGet();
  assertThat(client().admin().cluster().prepareHealth("test").setWaitForGreenStatus().execute().actionGet().isTimedOut(),equalTo(false));
}

{
  builder.startArray();
  for (  GeoPoint point : points) {
    toXContent(builder,point);
  }
  if (closed) {
    GeoPoint start=points.get(0);
    GeoPoint end=points.get(points.size() - 1);
    if (start.x != end.x || start.y != end.y) {
      toXContent(builder,points.get(0));
    }
  }
  builder.endArray();
  return builder;
}

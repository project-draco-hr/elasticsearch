{
  Event event;
  if (scanner.checkToken(Token.ID.Directive,Token.ID.DocumentStart,Token.ID.DocumentEnd,Token.ID.StreamEnd)) {
    event=processEmptyScalar(scanner.peekToken().getStartMark());
    state=states.pop();
    return event;
  }
 else {
    Production p=new ParseBlockNode();
    return p.produce();
  }
}

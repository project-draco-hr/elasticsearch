{
  if (indices == null) {
    throw new ElasticSearchIllegalArgumentException("indices must not be null");
  }
 else {
    for (int i=0; i < indices.length; i++) {
      if (indices[i] == null) {
        throw new ElasticSearchIllegalArgumentException("indices[" + i + "] must not be null");
      }
    }
  }
  this.indices=indices;
  return this;
}

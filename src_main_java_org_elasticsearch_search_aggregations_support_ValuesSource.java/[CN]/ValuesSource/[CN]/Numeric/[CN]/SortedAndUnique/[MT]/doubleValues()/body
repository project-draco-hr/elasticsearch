{
  if (doubleValues == null) {
    doubleValues=delegate.doubleValues();
    if (doubleValues.isMultiValued() && (!delegate.metaData().uniqueness.unique() || doubleValues.getOrder() != Order.NUMERIC)) {
      doubleValues=new SortedUniqueDoubleValues(doubleValues);
    }
  }
  return doubleValues;
}

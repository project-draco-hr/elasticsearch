{
  field=in.readOptionalString();
  if (in.readBoolean()) {
    script=Script.readScript(in);
  }
  if (in.readBoolean()) {
    valueType=ValueType.readFromStream(in);
  }
  format=in.readOptionalString();
  missing=in.readGenericValue();
  if (in.readBoolean()) {
    timeZone=DateTimeZone.forID(in.readString());
  }
}

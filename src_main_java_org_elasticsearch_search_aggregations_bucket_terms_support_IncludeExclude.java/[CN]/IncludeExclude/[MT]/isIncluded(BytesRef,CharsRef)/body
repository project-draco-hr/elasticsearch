{
  if ((includeValues == null) && (include == null)) {
    return true;
  }
  if (include != null) {
    if (include.reset(scratch.get()).matches()) {
      return true;
    }
  }
  if (includeValues != null) {
    if (includeValues.contains(value)) {
      return true;
    }
  }
  return false;
}

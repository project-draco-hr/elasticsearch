{
  assertEquals(FileSystemUtils.append(Paths.get("/foo/bar"),Paths.get("/hello/world/this_is/awesome"),0),Paths.get("/foo/bar/hello/world/this_is/awesome"));
  assertEquals(FileSystemUtils.append(Paths.get("/foo/bar"),Paths.get("/hello/world/this_is/awesome"),2),Paths.get("/foo/bar/this_is/awesome"));
  assertEquals(FileSystemUtils.append(Paths.get("/foo/bar"),Paths.get("/hello/world/this_is/awesome"),1),Paths.get("/foo/bar/world/this_is/awesome"));
}

{
  assertEquals(FileSystemUtils.append(PathUtils.get("/foo/bar"),PathUtils.get("/hello/world/this_is/awesome"),0),PathUtils.get("/foo/bar/hello/world/this_is/awesome"));
  assertEquals(FileSystemUtils.append(PathUtils.get("/foo/bar"),PathUtils.get("/hello/world/this_is/awesome"),2),PathUtils.get("/foo/bar/this_is/awesome"));
  assertEquals(FileSystemUtils.append(PathUtils.get("/foo/bar"),PathUtils.get("/hello/world/this_is/awesome"),1),PathUtils.get("/foo/bar/world/this_is/awesome"));
}

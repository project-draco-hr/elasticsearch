{
  int actual=0;
  DocIdSet docIdSet=filt.getDocIdSet(reader.getContext(),reader.getLiveDocs());
  if (docIdSet != null) {
    DocIdSetIterator disi=docIdSet.iterator();
    if (disi != null) {
      while (disi.nextDoc() != DocIdSetIterator.NO_MORE_DOCS) {
        actual++;
      }
    }
  }
  assertThat(mes,actual,equalTo(expected));
}

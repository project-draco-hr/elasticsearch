{
  return Objects.equals(this.query,other.query) && Arrays.equals(this.filterFunctionBuilders,other.filterFunctionBuilders) && Objects.equals(this.boostMode,other.boostMode)&& Objects.equals(this.scoreMode,other.scoreMode)&& Objects.equals(this.minScore,other.minScore)&& Objects.equals(this.maxBoost,other.maxBoost);
}

{
  Node<E> t, p, q;
  restartFromTail:   while ((t=tail).item == null && (p=t.next) != null) {
    for (; ; ) {
      if ((q=p.next) == null || (q=(p=q).next) == null) {
        if (casTail(t,p))         return;
 else         continue restartFromTail;
      }
 else       if (t != tail)       continue restartFromTail;
 else       p=q;
    }
  }
}

{
  s.defaultReadObject();
  Node<E> h=null, t=null;
  Object item;
  while ((item=s.readObject()) != null) {
    @SuppressWarnings("unchecked") Node<E> newNode=new Node<E>((E)item);
    if (h == null)     h=t=newNode;
 else {
      t.lazySetNext(newNode);
      newNode.lazySetPrev(t);
      t=newNode;
    }
  }
  initHeadTail(h,t);
}

{
  Node<E> h, p, q;
  restartFromHead:   while ((h=head).item == null && (p=h.prev) != null) {
    for (; ; ) {
      if ((q=p.prev) == null || (q=(p=q).prev) == null) {
        if (casHead(h,p))         return;
 else         continue restartFromHead;
      }
 else       if (h != head)       continue restartFromHead;
 else       p=q;
    }
  }
}

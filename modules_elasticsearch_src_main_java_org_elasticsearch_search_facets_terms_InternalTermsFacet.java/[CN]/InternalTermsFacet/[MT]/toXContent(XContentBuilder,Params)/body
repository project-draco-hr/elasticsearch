{
  builder.startObject(name);
  builder.field("_type","terms");
  builder.field("_field",fieldName);
  builder.startArray("terms");
  for (  Entry entry : entries) {
    builder.startObject();
    builder.field("term",entry.term());
    builder.field("count",entry.count());
    builder.endObject();
  }
  builder.endArray();
  builder.endObject();
}

{
  Preconditions.checkNotNull(rootObjectMapper,"Mapper builder must have the root object mapper set");
  return new XContentDocumentMapper(index,docMapperParser,rootObjectMapper,meta,uidFieldMapper,idFieldMapper,typeFieldMapper,indexFieldMapper,sourceFieldMapper,routingFieldMapper,allFieldMapper,analyzerMapper,indexAnalyzer,searchAnalyzer,boostFieldMapper);
}

{
  CodeSource codeSource=domain.getCodeSource();
  if (codeSource != null) {
    URL location=codeSource.getLocation();
    if (location != null) {
      if ("/groovy/script".equals(location.getFile())) {
        return false;
      }
    }
  }
  return template.implies(domain,permission) || dynamic.implies(permission);
}

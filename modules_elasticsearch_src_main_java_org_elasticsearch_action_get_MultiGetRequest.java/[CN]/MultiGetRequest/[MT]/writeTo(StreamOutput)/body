{
  if (preference == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(preference);
  }
  out.writeBoolean(refresh);
  if (realtime == null) {
    out.writeByte((byte)-1);
  }
 else   if (realtime == false) {
    out.writeByte((byte)0);
  }
 else {
    out.writeByte((byte)1);
  }
  out.writeVInt(items.size());
  for (  Item item : items) {
    item.writeTo(out);
  }
}

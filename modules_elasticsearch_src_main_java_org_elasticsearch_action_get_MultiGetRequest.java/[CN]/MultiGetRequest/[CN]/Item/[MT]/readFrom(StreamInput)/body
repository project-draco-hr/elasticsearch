{
  index=in.readUTF();
  if (in.readBoolean()) {
    type=in.readUTF();
  }
  id=in.readUTF();
  if (in.readBoolean()) {
    routing=in.readUTF();
  }
  int size=in.readVInt();
  if (size > 0) {
    fields=new String[size];
    for (int i=0; i < size; i++) {
      fields[i]=in.readUTF();
    }
  }
}

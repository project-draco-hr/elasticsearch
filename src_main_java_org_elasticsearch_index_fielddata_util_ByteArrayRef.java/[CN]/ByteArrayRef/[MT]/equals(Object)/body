{
  if (object == this) {
    return true;
  }
  if (object instanceof ByteArrayRef) {
    ByteArrayRef that=(ByteArrayRef)object;
    int size=size();
    if (that.size() != size) {
      return false;
    }
    for (int i=0; i < size; i++) {
      if (values[start + i] != that.values[that.start + i]) {
        return false;
      }
    }
    return true;
  }
  return super.equals(object);
}

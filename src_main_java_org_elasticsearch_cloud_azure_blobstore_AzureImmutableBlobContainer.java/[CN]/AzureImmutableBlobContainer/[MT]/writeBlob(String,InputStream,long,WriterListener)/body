{
  blobStore.executor().execute(new Runnable(){
    @Override public void run(){
      try {
        blobStore.client().putObject(blobStore.container(),buildKey(blobName),is,sizeInBytes);
        listener.onCompleted();
      }
 catch (      Throwable e) {
        listener.onFailure(e);
      }
    }
  }
);
}

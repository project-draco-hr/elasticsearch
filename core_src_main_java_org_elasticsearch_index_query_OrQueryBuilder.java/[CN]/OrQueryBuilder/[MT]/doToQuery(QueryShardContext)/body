{
  if (filters.isEmpty()) {
    return null;
  }
  BooleanQuery query=new BooleanQuery();
  for (  QueryBuilder f : filters) {
    Query innerQuery=f.toQuery(context);
    if (innerQuery != null) {
      query.add(innerQuery,Occur.SHOULD);
    }
  }
  if (query.clauses().isEmpty()) {
    return null;
  }
  return query;
}

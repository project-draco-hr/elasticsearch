{
  if (filters.isEmpty()) {
    return null;
  }
  BooleanQuery query=new BooleanQuery();
  for (  QueryBuilder f : filters) {
    Query innerQuery=f.toQuery(parseContext);
    if (innerQuery != null) {
      query.add(innerQuery,Occur.SHOULD);
    }
  }
  if (queryName != null) {
    parseContext.addNamedQuery(queryName,query);
  }
  return query;
}

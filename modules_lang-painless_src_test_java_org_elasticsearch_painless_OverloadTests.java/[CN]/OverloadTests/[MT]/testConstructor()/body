{
  assertEquals(true,exec("org.elasticsearch.painless.FeatureTest f = new org.elasticsearch.painless.FeatureTest();" + "return f.x == 0 && f.y == 0;"));
  assertEquals(true,exec("org.elasticsearch.painless.FeatureTest f = new org.elasticsearch.painless.FeatureTest(1, 2);" + "return f.x == 1 && f.y == 2;"));
}

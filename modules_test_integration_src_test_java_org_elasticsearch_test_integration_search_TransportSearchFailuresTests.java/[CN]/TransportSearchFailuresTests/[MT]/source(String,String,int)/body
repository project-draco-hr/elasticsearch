{
  StringBuilder multi=new StringBuilder().append(nameValue);
  for (int i=0; i < age; i++) {
    multi.append(" ").append(nameValue);
  }
  return binaryJsonBuilder().startObject().field("id",id).field("name",nameValue + id).field("age",age).field("multi",multi.toString()).field("_boost",age * 10).endObject();
}

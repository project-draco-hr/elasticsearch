{
  int num=scaledRandomIntBetween(2,20);
  for (int j=0; j < num; j++) {
    final int mod=1 + randomInt(40);
    for (int i=0; i < 797; i++) {
      long count=hash.size();
      long key=hash.add(randomLong());
      if (key < 0)       assertEquals(hash.size(),count);
 else       assertEquals(hash.size(),count + 1);
      if (i % mod == 0) {
        newHash();
      }
    }
  }
  hash.release();
}

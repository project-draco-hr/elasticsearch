{
  id=in.readVInt();
  opType=in.readString();
  byte type=in.readByte();
  if (type == 0) {
    response=new IndexResponse();
    response.readFrom(in);
  }
 else   if (type == 1) {
    response=new DeleteResponse();
    response.readFrom(in);
  }
  if (in.readBoolean()) {
    failure=new Failure(in.readString(),in.readString(),in.readString(),in.readString());
  }
}

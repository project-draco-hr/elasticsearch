def reindex_docs(es, index_name, type, num_docs):
    logging.info(('Re-indexing %s docs' % num_docs))
    for id in range(0, num_docs):
        es.index(index=index_name, doc_type=type, id=id, body={'string': str(random.randint(0, 100)), 'long_sort': random.randint(0, 100), 'double_sort': float(random.randint(0, 100)), 'bool': random.choice([True, False]), })

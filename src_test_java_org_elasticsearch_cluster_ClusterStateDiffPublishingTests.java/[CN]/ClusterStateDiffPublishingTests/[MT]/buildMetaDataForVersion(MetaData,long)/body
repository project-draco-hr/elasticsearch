{
  ImmutableOpenMap.Builder<String,IndexMetaData> indices=ImmutableOpenMap.builder(metaData.indices());
  indices.put("test" + version,IndexMetaData.builder("test" + version).settings(ImmutableSettings.builder().put(IndexMetaData.SETTING_VERSION_CREATED,Version.CURRENT)).numberOfShards((int)version).numberOfReplicas(0).build());
  return MetaData.builder(metaData).transientSettings(ImmutableSettings.builder().put("test",version).build()).indices(indices.build()).build();
}

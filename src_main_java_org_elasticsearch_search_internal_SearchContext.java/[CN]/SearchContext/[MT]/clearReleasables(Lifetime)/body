{
  if (clearables != null) {
    List<Collection<Releasable>> releasables=new ArrayList<>();
    for (    Lifetime lc : Lifetime.values()) {
      if (lc.compareTo(lifetime) > 0) {
        break;
      }
      releasables.add(clearables.removeAll(lc));
    }
    Releasables.close(Iterables.concat(releasables));
  }
}

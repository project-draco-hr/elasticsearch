{
  builder.startObject();
  builder.field(FIELD_TYPE,TYPE.shapename);
  builder.startArray(FIELD_COORDINATES);
  for (  BasePolygonBuilder<?> polygon : polygons) {
    builder.startArray();
    polygon.coordinatesArray(builder,params);
    builder.endArray();
  }
  builder.endArray();
  return builder.endObject();
}

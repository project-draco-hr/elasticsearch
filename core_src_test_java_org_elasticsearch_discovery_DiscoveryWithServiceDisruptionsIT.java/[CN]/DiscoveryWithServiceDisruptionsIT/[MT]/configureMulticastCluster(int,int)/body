{
  if (minimumMasterNode < 0) {
    minimumMasterNode=numberOfNodes / 2 + 1;
  }
  logger.info("---> configured multicast");
  Settings settings=Settings.builder().put(DEFAULT_SETTINGS).put("discovery.zen.ping.multicast.enabled",true).put(ElectMasterService.DISCOVERY_ZEN_MINIMUM_MASTER_NODES,minimumMasterNode).put().build();
  if (discoveryConfig == null) {
    discoveryConfig=new ClusterDiscoveryConfiguration(numberOfNodes,settings);
  }
}

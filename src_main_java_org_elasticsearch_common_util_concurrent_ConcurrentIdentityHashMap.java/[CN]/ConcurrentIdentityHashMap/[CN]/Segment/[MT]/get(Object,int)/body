{
  if (count != 0) {
    HashEntry<K,V> e=getFirst(hash);
    while (e != null) {
      if (e.hash == hash && keyEq(key,e.key())) {
        V opaque=e.value();
        if (opaque != null) {
          return opaque;
        }
        return readValueUnderLock(e);
      }
      e=e.next;
    }
  }
  return null;
}

{
  if (count != 0) {
    HashEntry<K,V>[] tab=table;
    int len=tab.length;
    for (int i=0; i < len; i++) {
      for (HashEntry<K,V> e=tab[i]; e != null; e=e.next) {
        V opaque=e.value();
        V v;
        if (opaque == null) {
          v=readValueUnderLock(e);
        }
 else {
          v=opaque;
        }
        if (value.equals(v)) {
          return true;
        }
      }
    }
  }
  return false;
}

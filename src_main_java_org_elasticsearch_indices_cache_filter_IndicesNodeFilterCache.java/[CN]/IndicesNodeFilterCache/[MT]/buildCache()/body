{
  TimeValue catchupTime=componentSettings.getAsTime("catchup",TimeValue.timeValueSeconds(10));
  int weightedSize=(int)Math.min(sizeInBytes / AbstractWeightedFilterCache.FilterCacheValueWeigher.FACTOR,Integer.MAX_VALUE);
  cache=new ConcurrentLinkedHashMap.Builder<AbstractWeightedFilterCache.FilterCacheKey,FilterCacheValue<DocSet>>().maximumWeightedCapacity(weightedSize).weigher(new AbstractWeightedFilterCache.FilterCacheValueWeigher()).listener(this).catchup(this.threadPool.scheduler(),catchupTime.millis(),TimeUnit.MILLISECONDS).build();
}

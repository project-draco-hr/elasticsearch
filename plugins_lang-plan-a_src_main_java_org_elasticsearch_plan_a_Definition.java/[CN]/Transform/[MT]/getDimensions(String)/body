{
  int dimensions=0;
  int index=name.indexOf('[');
  if (index != -1) {
    final int length=name.length();
    while (index < length) {
      if (name.charAt(index) == '[' && ++index < length && name.charAt(index++) == ']') {
        ++dimensions;
      }
 else {
        throw new IllegalArgumentException("Invalid array braces in canonical name [" + name + "].");
      }
    }
  }
  return dimensions;
}

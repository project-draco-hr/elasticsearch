{
  ensureOpen();
  Builder builder=settingsBuilder().put(settings).put(Node.NODE_MASTER_SETTING.getKey(),false).put(Node.NODE_DATA_SETTING.getKey(),false).put(Node.NODE_INGEST_SETTING.getKey(),false);
  if (size() == 0) {
    builder.put(DiscoverySettings.INITIAL_STATE_TIMEOUT_SETTING.getKey(),0);
  }
  String name=startNode(builder);
  return nodes.get(name).nodeClient();
}

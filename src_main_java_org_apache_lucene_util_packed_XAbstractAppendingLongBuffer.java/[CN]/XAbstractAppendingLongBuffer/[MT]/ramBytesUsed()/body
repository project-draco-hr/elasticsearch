{
  long bytesUsed=RamUsageEstimator.alignObjectSize(baseRamBytesUsed()) + (pending != null ? RamUsageEstimator.sizeOf(pending) : 0L) + RamUsageEstimator.alignObjectSize(RamUsageEstimator.NUM_BYTES_ARRAY_HEADER + (long)RamUsageEstimator.NUM_BYTES_OBJECT_REF * values.length);
  return bytesUsed + valuesBytes;
}

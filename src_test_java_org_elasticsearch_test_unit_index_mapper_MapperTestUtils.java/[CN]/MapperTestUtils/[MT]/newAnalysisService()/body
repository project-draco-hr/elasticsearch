{
  Injector parentInjector=new ModulesBuilder().add(new SettingsModule(ImmutableSettings.Builder.EMPTY_SETTINGS),new EnvironmentModule(new Environment(ImmutableSettings.Builder.EMPTY_SETTINGS)),new IndicesAnalysisModule()).createInjector();
  Injector injector=new ModulesBuilder().add(new IndexSettingsModule(new Index("test"),ImmutableSettings.Builder.EMPTY_SETTINGS),new IndexNameModule(new Index("test")),new AnalysisModule(ImmutableSettings.Builder.EMPTY_SETTINGS,parentInjector.getInstance(IndicesAnalysisService.class))).createChildInjector(parentInjector);
  return injector.getInstance(AnalysisService.class);
}

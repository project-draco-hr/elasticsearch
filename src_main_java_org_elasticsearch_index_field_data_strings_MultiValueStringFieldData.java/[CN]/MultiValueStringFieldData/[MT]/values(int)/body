{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] == 0) {
      break;
    }
    length++;
  }
  if (length == 0) {
    return Strings.EMPTY_ARRAY;
  }
  String[] strings;
  if (length < VALUE_CACHE_SIZE) {
    strings=valuesCache.get().get()[length];
  }
 else {
    strings=new String[length];
  }
  for (int i=0; i < length; i++) {
    strings[i]=values[ordinals[i][docId]];
  }
  return strings;
}

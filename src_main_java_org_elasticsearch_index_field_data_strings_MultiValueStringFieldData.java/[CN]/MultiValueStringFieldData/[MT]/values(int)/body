{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] == 0) {
      break;
    }
    length++;
  }
  if (length == 0) {
    return EMPTY_ARRAY;
  }
  BytesRef[] refs;
  if (length < VALUE_CACHE_SIZE) {
    refs=valuesCache.get().get()[length];
  }
 else {
    refs=new BytesRef[length];
  }
  for (int i=0; i < length; i++) {
    refs[i]=values[ordinals[i][docId]];
  }
  return refs;
}

{
  node=DiscoveryNode.readNode(in);
  id=in.readLong();
  type=in.readString();
  action=in.readString();
  description=in.readOptionalString();
  if (in.readBoolean()) {
    status=in.readTaskStatus();
  }
 else {
    status=null;
  }
  parentNode=in.readOptionalString();
  parentId=in.readLong();
}

{
  builder.startObject();
  builder.field("node",node.getId());
  builder.field("id",id);
  builder.field("type",type);
  builder.field("action",action);
  if (description != null) {
    builder.field("description",description);
  }
  if (parentNode != null) {
    builder.field("parent_node",parentNode);
    builder.field("parent_id",parentId);
  }
  builder.endObject();
  return builder;
}

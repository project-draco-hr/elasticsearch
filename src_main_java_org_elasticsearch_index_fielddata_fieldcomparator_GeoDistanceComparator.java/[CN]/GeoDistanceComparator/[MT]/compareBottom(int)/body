{
  double distance;
  GeoPoint geoPoint=readerValues.getValue(doc);
  if (geoPoint == null) {
    distance=Double.MAX_VALUE;
  }
 else {
    distance=fixedSourceDistance.calculate(geoPoint.lat(),geoPoint.lon());
  }
  final double v2=distance;
  if (bottom > v2) {
    return 1;
  }
 else   if (bottom < v2) {
    return -1;
  }
 else {
    return 0;
  }
}

{
  this.path=path;
  this.indicesPath=path.resolve(INDICES_FOLDER);
  this.fileStore=getFileStore(path);
  Boolean spins;
  if (Constants.LINUX) {
    try {
      spins=IOUtils.spins(path);
    }
 catch (    Exception e) {
      spins=null;
    }
  }
 else {
    spins=null;
  }
  this.spins=spins;
}

{
  final MapperService mapperService=new MapperService(idxSettings,mockAnalysisService,null,new IndicesModule().getMapperRegistry(),null){
    @Override public MappedFieldType fullName(    String fullName){
      StringFieldType type=new StringFieldType();
      if (randomBoolean()) {
        type.setSearchAnalyzer(new NamedAnalyzer("foo",new WhitespaceAnalyzer()));
      }
      return type;
    }
  }
;
  return new Tuple<>(mapperService,suggestBuilder);
}

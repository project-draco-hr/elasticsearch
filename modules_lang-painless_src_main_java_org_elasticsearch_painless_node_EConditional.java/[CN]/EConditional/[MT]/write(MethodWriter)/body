{
  writer.writeDebugInfo(offset);
  Label localfals=new Label();
  Label end=new Label();
  condition.fals=localfals;
  left.tru=right.tru=tru;
  left.fals=right.fals=fals;
  condition.write(writer);
  left.write(writer);
  writer.goTo(end);
  writer.mark(localfals);
  right.write(writer);
  writer.mark(end);
}

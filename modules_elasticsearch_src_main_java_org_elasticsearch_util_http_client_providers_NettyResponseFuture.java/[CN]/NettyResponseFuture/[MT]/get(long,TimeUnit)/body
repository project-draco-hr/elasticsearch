{
  if (!isDone() && !isCancelled()) {
    if (!latch.await(l,tu)) {
      isCancelled.set(true);
      TimeoutException te=new TimeoutException("No response received");
      onThrowable(te);
      throw te;
    }
    isDone.set(true);
    if (exEx.get() != null) {
      throw exEx.getAndSet(null);
    }
  }
  return (V)getContent();
}

{
  Object compiled=cache.get(script);
  if (compiled != null) {
    return compiled;
  }
synchronized (cache) {
    compiled=cache.get(script);
    if (compiled != null) {
      return compiled;
    }
    compiled=MVEL.compileExpression(script,parserContext);
    cache.put(script,compiled);
  }
  return compiled;
}

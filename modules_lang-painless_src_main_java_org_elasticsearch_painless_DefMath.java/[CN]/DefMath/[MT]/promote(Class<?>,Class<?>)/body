{
  if (a.isPrimitive() == false || b.isPrimitive() == false) {
    return Object.class;
  }
  if (a == boolean.class && b == boolean.class) {
    return boolean.class;
  }
  if (a == double.class || b == double.class) {
    return double.class;
  }
 else   if (a == float.class || b == float.class) {
    return float.class;
  }
 else   if (a == long.class || b == long.class) {
    return long.class;
  }
 else {
    return int.class;
  }
}

{
  if (event.indicesStateChanged()) {
    try {
      nodeIndicesStateUpdatedAction.nodeIndexStateUpdated(event.state(),new NodeIndicesStateUpdatedAction.NodeIndexStateUpdatedResponse(event.state().nodes().localNodeId(),event.state().version()));
    }
 catch (    Throwable e) {
      logger.debug("failed to send to master indices state change event",e);
    }
  }
}

{
  String mapping=XContentFactory.jsonBuilder().startObject().startObject("type").startObject("_parent").field("type","p_type").endObject().endObject().endObject().string();
  XContentDocumentMapper docMapper=MapperTests.newParser().parse(mapping);
  ParsedDocument doc=docMapper.parse(SourceToParse.source(XContentFactory.jsonBuilder().startObject().field("x_field","x_value").endObject().copiedBytes()).type("type").id("1").parent("1122"));
  assertThat(doc.doc().get("_parent"),equalTo(Uid.createUid("p_type","1122")));
}

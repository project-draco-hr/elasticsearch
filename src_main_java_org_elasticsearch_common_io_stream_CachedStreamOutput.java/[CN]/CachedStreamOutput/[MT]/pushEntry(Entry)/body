{
  if (entry.bytes().underlyingBytes().length > BYTES_LIMIT) {
    return;
  }
  Queue<Entry> ref=cache.get();
  if (ref == null) {
    ref=new LinkedTransferQueue<Entry>();
    counter.set(0);
    cache.set(ref);
  }
  if (counter.incrementAndGet() > COUNT_LIMIT) {
    counter.decrementAndGet();
  }
 else {
    ref.add(entry);
  }
}

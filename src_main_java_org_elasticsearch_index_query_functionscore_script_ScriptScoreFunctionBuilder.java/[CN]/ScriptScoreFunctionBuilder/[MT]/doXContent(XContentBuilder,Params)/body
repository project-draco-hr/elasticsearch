{
  builder.startObject(getName());
  if (script != null) {
    builder.field(ScriptField.SCRIPT.getPreferredName(),script);
  }
 else {
    if (scriptString != null) {
      builder.field("script",scriptString);
    }
    if (lang != null) {
      builder.field("lang",lang);
    }
    if (this.params != null) {
      builder.field("params",this.params);
    }
  }
  builder.endObject();
}

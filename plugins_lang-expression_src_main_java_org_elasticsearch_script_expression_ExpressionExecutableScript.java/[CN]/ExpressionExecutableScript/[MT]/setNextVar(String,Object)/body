{
  if (functionValuesMap.containsKey(name)) {
    if (value instanceof Number) {
      double doubleValue=((Number)value).doubleValue();
      functionValuesMap.get(name).setValue(doubleValue);
    }
 else {
      throw new ScriptException("Error using " + compiledScript + ". "+ "Executable expressions scripts can only process numbers."+ "  The variable ["+ name+ "] is not a number.");
    }
  }
 else {
    throw new ScriptException("Error using " + compiledScript + ". "+ "The variable ["+ name+ "] does not exist in the executable expressions script.");
  }
}

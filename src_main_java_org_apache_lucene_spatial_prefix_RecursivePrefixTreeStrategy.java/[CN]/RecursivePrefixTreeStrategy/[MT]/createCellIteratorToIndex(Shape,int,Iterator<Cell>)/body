{
  if (shape instanceof Point || !pruneLeafyBranches || grid instanceof PackedQuadPrefixTree)   return super.createCellIteratorToIndex(shape,detailLevel,reuse);
  List<Cell> cells=new ArrayList<>(4096);
  recursiveTraverseAndPrune(grid.getWorldCell(),shape,detailLevel,cells);
  return cells.iterator();
}

{
  int from=0;
  int to=0;
  for (; from < list.size(); from++) {
    T element=list.get(from);
    if (!predicate.apply(element)) {
      if (from > to) {
        list.set(to,element);
      }
      to++;
    }
  }
  ListIterator<T> iter=list.listIterator(list.size());
  for (int idx=from - to; idx > 0; idx--) {
    iter.previous();
    iter.remove();
  }
  return from != to;
}

{
  createIndex("idx");
  numDocs=randomIntBetween(10,20);
  IndexRequestBuilder[] builders=new IndexRequestBuilder[numDocs];
  for (int i=0; i < builders.length; i++) {
    builders[i]=client().prepareIndex("idx","type").setSource(jsonBuilder().startObject().field(SINGLE_VALUED_FIELD_NAME,i + 1).startArray(MULTI_VALUED_FIELD_NAME).value(i + 1).value(i + 2).endArray().endObject());
  }
  indexRandom(true,builders);
  createIndex("idx_unmapped");
  ensureSearchable();
}

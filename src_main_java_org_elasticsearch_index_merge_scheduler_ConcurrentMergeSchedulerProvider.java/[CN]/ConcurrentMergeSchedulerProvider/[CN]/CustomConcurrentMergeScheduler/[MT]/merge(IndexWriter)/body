{
  try {
    if (writer.getConfig().getMergePolicy() instanceof EnableMergePolicy) {
      if (!((EnableMergePolicy)writer.getConfig().getMergePolicy()).isMergeEnabled()) {
        return;
      }
    }
  }
 catch (  AlreadyClosedException e) {
    return;
  }
  try {
    super.merge(writer);
  }
 catch (  IOException e) {
    logger.warn("failed to merge",e);
    throw e;
  }
}

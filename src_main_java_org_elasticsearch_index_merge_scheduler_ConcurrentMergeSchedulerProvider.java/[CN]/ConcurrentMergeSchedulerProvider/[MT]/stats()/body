{
  MergeStats mergeStats=new MergeStats();
  for (  CustomConcurrentMergeScheduler scheduler : schedulers) {
    mergeStats.add(scheduler.totalMerges(),scheduler.totalMergeTime(),scheduler.totalMergeNumDocs(),scheduler.totalMergeSizeInBytes(),scheduler.currentMerges(),scheduler.currentMergesNumDocs(),scheduler.currentMergesSizeInBytes(),scheduler.totalMergeStoppedTimeMillis(),scheduler.totalMergeThrottledTimeMillis(),autoThrottle ? scheduler.getIORateLimitMBPerSec() : Double.POSITIVE_INFINITY);
  }
  return mergeStats;
}

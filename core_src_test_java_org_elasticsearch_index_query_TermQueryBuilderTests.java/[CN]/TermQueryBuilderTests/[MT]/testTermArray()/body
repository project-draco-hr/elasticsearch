{
  String queryAsString="{\n" + "    \"term\": {\n" + "        \"age\": [34, 35]\n"+ "    }\n"+ "}";
  try {
    parseQuery(queryAsString);
    fail("Expected ParsingException");
  }
 catch (  ParsingException e) {
    assertThat(e.getMessage(),is("[term] query does not support array of values"));
  }
}

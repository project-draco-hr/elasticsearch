{
  SearchContext context=findContext(request.id());
  try {
    context.docIdsToLoad(request.docIds());
    fetchPhase.execute(context);
    if (context.scroll() == null) {
      freeContext(request.id());
    }
    return context.fetchResult();
  }
 catch (  RuntimeException e) {
    freeContext(context);
    throw e;
  }
}

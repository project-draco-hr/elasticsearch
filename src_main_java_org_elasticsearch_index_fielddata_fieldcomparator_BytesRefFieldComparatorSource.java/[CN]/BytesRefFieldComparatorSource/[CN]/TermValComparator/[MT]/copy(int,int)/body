{
  final BytesRef comparableBytes=getComparableBytes(doc,docTerms.get(doc));
  if (comparableBytes == null) {
    values[slot]=null;
  }
 else {
    if (tempBRs[slot] == null) {
      tempBRs[slot]=new BytesRef();
    }
    values[slot]=tempBRs[slot];
    values[slot].copyBytes(comparableBytes);
  }
}

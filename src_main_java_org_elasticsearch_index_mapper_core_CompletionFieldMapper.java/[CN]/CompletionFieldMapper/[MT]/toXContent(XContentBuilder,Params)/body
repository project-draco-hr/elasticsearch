{
  return builder.startObject(name()).field(Fields.TYPE,CONTENT_TYPE).field(Fields.INDEX_ANALYZER,indexAnalyzer.name()).field(Fields.SEARCH_ANALYZER,searchAnalyzer.name()).field(Fields.PAYLOADS,this.payloads).field(Fields.PRESERVE_SEPARATORS,this.preserveSeparators).field(Fields.PRESERVE_POSITION_INCREMENTS,this.preservePositionIncrements).endObject();
}

{
  if (s == null) {
    return "";
  }
  int numChars=s.length();
  for (int i=0; i < numChars; i++) {
    if (s.charAt(i) == '+' || s.charAt(i) == '%') {
      try {
        return URLDecoder.decode(s,"UTF8");
      }
 catch (      UnsupportedEncodingException e) {
        throw new UnsupportedCharsetException("UTF8");
      }
    }
  }
  return s;
}

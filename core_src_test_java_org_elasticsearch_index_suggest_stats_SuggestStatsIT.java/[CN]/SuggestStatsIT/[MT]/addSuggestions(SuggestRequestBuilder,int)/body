{
  for (int s=0; s < randomIntBetween(2,10); s++) {
    if (randomBoolean()) {
      request.addSuggestion("s" + s,new PhraseSuggestionBuilder().field("f").text("test" + i + " test"+ (i - 1)));
    }
 else {
      request.addSuggestion("s" + s,new TermSuggestionBuilder().field("f").text("test" + i));
    }
  }
  return request;
}

{
  while (true) {
    if (close) {
      closed=true;
      return;
    }
    try {
      client.client().prepareDeleteByQuery().setQuery(termQuery("num",nextNumValue())).execute().actionGet();
      Thread.sleep(deleteByQueryThrottle.millis());
    }
 catch (    Exception e) {
      logger.warn("failed to delete_by_query",e);
    }
  }
}

{
  for (  Thread t : searcherThreads) {
    t.start();
  }
  for (  Thread t : indexThreads) {
    t.start();
  }
  barrier1.await();
  StopWatch stopWatch=new StopWatch();
  stopWatch.start();
  barrier2.await();
  latch.await();
  stopWatch.stop();
  for (  Client client : clients) {
    client.close();
  }
  for (  Node node : nodes) {
    node.close();
  }
  System.out.println("Done, took [" + stopWatch.totalTime() + "]");
  Thread.sleep(sleepAfterDone.millis());
}

{
  super(new Names(name,indexName,indexName,name),Defaults.BOOST,new FieldType(Defaults.FIELD_TYPE),Version.indexCreated(indexSettings).onOrAfter(Version.V_2_0_0),Lucene.KEYWORD_ANALYZER,Lucene.KEYWORD_ANALYZER,null,null,fieldDataSettings,indexSettings);
  this.type=type;
  this.typeAsBytes=type == null ? null : new BytesRef(type);
}

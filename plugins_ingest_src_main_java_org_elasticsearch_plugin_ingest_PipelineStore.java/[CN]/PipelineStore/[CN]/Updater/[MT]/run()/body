{
  try {
    while (running) {
      try {
        Thread.sleep(3000);
        updatePipelines();
      }
 catch (      InterruptedException e) {
        Thread.currentThread().interrupt();
      }
catch (      Exception e) {
        logger.error("update error",e);
      }
    }
  }
  finally {
    latch.countDown();
  }
}

{
  BulkIndexByScrollResponse response=new BulkIndexByScrollResponse(randomPositiveLong(),randomPositiveLong(),randomPositiveInt(),randomPositiveLong(),randomPositiveLong(),randomIndexingFailures(),randomSearchFailures());
  BulkIndexByScrollResponse tripped=new BulkIndexByScrollResponse();
  roundTrip(response,tripped);
  assertResponseEquals(response,tripped);
}

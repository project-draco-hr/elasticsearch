{
  Base64.OutputStream bos=null;
  try {
    bos=new Base64.OutputStream(new java.io.FileOutputStream(filename),Base64.DECODE);
    bos.write(dataToDecode.getBytes(PREFERRED_ENCODING));
  }
 catch (  java.io.IOException e) {
    throw e;
  }
 finally {
    try {
      bos.close();
    }
 catch (    Exception e) {
    }
  }
}

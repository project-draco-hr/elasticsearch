{
  if (obj == null || obj.getClass() != PersistedTaskInfo.class) {
    return false;
  }
  PersistedTaskInfo other=(PersistedTaskInfo)obj;
  return Objects.equals(completed,other.completed) && Objects.equals(task,other.task) && Objects.equals(getErrorAsMap(),other.getErrorAsMap())&& Objects.equals(getResponseAsMap(),other.getResponseAsMap());
}

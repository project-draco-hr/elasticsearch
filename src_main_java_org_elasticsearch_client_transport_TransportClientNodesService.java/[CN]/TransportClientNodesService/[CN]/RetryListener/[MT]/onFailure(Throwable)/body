{
  if (ExceptionsHelper.unwrapCause(e) instanceof ConnectTransportException) {
    int i=++this.i;
    if (i == nodes.size()) {
      listener.onFailure(new NoNodeAvailableException());
    }
 else {
      try {
        callback.doWithNode(nodes.get((index + i) % nodes.size()),this);
      }
 catch (      Throwable e1) {
        onFailure(e);
      }
    }
  }
 else {
    listener.onFailure(e);
  }
}

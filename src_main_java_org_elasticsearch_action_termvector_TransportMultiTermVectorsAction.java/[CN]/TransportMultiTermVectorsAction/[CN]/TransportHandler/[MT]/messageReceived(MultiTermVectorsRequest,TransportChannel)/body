{
  request.listenerThreaded(false);
  execute(request,new ActionListener<MultiTermVectorsResponse>(){
    @Override public void onResponse(    MultiTermVectorsResponse response){
      try {
        channel.sendResponse(response);
      }
 catch (      Throwable t) {
        onFailure(t);
      }
    }
    @Override public void onFailure(    Throwable e){
      try {
        channel.sendResponse(e);
      }
 catch (      Throwable t) {
        logger.warn("Failed to send error response for action [" + MultiTermVectorsAction.NAME + "] and request ["+ request+ "]",t);
      }
    }
  }
);
}

{
  Path file=dir.resolve("settings.yml");
  if (Files.exists(file)) {
    return Settings.settingsBuilder().loadFromPath(file).put(defaults).build();
  }
  file=dir.resolve("settings.json");
  if (Files.exists(file)) {
    return Settings.settingsBuilder().loadFromPath(file).put(defaults).build();
  }
  return defaults;
}

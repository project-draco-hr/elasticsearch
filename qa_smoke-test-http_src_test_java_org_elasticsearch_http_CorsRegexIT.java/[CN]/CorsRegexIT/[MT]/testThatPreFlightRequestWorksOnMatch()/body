{
  String corsValue="http://localhost:9200";
  try (Response response=getRestClient().performRequest("OPTIONS","/",new BasicHeader("User-Agent","Mozilla Bar"),new BasicHeader("Origin",corsValue),new BasicHeader("Access-Control-Request-Method","GET"))){
    assertResponseWithOriginheader(response,corsValue);
    assertNotNull(response.getHeader("Access-Control-Allow-Methods"));
  }
 }

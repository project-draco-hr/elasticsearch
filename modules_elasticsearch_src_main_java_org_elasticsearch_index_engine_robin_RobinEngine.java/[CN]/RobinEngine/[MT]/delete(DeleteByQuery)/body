{
  rwl.readLock().lock();
  try {
    indexWriter.deleteDocuments(delete.query());
    translog.add(new Translog.DeleteByQuery(delete));
    dirty=true;
  }
 catch (  IOException e) {
    throw new DeleteByQueryFailedEngineException(shardId,delete,e);
  }
 finally {
    rwl.readLock().unlock();
  }
}

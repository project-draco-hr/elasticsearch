{
  rwl.readLock().lock();
  try {
    indexWriter.updateDocument(index.uid(),index.doc(),index.analyzer());
    translog.add(new Translog.Index(index));
    dirty=true;
  }
 catch (  IOException e) {
    throw new IndexFailedEngineException(shardId,index,e);
  }
 finally {
    rwl.readLock().unlock();
  }
}

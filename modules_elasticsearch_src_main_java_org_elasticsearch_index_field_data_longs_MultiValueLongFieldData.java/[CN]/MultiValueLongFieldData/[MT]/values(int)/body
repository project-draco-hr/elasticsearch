{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] != 0) {
      length++;
    }
  }
  if (length == 0) {
    return EMPTY_LONG_ARRAY;
  }
  long[] longs;
  if (length < VALUE_CACHE_SIZE) {
    longs=valuesCache.get().get()[length];
  }
 else {
    longs=new long[length];
  }
  int i=0;
  for (  int[] ordinal : ordinals) {
    int loc=ordinal[docId];
    if (loc != 0) {
      longs[i++]=values[loc];
    }
  }
  return longs;
}

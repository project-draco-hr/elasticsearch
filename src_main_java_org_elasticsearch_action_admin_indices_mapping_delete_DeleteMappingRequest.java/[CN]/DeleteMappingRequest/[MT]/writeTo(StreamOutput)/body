{
  super.writeTo(out);
  if (indices == null) {
    out.writeVInt(0);
  }
 else {
    out.writeVInt(indices.length);
    for (    String index : indices) {
      out.writeString(index);
    }
  }
  if (type == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeString(type);
  }
}

{
  TermVectorsRequest termVectorsRequest=new TermVectorsRequest(index,type,id).selectedFields(fields).routing(routing).version(version).versionType(versionType).perFieldAnalyzer(perFieldAnalyzer).positions(false).offsets(false).payloads(false).fieldStatistics(false).termStatistics(false).dfs(false);
  if (doc != null) {
    termVectorsRequest.doc(doc,true);
    this.id(termVectorsRequest.id());
  }
  return termVectorsRequest;
}

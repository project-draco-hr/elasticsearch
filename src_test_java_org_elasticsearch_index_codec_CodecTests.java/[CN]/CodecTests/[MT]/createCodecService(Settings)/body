{
  Index index=new Index("test");
  Injector injector=new ModulesBuilder().add(new SettingsModule(settings)).add(new IndexNameModule(index)).add(new IndexSettingsModule(index,settings)).add(new SimilarityModule(settings)).add(new CodecModule(settings)).add(new MapperServiceModule()).add(new AnalysisModule(settings)).createInjector();
  return injector.getInstance(CodecService.class);
}

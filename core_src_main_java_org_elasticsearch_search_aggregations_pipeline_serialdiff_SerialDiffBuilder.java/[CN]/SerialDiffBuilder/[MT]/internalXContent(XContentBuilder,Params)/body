{
  if (format != null) {
    builder.field(SerialDiffParser.FORMAT.getPreferredName(),format);
  }
  if (gapPolicy != null) {
    builder.field(SerialDiffParser.GAP_POLICY.getPreferredName(),gapPolicy.getName());
  }
  if (lag != null) {
    builder.field(SerialDiffParser.LAG.getPreferredName(),lag);
  }
  return builder;
}

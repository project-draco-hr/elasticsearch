{
  entry.foundInDoc=true;
  entry.count++;
  int seek=valueValues.setDocument(docId);
  for (int i=0; i < seek; i++) {
    double value=valueValues.nextValue();
    entry.totalCount++;
    entry.total+=value;
    if (value < entry.min) {
      entry.min=value;
    }
    if (value > entry.max) {
      entry.max=value;
    }
  }
}

{
  entry.foundInDoc=true;
  entry.count++;
  DoubleValues.Iter iter=valueValues.getIter(docId);
  while (iter.hasNext()) {
    double value=iter.next();
    entry.totalCount++;
    entry.total+=value;
    if (value < entry.min) {
      entry.min=value;
    }
    if (value > entry.max) {
      entry.max=value;
    }
  }
}

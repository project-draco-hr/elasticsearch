{
  Object val=searchScript.execute(doc);
  if (val == null) {
    return false;
  }
  if (val instanceof Boolean) {
    return (Boolean)val;
  }
  if (val instanceof Number) {
    return ((Number)val).longValue() != 0;
  }
  throw new IOException("Can't handle type [" + val + "] in script filter");
}

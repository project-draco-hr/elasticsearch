{
  int[] docOrders=order[docId];
  if (docOrders == null) {
    return EMPTY_ARRAY;
  }
  GeoPoint[] points;
  if (docOrders.length < VALUE_CACHE_SIZE) {
    points=valuesCache.get().get()[docOrders.length];
  }
 else {
    points=new GeoPoint[docOrders.length];
  }
  for (int i=0; i < docOrders.length; i++) {
    points[i]=values[docOrders[i]];
  }
  return points;
}

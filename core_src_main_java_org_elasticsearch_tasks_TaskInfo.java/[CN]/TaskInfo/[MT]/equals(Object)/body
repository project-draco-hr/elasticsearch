{
  if (obj == null || obj.getClass() != TaskInfo.class) {
    return false;
  }
  TaskInfo other=(TaskInfo)obj;
  return Objects.equals(taskId,other.taskId) && Objects.equals(type,other.type) && Objects.equals(action,other.action)&& Objects.equals(description,other.description)&& Objects.equals(startTime,other.startTime)&& Objects.equals(runningTimeNanos,other.runningTimeNanos)&& Objects.equals(parentTaskId,other.parentTaskId)&& Objects.equals(cancellable,other.cancellable)&& Objects.equals(status,other.status);
}

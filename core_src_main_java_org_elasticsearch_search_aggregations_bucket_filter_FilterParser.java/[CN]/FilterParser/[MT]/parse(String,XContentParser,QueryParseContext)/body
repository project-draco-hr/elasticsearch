{
  QueryBuilder<?> filter=context.parseInnerQueryBuilder();
  if (filter == null) {
    throw new ParsingException(null,"filter cannot be null in filter aggregation [{}]",aggregationName);
  }
  FilterAggregator.FilterAggregatorBuilder factory=new FilterAggregator.FilterAggregatorBuilder(aggregationName,filter == null ? new MatchAllQueryBuilder() : filter);
  return factory;
}

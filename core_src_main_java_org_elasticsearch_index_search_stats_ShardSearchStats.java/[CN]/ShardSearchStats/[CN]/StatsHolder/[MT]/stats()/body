{
  return new SearchStats.Stats(queryMetric.count(),TimeUnit.NANOSECONDS.toMillis(queryMetric.sum()),queryCurrent.count(),fetchMetric.count(),TimeUnit.NANOSECONDS.toMillis(fetchMetric.sum()),fetchCurrent.count(),scrollMetric.count(),TimeUnit.NANOSECONDS.toMillis(scrollMetric.sum()),scrollCurrent.count());
}

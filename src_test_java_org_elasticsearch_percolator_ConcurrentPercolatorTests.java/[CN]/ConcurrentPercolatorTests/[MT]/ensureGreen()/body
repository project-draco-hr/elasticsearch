{
  ClusterHealthResponse actionGet=client().admin().cluster().health(Requests.clusterHealthRequest().waitForGreenStatus().waitForEvents(Priority.LANGUID).waitForRelocatingShards(0)).actionGet();
  assertThat(actionGet.isTimedOut(),equalTo(false));
  assertThat(actionGet.getStatus(),equalTo(ClusterHealthStatus.GREEN));
}

{
  for (  QueryBuilder<?> query : clauses) {
    Query luceneQuery=null;
switch (occurs) {
case MUST:
case SHOULD:
      luceneQuery=query.toQuery(context);
    break;
case FILTER:
case MUST_NOT:
  luceneQuery=query.toFilter(context);
break;
}
if (luceneQuery != null) {
booleanQueryBuilder.add(new BooleanClause(luceneQuery,occurs));
}
}
}

{
  if (storeValueType == null) {
    throw new IllegalStateException(error("Illegal tree structure."));
  }
  final String desc=Type.getMethodDescriptor(definition.voidType.type,definition.defType.type,index.actual.type,storeValueType.type);
  adapter.invokeDynamic("arrayStore",desc,DEF_BOOTSTRAP_HANDLE,DefBootstrap.ARRAY_STORE);
}

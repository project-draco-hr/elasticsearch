{
  builder.startObject(getName());
  builder.startObject(fieldName);
  builder.field(REFERNECE,reference);
  builder.field(SCALE,scale);
  if (scaleWeight > 0) {
    builder.field(SCALE_WEIGHT,scaleWeight);
  }
  if (offset != null) {
    builder.field(OFFSET,offset);
  }
  builder.endObject();
  builder.endObject();
  return builder;
}

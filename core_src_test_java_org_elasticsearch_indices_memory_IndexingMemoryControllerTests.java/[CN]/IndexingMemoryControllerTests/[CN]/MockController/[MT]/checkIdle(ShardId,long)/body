{
  Long ns=lastIndexTimeNanos.get(shardId);
  if (ns == null) {
    return null;
  }
 else   if (currentTimeInNanos() - ns >= inactiveTimeNS) {
    indexingBuffers.put(shardId,INACTIVE);
    translogBuffers.put(shardId,INACTIVE);
    activeShards.remove(shardId);
    return true;
  }
 else {
    return false;
  }
}

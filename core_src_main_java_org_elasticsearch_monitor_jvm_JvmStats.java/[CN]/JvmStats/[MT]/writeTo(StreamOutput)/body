{
  out.writeVLong(timestamp);
  out.writeVLong(uptime);
  mem.writeTo(out);
  threads.writeTo(out);
  gc.writeTo(out);
  if (bufferPools == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeVInt(bufferPools.size());
    for (    BufferPool bufferPool : bufferPools) {
      bufferPool.writeTo(out);
    }
  }
}

{
  if (collector instanceof XCollector) {
    ((XCollector)collector).postCollection();
  }
  if (currentContext != null) {
    if (currentHasDocs) {
      docSets.add(currentContext);
    }
 else {
      docSetCache.release(currentContext);
    }
    currentContext=null;
    currentSet=null;
    currentHasDocs=false;
  }
}

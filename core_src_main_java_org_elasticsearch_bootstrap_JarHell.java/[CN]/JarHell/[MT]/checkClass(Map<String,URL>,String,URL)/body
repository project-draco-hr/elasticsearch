{
  URL previous=clazzes.put(clazz,url);
  if (previous != null) {
    if (previous.equals(url)) {
      if (clazz.startsWith("org.apache.xmlbeans")) {
        return;
      }
      throw new IllegalStateException("jar hell!" + System.lineSeparator() + "class: "+ clazz+ System.lineSeparator()+ "exists multiple times in jar: "+ url.getPath()+ " !!!!!!!!!");
    }
 else {
      if (clazz.startsWith("org.apache.log4j")) {
        return;
      }
      if (clazz.equals("org.joda.time.base.BaseDateTime")) {
        return;
      }
      throw new IllegalStateException("jar hell!" + System.lineSeparator() + "class: "+ clazz+ System.lineSeparator()+ "jar1: "+ previous.getPath()+ System.lineSeparator()+ "jar2: "+ url.getPath());
    }
  }
}

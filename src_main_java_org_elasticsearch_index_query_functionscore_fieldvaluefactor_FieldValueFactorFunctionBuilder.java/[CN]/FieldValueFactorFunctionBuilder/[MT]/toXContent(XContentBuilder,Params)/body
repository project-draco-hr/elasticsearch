{
  builder.startObject(getName());
  if (field != null) {
    builder.field("field",field);
  }
  if (factor != null) {
    builder.field("factor",factor);
  }
  if (modifier != null) {
    builder.field("modifier",modifier.toString().toLowerCase(Locale.ROOT));
  }
  builder.endObject();
  return builder;
}

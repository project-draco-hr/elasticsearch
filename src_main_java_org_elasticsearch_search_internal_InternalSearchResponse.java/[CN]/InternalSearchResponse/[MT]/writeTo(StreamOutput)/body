{
  hits.writeTo(out);
  if (facets == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    facets.writeTo(out);
  }
  if (suggest == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    suggest.writeTo(out);
  }
  out.writeBoolean(timedOut);
}

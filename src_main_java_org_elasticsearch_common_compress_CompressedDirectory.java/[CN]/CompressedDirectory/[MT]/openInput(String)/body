{
  if (decompressExtensions.contains(getExtension(name))) {
    IndexInput in=dir.openInput(name);
    Compressor compressor1=CompressorFactory.compressor(in);
    if (compressor1 != null) {
      return compressor1.indexInput(in);
    }
 else {
      return in;
    }
  }
  return dir.openInput(name);
}

{
  StringBuilder sb=new StringBuilder();
  if (nodeName.length() > 0) {
    sb.append('[').append(nodeName).append(']');
  }
  if (nodeId != null) {
    sb.append('[').append(nodeId).append(']');
  }
  if (dataNode) {
    sb.append("[data]");
  }
  if (address != null) {
    sb.append('[').append(address).append(']');
  }
  return sb.toString();
}

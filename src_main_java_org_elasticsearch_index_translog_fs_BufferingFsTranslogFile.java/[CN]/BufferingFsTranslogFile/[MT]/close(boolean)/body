{
  if (!closed.compareAndSet(false,true)) {
    return;
  }
  try {
    if (!delete) {
      try {
        sync();
        translogStream.close();
      }
 catch (      Exception e) {
        throw new TranslogException(shardId,"failed to sync on close",e);
      }
    }
  }
  finally {
    raf.decreaseRefCount(delete);
  }
}

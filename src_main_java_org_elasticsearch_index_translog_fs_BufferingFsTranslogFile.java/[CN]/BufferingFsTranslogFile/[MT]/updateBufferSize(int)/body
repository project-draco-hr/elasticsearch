{
  rwl.writeLock().lock();
  try {
    if (this.buffer.length == bufferSize) {
      return;
    }
    flushBuffer();
    this.buffer=new byte[bufferSize];
  }
 catch (  IOException e) {
    throw new TranslogException(shardId,"failed to flush",e);
  }
 finally {
    rwl.writeLock().unlock();
  }
}

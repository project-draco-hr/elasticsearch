{
  index=in.readString();
  type=in.readString();
  id=in.readString();
  docVersion=in.readVLong();
  exists=in.readBoolean();
  artificial=in.readBoolean();
  tookInMillis=in.readVLong();
  if (in.readBoolean()) {
    headerRef=in.readBytesReference();
    termVectors=in.readBytesReference();
  }
}

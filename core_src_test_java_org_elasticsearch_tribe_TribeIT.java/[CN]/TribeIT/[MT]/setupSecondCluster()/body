      return null;
    }
  }
;
  cluster2=new InternalTestCluster(InternalTestCluster.configuredNodeMode(),randomLong(),createTempDir(),true,2,2,UUIDs.randomBase64UUID(random()),nodeConfigurationSource,0,false,SECOND_CLUSTER_NODE_PREFIX,Collections.emptyList(),Function.identity());

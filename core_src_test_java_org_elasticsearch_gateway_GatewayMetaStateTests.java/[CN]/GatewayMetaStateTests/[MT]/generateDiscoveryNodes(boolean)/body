{
  Set<DiscoveryNode.Role> dataOnlyRoles=Collections.singleton(DiscoveryNode.Role.DATA);
  return DiscoveryNodes.builder().put(newNode("node1",masterEligible ? MASTER_DATA_ROLES : dataOnlyRoles)).put(newNode("master_node",MASTER_DATA_ROLES)).localNodeId("node1").masterNodeId(masterEligible ? "node1" : "master_node");
}

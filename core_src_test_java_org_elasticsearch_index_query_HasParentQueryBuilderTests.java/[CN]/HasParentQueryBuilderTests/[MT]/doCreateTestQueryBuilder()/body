{
  InnerHitsBuilder.InnerHit innerHit=new InnerHitsBuilder.InnerHit().setSize(100).addSort(STRING_FIELD_NAME,SortOrder.ASC);
  return new HasParentQueryBuilder(PARENT_TYPE,RandomQueryBuilder.createQuery(random()),randomBoolean(),SearchContext.current() == null ? null : new QueryInnerHits("inner_hits_name",innerHit));
}

{
  QueryBuilder query=RandomQueryBuilder.createQuery(random());
  try {
    new HasParentQueryBuilder(null,query);
    fail("must not be null");
  }
 catch (  IllegalArgumentException ex) {
  }
  try {
    new HasParentQueryBuilder("foo",null);
    fail("must not be null");
  }
 catch (  IllegalArgumentException ex) {
  }
}

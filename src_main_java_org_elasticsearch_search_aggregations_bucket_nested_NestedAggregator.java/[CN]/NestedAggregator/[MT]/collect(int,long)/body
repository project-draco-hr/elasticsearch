{
  if (parentDoc == 0 || parentDocs == null) {
    return;
  }
  int prevParentDoc=parentDocs.prevSetBit(parentDoc - 1);
  int numChildren=0;
  for (int i=(parentDoc - 1); i > prevParentDoc; i--) {
    if (childDocs.get(i)) {
      ++numChildren;
      collectBucketNoCounts(i,bucketOrd);
    }
  }
  incrementBucketDocCount(bucketOrd,numChildren);
}

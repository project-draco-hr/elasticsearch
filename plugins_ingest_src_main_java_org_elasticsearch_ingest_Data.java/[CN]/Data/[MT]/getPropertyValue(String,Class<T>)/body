{
  Object property=get(path);
  if (property == null) {
    return null;
  }
  if (clazz.isInstance(property)) {
    return clazz.cast(property);
  }
  throw new IllegalArgumentException("field [" + path + "] of type ["+ property.getClass().getName()+ "] cannot be cast to ["+ clazz.getName()+ "]");
}

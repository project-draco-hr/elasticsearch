{
  if (randomPort == -1) {
    randomPort=randomIntBetween(49152,65525);
    randomPortRange=String.format(Locale.ROOT,"%s-%s",randomPort,randomPort + 10);
  }
  return settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put("network.host","127.0.0.1").put(TransportModule.TRANSPORT_TYPE_KEY,NettyTransport.class.getName()).put("node.mode","network").put("transport.profiles.client1.port",randomPortRange).put("transport.profiles.client1.publish_host","10.0.254.253").put("transport.profiles.client1.publish_port","4321").put("transport.profiles.client1.reuse_address",true).build();
}

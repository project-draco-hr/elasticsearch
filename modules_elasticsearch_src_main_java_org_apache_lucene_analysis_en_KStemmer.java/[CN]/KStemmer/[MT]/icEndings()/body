{
  if (endsIn('i','c')) {
    word.setLength(j + 3);
    word.append("al");
    k=j + 4;
    if (lookup())     return;
    word.setCharAt(j + 1,'y');
    word.setLength(j + 2);
    k=j + 1;
    if (lookup())     return;
    word.setCharAt(j + 1,'e');
    if (lookup())     return;
    word.setLength(j + 1);
    k=j;
    if (lookup())     return;
    word.append("ic");
    k=j + 2;
  }
  return;
}

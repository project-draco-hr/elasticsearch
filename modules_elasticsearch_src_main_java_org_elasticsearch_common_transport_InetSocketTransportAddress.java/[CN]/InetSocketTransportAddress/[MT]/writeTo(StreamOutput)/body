{
  if (address.getAddress() != null) {
    if (address.getAddress().getHostAddress() != null) {
      out.writeUTF(address.getAddress().getHostAddress());
    }
 else {
      out.writeUTF(address.getHostName());
    }
  }
 else {
    out.writeUTF(address.getHostName());
  }
  out.writeInt(address.getPort());
}

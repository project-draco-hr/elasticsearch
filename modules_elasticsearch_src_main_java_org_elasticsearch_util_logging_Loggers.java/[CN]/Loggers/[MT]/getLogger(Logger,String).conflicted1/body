{
  if (prefixes == null || prefixes.length == 0) {
    return logger;
  }
  StringBuilder sb=new StringBuilder();
  for (  String prefix : prefixes) {
    if (prefix != null) {
      sb.append("[").append(prefix).append("]");
    }
  }
  if (sb.length() == 0) {
    return logger;
  }
  sb.append(" ");
  return new PrefixLoggerAdapter(sb.toString(),logger);
}

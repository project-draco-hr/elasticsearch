{
  if (path.isEmpty()) {
    return this;
  }
 else   if (path.size() == 1 && "value".equals(path.get(0))) {
    return value();
  }
 else   if (path.size() == 1 && "keys".equals(path.get(0))) {
    return keys();
  }
 else {
    throw new ElasticsearchIllegalArgumentException("path not supported for [" + getName() + "]: "+ path);
  }
}

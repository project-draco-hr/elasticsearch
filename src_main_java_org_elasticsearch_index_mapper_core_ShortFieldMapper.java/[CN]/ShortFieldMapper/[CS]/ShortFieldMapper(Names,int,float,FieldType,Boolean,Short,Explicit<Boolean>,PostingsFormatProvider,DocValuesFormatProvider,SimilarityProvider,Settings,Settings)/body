{
  super(names,precisionStep,boost,fieldType,docValues,ignoreMalformed,new NamedAnalyzer("_short/" + precisionStep,new NumericIntegerAnalyzer(precisionStep)),new NamedAnalyzer("_short/max",new NumericIntegerAnalyzer(Integer.MAX_VALUE)),postingsProvider,docValuesProvider,similarity,fieldDataSettings,indexSettings);
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();
}

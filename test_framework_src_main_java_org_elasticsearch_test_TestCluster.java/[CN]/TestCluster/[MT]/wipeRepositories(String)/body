{
  if (size() > 0) {
    if (repositories.length == 0) {
      repositories=new String[]{"*"};
    }
    for (    String repository : repositories) {
      try {
        client().admin().cluster().prepareDeleteRepository(repository).execute().actionGet();
      }
 catch (      RepositoryMissingException ex) {
      }
    }
  }
}

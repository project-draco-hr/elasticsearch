{
  builder.startObject(ScriptFilterParser.NAME);
  builder.field("script",script);
  if (this.params != null) {
    builder.field("params",this.params);
  }
  if (this.lang != null) {
    builder.field("lang",lang);
  }
  if (filterName != null) {
    builder.field("_name",filterName);
  }
  builder.endObject();
}

{
  Settings settings=Settings.builder().put("path.home",createTempDir()).put("my.setting","test").put(IndexModule.STORE_TYPE,IndexModule.Type.SIMPLEFS.getSettingsKey()).build();
  PluginsService service=newPluginsService(settings,AdditionalSettingsPlugin1.class);
  Settings newSettings=service.updatedSettings();
  assertEquals("test",newSettings.get("my.setting"));
  assertEquals("1",newSettings.get("foo.bar"));
  assertEquals(IndexModule.Type.SIMPLEFS.getSettingsKey(),newSettings.get(IndexModule.STORE_TYPE));
}

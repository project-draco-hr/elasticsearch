{
  Settings settings=Settings.builder().put("path.home",createTempDir()).put("my.setting","test").put(IndexStoreModule.STORE_TYPE,IndexStoreModule.Type.SIMPLEFS.getSettingsKey()).putArray("plugin.types",AdditionalSettingsPlugin1.class.getName()).build();
  PluginsService service=new PluginsService(settings,new Environment(settings));
  Settings newSettings=service.updatedSettings();
  assertEquals("test",newSettings.get("my.setting"));
  assertEquals("1",newSettings.get("foo.bar"));
  assertEquals(IndexStoreModule.Type.SIMPLEFS.getSettingsKey(),newSettings.get(IndexStoreModule.STORE_TYPE));
}

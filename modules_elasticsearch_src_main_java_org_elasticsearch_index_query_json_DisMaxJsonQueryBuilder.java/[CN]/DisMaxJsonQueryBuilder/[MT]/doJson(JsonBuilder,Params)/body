{
  builder.startObject("disMax");
  if (tieBreaker != -1) {
    builder.field("tieBreaker",tieBreaker);
  }
  if (boost != -1) {
    builder.field("boost",boost);
  }
  builder.startArray("queries");
  for (  JsonQueryBuilder queryBuilder : queries) {
    queryBuilder.toJson(builder,params);
  }
  builder.endArray();
  builder.endObject();
}

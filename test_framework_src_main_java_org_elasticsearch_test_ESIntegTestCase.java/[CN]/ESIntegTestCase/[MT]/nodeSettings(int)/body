{
  Settings.Builder builder=settingsBuilder().put(DiskThresholdDecider.CLUSTER_ROUTING_ALLOCATION_LOW_DISK_WATERMARK_SETTING.getKey(),"1b").put(DiskThresholdDecider.CLUSTER_ROUTING_ALLOCATION_HIGH_DISK_WATERMARK_SETTING.getKey(),"1b").put("script.indexed","on").put("script.inline","on").put(IndicesStore.INDICES_STORE_DELETE_SHARD_TIMEOUT.getKey(),new TimeValue(1,TimeUnit.SECONDS));
  return builder.build();
}

{
  Settings.Builder builder=Settings.builder().put(DiskThresholdDecider.CLUSTER_ROUTING_ALLOCATION_LOW_DISK_WATERMARK_SETTING.getKey(),"1b").put(DiskThresholdDecider.CLUSTER_ROUTING_ALLOCATION_HIGH_DISK_WATERMARK_SETTING.getKey(),"1b").put("script.stored","true").put("script.inline","true").put(IndicesStore.INDICES_STORE_DELETE_SHARD_TIMEOUT.getKey(),new TimeValue(1,TimeUnit.SECONDS));

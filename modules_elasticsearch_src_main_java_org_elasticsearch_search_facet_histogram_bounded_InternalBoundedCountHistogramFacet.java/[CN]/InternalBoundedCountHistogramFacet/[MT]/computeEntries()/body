{
  if (entries != null) {
    return entries;
  }
  entries=new CountEntry[size];
  for (int i=0; i < size; i++) {
    entries[i]=new CountEntry((i * interval) + offset,counts[i]);
  }
  if (comparatorType != ComparatorType.KEY) {
    Arrays.sort(entries,comparatorType.comparator());
  }
  return entries;
}

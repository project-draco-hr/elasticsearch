{
  if (this == obj) {
    return true;
  }
  if ((obj == null) || (obj.getClass() != this.getClass())) {
    return false;
  }
  XTermsFilter test=(XTermsFilter)obj;
  if (filterTerms != test.filterTerms) {
    if (length == test.length) {
      for (int i=0; i < length; i++) {
        if (!filterTerms[i].equals(test.filterTerms[i])) {
          return false;
        }
      }
    }
 else {
      return false;
    }
  }
  return true;
}

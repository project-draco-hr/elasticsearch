{
  long bucket=bucket(value,interval);
  InternalFullHistogramFacet.FullEntry entry=entries.get(bucket);
  if (entry == null) {
    entry=new InternalFullHistogramFacet.FullEntry(bucket,1,1,value);
    entries.put(bucket,entry);
  }
 else {
    entry.count++;
    entry.totalCount++;
    entry.total+=value;
  }
}

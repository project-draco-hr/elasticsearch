{
  this.orderedTranslogs=orderedTranslogs;
  int ops=0;
  for (  Translog.Snapshot translog : orderedTranslogs) {
    final int tops=translog.estimatedTotalOperations();
    if (tops < 0) {
      ops=TranslogReader.UNKNOWN_OP_COUNT;
      break;
    }
    ops+=tops;
  }
  estimatedTotalOperations=ops;
  currentTranslog=0;
}

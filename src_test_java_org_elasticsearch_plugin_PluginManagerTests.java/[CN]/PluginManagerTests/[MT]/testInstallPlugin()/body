{
  PluginManager pluginManager=pluginManager("file://".concat(URI.create(PluginManagerTests.class.getResource("plugin_with_classfile.zip").toString()).getPath()));
  pluginManager.downloadAndExtract("plugin");
  File[] plugins=pluginManager.getListInstalledPlugins();
  assertThat(plugins,notNullValue());
  assertThat(plugins.length,is(1));
}

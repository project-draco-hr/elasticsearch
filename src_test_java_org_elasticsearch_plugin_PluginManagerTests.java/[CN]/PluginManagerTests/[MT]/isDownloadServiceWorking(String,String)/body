{
  HttpClient client=new HttpClient(url);
  try {
    if (client.request(resource).errorCode() != 200) {
      logger.warn("[{}{}] download service is not working. Disabling current test.",url,resource);
      return false;
    }
    return true;
  }
 catch (  Throwable t) {
    logger.warn("[{}{}] download service is not working. Disabling current test.",url,resource);
  }
  return false;
}

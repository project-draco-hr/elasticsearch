{
  return new NodeStats(discovery.localNode(),System.currentTimeMillis(),indicesService.stats(true),monitorService.osService().stats(),monitorService.processService().stats(),monitorService.jvmService().stats(),threadPool.stats(),monitorService.fsService().stats(),transportService.stats(),httpServer.stats(),circuitBreakerService.stats(),scriptService.stats(),discovery.stats(),ingestService.getPipelineExecutionService().stats());
}

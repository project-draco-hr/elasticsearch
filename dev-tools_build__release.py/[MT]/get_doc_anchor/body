def get_doc_anchor(release, esversion):
    plugin_digits = list(map(int, re.findall('\\d+', release)))
    es_digits = list(map(int, re.findall('\\d+', esversion)))
    return ('#version-%s%s%s-for-elasticsearch-%s%s' % (plugin_digits[0], plugin_digits[1], plugin_digits[2], es_digits[0], es_digits[1]))

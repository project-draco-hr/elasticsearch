{
  if (wrappers.size() > 1) {
    throw new IllegalStateException("wrapping of the index searcher by more than one wrappers is forbidden, found the following wrappers [" + wrappers + "]");
  }
  if (wrappers.isEmpty()) {
    this.wrapper=null;
  }
 else {
    this.wrapper=wrappers.iterator().next();
  }
}

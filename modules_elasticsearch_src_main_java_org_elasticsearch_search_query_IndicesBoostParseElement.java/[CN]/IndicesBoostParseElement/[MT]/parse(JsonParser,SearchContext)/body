{
  JsonToken token;
  while ((token=jp.nextToken()) != JsonToken.END_OBJECT) {
    if (token == JsonToken.FIELD_NAME) {
      String indexName=jp.getCurrentName();
      if (indexName.equals(context.shardTarget().index())) {
        jp.nextToken();
        context.queryBoost(jp.getFloatValue());
      }
    }
  }
}

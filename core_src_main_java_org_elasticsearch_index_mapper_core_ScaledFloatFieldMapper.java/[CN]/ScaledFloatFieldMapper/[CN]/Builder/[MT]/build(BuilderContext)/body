{
  if (scalingFactorSet == false) {
    throw new IllegalArgumentException("Field [" + name + "] misses required parameter [scaling_factor]");
  }
  setupFieldType(context);
  ScaledFloatFieldMapper fieldMapper=new ScaledFloatFieldMapper(name,fieldType,defaultFieldType,ignoreMalformed(context),coerce(context),context.indexSettings(),multiFieldsBuilder.build(this,context),copyTo);
  return (ScaledFloatFieldMapper)fieldMapper.includeInAll(includeInAll);
}

{
  if (parser.currentToken() == XContentParser.Token.START_ARRAY) {
    while (parser.nextToken() != XContentParser.Token.END_ARRAY) {
      context.addRescore(parseSingleRescoreContext(parser,context.indexShard().getQueryShardContext()));
    }
  }
 else {
    context.addRescore(parseSingleRescoreContext(parser,context.indexShard().getQueryShardContext()));
  }
}

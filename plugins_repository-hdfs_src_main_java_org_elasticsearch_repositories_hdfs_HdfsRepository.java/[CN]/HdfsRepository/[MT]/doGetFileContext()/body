{
  if (fc != null) {
    try {
      fc.util().exists(fc.getWorkingDirectory());
    }
 catch (    IOException ex) {
      if (ex.getMessage().contains("Filesystem closed")) {
        fc=null;
      }
 else {
        throw ex;
      }
    }
  }
  if (fc == null) {
    return initFileContext(repositorySettings);
  }
  return fc;
}

{
  String pluginName="fake-plugin";
  Path pluginDir=createTempDir().resolve(pluginName);
  String pluginUrl=createPlugin(pluginDir,"description","fake desc","version","1.0","elasticsearch.version",Version.CURRENT.toString(),"jvm","true","classname","FakePlugin");
  assertStatusOk(String.format(Locale.ROOT,"install %s --url %s --verbose",pluginName,pluginUrl));
  assertThatPluginIsListed(pluginName);
}

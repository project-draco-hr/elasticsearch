{
  JodaPatternDateParser parser=new JodaPatternDateParser("MMM dd HH:mm:ss Z",DateTimeZone.forOffsetHours(-8),Locale.ENGLISH);
  assertThat(Instant.ofEpochMilli(parser.parseDateTime("Nov 24 01:29:01 -0800").getMillis()).atZone(ZoneId.of("GMT-8")).format(DateTimeFormatter.ofPattern("MM dd HH:mm:ss",Locale.ENGLISH)),equalTo("11 24 01:29:01"));
}

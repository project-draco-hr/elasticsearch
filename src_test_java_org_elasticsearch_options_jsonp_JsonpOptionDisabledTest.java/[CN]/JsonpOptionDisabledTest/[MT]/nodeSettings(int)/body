{
  if (randomBoolean()) {
    logger.info("using default jsonp settings (should be false)");
    return ImmutableSettings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put(Node.HTTP_ENABLED,true).build();
  }
  return ImmutableSettings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put(Node.HTTP_ENABLED,true).put(RestController.HTTP_JSON_ENABLE,false).build();
}

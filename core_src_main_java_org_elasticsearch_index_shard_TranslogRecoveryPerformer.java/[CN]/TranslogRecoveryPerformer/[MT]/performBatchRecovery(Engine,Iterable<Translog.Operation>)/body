{
  int numOps=0;
  try {
    for (    Translog.Operation operation : operations) {
      performRecoveryOperation(engine,operation,false);
      numOps++;
    }
  }
 catch (  Throwable t) {
    throw new BatchOperationException(shardId,"failed to apply batch translog operation [" + t.getMessage() + "]",numOps,t);
  }
  return numOps;
}

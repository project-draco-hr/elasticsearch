{
  for (  Map.Entry<String,Filter> entry : namedFilters.entrySet()) {
    String name=entry.getKey();
    Filter filter=entry.getValue();
    try {
      DocIdSet docIdSet=filter.getDocIdSet(hitContext.readerContext(),null);
      if (docIdSet != null) {
        Bits docSet=DocIdSets.toSafeBits(hitContext.reader(),docIdSet);
        if (docSet.get(hitContext.docId())) {
          matchedFilters.add(name);
        }
      }
    }
 catch (    IOException e) {
    }
  }
}

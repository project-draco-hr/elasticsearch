{
  if (docs != null) {
    return docs;
  }
  DocsStatus docs=null;
  for (  ShardStatus shard : shards()) {
    if (!shard.shardRouting().primary()) {
      continue;
    }
    if (shard.docs() == null) {
      continue;
    }
    if (docs == null) {
      docs=new DocsStatus();
    }
    docs.numDocs+=shard.docs().numDocs();
    docs.maxDoc+=shard.docs().maxDoc();
    docs.deletedDocs+=shard.docs().deletedDocs();
  }
  this.docs=docs;
  return this.docs;
}

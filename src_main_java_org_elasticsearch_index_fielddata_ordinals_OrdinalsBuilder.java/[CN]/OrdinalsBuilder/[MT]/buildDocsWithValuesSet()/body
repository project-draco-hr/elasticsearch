{
  if (numDocsWithValue == maxDoc) {
    return null;
  }
  final FixedBitSet bitSet=new FixedBitSet(maxDoc);
  if (svOrds != null) {
    for (int docId=0; docId < maxDoc; docId++) {
      int ord=(int)svOrds.get(docId);
      if (ord != 0) {
        bitSet.set(docId);
      }
    }
  }
 else {
    for (int docId=0; docId < maxDoc; docId++) {
      if (mvOrds[docId] != 0) {
        bitSet.set(docId);
      }
    }
  }
  return bitSet;
}

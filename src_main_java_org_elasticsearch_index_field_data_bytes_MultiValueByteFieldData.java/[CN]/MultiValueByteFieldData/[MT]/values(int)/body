{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] == 0) {
      break;
    }
    length++;
  }
  if (length == 0) {
    return EMPTY_BYTE_ARRAY;
  }
  byte[] bytes;
  if (length < VALUE_CACHE_SIZE) {
    bytes=valuesCache.get().get()[length];
  }
 else {
    bytes=new byte[length];
  }
  for (int i=0; i < length; i++) {
    bytes[i]=values[ordinals[i][docId]];
  }
  return bytes;
}

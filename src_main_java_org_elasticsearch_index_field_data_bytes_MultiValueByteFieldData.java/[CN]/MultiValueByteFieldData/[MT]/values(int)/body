{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] != 0) {
      length++;
    }
  }
  if (length == 0) {
    return EMPTY_BYTE_ARRAY;
  }
  byte[] bytes;
  if (length < VALUE_CACHE_SIZE) {
    bytes=valuesCache.get().get()[length];
  }
 else {
    bytes=new byte[length];
  }
  int i=0;
  for (  int[] ordinal : ordinals) {
    int loc=ordinal[docId];
    if (loc != 0) {
      bytes[i++]=values[loc];
    }
  }
  return bytes;
}

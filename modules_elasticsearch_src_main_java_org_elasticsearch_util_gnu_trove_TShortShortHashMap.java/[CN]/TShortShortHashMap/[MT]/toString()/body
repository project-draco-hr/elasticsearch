{
  final StringBuilder buf=new StringBuilder("{");
  forEachEntry(new TShortShortProcedure(){
    private boolean first=true;
    public boolean execute(    short key,    short value){
      if (first)       first=false;
 else       buf.append(",");
      buf.append(key);
      buf.append("=");
      buf.append(value);
      return true;
    }
  }
);
  buf.append("}");
  return buf.toString();
}

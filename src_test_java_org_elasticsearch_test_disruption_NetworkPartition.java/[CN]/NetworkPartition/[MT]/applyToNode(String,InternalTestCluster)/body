{
  if (!autoExpand || nodesSideOne.contains(node) || nodesSideTwo.contains(node)) {
    return;
  }
  if (nodesSideOne.isEmpty()) {
    nodesSideOne.add(node);
  }
 else   if (nodesSideTwo.isEmpty()) {
    nodesSideTwo.add(node);
  }
 else   if (random.nextBoolean()) {
    nodesSideOne.add(node);
  }
 else {
    nodesSideTwo.add(node);
  }
}

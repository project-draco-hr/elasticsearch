{
  if (this == obj) {
    return true;
  }
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  @SuppressWarnings("unchecked") T other=(T)obj;
  return Objects.equals(name,other.name()) && Objects.equals(text,other.text()) && Objects.equals(prefix,other.prefix())&& Objects.equals(regex,other.regex())&& Objects.equals(fieldname,other.field())&& Objects.equals(analyzer,other.analyzer())&& Objects.equals(size,other.size())&& Objects.equals(shardSize,other.shardSize())&& doEquals(other);
}

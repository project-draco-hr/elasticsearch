{
  final UnassignedInfo unassignedInfo=new UnassignedInfo(UnassignedInfo.Reason.NEW_INDEX_RESTORED,"restore_source[" + restoreSource.snapshot().getRepository() + "/"+ restoreSource.snapshot().getSnapshotId().getName()+ "]");
  return initializeAsRestore(indexMetaData,restoreSource,ignoreShards,true,unassignedInfo);
}

{
  try {
    settings.getClassLoader().loadClass("org.apache.log4j.Logger");
    LogConfigurator.configure(settings);
  }
 catch (  ClassNotFoundException e) {
  }
catch (  NoClassDefFoundError e) {
  }
catch (  Exception e) {
    sysError("Failed to configure logging...",false);
    e.printStackTrace();
  }
}

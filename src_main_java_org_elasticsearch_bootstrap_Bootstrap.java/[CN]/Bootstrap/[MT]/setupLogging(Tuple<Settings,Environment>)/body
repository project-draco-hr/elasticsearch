{
  try {
    tuple.v1().getClassLoader().loadClass("org.apache.log4j.Logger");
    LogConfigurator.configure(tuple.v1());
  }
 catch (  ClassNotFoundException e) {
  }
catch (  NoClassDefFoundError e) {
  }
catch (  Exception e) {
    sysError("Failed to configure logging...",false);
    e.printStackTrace();
  }
}

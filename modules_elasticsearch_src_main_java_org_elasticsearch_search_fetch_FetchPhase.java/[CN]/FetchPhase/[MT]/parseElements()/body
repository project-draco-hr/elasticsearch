{
  ImmutableMap.Builder<String,SearchParseElement> parseElements=ImmutableMap.builder();
  parseElements.put("explain",new ExplainParseElement()).put("fields",new FieldsParseElement()).put("script_fields",new ScriptFieldsParseElement()).put("scriptFields",new ScriptFieldsParseElement()).putAll(highlightPhase.parseElements());
  return parseElements.build();
}

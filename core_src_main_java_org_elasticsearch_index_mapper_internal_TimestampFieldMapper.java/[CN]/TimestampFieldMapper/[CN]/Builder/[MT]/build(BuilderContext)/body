{
  if (explicitStore == false && context.indexCreatedVersion().before(Version.V_2_0_0)) {
    fieldType.setStored(false);
  }
  setupFieldType(context);
  return new TimestampFieldMapper(fieldType,docValues,enabledState,path,defaultTimestamp,ignoreMissing,ignoreMalformed(context),coerce(context),fieldDataSettings,context.indexSettings());
}

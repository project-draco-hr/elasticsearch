{
  if (field != null) {
    builder.field("field",field);
  }
  if (script != null) {
    builder.field("script",script);
  }
  if (scriptLang != null) {
    builder.field("script_lang",scriptLang);
  }
  if (this.params != null && !this.params.isEmpty()) {
    builder.field("params").map(this.params);
  }
}

{
  Object[] items;
  int n, len;
  if ((items=array) != null && (len=items.length) > 0) {
    if (origin < 0)     origin=0;
    if ((n=count) > len)     n=len;
    int fence=bound < 0 || bound > n ? n : bound;
    int i=(origin < 0) ? 0 : origin;
    if (i != fence)     return Arrays.copyOfRange(items,i,fence,Object[].class);
  }
  return new Object[0];
}

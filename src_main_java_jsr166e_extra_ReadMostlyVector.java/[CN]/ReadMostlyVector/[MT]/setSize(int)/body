{
  if (newSize < 0)   throw new ArrayIndexOutOfBoundsException(newSize);
  final SequenceLock lock=this.lock;
  lock.lock();
  try {
    int n=count;
    if (newSize > n)     grow(newSize);
 else {
      Object[] items=array;
      for (int i=newSize; i < n; i++)       items[i]=null;
    }
    count=newSize;
  }
  finally {
    lock.unlock();
  }
}

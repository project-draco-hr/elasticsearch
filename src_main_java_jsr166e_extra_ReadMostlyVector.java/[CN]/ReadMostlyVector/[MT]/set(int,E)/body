{
  E oldValue=null;
  boolean oobe=false;
  final StampedLock lock=this.lock;
  long stamp=lock.writeLock();
  try {
    Object[] items=array;
    if (items == null || index < 0 || index >= count)     oobe=true;
 else {
      oldValue=(E)items[index];
      items[index]=element;
    }
  }
  finally {
    lock.unlockWrite(stamp);
  }
  if (oobe)   throw new ArrayIndexOutOfBoundsException(index);
  return oldValue;
}

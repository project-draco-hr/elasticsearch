{
  final SequenceLock lock=this.lock;
  for (; ; ) {
    long seq=lock.awaitAvailability();
    Object[] items=array;
    int n=count;
    @SuppressWarnings("unchecked") E e=(items.length > 0) ? (E)items[0] : null;
    if (lock.getSequence() == seq) {
      if (n <= 0)       throw new NoSuchElementException();
      return e;
    }
  }
}

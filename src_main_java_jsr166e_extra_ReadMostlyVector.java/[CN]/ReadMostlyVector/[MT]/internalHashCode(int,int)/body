{
  int hash=1;
  Object[] items;
  int n, len;
  if ((items=array) != null && (len=items.length) > 0) {
    if (origin < 0)     origin=0;
    if ((n=count) > len)     n=len;
    int fence=bound < 0 || bound > n ? n : bound;
    for (int i=origin; i < fence; ++i) {
      Object e=items[i];
      hash=31 * hash + (e == null ? 0 : e.hashCode());
    }
  }
  return hash;
}

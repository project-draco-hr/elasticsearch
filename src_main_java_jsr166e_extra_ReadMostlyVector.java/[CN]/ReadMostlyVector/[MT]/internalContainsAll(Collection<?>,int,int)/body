{
  Object[] items;
  int n, len;
  if ((items=array) != null && (len=items.length) > 0) {
    if (origin < 0)     origin=0;
    if ((n=count) > len)     n=len;
    int fence=bound < 0 || bound > n ? n : bound;
    for (    Object e : c) {
      if (findFirstIndex(items,e,origin,fence) < 0)       return false;
    }
  }
 else   if (!c.isEmpty())   return false;
  return true;
}

{
  int n=count;
  Object[] items=array;
  if (index < 0 || index > n)   throw new ArrayIndexOutOfBoundsException(index);
  int len=elements.length;
  if (len == 0)   return false;
  int newCount=n + len;
  if (newCount >= items.length)   items=grow(newCount);
  int mv=n - index;
  if (mv > 0)   System.arraycopy(items,index,items,index + len,mv);
  System.arraycopy(elements,0,items,index,len);
  count=newCount;
  return true;
}

{
  Object[] a=null;
  int n;
  final StampedLock lock=this.lock;
  long stamp=lock.readLock();
  try {
    Object[] items=array;
    if (items == null)     n=0;
 else {
      int len=items.length;
      if ((n=count) > len)       n=len;
      a=Arrays.copyOf(items,n);
    }
  }
  finally {
    lock.unlockRead(stamp);
  }
  return new ReadMostlyVector<>(a,n,capacityIncrement);
}

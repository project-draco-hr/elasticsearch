{
  boolean oobe=false;
  E e=null;
  final StampedLock lock=this.lock;
  long stamp=lock.readLock();
  try {
    Object[] items;
    if ((items=array) != null && index < items.length && index < count && index >= 0)     e=(E)items[index];
 else     oobe=true;
  }
  finally {
    lock.unlockRead(stamp);
  }
  if (oobe)   throw new ArrayIndexOutOfBoundsException(index);
  return e;
}

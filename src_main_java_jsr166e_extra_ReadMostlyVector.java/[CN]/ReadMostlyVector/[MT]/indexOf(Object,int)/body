{
  final SequenceLock lock=this.lock;
  long seq=lock.awaitAvailability();
  Object[] items=array;
  int n=count;
  int idx=-1;
  if (n <= items.length)   idx=validatedIndexOf(o,items,index,n,seq);
  if (lock.getSequence() != seq) {
    lock.lock();
    try {
      idx=rawIndexOf(o,index,count);
    }
  finally {
      lock.unlock();
    }
  }
  return idx;
}

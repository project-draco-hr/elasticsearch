{
  int len;
  if (items != null && (len=items.length) > 0) {
    int last=(index < len) ? index : len - 1;
    int start=(origin < 0) ? 0 : origin;
    for (int i=last; i >= start; --i) {
      Object e=items[i];
      if ((x == null) ? e == null : x.equals(e))       return i;
    }
  }
  return -1;
}

{
  if (minCapacity > 0) {
    final StampedLock lock=this.lock;
    long stamp=lock.writeLock();
    try {
      Object[] items=array;
      int cap=(items == null) ? 0 : items.length;
      if (minCapacity - cap > 0)       grow(minCapacity);
    }
  finally {
      lock.unlockWrite(stamp);
    }
  }
}

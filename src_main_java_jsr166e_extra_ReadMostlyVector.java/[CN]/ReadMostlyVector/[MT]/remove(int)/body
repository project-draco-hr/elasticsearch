{
  E oldValue=null;
  boolean oobe=false;
  final StampedLock lock=this.lock;
  long stamp=lock.writeLock();
  try {
    if (index < 0 || index >= count)     oobe=true;
 else {
      oldValue=(E)array[index];
      rawRemoveAt(index);
    }
  }
  finally {
    lock.unlockWrite(stamp);
  }
  if (oobe)   throw new ArrayIndexOutOfBoundsException(index);
  return oldValue;
}

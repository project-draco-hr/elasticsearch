{
  final SequenceLock lock=this.lock;
  lock.lock();
  try {
    if (index < 0 || index >= count)     throw new ArrayIndexOutOfBoundsException(index);
    @SuppressWarnings("unchecked") E oldValue=(E)array[index];
    rawRemoveAt(index);
    return oldValue;
  }
  finally {
    lock.unlock();
  }
}

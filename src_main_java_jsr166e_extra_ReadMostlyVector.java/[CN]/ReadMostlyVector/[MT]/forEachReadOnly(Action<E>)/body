{
  final StampedLock lock=this.lock;
  long stamp=lock.readLock();
  try {
    Object[] items;
    int len, n;
    if ((items=array) != null && (len=items.length) > 0 && (n=count) <= len) {
      for (int i=0; i < n; ++i) {
        @SuppressWarnings("unchecked") E e=(E)items[i];
        action.apply(e);
      }
    }
  }
  finally {
    lock.unlockRead(stamp);
  }
}

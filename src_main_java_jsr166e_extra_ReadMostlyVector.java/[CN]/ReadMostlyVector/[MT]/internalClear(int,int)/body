{
  int n=count;
  int fence=bound < 0 || bound > n ? n : bound;
  if (origin >= 0 && origin < fence) {
    Object[] items=array;
    int removed=fence - origin;
    int newCount=n - removed;
    int mv=n - (origin + removed);
    if (mv > 0)     System.arraycopy(items,origin + removed,items,origin,mv);
    for (int i=n; i < newCount; ++i)     items[i]=null;
    count=newCount;
  }
}

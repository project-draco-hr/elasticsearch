{
  final SequenceLock lock=list.lock;
  lock.lock();
  try {
    Object[] items=list.array;
    int i=index + offset;
    if (index < 0 || index >= size || i >= items.length)     throw new ArrayIndexOutOfBoundsException(index);
    @SuppressWarnings("unchecked") E result=(E)items[i];
    list.rawRemoveAt(i);
    size--;
    return result;
  }
  finally {
    lock.unlock();
  }
}

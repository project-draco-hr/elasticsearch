{
  final SequenceLock lock=list.lock;
  long seq=lock.awaitAvailability();
  Object[] items=list.array;
  int c=list.count;
  if (c <= items.length) {
    int idx=list.validatedLastIndexOf(o,items,offset + size - 1,offset,seq);
    if (lock.getSequence() == seq)     return idx < 0 ? -1 : idx - offset;
  }
  lock.lock();
  try {
    int idx=list.rawLastIndexOf(o,offset + size - 1,offset);
    return idx < 0 ? -1 : idx - offset;
  }
  finally {
    lock.unlock();
  }
}

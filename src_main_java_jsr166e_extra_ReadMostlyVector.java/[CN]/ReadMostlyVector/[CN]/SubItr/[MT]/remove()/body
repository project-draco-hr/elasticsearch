{
  int i=lastRet;
  if (i < 0)   throw new IllegalStateException();
  if ((seq=lock.tryConvertToWriteLock(seq)) == 0)   throw new ConcurrentModificationException();
  try {
    list.rawRemoveAt(i);
    fence=origin + sublist.size;
    cursor=i;
    lastRet=-1;
  }
  finally {
    seq=lock.tryConvertToOptimisticRead(seq);
  }
}

{
  sbuf.append('[');
  if (!seenMap.containsKey(a)) {
    seenMap.put(a,null);
    final int len=a.length;
    for (int i=0; i < len; i++) {
      deeplyAppendParameter(sbuf,a[i],seenMap);
      if (i != len - 1)       sbuf.append(", ");
    }
    seenMap.remove(a);
  }
 else {
    sbuf.append("...");
  }
  sbuf.append(']');
}

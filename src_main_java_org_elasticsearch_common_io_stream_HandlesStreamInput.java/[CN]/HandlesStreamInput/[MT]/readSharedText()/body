{
  byte b=in.readByte();
  if (b == 0) {
    int handle=in.readVInt();
    Text s=in.readText();
    handlesText.put(handle,s);
    return s;
  }
 else   if (b == 1) {
    return handlesText.get(in.readVInt());
  }
 else   if (b == 2) {
    return in.readText();
  }
 else {
    throw new IOException("Expected handle header, got [" + b + "]");
  }
}

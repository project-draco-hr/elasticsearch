{
  Settings settings=ImmutableSettings.settingsBuilder().put("xxx.tag","A").put("xxx.group","B").build();
  DiscoveryNodeFilters filters=DiscoveryNodeFilters.buildFromSettings(AND,"xxx.",settings);
  DiscoveryNode node=new DiscoveryNode("name1","id1",DummyTransportAddress.INSTANCE,ImmutableMap.<String,String>of("tag","A","group","B"));
  assertThat(filters.match(node),equalTo(true));
  node=new DiscoveryNode("name2","id2",DummyTransportAddress.INSTANCE,ImmutableMap.<String,String>of("tag","A","group","B","name","X"));
  assertThat(filters.match(node),equalTo(true));
  node=new DiscoveryNode("name3","id3",DummyTransportAddress.INSTANCE,ImmutableMap.<String,String>of("tag","A","group","F","name","X"));
  assertThat(filters.match(node),equalTo(false));
  node=new DiscoveryNode("name4","id4",DummyTransportAddress.INSTANCE,ImmutableMap.<String,String>of());
  assertThat(filters.match(node),equalTo(false));
}

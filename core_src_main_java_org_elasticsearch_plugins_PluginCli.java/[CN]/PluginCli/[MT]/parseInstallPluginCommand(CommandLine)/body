{
  String[] args=cli.getArgs();
  if (args.length != 1) {
    return exitCmd(ExitStatus.USAGE,terminal,"Must supply a single plugin id argument");
  }
  boolean batch=System.console() == null;
  if (cli.hasOption("b")) {
    batch=true;
  }
  return new InstallPluginCommand(terminal,args[0],batch);
}

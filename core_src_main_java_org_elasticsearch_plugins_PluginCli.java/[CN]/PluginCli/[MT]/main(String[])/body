{
  String loggerLevel=System.getProperty("es.logger.level","INFO");
  String pathHome=System.getProperty("es.path.home");
  Environment loggingEnvironment=InternalSettingsPreparer.prepareEnvironment(Settings.builder().put("path.home",pathHome).put("appender.terminal.type","terminal").put("rootLogger","${logger.level}, terminal").put("logger.level",loggerLevel).build(),Terminal.DEFAULT);
  LogConfigurator.configure(loggingEnvironment.settings(),false);
  exit(new PluginCli().main(args,Terminal.DEFAULT));
}

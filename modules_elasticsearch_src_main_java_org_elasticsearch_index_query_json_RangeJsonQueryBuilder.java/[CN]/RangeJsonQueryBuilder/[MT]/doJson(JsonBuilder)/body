{
  builder.startObject(RangeJsonQueryParser.NAME);
  builder.startObject(name);
  builder.field("from",from);
  builder.field("to",to);
  builder.field("includeLower",includeLower);
  builder.field("includeUpper",includeUpper);
  if (boost != -1) {
    builder.field("boost",boost);
  }
  builder.endObject();
  builder.endObject();
}

{
  return settingsBuilder().put(DiskThresholdDecider.CLUSTER_ROUTING_ALLOCATION_LOW_DISK_WATERMARK,"1b").put(DiskThresholdDecider.CLUSTER_ROUTING_ALLOCATION_HIGH_DISK_WATERMARK,"1b").put(NodeEnvironment.SETTING_CUSTOM_DATA_PATH_ENABLED,true).put("script.indexed","on").put("script.inline","on").put(IndicesStore.INDICES_STORE_DELETE_SHARD_TIMEOUT,new TimeValue(1,TimeUnit.SECONDS)).build();
}

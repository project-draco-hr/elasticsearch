{
  int numDocs=inner.setDocument(docId);
  buffer=ArrayUtil.grow(buffer,numDocs);
  bufferSlot=0;
  int numAcceptedOrds=0;
  for (int slot=0; slot < numDocs; slot++) {
    long ord=inner.nextOrd();
    if (accepted.get(ord)) {
      buffer[numAcceptedOrds]=ord;
      numAcceptedOrds++;
    }
  }
  return numAcceptedOrds;
}

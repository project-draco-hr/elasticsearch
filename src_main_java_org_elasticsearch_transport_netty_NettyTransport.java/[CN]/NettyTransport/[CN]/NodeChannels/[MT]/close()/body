{
  List<ChannelFuture> futures=new ArrayList<>();
  for (  Channel channel : allChannels) {
    try {
      if (channel != null && channel.isOpen()) {
        futures.add(channel.close());
      }
    }
 catch (    Exception e) {
    }
  }
  for (  ChannelFuture future : futures) {
    future.awaitUninterruptibly();
  }
}

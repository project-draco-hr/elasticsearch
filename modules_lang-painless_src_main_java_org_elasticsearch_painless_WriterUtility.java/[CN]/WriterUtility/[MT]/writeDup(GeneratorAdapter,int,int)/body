{
  if (size == 1) {
    if (xsize == 2) {
      adapter.dupX2();
    }
 else     if (xsize == 1) {
      adapter.dupX1();
    }
 else {
      adapter.dup();
    }
  }
 else   if (size == 2) {
    if (xsize == 2) {
      adapter.dup2X2();
    }
 else     if (xsize == 1) {
      adapter.dup2X1();
    }
 else {
      adapter.dup2();
    }
  }
}

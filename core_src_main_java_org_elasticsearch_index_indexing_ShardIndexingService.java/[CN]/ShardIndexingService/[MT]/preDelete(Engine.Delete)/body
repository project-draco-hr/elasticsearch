{
  totalStats.deleteCurrent.inc();
  typeStats(delete.type()).deleteCurrent.inc();
  for (  IndexingOperationListener listener : listeners) {
    try {
      delete=listener.preDelete(delete);
    }
 catch (    Exception e) {
      logger.warn("preDelete listener [{}] failed",e,listener);
    }
  }
  return delete;
}

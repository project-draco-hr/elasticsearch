{
  totalStats.indexCurrent.inc();
  typeStats(create.type()).indexCurrent.inc();
  for (  IndexingOperationListener listener : listeners) {
    try {
      create=listener.preCreate(create);
    }
 catch (    Exception e) {
      logger.warn("preCreate listener [{}] failed",e,listener);
    }
  }
  return create;
}

{
  QuerySearchResult queryResult=searchService.executeQueryPhase(searchRequest(searchSource().query(wildcardQuery("name","te*")).facets(facets().facet("age2",termQuery("age",2)).facet("age1",termQuery("age",1)))));
  assertThat(queryResult.facets().countFacet("age2").count(),equalTo(4l));
  assertThat(queryResult.facets().countFacet("age1").count(),equalTo(1l));
}

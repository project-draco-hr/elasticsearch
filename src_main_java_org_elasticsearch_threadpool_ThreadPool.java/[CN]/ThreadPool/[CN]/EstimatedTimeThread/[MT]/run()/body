{
  while (running) {
    estimatedTimeInMillis=System.currentTimeMillis();
    try {
      Thread.sleep(interval);
    }
 catch (    InterruptedException e) {
      running=false;
      return;
    }
    try {
      FileSystemUtils.checkMkdirsStall(estimatedTimeInMillis);
    }
 catch (    Exception e) {
    }
  }
}

{
  out.writeUTF(name);
  out.writeUTF(type);
  out.writeInt(min);
  out.writeInt(max);
  if (keepAlive == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    keepAlive.writeTo(out);
  }
  if (capacity == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    capacity.writeTo(out);
  }
}

{
  FloatValues.Iter iter=delegate.getIter(docId);
  if (!iter.hasNext()) {
    return missing;
  }
  float currentVal=iter.next();
  float relevantVal=currentVal;
  while (true) {
    int cmp=Float.compare(currentVal,relevantVal);
    if (reversed) {
      if (cmp > 0) {
        relevantVal=currentVal;
      }
    }
 else {
      if (cmp < 0) {
        relevantVal=currentVal;
      }
    }
    if (!iter.hasNext()) {
      break;
    }
    currentVal=iter.next();
  }
  return relevantVal;
}

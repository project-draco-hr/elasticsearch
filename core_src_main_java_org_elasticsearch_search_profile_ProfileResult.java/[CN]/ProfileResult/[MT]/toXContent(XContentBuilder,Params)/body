{
  builder=builder.startObject().field(QUERY_TYPE.getPreferredName(),queryType).field(LUCENE_DESCRIPTION.getPreferredName(),luceneDescription).field(NODE_TIME.getPreferredName(),String.format(Locale.US,"%.10gms",(double)(getTime() / 1000000.0))).field(BREAKDOWN.getPreferredName(),timings);
  if (!children.isEmpty()) {
    builder=builder.startArray(CHILDREN.getPreferredName());
    for (    ProfileResult child : children) {
      builder=child.toXContent(builder,params);
    }
    builder=builder.endArray();
  }
  builder=builder.endObject();
  return builder;
}

{
  super.readFrom(in);
  minScore=in.readFloat();
  querySource=new byte[in.readInt()];
  in.readFully(querySource);
  if (in.readBoolean()) {
    queryParserName=in.readUTF();
  }
  int typesSize=in.readInt();
  if (typesSize > 0) {
    types=new String[typesSize];
    for (int i=0; i < typesSize; i++) {
      types[i]=in.readUTF();
    }
  }
}

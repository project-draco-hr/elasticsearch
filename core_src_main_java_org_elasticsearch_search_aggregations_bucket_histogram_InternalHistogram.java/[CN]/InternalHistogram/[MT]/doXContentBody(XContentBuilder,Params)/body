{
  if (keyed) {
    builder.startObject(CommonFields.BUCKETS);
  }
 else {
    builder.startArray(CommonFields.BUCKETS);
  }
  for (  B bucket : buckets) {
    bucket.toXContent(builder,params);
  }
  if (keyed) {
    builder.endObject();
  }
 else {
    builder.endArray();
  }
  return builder;
}

{
  super(names,precisionStep,index,store,boost,omitNorms,omitTermFreqAndPositions,new NamedAnalyzer("_double/" + precisionStep,new NumericDoubleAnalyzer(precisionStep)),new NamedAnalyzer("_double/max",new NumericDoubleAnalyzer(Integer.MAX_VALUE)));
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();
}

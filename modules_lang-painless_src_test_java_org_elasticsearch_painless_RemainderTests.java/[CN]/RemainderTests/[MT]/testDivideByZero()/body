{
  try {
    exec("int x = 1; int y = 0; return x % y;");
    fail("should have hit exception");
  }
 catch (  ArithmeticException expected) {
  }
  try {
    exec("long x = 1L; long y = 0L; return x % y;");
    fail("should have hit exception");
  }
 catch (  ArithmeticException expected) {
  }
}

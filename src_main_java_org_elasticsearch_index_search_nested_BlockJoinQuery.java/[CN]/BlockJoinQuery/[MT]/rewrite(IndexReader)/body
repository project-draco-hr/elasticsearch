{
  final Query childRewrite=childQuery.rewrite(reader);
  if (childRewrite != childQuery) {
    Query rewritten=new BlockJoinQuery(childQuery,childRewrite,parentsFilter,scoreMode).setCollector(childCollector);
    rewritten.setBoost(getBoost());
    return rewritten;
  }
 else {
    return this;
  }
}

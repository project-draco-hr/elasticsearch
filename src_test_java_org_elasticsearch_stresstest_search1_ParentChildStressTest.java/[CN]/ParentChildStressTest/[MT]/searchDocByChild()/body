{
  String dslString="{\"query\":{" + "\"has_child\":{" + "\"query\":{"+ "\"field\":{"+ "\"description\":\"child test\"}},"+ "\"type\":\"contentFiles\"}}}";
  int numTries=0;
  List<String> items=new ArrayList<String>();
  while (items.size() != 1 && numTries < 20) {
    items=executeSearch(dslString);
    numTries++;
    if (items.size() != 1) {
      Thread.sleep(250);
    }
  }
  if (items.size() != 1) {
    System.out.println("Exceeded number of retries");
    System.exit(1);
  }
}

{
  InnerHitsBuilder.InnerHit innerHit=new InnerHitsBuilder.InnerHit().setSize(100).addSort(STRING_FIELD_NAME,SortOrder.ASC);
  return new NestedQueryBuilder("nested1",RandomQueryBuilder.createQuery(random()),RandomPicks.randomFrom(random(),ScoreMode.values()),SearchContext.current() == null ? null : new QueryInnerHits("inner_hits_name",innerHit));
}

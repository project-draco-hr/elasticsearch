{
  ImmutableSettings.Builder nodeSettings=ImmutableSettings.settingsBuilder().put("cloud.gce.api.impl",mock).put("cloud.gce.refresh_interval","5s");
  if (filteredTags != null) {
    nodeSettings.put("discovery.gce.tags",filteredTags);
  }
 else {
    nodeSettings.put("discovery.gce.tags","");
  }
  cluster().startNode(nodeSettings);
}

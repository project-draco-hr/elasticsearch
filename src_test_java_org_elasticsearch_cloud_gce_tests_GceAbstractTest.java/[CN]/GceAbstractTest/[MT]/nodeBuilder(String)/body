{
  ImmutableSettings.Builder builder=ImmutableSettings.settingsBuilder().put("path.data","./target/es/data").put("path.logs","./target/es/logs").put("path.work","./target/es/work").put("cloud.gce.api.impl",mock).put("node.name",(nodes.size() + 1) + "#" + mock.getSimpleName());
  if (filteredTags != null) {
    builder.put("discovery.gce.tags",filteredTags);
  }
 else {
    builder.put("discovery.gce.tags","");
  }
  Node node=NodeBuilder.nodeBuilder().settings(builder).node();
  nodes.add(node);
}

{
  boolean modified=false;
  byte[] states=_states;
  int[] keys=_set;
  int[] values=_values;
  tempDisableAutoCompaction();
  try {
    for (int i=keys.length; i-- > 0; ) {
      if (states[i] == FULL && !procedure.execute(keys[i],values[i])) {
        removeAt(i);
        modified=true;
      }
    }
  }
  finally {
    reenableAutoCompaction(true);
  }
  return modified;
}

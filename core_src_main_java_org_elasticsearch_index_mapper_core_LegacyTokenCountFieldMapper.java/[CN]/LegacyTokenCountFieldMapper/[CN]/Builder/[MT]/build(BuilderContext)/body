{
  if (context.indexCreatedVersion().onOrAfter(Version.V_5_0_0_alpha2)) {
    throw new IllegalStateException("Cannot use legacy numeric types after 5.0");
  }
  setupFieldType(context);
  LegacyTokenCountFieldMapper fieldMapper=new LegacyTokenCountFieldMapper(name,fieldType,defaultFieldType,ignoreMalformed(context),coerce(context),context.indexSettings(),analyzer,multiFieldsBuilder.build(this,context),copyTo);
  return (LegacyTokenCountFieldMapper)fieldMapper.includeInAll(includeInAll);
}

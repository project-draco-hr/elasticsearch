{
  shardId.writeTo(out);
  out.writeUTF(name);
  out.writeVLong(position);
  out.writeVLong(length);
  if (checksum == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(checksum);
  }
  out.writeVInt(contentLength);
  out.writeBytes(content,0,contentLength);
}

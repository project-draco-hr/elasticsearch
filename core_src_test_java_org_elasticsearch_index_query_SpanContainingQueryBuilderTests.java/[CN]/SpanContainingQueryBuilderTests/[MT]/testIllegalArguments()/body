{
  try {
    new SpanContainingQueryBuilder(null,new SpanTermQueryBuilder("field","value"));
    fail("cannot be null");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new SpanContainingQueryBuilder(new SpanTermQueryBuilder("field","value"),null);
    fail("cannot be null");
  }
 catch (  IllegalArgumentException e) {
  }
}

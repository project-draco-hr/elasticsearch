{
  long delay=shard.unassignedInfo().getLastComputedLeftDelayNanos();
  if (delay > 0) {
    logger.debug("[{}][{}]: delaying allocation of [{}] for [{}]",shard.index(),shard.id(),shard,TimeValue.timeValueNanos(delay));
    unassignedIterator.removeAndIgnore();
    return true;
  }
  return false;
}

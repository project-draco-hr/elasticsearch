{
  if (objectName == null) {
    builder.startObject();
  }
 else {
    builder.startObject(objectName);
  }
  builder.field("index",command.shardId().index().name());
  builder.field("shard",command.shardId().id());
  builder.field("from_node",command.fromNode());
  builder.field("to_node",command.toNode());
  builder.endObject();
}

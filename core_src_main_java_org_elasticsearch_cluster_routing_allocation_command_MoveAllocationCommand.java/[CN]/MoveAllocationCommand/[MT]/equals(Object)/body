{
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  MoveAllocationCommand other=(MoveAllocationCommand)obj;
  return Objects.equals(index,other.index) && Objects.equals(shardId,other.shardId) && Objects.equals(fromNode,other.fromNode)&& Objects.equals(toNode,other.toNode);
}

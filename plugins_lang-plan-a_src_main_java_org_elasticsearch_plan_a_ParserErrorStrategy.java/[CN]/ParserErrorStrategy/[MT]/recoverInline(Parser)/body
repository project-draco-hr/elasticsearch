{
  Token token=recognizer.getCurrentToken();
  String message="Error[" + token.getLine() + ":"+ token.getCharPositionInLine()+ "]:"+ " unexpected token ["+ getTokenErrorDisplay(token)+ "]"+ " was expecting one of ["+ recognizer.getExpectedTokens().toString(recognizer.getVocabulary())+ "].";
  ParseException parseException=new ParseException(message,token.getStartIndex());
  throw new RuntimeException(parseException);
}

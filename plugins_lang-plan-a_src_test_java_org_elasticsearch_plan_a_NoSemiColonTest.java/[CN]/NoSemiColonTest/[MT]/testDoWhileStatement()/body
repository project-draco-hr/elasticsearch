{
  assertEquals("aaaaaa",exec("String c = \"a\" int x do { c ..= \"a\" ++x } while (x < 5) return c"));
  Object value=exec(" long[][] l = new long[5][5]     \n" + " long x = 0, y                   \n" + "                                 \n"+ " do {                            \n"+ "     y = 0                       \n"+ "                                 \n"+ "     do {                        \n"+ "         l[(int)x][(int)y] = x*y \n"+ "         ++y                     \n"+ "     } while (y < 5)             \n"+ "                                 \n"+ "     ++x                         \n"+ " } while (x < 5)                 \n"+ "                                 \n"+ " return l                        \n");
  long[][] l=(long[][])value;
  for (long x=0; x < 5; ++x) {
    for (long y=0; y < 5; ++y) {
      assertEquals(x * y,l[(int)x][(int)y]);
    }
  }
}

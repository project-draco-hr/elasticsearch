{
  if (size == -1) {
    long size=RamUsage.NUM_BYTES_ARRAY_HEADER;
    for (    BytesRef value : values) {
      if (value != null) {
        size+=RamUsage.NUM_BYTES_OBJECT_REF + RamUsage.NUM_BYTES_OBJECT_HEADER + RamUsage.NUM_BYTES_ARRAY_HEADER+ (value.length + (2 * RamUsage.NUM_BYTES_INT));
      }
    }
    size+=ordinals.getMemorySizeInBytes();
    this.size=size;
  }
  return size;
}

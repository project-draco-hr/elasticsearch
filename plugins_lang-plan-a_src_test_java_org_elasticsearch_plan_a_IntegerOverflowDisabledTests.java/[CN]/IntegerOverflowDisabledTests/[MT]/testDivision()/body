{
  try {
    exec("int x = -2147483647 - 1; int y = -1; return x / y;");
    fail("should have hit exception");
  }
 catch (  ArithmeticException expected) {
  }
  try {
    exec("long x = -9223372036854775808L; long y = -1L; return x / y;");
    fail("should have hit exception");
  }
 catch (  ArithmeticException expected) {
  }
}

{
  try {
    exec("int x = -(-2147483648); return x;");
    fail("did not get expected exception");
  }
 catch (  ArithmeticException expected) {
  }
  try {
    exec("long x = -(-9223372036854775808L); return x;");
    fail("did not get expected exception");
  }
 catch (  ArithmeticException expected) {
  }
}

{
  String uid=fieldData.stringValue(doc);
  if (uid == null) {
    return;
  }
  String id=Uid.idFromUid(uid);
  Query query=queries.get(id);
  if (query == null) {
    return;
  }
  try {
    searcher.search(query,collector);
    if (collector.exists()) {
      matches.add(id);
    }
  }
 catch (  IOException e) {
    logger.warn("[" + id + "] failed to execute query",e);
  }
}

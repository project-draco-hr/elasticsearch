{
  final Iterator<V> iterator=delegate.values().iterator();
  return new Iterator<V>(){
    V valueToRemove;
    public boolean hasNext(){
      return iterator.hasNext();
    }
    public V next(){
      return valueToRemove=iterator.next();
    }
    public void remove(){
      iterator.remove();
      removeFromInverseMap(valueToRemove);
    }
  }
;
}

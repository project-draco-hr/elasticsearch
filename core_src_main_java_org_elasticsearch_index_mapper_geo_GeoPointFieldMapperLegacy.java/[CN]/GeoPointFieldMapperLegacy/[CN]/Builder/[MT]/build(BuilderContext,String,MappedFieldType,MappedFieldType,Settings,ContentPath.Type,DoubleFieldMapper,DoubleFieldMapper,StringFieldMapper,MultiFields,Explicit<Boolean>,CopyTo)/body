{
  fieldType.setTokenized(false);
  setupFieldType(context);
  fieldType.setHasDocValues(false);
  defaultFieldType.setHasDocValues(false);
  return new GeoPointFieldMapperLegacy(simpleName,fieldType,defaultFieldType,indexSettings,pathType,latMapper,lonMapper,geoHashMapper,multiFields,ignoreMalformed,coerce(context),copyTo);
}

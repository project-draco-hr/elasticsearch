{
  TokenFilter filter=evaluate(name,filters);
  if (inclusive) {
    if (filter == MATCHING) {
      return TokenFilter.INCLUDE_ALL;
    }
 else     if (filter == NO_MATCHING) {
      return null;
    }
  }
 else {
    if (filter == MATCHING) {
      return null;
    }
 else     if (filter == NO_MATCHING) {
      return TokenFilter.INCLUDE_ALL;
    }
  }
  return filter;
}

{
  super(names,boost,fieldType,docValues,indexAnalyzer,searchAnalyzer,postingsProvider,docValuesProvider,similarity,normsLoading,fieldDataSettings,indexSettings,multiFields);
  if (precisionStep <= 0 || precisionStep >= maxPrecisionStep()) {
    this.precisionStep=Integer.MAX_VALUE;
  }
 else {
    this.precisionStep=precisionStep;
  }
  this.ignoreMalformed=ignoreMalformed;
  this.coerce=coerce;
}

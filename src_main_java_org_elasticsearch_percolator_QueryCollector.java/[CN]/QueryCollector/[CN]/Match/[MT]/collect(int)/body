{
  BytesRef uid=values.getValue(doc);
  if (uid == null) {
    return;
  }
  Text id=new BytesText(Uid.idFromUid(uid));
  Query query=queries.get(id);
  if (query == null) {
    return;
  }
  try {
    collector.reset();
    searcher.search(query,collector);
    if (collector.exists()) {
      if (!limit) {
        matches.add(id);
      }
 else       if (counter < size) {
        matches.add(id);
      }
      counter++;
    }
  }
 catch (  IOException e) {
    logger.warn("[" + id + "] failed to execute query",e);
  }
}

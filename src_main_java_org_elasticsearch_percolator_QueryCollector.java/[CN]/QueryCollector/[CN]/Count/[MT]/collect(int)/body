{
  final Query query=getQuery(doc);
  if (query == null) {
    return;
  }
  try {
    collector.reset();
    searcher.search(query,collector);
    if (collector.exists()) {
      counter++;
      if (facetAndAggregatorCollector != null) {
        facetAndAggregatorCollector.collect(doc);
      }
    }
  }
 catch (  IOException e) {
    logger.warn("[" + spare.bytes.utf8ToString() + "] failed to execute query",e);
  }
}

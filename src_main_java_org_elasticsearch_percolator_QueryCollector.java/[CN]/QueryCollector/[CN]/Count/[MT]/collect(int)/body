{
  final Query query=getQuery(doc);
  if (query == null) {
    return;
  }
  try {
    collector.reset();
    searcher.search(query,collector);
    if (collector.exists()) {
      counter++;
      postMatch(doc);
    }
  }
 catch (  IOException e) {
    logger.warn("[" + current.utf8ToString() + "] failed to execute query",e);
  }
}

{
  BytesValues.Iter iter=readerValues.getIter(docId);
  if (!iter.hasNext()) {
    return null;
  }
  BytesRef currentVal=iter.next();
  BytesRef relevantVal=currentVal;
  while (true) {
    int cmp=currentVal.compareTo(relevantVal);
    if (reversed) {
      if (cmp > 0) {
        relevantVal=currentVal;
      }
    }
 else {
      if (cmp < 0) {
        relevantVal=currentVal;
      }
    }
    if (!iter.hasNext()) {
      break;
    }
    currentVal=iter.next();
  }
  return relevantVal;
}

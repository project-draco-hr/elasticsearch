{
  if (readerGen[slot1] == readerGen[slot2]) {
    return ords[slot1] - ords[slot2];
  }
  final BytesRef val1=values[slot1];
  final BytesRef val2=values[slot2];
  if (val1 == null) {
    if (val2 == null) {
      return 0;
    }
    return -1;
  }
 else   if (val2 == null) {
    return 1;
  }
  return val1.compareTo(val2);
}

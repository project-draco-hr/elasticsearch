{
  if (this == obj) {
    return true;
  }
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  QueryRescorer other=(QueryRescorer)obj;
  return Objects.equals(scoreMode,other.scoreMode) && Objects.equals(queryWeight,other.queryWeight) && Objects.equals(rescoreQueryWeight,other.rescoreQueryWeight)&& Objects.equals(queryBuilder,other.queryBuilder);
}

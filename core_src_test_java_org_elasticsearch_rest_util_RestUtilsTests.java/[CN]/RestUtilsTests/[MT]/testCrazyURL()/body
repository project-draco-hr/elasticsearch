{
  Map<String,String> params=newHashMap();
  String uri="example.com/:@-._~!$&'()*+,=;:@-._~!$&'()*+,=:@-._~!$&'()*+,==?/?:@-._~!$'()*+,;=/?:@-._~!$'()*+,;==#/?:@-._~!$&'()*+,;=";
  RestUtils.decodeQueryString(uri,uri.indexOf('?') + 1,params);
  assertThat(params.get("/?:@-._~!$'()* ,;"),equalTo("/?:@-._~!$'()* ,;=="));
  assertThat(params.size(),equalTo(1));
}

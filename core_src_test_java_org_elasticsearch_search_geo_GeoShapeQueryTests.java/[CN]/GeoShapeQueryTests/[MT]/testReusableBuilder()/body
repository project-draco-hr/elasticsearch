{
  ShapeBuilder polygon=ShapeBuilders.newPolygon().point(170,-10).point(190,-10).point(190,10).point(170,10).hole().point(175,-5).point(185,-5).point(185,5).point(175,5).close().close();
  assertUnmodified(polygon);
  ShapeBuilder linestring=ShapeBuilders.newLineString().point(170,-10).point(190,-10).point(190,10).point(170,10);
  assertUnmodified(linestring);
}

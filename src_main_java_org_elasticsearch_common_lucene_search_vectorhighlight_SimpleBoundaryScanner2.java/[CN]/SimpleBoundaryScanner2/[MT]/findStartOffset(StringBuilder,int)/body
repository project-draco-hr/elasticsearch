{
  if (start > buffer.length() || start < 1)   return start;
  int offset, count=maxScan;
  for (offset=start; offset > 0 && count > 0; count--) {
    if (boundaryChars.contains(buffer.charAt(offset - 1)))     return offset;
    offset--;
  }
  if (offset == 0) {
    return 0;
  }
  return start;
}

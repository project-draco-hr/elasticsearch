{
  List<ShardRouting> initializingShards=clusterState.getRoutingNodes().shardsWithState(INITIALIZING);
  if (initializingShards.isEmpty()) {
    return clusterState;
  }
  RoutingAllocation.Result routingResult=strategy.applyStartedShards(clusterState,arrayAsArrayList(initializingShards.get(randomInt(initializingShards.size() - 1))));
  return ClusterState.builder(clusterState).routingResult(routingResult).build();
}

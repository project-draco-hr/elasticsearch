{
  if (randomBoolean()) {
    if (randomBoolean()) {
      return constantScoreQuery(hasParentQuery(type,termQuery(field,value)));
    }
 else {
      return boolQuery().must(matchAllQuery()).filter(hasParentQuery(type,termQuery(field,value)));
    }
  }
 else {
    return hasParentQuery(type,termQuery(field,value));
  }
}

{
  out.writeVLong(timestamp);
  out.writeLong(openFileDescriptors);
  out.writeLong(maxFileDescriptors);
  if (cpu == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    cpu.writeTo(out);
  }
  if (mem == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    mem.writeTo(out);
  }
}

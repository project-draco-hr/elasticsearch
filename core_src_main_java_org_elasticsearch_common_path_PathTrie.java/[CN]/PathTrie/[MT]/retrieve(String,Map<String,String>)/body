{
  if (path.length() == 0) {
    return rootValue;
  }
  String[] strings=Strings.splitStringToArray(path,separator);
  if (strings.length == 0) {
    return rootValue;
  }
  int index=0;
  if (strings.length > 0 && strings[0].isEmpty()) {
    index=1;
  }
  return root.retrieve(strings,index,params);
}

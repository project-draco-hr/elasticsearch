{
switch (format) {
case ISO8601:
    return new ISO8601DateParser(timezone);
case UNIX:
  return new UnixDateParser(timezone);
case UNIX_MS:
return new UnixMsDateParser(timezone);
case TAI64N:
return new TAI64NDateParser(timezone);
default :
return new JodaPatternDateParser(format,timezone,locale);
}
}

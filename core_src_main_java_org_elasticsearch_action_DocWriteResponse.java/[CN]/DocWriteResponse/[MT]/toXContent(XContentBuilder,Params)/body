{
  ReplicationResponse.ShardInfo shardInfo=getShardInfo();
  builder.field("_index",shardId.getIndexName()).field("_type",type).field("_id",id).field("_version",version).field("_operation",getOperation().getLowercase()).field("forced_refresh",forcedRefresh);
  shardInfo.toXContent(builder,params);
  return builder;
}

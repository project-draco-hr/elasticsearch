{
  this.key=key;
  this.wildcard=wildcard;
  this.isWildcard=(key.equals(wildcard));
  this.parent=parent;
  this.value=value;
  this.children=ImmutableMap.of();
  if (isNamedWildcard(key)) {
    namedWildcard=key.substring(key.indexOf('{') + 1,key.indexOf('}'));
  }
 else {
    namedWildcard=null;
  }
}

{
  BoolQueryBuilder booleanQuery=new BoolQueryBuilder();
  try {
    booleanQuery.must(null);
    fail("cannot be null");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    booleanQuery.mustNot(null);
    fail("cannot be null");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    booleanQuery.filter(null);
    fail("cannot be null");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    booleanQuery.should(null);
    fail("cannot be null");
  }
 catch (  IllegalArgumentException e) {
  }
}

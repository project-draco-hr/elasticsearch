{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  final Pair<?,?> pair=(Pair<?,?>)o;
  return (firstValue != null ? firstValue.equals(pair.firstValue) : pair.firstValue == null) && (secondValue != null ? secondValue.equals(pair.secondValue) : pair.secondValue == null);
}

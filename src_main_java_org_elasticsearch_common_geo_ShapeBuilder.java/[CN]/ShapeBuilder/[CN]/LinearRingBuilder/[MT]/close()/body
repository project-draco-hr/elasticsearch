{
  Point first=points.get(0);
  Point last=points.get(points.size() - 1);
  if (first.getX() != last.getX() || first.getY() != last.getY()) {
    points.add(first);
  }
  if (points.size() < 4) {
    throw new ElasticSearchIllegalArgumentException("A linear ring is defined by a least four points");
  }
  return parent;
}

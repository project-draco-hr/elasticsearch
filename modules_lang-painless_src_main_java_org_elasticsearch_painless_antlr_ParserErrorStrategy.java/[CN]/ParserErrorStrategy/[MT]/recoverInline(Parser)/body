{
  final Token token=recognizer.getCurrentToken();
  final String message="Error[" + token.getLine() + ":"+ token.getCharPositionInLine()+ "]:"+ " unexpected token ["+ getTokenErrorDisplay(token)+ "]"+ " was expecting one of ["+ recognizer.getExpectedTokens().toString(recognizer.getVocabulary())+ "].";
  final ParseException parseException=new ParseException(message,token.getStartIndex());
  throw new RuntimeException(parseException);
}

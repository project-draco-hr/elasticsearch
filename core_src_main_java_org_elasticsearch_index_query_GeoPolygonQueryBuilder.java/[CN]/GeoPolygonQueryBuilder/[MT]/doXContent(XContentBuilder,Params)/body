{
  builder.startObject(NAME);
  builder.startObject(name);
  builder.startArray(POINTS);
  for (  GeoPoint point : shell) {
    builder.startArray().value(point.lon()).value(point.lat()).endArray();
  }
  builder.endArray();
  builder.endObject();
  printBoostAndQueryName(builder);
  builder.endObject();
}

{
  ImmutableSettings.Builder builder=ImmutableSettings.settingsBuilder().put("discovery.type","gce").put("cloud.gce.api.impl",mock).put("node.mode","network").put("discovery.zen.join.timeout","100ms").put("discovery.zen.ping.timeout","10ms").put("discovery.initial_state_timeout","300ms").put("transport.tcp.port",getPort(nodeOrdinal)).put("http.enabled",false).put("plugins." + PluginsService.LOAD_PLUGIN_FROM_CLASSPATH,true).put(settings).put(super.nodeSettings(nodeOrdinal));
  return builder.build();
}

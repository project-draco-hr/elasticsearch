{
  if (from.equals(to)) {
    throw new IllegalArgumentException("Transform cannot" + " have cast type from [" + from.name + "] be the same as cast type to ["+ to.name+ "].");
  }
  if (!from.sort.primitive || !to.sort.primitive) {
    throw new IllegalArgumentException("Only transforms between two primitives may be a simple cast, but" + "found [" + from.name + "] and ["+ to.name+ "].");
  }
  final Cast cast=new Cast(from,to,explicit);
  if (transformsMap.containsKey(cast)) {
    throw new IllegalArgumentException("Transform with " + " cast type from [" + from.name + "] to cast type to ["+ to.name+ "] already defined.");
  }
  transformsMap.put(cast,cast);
}

{
  super.readFrom(in);
  querySourceUnsafe=false;
  querySourceOffset=0;
  querySourceLength=in.readVInt();
  querySource=new byte[querySourceLength];
  in.readFully(querySource);
  if (in.readBoolean()) {
    queryParserName=in.readUTF();
  }
  if (in.readBoolean()) {
    routing=in.readUTF();
  }
}

{
  setupFieldType(context);
  Murmur3FieldMapper fieldMapper=new Murmur3FieldMapper(name,fieldType,defaultFieldType,ignoreMalformed(context),coerce(context),context.indexSettings(),multiFieldsBuilder.build(this,context),copyTo);
  return (Murmur3FieldMapper)fieldMapper.includeInAll(includeInAll);
}

{
  intsScratch.end=0;
  int i;
  for (i=0; i < ordinals.length; i++) {
    int ordinal=ordinals[i][docId];
    if (ordinal == 0) {
      if (i == 0)       return IntArrayRef.EMPTY;
      break;
    }
    if (i >= intsScratch.values.length) {
      intsScratch.values=ArrayUtil.grow(intsScratch.values);
    }
    intsScratch.values[i]=ordinal;
  }
  intsScratch.end=i;
  return intsScratch;
}

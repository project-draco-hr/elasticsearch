{
  intsScratch.end=0;
  int i;
  for (i=0; i < ordinals.length; i++) {
    int ordinal=ordinals[i][docId];
    if (ordinal == 0) {
      if (i == 0)       return IntArrayRef.EMPTY;
      break;
    }
    intsScratch.growIfNeeded(i);
    intsScratch.values[i]=ordinal;
  }
  intsScratch.end=i;
  return intsScratch;
}

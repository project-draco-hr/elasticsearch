{
  intsScratch.offset=0;
  int i;
  for (i=0; i < ordinals.length; i++) {
    int ordinal=ordinals[i][docId];
    if (ordinal == 0) {
      if (i == 0) {
        intsScratch.length=0;
        return intsScratch;
      }
      break;
    }
    intsScratch.grow(i + 1);
    intsScratch.ints[i]=ordinal;
  }
  intsScratch.length=i;
  return intsScratch;
}

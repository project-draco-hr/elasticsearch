{
  DocumentMapper mapper=mappers.get(type);
  if (mapper != null) {
    return mapper;
  }
  if (!dynamic) {
    return null;
  }
synchronized (mutex) {
    mapper=mappers.get(type);
    if (mapper != null) {
      return mapper;
    }
    add(type,dynamicMappingSource);
    return mappers.get(type);
  }
}

{
  return new NodeStats(discovery.localNode(),System.currentTimeMillis(),indicesService.stats(true),monitorService.osService().stats(),monitorService.processService().stats(),monitorService.jvmService().stats(),threadPool.stats(),monitorService.networkService().stats(),monitorService.fsService().stats(),transportService.stats(),httpServer == null ? null : httpServer.stats(),circuitBreakerService.stats());
}

{
  return new NodeStats(clusterService.state().nodes().localNode(),System.currentTimeMillis(),hostname,indicesService.stats(true),monitorService.osService().stats(),monitorService.processService().stats(),monitorService.jvmService().stats(),threadPool.stats(),monitorService.networkService().stats(),monitorService.fsService().stats(),transportService.stats(),httpServer == null ? null : httpServer.stats());
}

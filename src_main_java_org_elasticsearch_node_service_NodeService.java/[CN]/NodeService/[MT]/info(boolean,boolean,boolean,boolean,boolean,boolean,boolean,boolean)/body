{
  return new NodeInfo(hostname,clusterService.state().nodes().localNode(),serviceAttributes,settings ? this.settings : null,os ? monitorService.osService().info() : null,process ? monitorService.processService().info() : null,jvm ? monitorService.jvmService().info() : null,threadPool ? this.threadPool.info() : null,network ? monitorService.networkService().info() : null,transport ? transportService.info() : null,http ? (httpServer == null ? null : httpServer.info()) : null);
}

{
  if (!state.routingTable().hasIndex(index) && !previousState.routingTable().hasIndex(index)) {
    return false;
  }
  if (state.routingTable().hasIndex(index) && previousState.routingTable().hasIndex(index)) {
    return state.routingTable().index(index) != previousState.routingTable().index(index);
  }
  return true;
}

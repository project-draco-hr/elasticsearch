{
  assertEquals("aaaaaa",exec("String c = \"a\" for (int x = 0; x < 5; ++x) c ..= \"a\" return c"));
  Object value=exec(" int[][] i = new int[5][5]         \n" + " for (int x = 0; x < 5; ++x) {     \n" + "     for (int y = 0; y < 5; ++y) { \n"+ "         i[x][y] = x*y             \n"+ "     }                             \n"+ " }                                 \n"+ "                                   \n"+ " return i                          \n");
  int[][] i=(int[][])value;
  for (int x=0; x < 5; ++x) {
    for (int y=0; y < 5; ++y) {
      assertEquals(x * y,i[x][y]);
    }
  }
}

{
  Settings.Builder builder=Settings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put("discovery.zen.minimum_master_nodes",2).put(ZenDiscovery.JOIN_TIMEOUT_SETTING.getKey(),"50ms").put(ZenDiscovery.PING_TIMEOUT_SETTING.getKey(),"10ms").put("discovery.initial_state_timeout","100ms");
  return builder.build();
}

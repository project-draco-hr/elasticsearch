{
  Settings.Builder builder=Settings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put("discovery.zen.minimum_master_nodes",2).put(ZenDiscovery.SETTING_JOIN_TIMEOUT,"50ms").put(ZenDiscovery.SETTING_PING_TIMEOUT,"10ms").put("discovery.initial_state_timeout","100ms");
  return builder.build();
}

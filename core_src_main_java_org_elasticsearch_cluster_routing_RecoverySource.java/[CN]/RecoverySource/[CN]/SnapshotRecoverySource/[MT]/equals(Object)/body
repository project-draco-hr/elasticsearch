{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  @SuppressWarnings("unchecked") SnapshotRecoverySource that=(SnapshotRecoverySource)o;
  return snapshot.equals(that.snapshot) && index.equals(that.index) && version.equals(that.version);
}

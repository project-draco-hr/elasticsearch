{
  int counter=this.counter;
  int index=this.index;
  while (counter++ < size()) {
    ShardRouting shardRouting=shardModulo(index++);
    if (shardRouting.assignedToNode()) {
      this.counter=counter;
      this.index=index;
      return shardRouting;
    }
  }
  this.counter=counter;
  this.index=index;
  return null;
}

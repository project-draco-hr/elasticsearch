{
  final double[] pcts=randomPercents(minValue,maxValue);
  SearchResponse searchResponse=client().prepareSearch("idx").setQuery(matchAllQuery()).addAggregation(randomCompression(percentileRanks("percentile_ranks")).script(new Script("doc['value'].value")).values(pcts)).execute().actionGet();
  assertHitCount(searchResponse,10);
  final PercentileRanks values=searchResponse.getAggregations().get("percentile_ranks");
  assertConsistent(pcts,values,minValue,maxValue);
}

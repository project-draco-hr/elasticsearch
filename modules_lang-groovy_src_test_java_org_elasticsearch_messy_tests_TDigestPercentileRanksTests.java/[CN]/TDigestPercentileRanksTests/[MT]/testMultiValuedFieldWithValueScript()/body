{
  final double[] pcts=randomPercents(minValues - 1,maxValues - 1);
  SearchResponse searchResponse=client().prepareSearch("idx").setQuery(matchAllQuery()).addAggregation(randomCompression(percentileRanks("percentile_ranks")).field("values").script(new Script("_value - 1")).values(pcts)).execute().actionGet();
  assertHitCount(searchResponse,10);
  final PercentileRanks values=searchResponse.getAggregations().get("percentile_ranks");
  assertConsistent(pcts,values,minValues - 1,maxValues - 1);
}

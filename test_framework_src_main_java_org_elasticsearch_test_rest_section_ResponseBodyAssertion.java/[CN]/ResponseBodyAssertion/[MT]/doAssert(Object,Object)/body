{
  if (false == expectedValue.equals(actualValue)) {
    @SuppressWarnings("unchecked") Map<String,Object> actual=(Map<String,Object>)actualValue;
    @SuppressWarnings("unchecked") Map<String,Object> expected=(Map<String,Object>)expectedValue;
    FailureMessage message=new FailureMessage();
    message.compareMaps(actual,expected);
    throw new AssertionError(message.message);
  }
}

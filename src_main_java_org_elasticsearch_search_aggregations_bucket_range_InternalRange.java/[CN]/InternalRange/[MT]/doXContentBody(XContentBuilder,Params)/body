{
  if (keyed) {
    builder.startObject(CommonFields.BUCKETS);
  }
 else {
    builder.startArray(CommonFields.BUCKETS);
  }
  for (  B range : ranges) {
    range.toXContent(builder,params,formatter,keyed);
  }
  if (keyed) {
    builder.endObject();
  }
 else {
    builder.endArray();
  }
  return builder;
}

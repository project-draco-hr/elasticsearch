{
  if (isChecksum(name)) {
    return;
  }
  try {
    delegate.deleteFile(name);
  }
 catch (  IOException e) {
    if (delegate.fileExists(name)) {
      throw e;
    }
  }
synchronized (mutex) {
    filesMetadata=MapBuilder.newMapBuilder(filesMetadata).remove(name).immutableMap();
    files=filesMetadata.keySet().toArray(new String[filesMetadata.size()]);
  }
}

{
  super(names,precisionStep,index,store,boost,omitNorms,omitTermFreqAndPositions,new NamedAnalyzer("_date/" + precisionStep,new NumericDateAnalyzer(precisionStep,dateTimeFormatter)),new NamedAnalyzer("_date/max",new NumericDateAnalyzer(Integer.MAX_VALUE,dateTimeFormatter)));
  this.dateTimeFormatter=dateTimeFormatter;
  this.nullValue=nullValue;
}

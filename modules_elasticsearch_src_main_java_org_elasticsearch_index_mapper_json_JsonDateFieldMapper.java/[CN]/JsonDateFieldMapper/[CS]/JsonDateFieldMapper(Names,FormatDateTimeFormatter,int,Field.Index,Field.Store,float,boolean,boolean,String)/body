{
  super(names,precisionStep,index,store,boost,omitNorms,omitTermFreqAndPositions,new NamedAnalyzer("_date/" + precisionStep,new NumericDateAnalyzer(precisionStep,dateTimeFormatter.formatter())),new NamedAnalyzer("_date/max",new NumericDateAnalyzer(Integer.MAX_VALUE,dateTimeFormatter.formatter())));
  this.dateTimeFormatter=dateTimeFormatter;
  this.nullValue=nullValue;
}

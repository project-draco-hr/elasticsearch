{
  builder.startObject(Fields.FS);
  builder.field(Fields.TIMESTAMP,timestamp);
  builder.field(Fields.TOTAL);
  total().toXContent(builder,params);
  builder.startArray(Fields.DATA);
  for (  Path path : paths) {
    path.toXContent(builder,params);
  }
  builder.endArray();
  builder.endObject();
  return builder;
}

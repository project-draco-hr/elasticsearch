{
  List<String> result=newArrayList();
  int counter=0;
  while (true) {
    String value=get(settingPrefix + '.' + (counter++));
    if (value == null) {
      break;
    }
    result.add(value);
  }
  if (result.isEmpty()) {
    return defaultArray;
  }
  return result.toArray(new String[result.size()]);
}

{
  if (queryBuilder.filters().isEmpty()) {
    return null;
  }
  BooleanQuery query=new BooleanQuery();
  for (  QueryBuilder subQuery : queryBuilder.filters()) {
    Query innerQuery=subQuery.toQuery(context);
    if (innerQuery != null) {
      query.add(innerQuery,Occur.SHOULD);
    }
  }
  return query;
}

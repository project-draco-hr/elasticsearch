{
  Cache<K,V> cache=new Cache();
  if (maximumWeight != -1) {
    cache.setMaximumWeight(maximumWeight);
  }
  if (expireAfter != -1) {
    cache.setExpireAfter(expireAfter);
  }
  if (weigher != null) {
    cache.setWeigher(weigher);
  }
  if (removalListener != null) {
    cache.setRemovalListener(removalListener);
  }
  return cache;
}

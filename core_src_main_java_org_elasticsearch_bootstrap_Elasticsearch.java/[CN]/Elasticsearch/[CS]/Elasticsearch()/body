{
  super("starts elasticsearch");
  versionOption=parser.acceptsAll(Arrays.asList("V","version"),"Prints elasticsearch version information and exits");
  daemonizeOption=parser.acceptsAll(Arrays.asList("d","daemonize"),"Starts Elasticsearch in the background");
  pathHomeOption=parser.acceptsAll(Arrays.asList("H","path.home"),"").withRequiredArg();
  pidfileOption=parser.acceptsAll(Arrays.asList("p","pidfile"),"Creates a pid file in the specified path on start").withRequiredArg();
  propertyOption=parser.accepts("E","Configure an Elasticsearch setting").withRequiredArg().ofType(KeyValuePair.class);
}

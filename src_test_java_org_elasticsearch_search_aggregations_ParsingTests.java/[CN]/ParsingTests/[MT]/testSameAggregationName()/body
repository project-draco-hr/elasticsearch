{
  createIndex("idx");
  ensureGreen();
  final String name=RandomStrings.randomAsciiOfLength(getRandom(),10);
  client().prepareSearch("idx").setAggregations(JsonXContent.contentBuilder().startObject().startObject(name).startObject("terms").field("field","a").endObject().endObject().startObject(name).startObject("terms").field("field","b").endObject().endObject().endObject()).execute().actionGet();
}

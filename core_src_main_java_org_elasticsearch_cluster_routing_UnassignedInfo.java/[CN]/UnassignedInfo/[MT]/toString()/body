{
  StringBuilder sb=new StringBuilder();
  sb.append("unassigned_info[[reason=").append(reason).append("]");
  sb.append(", at[").append(DATE_TIME_FORMATTER.printer().print(timestamp)).append("]");
  if (details != null) {
    sb.append(", details[").append(details).append("]");
  }
  sb.append("]");
  return sb.toString();
}

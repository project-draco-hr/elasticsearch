{
  if (!listLoaded) {
    list.offset=0;
    list.length=0;
    Iter iter=values.getIter(docId);
    while (iter.hasNext()) {
      BytesRef next=iter.next();
      UnicodeUtil.UTF8toUTF16(next,spare);
      list.values[list.length++]=spare.toString();
    }
    listLoaded=true;
  }
  return list;
}

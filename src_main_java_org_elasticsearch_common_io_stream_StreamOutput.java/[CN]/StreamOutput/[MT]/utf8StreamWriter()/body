{
  SoftReference<UTF8StreamWriter> ref=utf8StreamWriter.get();
  UTF8StreamWriter writer=(ref == null) ? null : ref.get();
  if (writer == null) {
    writer=new UTF8StreamWriter(1024 * 4);
    utf8StreamWriter.set(new SoftReference<UTF8StreamWriter>(writer));
  }
  writer.reset();
  return writer;
}

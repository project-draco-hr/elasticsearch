{
  int sigDigits=randomSignificantDigits();
  final double[] pcts=randomPercents(minValues,maxValues);
  SearchResponse searchResponse=client().prepareSearch("idx").setQuery(matchAllQuery()).addAggregation(percentileRanks("percentile_ranks").method(PercentilesMethod.HDR).numberOfSignificantValueDigits(sigDigits).script(new Script("doc['values'].values")).values(pcts)).execute().actionGet();
  assertHitCount(searchResponse,10);
  final PercentileRanks values=searchResponse.getAggregations().get("percentile_ranks");
  assertConsistent(pcts,values,minValues,maxValues,sigDigits);
}

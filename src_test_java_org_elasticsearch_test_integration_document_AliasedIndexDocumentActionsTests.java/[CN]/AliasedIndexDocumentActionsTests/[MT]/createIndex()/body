{
  logger.info("Creating index [test1] with alias [test]");
  try {
    client1.admin().indices().prepareDelete("test1").execute().actionGet();
  }
 catch (  Exception e) {
  }
  logger.info("--> creating index test");
  client1.admin().indices().create(createIndexRequest("test1").setSettings(settingsBuilder().putArray("index.aliases","test"))).actionGet();
}

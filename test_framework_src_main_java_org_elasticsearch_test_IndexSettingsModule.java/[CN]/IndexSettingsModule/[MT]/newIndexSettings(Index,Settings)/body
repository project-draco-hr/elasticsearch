{
  Settings build=Settings.settingsBuilder().put(IndexMetaData.SETTING_VERSION_CREATED,Version.CURRENT).put(IndexMetaData.SETTING_NUMBER_OF_REPLICAS,1).put(IndexMetaData.SETTING_NUMBER_OF_SHARDS,1).put(settings).build();
  IndexMetaData metaData=IndexMetaData.builder(index.getName()).settings(build).build();
  return new IndexSettings(metaData,Settings.EMPTY,Collections.emptyList());
}

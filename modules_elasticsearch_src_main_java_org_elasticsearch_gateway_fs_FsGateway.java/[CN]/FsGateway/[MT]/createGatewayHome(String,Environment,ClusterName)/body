{
  File f;
  if (location != null) {
    f=new File(new File(location),clusterName.value());
  }
 else {
    f=new File(environment.workWithClusterFile(),"gateway");
  }
  if (f.exists() && f.isDirectory()) {
    return f;
  }
  boolean result;
  for (int i=0; i < 5; i++) {
    result=f.mkdirs();
    if (result) {
      break;
    }
  }
  return f;
}

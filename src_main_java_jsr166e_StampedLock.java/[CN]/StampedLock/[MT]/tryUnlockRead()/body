{
  long s, m;
  WNode h;
  while ((m=(s=state) & ABITS) != 0L && m < WBIT) {
    if (m < RFULL) {
      if (U.compareAndSwapLong(this,STATE,s,s - RUNIT)) {
        if (m == RUNIT && (h=whead) != null && h.status != 0)         release(h);
        return true;
      }
    }
 else     if (tryDecReaderOverflow(s) != 0L)     return true;
  }
  return false;
}

{
  IndicesModule indicesModule=new IndicesModule(emptyList()){
    @Override protected void configure(){
      bindMapperExtension();
    }
  }
;
  SearchModule searchModule=new SearchModule(Settings.EMPTY,false,Collections.singletonList(new FetchSubPhasePluginIT.FetchTermVectorsPlugin())){
    @Override protected void configureSearch(){
    }
  }
;
  List<NamedWriteableRegistry.Entry> entries=new ArrayList<>();
  entries.addAll(indicesModule.getNamedWriteables());
  entries.addAll(searchModule.getNamedWriteables());
  namedWriteableRegistry=new NamedWriteableRegistry(entries);
}

{
  if (concreteIndices.length == metaData.concreteAllIndices().length && indicesOrAliases.length > 0) {
    if (indicesOrAliases[0].charAt(0) == '-') {
      return true;
    }
    for (    String indexOrAlias : indicesOrAliases) {
      if (Regex.isSimpleMatchPattern(indexOrAlias)) {
        return true;
      }
    }
  }
  return false;
}

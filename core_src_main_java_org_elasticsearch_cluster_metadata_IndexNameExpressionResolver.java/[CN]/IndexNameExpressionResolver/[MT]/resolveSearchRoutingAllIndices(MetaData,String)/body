{
  if (routing != null) {
    Set<String> r=Strings.splitStringByCommaToSet(routing);
    Map<String,Set<String>> routings=newHashMap();
    String[] concreteIndices=metaData.concreteAllIndices();
    for (    String index : concreteIndices) {
      routings.put(index,r);
    }
    return routings;
  }
  return null;
}

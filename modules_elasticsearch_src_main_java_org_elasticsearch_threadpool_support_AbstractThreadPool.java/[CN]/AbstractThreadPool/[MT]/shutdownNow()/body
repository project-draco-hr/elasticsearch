{
  started=false;
  if (!executorService.isTerminated()) {
    executorService.shutdownNow();
  }
  if (!scheduledExecutorService.isTerminated()) {
    scheduledExecutorService.shutdownNow();
  }
  if (cached != executorService) {
    if (!cached.isTerminated()) {
      cached.shutdownNow();
    }
  }
}

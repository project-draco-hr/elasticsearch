{
  StringBuilder buf=new StringBuilder();
  buf.append("NodeGatewayStartedShards[").append("allocationId=").append(allocationId).append(",primary=").append(primary).append(",legacyVersion=").append(legacyVersion);
  if (storeException != null) {
    buf.append(",storeException=").append(storeException);
  }
  buf.append("]");
  return buf.toString();
}

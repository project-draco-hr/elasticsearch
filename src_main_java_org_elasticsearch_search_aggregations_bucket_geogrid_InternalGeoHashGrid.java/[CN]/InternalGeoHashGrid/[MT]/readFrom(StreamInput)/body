{
  this.name=in.readString();
  this.requiredSize=readSize(in);
  int size=in.readVInt();
  List<Bucket> buckets=new ArrayList<>(size);
  for (int i=0; i < size; i++) {
    Bucket bucket=new Bucket();
    bucket.readFrom(in);
    buckets.add(bucket);
  }
  this.buckets=buckets;
  this.bucketMap=null;
}

{
  builder.startObject(TermsFilterParser.NAME);
  builder.field(name,values);
  if (execution != null) {
    builder.field("execution",execution);
  }
  if (filterName != null) {
    builder.field("_name",filterName);
  }
  if (cache != null) {
    builder.field("_cache",cache);
  }
  if (cacheKey != null) {
    builder.field("_cache_key",cacheKey);
  }
  builder.endObject();
}

{
  super(names,precisionStep,boost,fieldType,ignoreMalformed,NumericFloatAnalyzer.buildNamedAnalyzer(precisionStep),NumericFloatAnalyzer.buildNamedAnalyzer(Integer.MAX_VALUE),postingsProvider,docValuesProvider,similarity,fieldDataSettings,indexSettings);
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();

{
  super(names,precisionStep,fuzzyFactor,boost,fieldType,ignoreMalformed,new NamedAnalyzer("_float/" + precisionStep,new NumericFloatAnalyzer(precisionStep)),new NamedAnalyzer("_float/max",new NumericFloatAnalyzer(Integer.MAX_VALUE)),provider,similarity);
  this.nullValue=nullValue;

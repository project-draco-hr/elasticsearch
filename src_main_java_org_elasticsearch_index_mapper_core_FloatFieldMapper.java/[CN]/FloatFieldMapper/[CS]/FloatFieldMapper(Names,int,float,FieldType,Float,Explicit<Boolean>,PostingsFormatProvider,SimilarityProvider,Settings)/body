{
  super(names,precisionStep,boost,fieldType,ignoreMalformed,NumericFloatAnalyzer.buildNamedAnalyzer(precisionStep),NumericFloatAnalyzer.buildNamedAnalyzer(Integer.MAX_VALUE),provider,similarity,fieldDataSettings);
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();

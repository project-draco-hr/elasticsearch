{
  for (int doc=0; doc < numOfDoc; doc++) {
    final IndexRequest indexRequest=new IndexRequest(index.getName(),"type",Integer.toString(docId.incrementAndGet())).source("{}").waitForActiveShards(ActiveShardCount.NONE);
    final IndexResponse response=index(indexRequest);
    assertEquals(DocWriteResponse.Result.CREATED,response.getResult());
  }
  return numOfDoc;
}

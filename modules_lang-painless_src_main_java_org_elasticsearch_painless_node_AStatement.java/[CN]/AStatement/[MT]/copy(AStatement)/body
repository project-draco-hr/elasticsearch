{
  lastSource=statement.lastSource;
  beginLoop=statement.beginLoop;
  inLoop=statement.inLoop;
  lastLoop=statement.lastLoop;
  methodEscape=statement.methodEscape;
  loopEscape=statement.loopEscape;

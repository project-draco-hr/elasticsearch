{
  String type;
  if (settings.get("name") != null) {
    type=settings.get("name") + " [" + nodeDescription+ "]";
  }
 else {
    type=nodeDescription;
  }
  type=type.replace(':','_').replace('/','_').replace('.','_').replace(',',' ').replace('\"',' ');
  return jmxDomain + ":" + "type="+ type+ ","+ resourceName;
}

{
  for (  FileChangesListener listener : listeners()) {
    try {
      if (initial) {
        listener.onDirectoryInit(file);
      }
 else {
        listener.onDirectoryCreated(file);
      }
    }
 catch (    Throwable t) {
      logger.warn("cannot notify file changes listener",t);
    }
  }
  children=listChildren(initial);
}

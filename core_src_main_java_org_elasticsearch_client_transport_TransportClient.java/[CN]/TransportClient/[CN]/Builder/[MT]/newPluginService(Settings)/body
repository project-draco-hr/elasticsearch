{
  final Settings.Builder settingsBuilder=settingsBuilder().put(NettyTransport.PING_SCHEDULE,"5s").put(InternalSettingsPreparer.prepareSettings(settings)).put("network.server",false).put("node.client",true).put(CLIENT_TYPE_SETTING,CLIENT_TYPE);
  return new PluginsService(settingsBuilder.build(),null,null,pluginClasses);
}

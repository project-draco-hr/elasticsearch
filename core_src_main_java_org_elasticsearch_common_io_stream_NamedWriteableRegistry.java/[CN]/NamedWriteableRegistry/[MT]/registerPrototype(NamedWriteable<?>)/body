{
  if (registry.containsKey(namedWriteable.getName())) {
    throw new IllegalArgumentException("named writeable of type [" + namedWriteable.getClass().getName() + "] with name ["+ namedWriteable.getName()+ "] "+ "is already registered by type ["+ registry.get(namedWriteable.getName()).getClass().getName()+ "]");
  }
  registry.put(namedWriteable.getName(),namedWriteable);
}

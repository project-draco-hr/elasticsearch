{
  startServer("server1");
  startServer("server2");
  client("server1").admin().indices().create(createIndexRequest("test")).actionGet();
  for (int i=0; i < 100; i++) {
    index(client("server1"),Integer.toString(i),"test",i);
  }
  client("server1").admin().indices().refresh(refreshRequest("test")).actionGet();
}

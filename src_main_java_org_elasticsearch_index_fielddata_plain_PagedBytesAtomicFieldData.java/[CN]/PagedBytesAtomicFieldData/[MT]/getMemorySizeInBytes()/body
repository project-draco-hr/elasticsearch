{
  if (size == -1) {
    long size=ordinals.getMemorySizeInBytes();
    size+=RamUsage.NUM_BYTES_ARRAY_HEADER + bytes.getBlocks().length;
    for (    byte[] b : bytes.getBlocks()) {
      size+=b.length;
    }
    size+=termOrdToBytesOffset.ramBytesUsed();
    this.size=size;
  }
  return size;
}

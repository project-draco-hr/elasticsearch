{
  BulkRequest bulkRequest=new BulkRequest();
  for (  SearchHit doc : docs) {
    if (accept(doc)) {
      RequestWrapper<?> request=scriptApplier.apply(copyMetadata(buildRequest(doc),doc),doc);
      if (request != null) {
        bulkRequest.add(request.self());
      }
    }
  }
  return bulkRequest;
}

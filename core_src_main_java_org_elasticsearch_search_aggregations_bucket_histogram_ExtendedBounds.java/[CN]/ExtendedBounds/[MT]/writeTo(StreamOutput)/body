{
  if (min != null) {
    out.writeBoolean(true);
    out.writeLong(min);
  }
 else {
    out.writeBoolean(false);
  }
  if (max != null) {
    out.writeBoolean(true);
    out.writeLong(max);
  }
 else {
    out.writeBoolean(false);
  }
  out.writeOptionalString(minAsStr);
  out.writeOptionalString(maxAsStr);
}

{
  StringBuilder b=new StringBuilder();
  if (min != null) {
    b.append(min);
    if (minAsStr != null) {
      b.append('(').append(minAsStr).append(')');
    }
  }
 else {
    if (minAsStr != null) {
      b.append(minAsStr);
    }
  }
  b.append("--");
  if (max != null) {
    b.append(min);
    if (maxAsStr != null) {
      b.append('(').append(maxAsStr).append(')');
    }
  }
 else {
    if (maxAsStr != null) {
      b.append(maxAsStr);
    }
  }
  return b.toString();
}

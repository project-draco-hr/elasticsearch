{
  String[] files=directory.listAll();
  IOException lastException=null;
  for (  String file : files) {
    try {
      directory.deleteFile(file);
    }
 catch (    FileNotFoundException e) {
    }
catch (    IOException e) {
      lastException=e;
    }
  }
  if (lastException != null) {
    throw lastException;
  }
}

{
  try {
    exec("int x = -2147483648; x = -x; return x;");
    fail("did not get expected exception");
  }
 catch (  ArithmeticException expected) {
  }
  try {
    exec("long x = -9223372036854775808L; x = -x; return x;");
    fail("did not get expected exception");
  }
 catch (  ArithmeticException expected) {
  }
}

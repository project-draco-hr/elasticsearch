{
  if (sysPropName.equals(SYSPROP_ITERATIONS())) {
    return this;
  }
  if (sysPropName.equals(SYSPROP_TESTMETHOD())) {
    return this;
  }
  if (sysPropName.equals(SYSPROP_PREFIX())) {
    return this;
  }
  if (Strings.hasLength(value)) {
    return super.appendOpt(sysPropName,value);
  }
  return this;
}

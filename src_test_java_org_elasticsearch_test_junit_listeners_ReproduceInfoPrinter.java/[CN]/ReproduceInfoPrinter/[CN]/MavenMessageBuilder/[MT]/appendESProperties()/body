{
  appendProperties("es.logger.level","es.node.mode","es.node.local",TESTS_CLUSTER,TestCluster.TESTS_ENABLE_MOCK_MODULES,"tests.assertion.disabled","tests.security.manager","tests.nightly","tests.jvms","tests.client.ratio","tests.heap.size","tests.bwc","tests.bwc.path","tests.bwc.version");
  if (System.getProperty("tests.jvm.argline") != null && !System.getProperty("tests.jvm.argline").isEmpty()) {
    appendOpt("tests.jvm.argline","\"" + System.getProperty("tests.jvm.argline") + "\"");
  }
  appendOpt(AbstractRandomizedTest.SYSPROP_PROCESSORS,Integer.toString(AbstractRandomizedTest.TESTS_PROCESSORS));
  return this;
}

{
  appendProperties("es.logger.level","es.node.mode","es.node.local",TestCluster.TESTS_ENABLE_MOCK_MODULES,"tests.assertion.disabled","tests.security.manager","tests.nighly");
  if (System.getProperty("tests.jvm.argline") != null && !System.getProperty("tests.jvm.argline").isEmpty()) {
    appendOpt("tests.jvm.argline","\"" + System.getProperty("tests.jvm.argline") + "\"");
  }
  return this;
}

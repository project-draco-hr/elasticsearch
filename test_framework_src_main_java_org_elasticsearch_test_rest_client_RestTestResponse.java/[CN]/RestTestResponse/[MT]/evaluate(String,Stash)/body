{
  if (response == null) {
    return null;
  }
  ObjectPath objectPath=parsedResponse();
  if (objectPath == null) {
    if ("".equals(path) && HttpHead.METHOD_NAME.equals(response.getRequestLine().getMethod())) {
      return isError() == false;
    }
    return null;
  }
  return objectPath.evaluate(path,stash);
}

{
  if (response == null) {
    return null;
  }
  JsonPath jsonPath=parsedResponse();
  if (jsonPath == null) {
    if ("".equals(path) && HttpHead.METHOD_NAME.equals(response.getRequestLine().getMethod())) {
      return isError() == false;
    }
    return null;
  }
  return jsonPath.evaluate(path,stash);
}

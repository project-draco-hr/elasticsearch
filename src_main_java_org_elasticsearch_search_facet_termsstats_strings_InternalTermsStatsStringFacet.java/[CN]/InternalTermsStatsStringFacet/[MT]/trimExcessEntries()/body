{
  if (requiredSize == 0 || requiredSize >= entries.size()) {
    return;
  }
  if (entries instanceof List) {
    entries=((List)entries).subList(0,requiredSize);
    return;
  }
  int i=0;
  for (Iterator<StringEntry> iter=entries.iterator(); iter.hasNext(); ) {
    iter.next();
    if (i++ >= requiredSize) {
      iter.remove();
    }
  }
}

{
  if (!more) {
    return false;
  }
  doc=spans.doc();
  freq=0.0f;
  payloadScore=0;
  payloadsSeen=0;
  while (more && doc == spans.doc()) {
    int matchLength=spans.end() - spans.start();
    freq+=docScorer.computeSlopFactor(matchLength);
    processPayload();
    more=spans.next();
  }
  return more || (freq != 0);
}

{
  if (!more) {
    return false;
  }
  doc=spans.doc();
  freq=0.0f;
  numMatches=0;
  payloadScore=0;
  payloadsSeen=0;
  do {
    int matchLength=spans.end() - spans.start();
    freq+=docScorer.computeSlopFactor(matchLength);
    numMatches++;
    processPayload();
    more=spans.next();
  }
 while (more && (doc == spans.doc()));
  return true;
}

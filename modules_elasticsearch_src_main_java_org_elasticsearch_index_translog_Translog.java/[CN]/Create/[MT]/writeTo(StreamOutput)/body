{
  out.writeVInt(4);
  out.writeUTF(id);
  out.writeUTF(type);
  out.writeVInt(sourceLength);
  out.writeBytes(source,sourceOffset,sourceLength);
  if (routing == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(routing);
  }
  if (parent == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(parent);
  }
  out.writeLong(version);
  out.writeLong(timestamp);
}

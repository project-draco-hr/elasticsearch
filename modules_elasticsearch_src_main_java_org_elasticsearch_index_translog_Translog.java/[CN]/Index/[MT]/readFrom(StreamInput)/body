{
  int version=in.readVInt();
  id=in.readUTF();
  type=in.readUTF();
  sourceOffset=0;
  sourceLength=in.readVInt();
  source=new byte[sourceLength];
  in.readFully(source);
  if (version >= 1) {
    if (in.readBoolean()) {
      routing=in.readUTF();
    }
  }
  if (version >= 2) {
    if (in.readBoolean()) {
      parent=in.readUTF();
    }
  }
  if (version >= 3) {
    this.version=in.readLong();
  }
  if (version >= 4) {
    this.timestamp=in.readLong();
  }
}

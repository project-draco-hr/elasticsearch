{
  out.writeBoolean(isFailed());
  out.writeBoolean(isVerbose());
  if (failure != null) {
    out.writeThrowable(failure);
  }
 else   if (isVerbose()) {
    out.writeVInt(processorResultList.size());
    for (    SimulateProcessorResult p : processorResultList) {
      p.writeTo(out);
    }
  }
 else {
    data.writeTo(out);
  }
}

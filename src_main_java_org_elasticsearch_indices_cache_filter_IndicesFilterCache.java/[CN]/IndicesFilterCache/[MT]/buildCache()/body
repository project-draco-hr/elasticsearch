{
  CacheBuilder<WeightedFilterCache.FilterCacheKey,DocSet> cacheBuilder=CacheBuilder.newBuilder().removalListener(this).maximumWeight(sizeInBytes).weigher(new WeightedFilterCache.FilterCacheValueWeigher());
  cacheBuilder.concurrencyLevel(16);
  if (expire != null) {
    cacheBuilder.expireAfterAccess(expire.millis(),TimeUnit.MILLISECONDS);
  }
  cache=cacheBuilder.build();
}

{
  CacheBuilder<WeightedFilterCache.FilterCacheKey,DocIdSet> cacheBuilder=CacheBuilder.newBuilder().removalListener(this).maximumWeight(sizeInBytes).weigher(new WeightedFilterCache.FilterCacheValueWeigher(minimumEntryWeight));
  cacheBuilder.concurrencyLevel(this.concurrencyLevel);
  if (expire != null) {
    cacheBuilder.expireAfterAccess(expire.millis(),TimeUnit.MILLISECONDS);
  }
  cache=cacheBuilder.build();
}

{
  if (randomBoolean()) {
    if (randomBoolean()) {
      return constantScoreQuery(hasChildFilter(type,termQuery(field,value)));
    }
 else {
      return filteredQuery(matchAllQuery(),hasChildFilter(type,termQuery(field,value)));
    }
  }
 else {
    return hasChildQuery(type,termQuery(field,value));
  }
}

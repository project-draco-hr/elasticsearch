{
  final Map<String,String> attributes=MapBuilder.<String,String>newMapBuilder().put(DiscoveryNode.MASTER_ATTR,isMasterEligible ? "true" : "false").put(DiscoveryNode.DATA_ATTR,isData ? "true" : "false").immutableMap();
  return new DiscoveryNode(nodeId,nodeId,DummyTransportAddress.INSTANCE,attributes,Version.CURRENT);
}

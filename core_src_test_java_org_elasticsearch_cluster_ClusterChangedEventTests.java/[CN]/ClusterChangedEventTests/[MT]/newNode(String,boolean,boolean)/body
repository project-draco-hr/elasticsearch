{
  final Map<String,String> attributes=MapBuilder.<String,String>newMapBuilder().put(DiscoveryNode.Role.MASTER.getRoleName(),isMasterEligible ? "true" : "false").put(DiscoveryNode.Role.DATA.getRoleName(),isData ? "true" : "false").immutableMap();
  return new DiscoveryNode(nodeId,nodeId,DummyTransportAddress.INSTANCE,attributes,Version.CURRENT);
}

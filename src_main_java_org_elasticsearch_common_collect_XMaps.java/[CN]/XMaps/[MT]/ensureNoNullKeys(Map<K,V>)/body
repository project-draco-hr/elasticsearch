{
  return new ForwardingMap<K,V>(){
    @Override public V put(    K key,    V value){
      if (key == null) {
        throw new ElasticSearchIllegalArgumentException("Map key must not be null");
      }
      return super.put(key,value);
    }
    @Override protected Map<K,V> delegate(){
      return delegate;
    }
  }
;
}

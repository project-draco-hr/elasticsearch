{
  try {
    new FuzzyQueryBuilder(null,"text");
    fail("must not be null");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new FuzzyQueryBuilder("","text");
    fail("must not be empty");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new FuzzyQueryBuilder("field",null);
    fail("must not be null");
  }
 catch (  IllegalArgumentException e) {
  }
}

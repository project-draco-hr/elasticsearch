{
  builder.startObject();
  builder.field(Fields.ERROR,failed());
  if (failed()) {
    builder.field(Fields.FAILURE,failure.toString());
  }
 else {
    builder.field(Fields.MODIFIED,data.isModified());
    builder.field(Fields.DOCUMENT,data.getDocument());
  }
  builder.endObject();
  return builder;
}

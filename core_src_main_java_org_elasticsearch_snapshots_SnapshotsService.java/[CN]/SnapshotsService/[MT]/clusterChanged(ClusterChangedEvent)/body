{
  try {
    if (event.localNodeMaster()) {
      if (event.nodesRemoved()) {
        processSnapshotsOnRemovedNodes(event);
      }
      if (event.routingTableChanged()) {
        processStartedShards(event);
      }
    }
  }
 catch (  Throwable t) {
    logger.warn("Failed to update snapshot state ",t);
  }
}

{
  super(location,1);
  this.pattern=pattern;
  int flags=0;
  for (int c=0; c < flagsString.length(); c++) {
    flags|=flagForChar(flagsString.charAt(c));
  }
  this.flags=flags;
  try {
    Pattern.compile(pattern,flags);
  }
 catch (  PatternSyntaxException e) {
    throw createError(e);
  }
}

{
  List<Map<String,Object>> docs=ConfigurationUtils.readList(config,Fields.DOCS);
  List<Data> dataList=new ArrayList<>();
  for (  Map<String,Object> dataMap : docs) {
    Map<String,Object> document=ConfigurationUtils.readMap(dataMap,Fields.SOURCE);
    Data data=new Data(ConfigurationUtils.readStringProperty(dataMap,Fields.INDEX),ConfigurationUtils.readStringProperty(dataMap,Fields.TYPE),ConfigurationUtils.readStringProperty(dataMap,Fields.ID),document);
    dataList.add(data);
  }
  return dataList;
}

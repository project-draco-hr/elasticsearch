{
  List<Map<String,Object>> docs=ConfigurationUtils.readList(config,Fields.DOCS);
  List<IngestDocument> ingestDocumentList=new ArrayList<>();
  for (  Map<String,Object> dataMap : docs) {
    Map<String,Object> document=ConfigurationUtils.readMap(dataMap,Fields.SOURCE);
    IngestDocument ingestDocument=new IngestDocument(ConfigurationUtils.readStringProperty(dataMap,Fields.INDEX),ConfigurationUtils.readStringProperty(dataMap,Fields.TYPE),ConfigurationUtils.readStringProperty(dataMap,Fields.ID),document);
    ingestDocumentList.add(ingestDocument);
  }
  return ingestDocumentList;
}

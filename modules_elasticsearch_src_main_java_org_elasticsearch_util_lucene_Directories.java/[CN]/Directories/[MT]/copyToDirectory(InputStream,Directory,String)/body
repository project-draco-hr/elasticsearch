{
  IndexOutput output=null;
  try {
    output=dir.createOutput(fileName);
    copyToDirectory(is,output);
  }
  finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
      }
    }
    if (output != null) {
      try {
        output.close();
      }
 catch (      IOException e) {
      }
    }
  }
  sync(dir,fileName);
}

{
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  CancelAllocationCommand other=(CancelAllocationCommand)obj;
  return Objects.equals(index,other.index) && Objects.equals(shardId,other.shardId) && Objects.equals(node,other.node)&& Objects.equals(allowPrimary,other.allowPrimary);
}

{
  if (objectName == null) {
    builder.startObject();
  }
 else {
    builder.startObject(objectName);
  }
  builder.field("index",command.index());
  builder.field("shard",command.shardId());
  builder.field("node",command.node());
  builder.field("allow_primary",command.allowPrimary());
  builder.endObject();
}

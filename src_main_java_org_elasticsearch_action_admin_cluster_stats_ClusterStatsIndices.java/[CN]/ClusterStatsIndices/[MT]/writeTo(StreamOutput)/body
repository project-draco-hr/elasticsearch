{
  out.writeVInt(indexCount);
  shards.writeTo(out);
  docs.writeTo(out);
  store.writeTo(out);
  fieldData.writeTo(out);
  filterCache.writeTo(out);
  idCache.writeTo(out);
  completion.writeTo(out);
  segments.writeTo(out);
  if (out.getVersion().onOrAfter(Version.V_1_0_0_RC1)) {
    peroclate.writeTo(out);
  }
}

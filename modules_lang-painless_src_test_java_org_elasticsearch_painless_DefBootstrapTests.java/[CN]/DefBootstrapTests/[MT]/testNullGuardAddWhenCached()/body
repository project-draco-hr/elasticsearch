{
  DefBootstrap.MIC site=(DefBootstrap.MIC)DefBootstrap.bootstrap(MethodHandles.publicLookup(),"add",MethodType.methodType(Object.class,Object.class,Object.class),DefBootstrap.BINARY_OPERATOR,DefBootstrap.OPERATOR_ALLOWS_NULL);
  MethodHandle handle=site.dynamicInvoker();
  assertEquals(2,(Object)handle.invokeExact((Object)1,(Object)1));
  assertEquals("nulltest",(Object)handle.invoke((Object)null,(Object)"test"));
}

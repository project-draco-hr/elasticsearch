{
  CallSite site=DefBootstrap.bootstrap(MethodHandles.publicLookup(),"toString",MethodType.methodType(String.class,Object.class),DefBootstrap.METHOD_CALL,0L);
  MethodHandle handle=site.dynamicInvoker();
  assertDepthEquals(site,0);
  assertEquals("5",(String)handle.invokeExact((Object)5));
  assertDepthEquals(site,1);
  assertEquals("6",(String)handle.invokeExact((Object)6));
  assertDepthEquals(site,1);
}

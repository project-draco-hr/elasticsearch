{
  CallSite site=DefBootstrap.bootstrap(MethodHandles.publicLookup(),"toString",MethodType.methodType(String.class,Object.class),DefBootstrap.METHOD_CALL,0);
  MethodHandle handle=site.dynamicInvoker();
  assertDepthEquals(site,0);
  assertEquals("5",handle.invoke(Integer.valueOf(5)));
  assertDepthEquals(site,1);
  assertEquals("6",handle.invoke(Integer.valueOf(6)));
  assertDepthEquals(site,1);
}

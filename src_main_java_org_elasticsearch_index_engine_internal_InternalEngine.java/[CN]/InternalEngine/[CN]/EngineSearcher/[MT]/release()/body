{
  if (!released.compareAndSet(false,true)) {
    logger.warn("Searcher was released twice",new ElasticsearchIllegalStateException("Double release"));
    return false;
  }
  try {
    manager.release(searcher);
    return true;
  }
 catch (  IOException e) {
    return false;
  }
catch (  AlreadyClosedException e) {
    return false;
  }
 finally {
    store.decRef();
  }
}

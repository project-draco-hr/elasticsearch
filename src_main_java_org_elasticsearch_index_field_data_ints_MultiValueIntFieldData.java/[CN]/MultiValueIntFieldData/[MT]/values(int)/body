{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] == 0) {
      break;
    }
    length++;
  }
  if (length == 0) {
    return EMPTY_INT_ARRAY;
  }
  int[] ints;
  if (length < VALUE_CACHE_SIZE) {
    ints=valuesCache.get().get()[length];
  }
 else {
    ints=new int[length];
  }
  for (int i=0; i < length; i++) {
    ints[i]=values[ordinals[i][docId]];
  }
  return ints;
}

{
  MappingMetaData md=new MappingMetaData("type1",new CompressedString(""),new MappingMetaData.Routing(true,"obj1.routing"),new MappingMetaData.Timestamp(true,"obj1.timestamp","dateOptionalTime"));
  byte[] bytes=jsonBuilder().startObject().field("field1","value1").field("field2","value2").startObject("obj0").field("field1","value1").field("field2","value2").endObject().startObject("obj1").field("routing","routing_value").field("timestamp","1").endObject().startObject("obj2").field("field1","value1").endObject().endObject().copiedBytes();
  MappingMetaData.ParseContext parseContext=md.createParseContext(null,null);
  md.parse(XContentFactory.xContent(bytes).createParser(bytes),parseContext);
  assertThat(parseContext.routing(),equalTo("routing_value"));
  assertThat(parseContext.timestamp(),equalTo("1"));
}

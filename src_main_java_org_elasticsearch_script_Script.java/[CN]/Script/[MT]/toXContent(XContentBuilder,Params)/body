{
  if (type == null) {
    return builder.value(script);
  }
  builder.startObject();
  scriptFieldToXContent(script,type,builder,builderParams);
  if (lang != null) {
    builder.field(ScriptField.LANG.getPreferredName(),lang);
  }
  if (params != null) {
    builder.field(ScriptField.PARAMS.getPreferredName(),params);
  }
  builder.endObject();
  return builder;
}

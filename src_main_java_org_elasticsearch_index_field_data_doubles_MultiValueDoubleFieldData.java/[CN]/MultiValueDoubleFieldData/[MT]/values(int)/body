{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] == 0) {
      break;
    }
    length++;
  }
  if (length == 0) {
    return EMPTY_DOUBLE_ARRAY;
  }
  double[] doubles;
  if (length < VALUE_CACHE_SIZE) {
    doubles=valuesCache.get().get()[length];
  }
 else {
    doubles=new double[length];
  }
  for (int i=0; i < length; i++) {
    doubles[i]=values[ordinals[i][docId]];
  }
  return doubles;
}

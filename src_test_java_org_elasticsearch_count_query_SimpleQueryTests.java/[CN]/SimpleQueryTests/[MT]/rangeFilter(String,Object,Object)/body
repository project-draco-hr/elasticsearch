{
  if (randomBoolean()) {
    if (randomBoolean()) {
      return FilterBuilders.rangeFilter(field).from(from).to(to);
    }
 else {
      return FilterBuilders.rangeFilter(field).from(from).to(to).setExecution("fielddata");
    }
  }
 else {
    return FilterBuilders.numericRangeFilter(field).from(from).to(to);
  }
}

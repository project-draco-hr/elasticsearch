{
  try {
    Map<String,Object> allVars=new HashMap<>();
    if (vars != null) {
      allVars.putAll(vars);
    }
    Script scriptObject=createScript(compiledScript,allVars);
    return scriptObject.run();
  }
 catch (  Exception e) {
    throw new ScriptException("failed to execute script",e);
  }
}

{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] != 0) {
      length++;
    }
  }
  if (length == 0) {
    return EMPTY_INT_ARRAY;
  }
  int[] ints;
  if (length < VALUE_CACHE_SIZE) {
    ints=valuesCache.get().get()[length];
  }
 else {
    ints=new int[length];
  }
  int i=0;
  for (  int[] ordinal : ordinals) {
    int loc=ordinal[docId];
    if (loc != 0) {
      ints[i++]=values[loc];
    }
  }
  return ints;
}

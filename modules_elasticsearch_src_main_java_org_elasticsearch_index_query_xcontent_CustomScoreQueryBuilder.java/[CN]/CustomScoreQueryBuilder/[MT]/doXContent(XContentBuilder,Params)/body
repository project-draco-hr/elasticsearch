{
  builder.startObject(CustomScoreQueryParser.NAME);
  builder.field("query");
  queryBuilder.toXContent(builder,params);
  builder.field("script",script);
  if (lang != null) {
    builder.field("lang",lang);
  }
  if (this.params != null) {
    builder.field("params",this.params);
  }
  if (boost != -1) {
    builder.field("boost",boost);
  }
  builder.endObject();
}

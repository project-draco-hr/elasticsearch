{
  super(similarity);
  this.reader=reader;
  theScore=w.getValue();
  DocIdSet docIdSet=filter.getDocIdSet(reader);
  if (docIdSet == null) {
    _innerIter=DocIdSet.EMPTY_DOCIDSET.iterator();
  }
 else {
    DocIdSetIterator iter=docIdSet.iterator();
    if (iter == null) {
      _innerIter=DocIdSet.EMPTY_DOCIDSET.iterator();
    }
 else {
      _innerIter=iter;
    }
  }
}

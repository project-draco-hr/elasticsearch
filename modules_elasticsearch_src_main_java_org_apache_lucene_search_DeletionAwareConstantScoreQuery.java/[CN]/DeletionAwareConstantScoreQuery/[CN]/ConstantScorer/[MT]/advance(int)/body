{
  if (deletionAware) {
    int doc=docIdSetIterator.advance(target);
    if (doc == NO_MORE_DOCS) {
      return doc;
    }
    if (!reader.isDeleted(doc)) {
      return doc;
    }
    while ((doc=nextDoc()) < target) {
      if (!reader.isDeleted(doc)) {
        return doc;
      }
    }
    return doc;
  }
 else {
    return docIdSetIterator.advance(target);
  }
}

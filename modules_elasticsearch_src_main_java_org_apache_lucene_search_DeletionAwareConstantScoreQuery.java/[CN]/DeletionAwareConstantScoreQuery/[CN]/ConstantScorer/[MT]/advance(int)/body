{
  doc=_innerIter.advance(target);
  if (doc != NO_MORE_DOCS) {
    if (!reader.isDeleted(doc)) {
      return doc;
    }
 else {
      while ((doc=_innerIter.nextDoc()) != NO_MORE_DOCS) {
        if (!reader.isDeleted(doc)) {
          return doc;
        }
      }
      return doc;
    }
  }
  return doc;
}

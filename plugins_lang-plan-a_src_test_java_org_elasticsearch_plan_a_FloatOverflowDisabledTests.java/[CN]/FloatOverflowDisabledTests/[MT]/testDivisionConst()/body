    fail("didn't hit expected exception");
  }
 catch (  ArithmeticException expected) {
  }
  try {
    exec("return 1.0 / 0.0;");
    fail("didn't hit expected exception");
  }
 catch (  ArithmeticException expected) {
  }
}

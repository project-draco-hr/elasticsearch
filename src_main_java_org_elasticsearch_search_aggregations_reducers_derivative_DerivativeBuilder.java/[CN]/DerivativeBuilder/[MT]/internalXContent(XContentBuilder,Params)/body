{
  if (format != null) {
    builder.field(DerivativeParser.FORMAT.getPreferredName(),format);
  }
  if (gapPolicy != null) {
    builder.field(DerivativeParser.GAP_POLICY.getPreferredName(),gapPolicy.getName());
  }
  if (units != null) {
    builder.field(DerivativeParser.UNITS.getPreferredName(),units);
  }
  return builder;
}

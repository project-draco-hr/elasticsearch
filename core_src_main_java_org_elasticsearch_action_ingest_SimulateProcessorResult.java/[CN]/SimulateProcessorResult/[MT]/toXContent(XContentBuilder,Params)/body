{
  builder.startObject();
  if (processorTag != null) {
    builder.field(AbstractProcessorFactory.TAG_KEY,processorTag);
  }
  if (failure == null) {
    ingestDocument.toXContent(builder,params);
  }
 else {
    ElasticsearchException.renderThrowable(builder,params,failure);
  }
  builder.endObject();
  return builder;
}

{
  GZIPInputStream in=new GZIPInputStream(new FastByteArrayInputStream(value));
  try {
    int bytesRead;
    while ((bytesRead=in.read(ch.buffer)) != -1) {
      ch.bos.write(ch.buffer,0,bytesRead);
    }
    ch.bos.flush();
  }
  finally {
    try {
      in.close();
    }
 catch (    IOException ex) {
    }
  }
}

{
  this.size=size;
  this.pageSize=pageSize;
  int lastPageSize=size % pageSize;
  int fullPageCount=size / pageSize;
  pageCount=fullPageCount + (lastPageSize == 0 ? 0 : 1);
  pages=new long[pageCount][];
  for (int i=0; i < fullPageCount; ++i)   pages[i]=new long[pageSize];
  if (lastPageSize != 0)   pages[pages.length - 1]=new long[lastPageSize];
}

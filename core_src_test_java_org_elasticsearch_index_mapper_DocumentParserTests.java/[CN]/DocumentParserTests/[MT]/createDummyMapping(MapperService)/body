{
  String mapping=jsonBuilder().startObject().startObject("type").startObject("properties").startObject("a").startObject("properties").startObject("b").field("type","object").startObject("properties").startObject("c").field("type","object").endObject().endObject().endObject().endObject().endObject().endObject().endObject().endObject().string();
  DocumentMapper defaultMapper=mapperService.documentMapperParser().parse("type",new CompressedXContent(mapping));
  return defaultMapper;
}

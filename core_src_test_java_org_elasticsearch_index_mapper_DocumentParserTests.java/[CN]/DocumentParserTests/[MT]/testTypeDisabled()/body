{
  DocumentMapperParser mapperParser=createIndex("test").mapperService().documentMapperParser();
  String mapping=XContentFactory.jsonBuilder().startObject().startObject("type").field("enabled",false).endObject().endObject().string();
  DocumentMapper mapper=mapperParser.parse(mapping);
  BytesReference bytes=XContentFactory.jsonBuilder().startObject().field("field","1234").endObject().bytes();
  ParsedDocument doc=mapper.parse("test","type","1",bytes);
  assertNull(doc.rootDoc().getField("field"));
}

{
  Builder builder=ImmutableSettings.settingsBuilder().put(defaultSettings).put(getRandomNodeSettings(nodeSeed,clusterName));
  Settings settings=nodeSettingsSource.settings(nodeOrdinal);
  if (settings != null) {
    builder.put(settings);
  }
  if (others != null) {
    builder.put(others);
  }
  return builder.build();
}

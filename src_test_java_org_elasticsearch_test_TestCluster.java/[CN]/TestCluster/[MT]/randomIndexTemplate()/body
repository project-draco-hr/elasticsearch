{
  if (size() > 0) {
    ImmutableSettings.Builder builder=setRandomNormsLoading(setRandomMerge(random,ImmutableSettings.builder()).put(SETTING_INDEX_SEED,random.nextLong())).put(SETTING_NUMBER_OF_SHARDS,RandomInts.randomIntBetween(random,DEFAULT_MIN_NUM_SHARDS,DEFAULT_MAX_NUM_SHARDS)).put(SETTING_NUMBER_OF_REPLICAS,RandomInts.randomIntBetween(random,0,1));
    client().admin().indices().preparePutTemplate("random_index_template").setTemplate("*").setOrder(0).setSettings(builder).execute().actionGet();
  }
}

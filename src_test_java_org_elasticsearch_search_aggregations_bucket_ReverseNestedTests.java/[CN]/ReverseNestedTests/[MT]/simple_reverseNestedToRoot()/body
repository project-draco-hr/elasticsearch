  assertThat(tagsBuckets.get(3).getDocCount(),equalTo(1l));
  assertThat(tagsBuckets.get(4).getKey(),equalTo("e"));
  assertThat(tagsBuckets.get(4).getDocCount(),equalTo(1l));
  bucket=usernameBuckets.get(2);

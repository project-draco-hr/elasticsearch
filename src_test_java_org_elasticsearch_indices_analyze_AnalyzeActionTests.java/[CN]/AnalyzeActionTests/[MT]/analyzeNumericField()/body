{
  createIndex("test");
  ensureGreen();
  client().prepareIndex("test","test","1").setSource(XContentFactory.jsonBuilder().startObject().field("long",1l).field("double",1.0d).endObject()).setRefresh(true).execute().actionGet();
  try {
    client().admin().indices().prepareAnalyze("test","123").setField("long").execute().actionGet();
  }
 catch (  ElasticsearchIllegalArgumentException ex) {
  }
  try {
    client().admin().indices().prepareAnalyze("test","123.0").setField("double").execute().actionGet();
  }
 catch (  ElasticsearchIllegalArgumentException ex) {
  }
}

{
  repositoriesService.unregisterRepository(new RepositoriesService.UnregisterRepositoryRequest("delete_repository [" + request.name() + "]",request.name()).masterNodeTimeout(request.masterNodeTimeout()).ackTimeout(request.timeout()),new ActionListener<RepositoriesService.UnregisterRepositoryResponse>(){
    @Override public void onResponse(    RepositoriesService.UnregisterRepositoryResponse unregisterRepositoryResponse){
      listener.onResponse(new DeleteRepositoryResponse(unregisterRepositoryResponse.isAcknowledged()));
    }
    @Override public void onFailure(    Throwable e){
      listener.onFailure(e);
    }
  }
);
}

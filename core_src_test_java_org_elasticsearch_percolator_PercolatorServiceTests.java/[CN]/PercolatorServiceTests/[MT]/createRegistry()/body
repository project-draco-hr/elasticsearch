{
  Index index=new Index("_index");
  IndexSettings indexSettings=new IndexSettings(new IndexMetaData.Builder("_index").settings(Settings.builder().put(IndexMetaData.SETTING_NUMBER_OF_SHARDS,1).put(IndexMetaData.SETTING_NUMBER_OF_REPLICAS,1).put(IndexMetaData.SETTING_VERSION_CREATED,Version.CURRENT)).build(),Settings.EMPTY,Collections.emptyList());
  return new PercolatorQueriesRegistry(new ShardId(index,0),indexSettings,null);
}

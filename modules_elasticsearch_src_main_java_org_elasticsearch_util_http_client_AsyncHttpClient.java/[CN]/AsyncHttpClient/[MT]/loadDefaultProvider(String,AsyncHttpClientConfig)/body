{
  try {
    Class<AsyncHttpProvider<?>> providerClass=(Class<AsyncHttpProvider<?>>)Thread.currentThread().getContextClassLoader().loadClass(className);
    return (AsyncHttpProvider<?>)providerClass.getDeclaredConstructor(new Class[]{AsyncHttpClientConfig.class}).newInstance(new Object[]{config});
  }
 catch (  Throwable t) {
    throw new RuntimeException(t);
  }
}

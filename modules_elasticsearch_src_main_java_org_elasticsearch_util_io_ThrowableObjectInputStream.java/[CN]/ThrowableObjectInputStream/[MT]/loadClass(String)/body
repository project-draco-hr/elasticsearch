{
  Class<?> clazz;
  ClassLoader classLoader=this.classLoader;
  if (classLoader == null) {
    classLoader=Thread.currentThread().getContextClassLoader();
  }
  if (classLoader != null) {
    clazz=classLoader.loadClass(className);
  }
 else {
    clazz=Class.forName(className);
  }
  return clazz;
}

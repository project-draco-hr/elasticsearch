{
  DateFieldMapper.DateFieldType dft=(DateFieldMapper.DateFieldType)ft;
switch (propNum) {
case 0:
    dft.setDateTimeFormatter(Joda.forPattern("basic_week_date",Locale.ROOT));
  break;
case 1:
dft.setTimeUnit(TimeUnit.HOURS);
break;
default :
super.modifyProperty(ft,numProperties() - propNum - 1);
}
}

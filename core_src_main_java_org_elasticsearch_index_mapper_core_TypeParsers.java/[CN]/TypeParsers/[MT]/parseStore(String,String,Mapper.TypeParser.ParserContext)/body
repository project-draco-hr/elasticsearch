{
  if (parserContext.indexVersionCreated().onOrAfter(Version.V_3_0_0)) {
    return XContentMapValues.nodeBooleanValue(store);
  }
 else {
    if ("no".equals(store)) {
      return false;
    }
 else     if ("yes".equals(store)) {
      return true;
    }
 else {
      return lenientNodeBooleanValue(store);
    }
  }
}

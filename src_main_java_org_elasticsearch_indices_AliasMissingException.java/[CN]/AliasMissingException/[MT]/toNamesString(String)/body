{
  if (names == null || names.length == 0) {
    return "";
  }
 else   if (names.length == 1) {
    return names[0];
  }
 else {
    StringBuilder builder=new StringBuilder(names[0]);
    for (int i=1; i < names.length; i++) {
      builder.append(',').append(names[i]);
    }
    return builder.toString();
  }
}

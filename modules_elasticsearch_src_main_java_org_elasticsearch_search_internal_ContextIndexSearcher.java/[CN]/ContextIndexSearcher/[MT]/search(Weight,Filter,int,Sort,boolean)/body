{
  int limit=reader.maxDoc();
  if (limit == 0) {
    limit=1;
  }
  nDocs=Math.min(nDocs,limit);
  TopFieldCollector collector=TopFieldCollector.create(sort,nDocs,fillFields,searchContext.trackScores(),searchContext.trackScores(),!weight.scoresDocsOutOfOrder());
  search(weight,filter,collector);
  return (TopFieldDocs)collector.topDocs();
}

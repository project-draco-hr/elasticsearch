{
  if (nodesIds == null) {
    out.writeVInt(0);
  }
 else {
    out.writeVInt(nodesIds.length);
    for (    String nodeId : nodesIds) {
      out.writeUTF(nodeId);
    }
  }
  if (timeout == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    timeout.writeTo(out);
  }
}

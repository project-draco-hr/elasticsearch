{
  Search1StressTest test=new Search1StressTest().setPeriod(TimeValue.timeValueMinutes(10)).setNumberOfNodes(2).setPreIndexDocs(SizeValue.parseSizeValue("100")).setIndexers(2).setIndexerThrottle(TimeValue.timeValueMillis(100)).setSearchers(10).setSearcherThrottle(TimeValue.timeValueMillis(10)).setFlusherThrottle(TimeValue.timeValueMillis(1000)).setNumberOfIndices(10).setNumberOfTypes(5).setNumberOfValues(50).setNumberOfHits(300);
  test.run();
}

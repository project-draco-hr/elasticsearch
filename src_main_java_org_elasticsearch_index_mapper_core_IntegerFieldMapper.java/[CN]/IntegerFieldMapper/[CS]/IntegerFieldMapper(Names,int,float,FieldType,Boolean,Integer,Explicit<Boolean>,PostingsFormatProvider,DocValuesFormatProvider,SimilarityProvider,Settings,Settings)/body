{
  super(names,precisionStep,boost,fieldType,docValues,ignoreMalformed,NumericIntegerAnalyzer.buildNamedAnalyzer(precisionStep),NumericIntegerAnalyzer.buildNamedAnalyzer(Integer.MAX_VALUE),postingsProvider,docValuesProvider,similarity,fieldDataSettings,indexSettings);
  this.nullValue=nullValue;

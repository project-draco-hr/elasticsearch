{
  super(names,precisionStep,boost,fieldType,ignoreMalformed,new NamedAnalyzer("_int/" + precisionStep,new NumericIntegerAnalyzer(precisionStep)),new NamedAnalyzer("_int/max",new NumericIntegerAnalyzer(Integer.MAX_VALUE)),provider,similarity,fieldDataSettings);
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();
}

{
  super(names,precisionStep,boost,fieldType,ignoreMalformed,NumericIntegerAnalyzer.buildNamedAnalyzer(precisionStep),NumericIntegerAnalyzer.buildNamedAnalyzer(Integer.MAX_VALUE),provider,similarity,fieldDataSettings);
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();

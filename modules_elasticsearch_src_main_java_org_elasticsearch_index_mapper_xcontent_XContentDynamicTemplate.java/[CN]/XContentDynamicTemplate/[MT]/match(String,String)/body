{
  if (!patternMatch(match,name)) {
    return false;
  }
  if (patternMatch(unmatch,name)) {
    return false;
  }
  if (matchMappingType != null) {
    if (suggestedMappingType == null) {
      return false;
    }
    if (!patternMatch(matchMappingType,suggestedMappingType)) {
      return false;
    }
  }
  return true;
}

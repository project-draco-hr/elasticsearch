{
  if (1 + bufferUpto == buffers.length) {
    int[][] newBuffers=new int[(int)(buffers.length * 1.5)][];
    System.arraycopy(buffers,0,newBuffers,0,buffers.length);
    buffers=newBuffers;
  }
  buffer=buffers[1 + bufferUpto]=new int[blockSize];
  bufferUpto++;
  intUpto=0;
  intOffset+=blockSize;
}

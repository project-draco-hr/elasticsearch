{
  double distance;
  if (!fieldData.hasValue(doc)) {
    distance=Double.MAX_VALUE;
  }
 else {
    distance=geoDistance.calculate(lat,lon,fieldData.latValue(doc),fieldData.lonValue(doc),unit);
  }
  final double v2=distance;
  if (bottom > v2) {
    return 1;
  }
 else   if (bottom < v2) {
    return -1;
  }
 else {
    return 0;
  }
}

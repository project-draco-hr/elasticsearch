{
  double distance;
  if (!fieldData.hasValue(doc)) {
    distance=Double.MAX_VALUE;
  }
 else {
    GeoPoint point=fieldData.value(doc);
    distance=geoDistance.calculate(lat,lon,point.lat(),point.lon(),unit);
  }
  final double v2=distance;
  if (bottom > v2) {
    return 1;
  }
 else   if (bottom < v2) {
    return -1;
  }
 else {
    return 0;
  }
}

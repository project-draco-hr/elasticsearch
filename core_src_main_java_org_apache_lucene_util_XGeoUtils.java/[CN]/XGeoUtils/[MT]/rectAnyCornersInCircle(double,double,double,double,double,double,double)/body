{
  return (SloppyMath.haversin(centerLat,centerLon,rMinY,rMinX) * 1000.0 <= radius || SloppyMath.haversin(centerLat,centerLon,rMaxY,rMinX) * 1000.0 <= radius || SloppyMath.haversin(centerLat,centerLon,rMaxY,rMaxX) * 1000.0 <= radius || SloppyMath.haversin(centerLat,centerLon,rMinY,rMaxX) * 1000.0 <= radius);
}

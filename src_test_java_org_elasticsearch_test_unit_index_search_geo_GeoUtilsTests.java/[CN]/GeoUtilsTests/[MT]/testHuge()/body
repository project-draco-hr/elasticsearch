{
  assertThat(GeoUtils.normalizeLon(-36000000000181.0),equalTo(GeoUtils.normalizeLon(-181.0)));
  assertThat(GeoUtils.normalizeLon(-36000000000180.0),equalTo(GeoUtils.normalizeLon(-180.0)));

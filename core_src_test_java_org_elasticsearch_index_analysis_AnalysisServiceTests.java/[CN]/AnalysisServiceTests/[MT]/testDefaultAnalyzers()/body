{
  Version version=VersionUtils.randomVersion(random());
  Settings settings=Settings.builder().put(IndexMetaData.SETTING_VERSION_CREATED,version).put(Environment.PATH_HOME_SETTING.getKey(),createTempDir().toString()).build();
  IndexSettings idxSettings=IndexSettingsModule.newIndexSettings("index",settings);
  AnalysisService analysisService=new AnalysisRegistry(new Environment(settings),emptyMap(),emptyMap(),emptyMap(),emptyMap()).build(idxSettings);
  assertThat(analysisService.defaultIndexAnalyzer().analyzer(),instanceOf(StandardAnalyzer.class));
  assertThat(analysisService.defaultSearchAnalyzer().analyzer(),instanceOf(StandardAnalyzer.class));
  assertThat(analysisService.defaultSearchQuoteAnalyzer().analyzer(),instanceOf(StandardAnalyzer.class));
}

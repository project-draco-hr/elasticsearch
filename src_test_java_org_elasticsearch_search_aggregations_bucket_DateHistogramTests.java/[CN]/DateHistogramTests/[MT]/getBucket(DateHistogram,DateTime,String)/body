{
  if (randomBoolean()) {
    if (randomBoolean()) {
      return histogram.getBucketByKey(key);
    }
    return histogram.getBucketByKey(key.getMillis());
  }
  if (randomBoolean()) {
    return histogram.getBucketByKey("" + key.getMillis());
  }
  return histogram.getBucketByKey(Joda.forPattern(format).printer().print(key));
}

{
  SearchResponse response=client().prepareSearch("idx_unmapped").addAggregation(dateHistogram("histo").field("date").interval(DateHistogramInterval.MONTH)).execute().actionGet();
  assertSearchResponse(response);
  Histogram histo=response.getAggregations().get("histo");
  assertThat(histo,notNullValue());
  assertThat(histo.getName(),equalTo("histo"));
  assertThat(histo.getBuckets().size(),equalTo(0));
}

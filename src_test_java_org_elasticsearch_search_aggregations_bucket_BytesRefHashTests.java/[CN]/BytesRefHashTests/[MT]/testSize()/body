{
  BytesRef ref=new BytesRef();
  int num=atLeast(2);
  for (int j=0; j < num; j++) {
    final int mod=1 + randomInt(40);
    for (int i=0; i < 797; i++) {
      String str;
      do {
        str=_TestUtil.randomRealisticUnicodeString(getRandom(),1000);
      }
 while (str.length() == 0);
      ref.copyChars(str);
      long count=hash.size();
      long key=hash.add(ref);
      if (key < 0)       assertEquals(hash.size(),count);
 else       assertEquals(hash.size(),count + 1);
      if (i % mod == 0) {
        newHash();
      }
    }
  }
  hash.release();
}

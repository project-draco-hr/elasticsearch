{
  if (event.getLevel() == level && event.getLoggerName().equals(logger)) {
    if (Regex.isSimpleMatchPattern(message)) {
      if (Regex.simpleMatch(message,event.getMessage().toString())) {
        saw=true;
      }
    }
 else {
      if (event.getMessage().toString().contains(message)) {
        saw=true;
      }
    }
  }
}

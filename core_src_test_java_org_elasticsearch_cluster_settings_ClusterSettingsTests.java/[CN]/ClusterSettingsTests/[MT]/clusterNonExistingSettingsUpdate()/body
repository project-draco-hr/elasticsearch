{
  String key1="no_idea_what_you_are_talking_about";
  int value1=10;
  ClusterUpdateSettingsResponse response=client().admin().cluster().prepareUpdateSettings().setTransientSettings(Settings.builder().put(key1,value1).build()).get();
  assertAcked(response);
  assertThat(response.getTransientSettings().getAsMap().entrySet(),Matchers.emptyIterable());
}

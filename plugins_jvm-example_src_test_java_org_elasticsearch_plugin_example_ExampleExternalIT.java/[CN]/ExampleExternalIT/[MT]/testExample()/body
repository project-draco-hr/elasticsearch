{
  String host=Objects.requireNonNull(System.getProperty("external.host"));
  int port=Integer.parseInt(System.getProperty("external.port"));
  try (Socket socket=new Socket(host,port);BufferedReader reader=new BufferedReader(new InputStreamReader(socket.getInputStream(),StandardCharsets.UTF_8))){
    assertEquals("TEST",reader.readLine());
  }
 }

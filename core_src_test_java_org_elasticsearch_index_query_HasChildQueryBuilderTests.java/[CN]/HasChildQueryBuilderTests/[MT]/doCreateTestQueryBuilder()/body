{
  int min=randomIntBetween(0,Integer.MAX_VALUE / 2);
  int max=randomIntBetween(min,Integer.MAX_VALUE);
  InnerHitsBuilder.InnerHit innerHit=new InnerHitsBuilder.InnerHit().setSize(100).addSort(STRING_FIELD_NAME,SortOrder.ASC);
  return new HasChildQueryBuilder(CHILD_TYPE,RandomQueryBuilder.createQuery(random()),max,min,RandomPicks.randomFrom(random(),ScoreType.values()),SearchContext.current() == null ? null : new QueryInnerHits("inner_hits_name",innerHit));
}

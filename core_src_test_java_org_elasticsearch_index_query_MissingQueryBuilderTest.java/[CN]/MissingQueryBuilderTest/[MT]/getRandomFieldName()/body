{
  if (randomBoolean()) {
    return randomAsciiOfLengthBetween(1,10);
  }
switch (randomIntBetween(0,3)) {
case 0:
    return BOOLEAN_FIELD_NAME;
case 1:
  return STRING_FIELD_NAME;
case 2:
return INT_FIELD_NAME;
case 3:
return DOUBLE_FIELD_NAME;
default :
throw new UnsupportedOperationException();
}
}

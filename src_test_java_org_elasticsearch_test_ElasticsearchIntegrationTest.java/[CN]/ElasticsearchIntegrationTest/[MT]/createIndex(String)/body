{
  List<String> created=new ArrayList<String>();
  for (  String name : names) {
    boolean success=false;
    try {
      assertAcked(prepareCreate(name));
      created.add(name);
      success=true;
    }
  finally {
      if (!success) {
        wipeIndices(created.toArray(new String[0]));
      }
    }
  }
}

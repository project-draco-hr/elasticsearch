{
  return updatedMatcher.matches(item.getUpdated()) && (batchesMatcher == null || batchesMatcher.matches(item.getBatches())) && versionConflictsMatcher.matches(item.getVersionConflicts())&& failuresMatcher.matches(item.getIndexingFailures().size())&& reasonCancelledMatcher.matches(item.getReasonCancelled());
}

{
  out.writeUTF(index);
  out.writeUTF(id);
  out.writeUTF(type);
  out.writeLong(version);
  if (matches == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeVInt(matches.size());
    for (    String match : matches) {
      out.writeUTF(match);
    }
  }
  if (getResult == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    getResult.writeTo(out);
  }
}

{
  out.writeUTF(index);
  out.writeUTF(id);
  out.writeUTF(type);
  out.writeLong(version);
  if (matches == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeVInt(matches.size());
    for (    String match : matches) {
      out.writeUTF(match);
    }
  }
  if (fields == null) {
    out.writeVInt(0);
  }
 else {
    out.writeVInt(fields.size());
    for (    GetField field : fields.values()) {
      field.writeTo(out);
    }
  }
}

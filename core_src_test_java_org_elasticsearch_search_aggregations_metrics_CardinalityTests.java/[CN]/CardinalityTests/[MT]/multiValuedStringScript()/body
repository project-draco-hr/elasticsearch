{
  SearchResponse response=client().prepareSearch("idx").setTypes("type").addAggregation(cardinality("cardinality").precisionThreshold(precisionThreshold).script(new Script("doc['str_values'].values"))).execute().actionGet();
  assertSearchResponse(response);
  Cardinality count=response.getAggregations().get("cardinality");
  assertThat(count,notNullValue());
  assertThat(count.getName(),equalTo("cardinality"));
  assertCount(count,numDocs * 2);
}

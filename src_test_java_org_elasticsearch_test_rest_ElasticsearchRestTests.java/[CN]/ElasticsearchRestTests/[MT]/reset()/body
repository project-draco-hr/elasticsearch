{
  restTestExecutionContext.resetClient(immutableCluster().httpAddresses());
  restTestExecutionContext.clear();
  assumeFalse(buildSkipMessage(testCandidate.getSuiteDescription(),testCandidate.getSetupSection().getSkipSection()),testCandidate.getSetupSection().getSkipSection().skip(restTestExecutionContext.esVersion()));
  assumeFalse(buildSkipMessage(testCandidate.getDescription(),testCandidate.getTestSection().getSkipSection()),testCandidate.getTestSection().getSkipSection().skip(restTestExecutionContext.esVersion()));
}

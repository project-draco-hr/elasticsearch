{
  String mapping=XContentFactory.jsonBuilder().startObject().startObject("type").field("dynamic","strict").startObject("properties").startObject("field1").field("type","string").endObject().endObject().endObject().endObject().string();
  DocumentMapper defaultMapper=createIndex("test").mapperService().documentMapperParser().parse(mapping);
  try {
    defaultMapper.parse("type","1",XContentFactory.jsonBuilder().startObject().field("field1","value1").field("field2","value2").bytes());
    fail();
  }
 catch (  StrictDynamicMappingException e) {
  }
  try {
    defaultMapper.parse("type","1",XContentFactory.jsonBuilder().startObject().field("field1","value1").field("field2",(String)null).bytes());
    fail();
  }
 catch (  StrictDynamicMappingException e) {
  }
}

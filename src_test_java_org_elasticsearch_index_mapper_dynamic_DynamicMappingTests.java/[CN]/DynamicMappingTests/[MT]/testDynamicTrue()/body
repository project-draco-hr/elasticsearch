{
  String mapping=XContentFactory.jsonBuilder().startObject().startObject("type").field("dynamic","true").startObject("properties").startObject("field1").field("type","string").endObject().endObject().endObject().endObject().string();
  DocumentMapper defaultMapper=createIndex("test").mapperService().documentMapperParser().parse(mapping);
  ParsedDocument doc=defaultMapper.parse("type","1",XContentFactory.jsonBuilder().startObject().field("field1","value1").field("field2","value2").bytes());
  assertThat(doc.rootDoc().get("field1"),equalTo("value1"));
  assertThat(doc.rootDoc().get("field2"),equalTo("value2"));
}

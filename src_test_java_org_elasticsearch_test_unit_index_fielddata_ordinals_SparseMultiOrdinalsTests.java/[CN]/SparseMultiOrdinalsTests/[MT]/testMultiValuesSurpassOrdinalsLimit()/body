{
  int maxDoc=2;
  int maxOrds=128;
  int[][] ords=new int[maxOrds][maxDoc];
  ords[0][0]=2;
  ords[1][0]=4;
  for (int i=0; i < maxOrds; i++) {
    ords[i][1]=(i + 1);
  }
  try {
    creationMultiOrdinals(ords,maxOrds);
    fail("Exception should have been throwed");
  }
 catch (  ElasticSearchException e) {
  }
}

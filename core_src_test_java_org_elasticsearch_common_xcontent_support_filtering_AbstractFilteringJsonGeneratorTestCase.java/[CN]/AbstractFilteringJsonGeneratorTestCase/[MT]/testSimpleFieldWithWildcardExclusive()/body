{
  XContentBuilder expected=newXContentBuilder(true).startObject().field("title","My awesome book").field("pages",456).field("timestamp",1428582942867L).nullField("default").startArray("tags").value("elasticsearch").value("java").endArray().startArray("authors").startObject().field("name","John Doe").field("lastname","John").field("firstname","Doe").endObject().startObject().field("name","William Smith").field("lastname","William").field("firstname","Smith").endObject().endArray().endObject();
  assertXContentBuilder(expected,sample(false,"pr*"));
}

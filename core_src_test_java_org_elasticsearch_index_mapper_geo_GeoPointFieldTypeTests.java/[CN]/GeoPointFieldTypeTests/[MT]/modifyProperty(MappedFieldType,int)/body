{
  GeoPointFieldMapper.GeoPointFieldType gft=(GeoPointFieldMapper.GeoPointFieldType)ft;
switch (propNum) {
case 0:
    gft.setGeohashEnabled(new StringFieldMapper.StringFieldType(),1,true);
  break;
case 1:
gft.setLatLonEnabled(new DoubleFieldMapper.DoubleFieldType(),new DoubleFieldMapper.DoubleFieldType());
break;
case 2:
gft.setIgnoreMalformed(!gft.ignoreMalformed());
break;
case 3:
gft.setCoerce(!gft.coerce());
break;
default :
super.modifyProperty(ft,propNum - 4);
}
}

{
  super.writeTo(out);
  out.writeVInt(items.size());
  for (  Item item : items) {
    out.writeVInt(item.slot);
    if (item.response != null) {
      out.writeBoolean(true);
      item.response.writeTo(out);
    }
 else {
      out.writeBoolean(false);
      out.writeThrowable(item.error);
    }
  }
}

{
  List<LikeText> likeTexts=new ArrayList<>();
  for (  MultiGetRequest.Item item : items) {
    for (    String field : item.fields()) {
      LikeText likeText=new LikeText(field,item.index() + " " + item.type()+ " "+ item.id()+ " "+ field);
      likeTexts.add(likeText);
    }
  }
  return likeTexts;
}

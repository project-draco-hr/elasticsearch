{
  IndexQueryParserService queryParser=queryParser();
  String query=copyToStringFromClasspath("/org/elasticsearch/index/query/not-filter3.json");
  Query parsedQuery=queryParser.parse(query).query();
  FilteredQuery expected=new FilteredQuery(new TermQuery(new Term("name.first","shay")),Queries.wrap(Queries.not(Queries.wrap(new TermQuery(new Term("name.first","shay1"))))));
  assertEquals(expected,parsedQuery);
}

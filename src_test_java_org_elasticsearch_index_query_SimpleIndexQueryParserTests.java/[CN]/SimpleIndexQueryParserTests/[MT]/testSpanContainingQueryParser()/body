{
  IndexQueryParserService queryParser=queryParser();
  Query expectedQuery=new SpanContainingQuery(new SpanTermQuery(new Term("age",longToPrefixCoded(34,0))),new SpanTermQuery(new Term("age",longToPrefixCoded(35,0))));
  String queryText=copyToStringFromClasspath("/org/elasticsearch/index/query/spanContaining.json");
  Query actualQuery=queryParser.parse(queryText).query();
  assertEquals(expectedQuery,actualQuery);
}

{
  IndexQueryParserService queryParser=queryParser();
  String query=copyToStringFromClasspath("/org/elasticsearch/index/query/not-filter2.json");
  Query parsedQuery=queryParser.parse(query).query();
  FilteredQuery expected=new FilteredQuery(new TermQuery(new Term("name.first","shay")),new QueryWrapperFilter(Queries.not(new TermQuery(new Term("name.first","shay1")))));
  assertEquals(expected,parsedQuery);
}

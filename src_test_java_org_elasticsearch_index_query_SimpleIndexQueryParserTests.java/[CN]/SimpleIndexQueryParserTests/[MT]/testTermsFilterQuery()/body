{
  IndexQueryParserService queryParser=queryParser();
  String query=copyToStringFromClasspath("/org/elasticsearch/index/query/terms-filter.json");
  Query parsedQuery=queryParser.parse(query).query();
  Query expected=Queries.filtered(new TermQuery(new Term("name.first","shay")),new TermsQuery("name.last",new BytesRef("banon"),new BytesRef("kimchy")));
  assertEquals(expected,parsedQuery);
}

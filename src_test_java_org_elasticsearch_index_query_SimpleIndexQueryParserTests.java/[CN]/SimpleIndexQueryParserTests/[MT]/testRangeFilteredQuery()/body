{
  IndexQueryParserService queryParser=queryParser();
  String query=copyToStringFromClasspath("/org/elasticsearch/index/query/range-filter.json");
  Query parsedQuery=queryParser.parse(query).query();
  FilteredQuery expected=new FilteredQuery(new TermQuery(new Term("name.first","shay")),Queries.wrap(NumericRangeQuery.newLongRange("age",23L,54L,true,false)));
  assertEquals(expected,parsedQuery);
}

{
  IndexQueryParserService queryParser=queryParser();
  String query=copyToStringFromClasspath("/org/elasticsearch/index/query/terms-filter-named.json");
  ParsedQuery parsedQuery=queryParser.parse(query);
  assertThat(parsedQuery.namedFilters().containsKey("test"),equalTo(true));
  Query expected=Queries.filtered(new TermQuery(new Term("name.first","shay")),new TermsQuery("name.last",new BytesRef("banon"),new BytesRef("kimchy")));
  assertEquals(expected,parsedQuery.query());
}

{
  JsonParser jp=null;
  try {
    jp=jsonFactory.createJsonParser(source);
    return parse(cache.get(),jp);
  }
 catch (  QueryParsingException e) {
    throw e;
  }
catch (  Exception e) {
    throw new QueryParsingException(index,"Failed to parse",e);
  }
 finally {
    if (jp != null) {
      try {
        jp.close();
      }
 catch (      IOException e) {
      }
    }
  }
}

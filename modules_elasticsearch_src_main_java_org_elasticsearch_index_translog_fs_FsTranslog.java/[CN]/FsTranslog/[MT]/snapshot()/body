{
synchronized (mutex) {
    try {
      raf.increaseRefCount();
      if (useStream) {
        return new FsStreamSnapshot(shardId,this.id,raf,lastPosition);
      }
 else {
        return new FsChannelSnapshot(shardId,this.id,raf,lastPosition);
      }
    }
 catch (    IOException e) {
      throw new TranslogException(shardId,"Failed to snapshot",e);
    }
  }
}

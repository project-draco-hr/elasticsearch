{
  Filter filter=mapperService().searchFilter(types);
  if (filter == null) {
    return aliasFilter;
  }
 else {
    filter=filterCache().cache(filter,null,indexService.queryParserService().autoFilterCachePolicy());
    if (aliasFilter != null) {
      return new AndFilter(ImmutableList.of(filter,aliasFilter));
    }
    return filter;
  }
}

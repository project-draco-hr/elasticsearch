{
  super.readFrom(in);
  if (in.readBoolean()) {
    os=OsStats.readOsStats(in);
  }
  if (in.readBoolean()) {
    process=ProcessStats.readProcessStats(in);
  }
  if (in.readBoolean()) {
    jvm=JvmStats.readJvmStats(in);
  }
  if (in.readBoolean()) {
    network=NetworkStats.readNetworkStats(in);
  }
}

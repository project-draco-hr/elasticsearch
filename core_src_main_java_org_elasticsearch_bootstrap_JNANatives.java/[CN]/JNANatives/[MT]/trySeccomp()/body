{
  if (Constants.LINUX && "amd64".equals(Constants.OS_ARCH)) {
    try {
      Seccomp.installFilter();
      LOCAL_SECCOMP=true;
    }
 catch (    Exception e) {
      if (logger.isDebugEnabled()) {
        logger.debug("unable to install seccomp filter",e);
      }
      logger.warn("unable to install seccomp filter: " + e.getMessage());
    }
  }
}

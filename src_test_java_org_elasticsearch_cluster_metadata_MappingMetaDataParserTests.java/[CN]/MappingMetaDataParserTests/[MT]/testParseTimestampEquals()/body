{
  MappingMetaData md1=new MappingMetaData("type1",new CompressedString(""),new MappingMetaData.Id("id"),new MappingMetaData.Routing(true,"routing"),new MappingMetaData.Timestamp(true,"timestamp","dateOptionalTime",TimestampFieldMapper.Defaults.DEFAULT_TIMESTAMP,null),false);
  MappingMetaData md2=new MappingMetaData("type1",new CompressedString(""),new MappingMetaData.Id("id"),new MappingMetaData.Routing(true,"routing"),new MappingMetaData.Timestamp(true,"timestamp","dateOptionalTime",TimestampFieldMapper.Defaults.DEFAULT_TIMESTAMP,null),false);
  assertThat(md1,equalTo(md2));
}

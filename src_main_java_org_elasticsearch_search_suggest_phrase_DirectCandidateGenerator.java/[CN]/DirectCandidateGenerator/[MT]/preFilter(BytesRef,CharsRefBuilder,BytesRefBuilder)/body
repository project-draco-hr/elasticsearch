{
  if (preFilter == null) {
    return term;
  }
  final BytesRefBuilder result=byteSpare;
  SuggestUtils.analyze(preFilter,term,field,new SuggestUtils.TokenConsumer(){
    @Override public void nextToken() throws IOException {
      this.fillBytesRef(result);
    }
  }
,spare);
  return result.get();
}

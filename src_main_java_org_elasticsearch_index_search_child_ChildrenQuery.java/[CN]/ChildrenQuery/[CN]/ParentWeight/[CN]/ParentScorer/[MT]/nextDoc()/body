{
  if (remaining == 0) {
    return NO_MORE_DOCS;
  }
  while (true) {
    currentDocId=parentsIterator.nextDoc();
    if (currentDocId == DocIdSetIterator.NO_MORE_DOCS) {
      return currentDocId;
    }
    HashedBytesArray uid=idTypeCache.idByDoc(currentDocId);
    currentScore=uidToScore.get(uid);
    if (currentScore != 0) {
      remaining--;
      return currentDocId;
    }
  }
}

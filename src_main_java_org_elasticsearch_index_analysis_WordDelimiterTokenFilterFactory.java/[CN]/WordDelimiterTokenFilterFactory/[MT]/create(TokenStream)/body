{
  return new WordDelimiterFilter(tokenStream,WordDelimiterIterator.DEFAULT_WORD_DELIM_TABLE,generateWordParts ? 1 : 0,generateNumberParts ? 1 : 0,catenateWords ? 1 : 0,catenateNumbers ? 1 : 0,catenateAll ? 1 : 0,splitOnCaseChange ? 1 : 0,preserveOriginal ? 1 : 0,splitOnNumerics ? 1 : 0,stemEnglishPossessive ? 1 : 0,protoWords);
}

{
  hits.writeTo(out);
  if (aggregations == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    aggregations.writeTo(out);
  }
  if (suggest == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    suggest.writeTo(out);
  }
  out.writeBoolean(timedOut);
  out.writeOptionalBoolean(terminatedEarly);
}

{
  try {
    client(nodeId).admin().indices().prepareDelete(index).execute().actionGet();
  }
 catch (  IndexMissingException ex) {
  }
  client(nodeId).admin().indices().prepareCreate(index).setSettings(settingsBuilder().put("index.store.distributor",distributor).put("index.store.type",storeType).put("index.store.throttle.type","none").put("index.number_of_replicas",0).put("index.number_of_shards",1)).execute().actionGet();
  assertThat(client("node1").admin().cluster().prepareHealth("test").setWaitForGreenStatus().setTimeout(TimeValue.timeValueSeconds(5)).execute().actionGet().isTimedOut(),equalTo(false));
}

{
  super(settings);
  Sigar sigar=null;
  try {
    sigar=new Sigar();
    sigar.getPid();
    logger.trace("sigar loaded successfully");
  }
 catch (  Throwable t) {
    logger.trace("failed to load sigar",t);
    if (sigar != null) {
      try {
        sigar.close();
      }
 catch (      Throwable t1) {
      }
 finally {
        sigar=null;
      }
    }
  }
  this.sigar=sigar;
}

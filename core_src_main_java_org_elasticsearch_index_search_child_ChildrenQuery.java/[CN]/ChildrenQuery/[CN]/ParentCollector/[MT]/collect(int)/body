{
  if (values != null) {
    final long globalOrdinal=values.getOrd(doc);
    if (globalOrdinal >= 0) {
      long parentIdx=parentIdxs.add(globalOrdinal);
      if (parentIdx >= 0) {
        newParent(parentIdx);
      }
 else {
        parentIdx=-1 - parentIdx;
        existingParent(parentIdx);
      }
    }
  }
}

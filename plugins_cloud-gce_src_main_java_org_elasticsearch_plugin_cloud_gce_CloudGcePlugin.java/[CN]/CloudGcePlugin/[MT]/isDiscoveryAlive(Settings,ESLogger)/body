{
  if (GceDiscovery.GCE.equalsIgnoreCase(settings.get("discovery.type")) == false) {
    logger.debug("discovery.type not set to {}",GceDiscovery.GCE);
    return false;
  }
  if (checkProperty(GceComputeService.Fields.PROJECT,settings.get(GceComputeService.Fields.PROJECT),logger) == false || checkProperty(GceComputeService.Fields.ZONE,settings.getAsArray(GceComputeService.Fields.ZONE),logger) == false) {
    logger.debug("one or more gce discovery settings are missing. " + "Check elasticsearch.yml file. Should have [{}] and [{}].",GceComputeService.Fields.PROJECT,GceComputeService.Fields.ZONE);
    return false;
  }
  logger.trace("all required properties for gce discovery are set!");
  return true;
}

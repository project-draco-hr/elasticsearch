{
  long ord=binarySearch(bytesValues,text);
  if (ord >= 0) {
    currentOrd=ord;
    currentTerm=bytesValues.getValueByOrd(currentOrd);
    return SeekStatus.FOUND;
  }
 else {
    currentOrd=-ord - 1;
    if (ord >= maxOrd) {
      return SeekStatus.END;
    }
 else {
      currentTerm=bytesValues.getValueByOrd(currentOrd);
      return SeekStatus.NOT_FOUND;
    }
  }
}

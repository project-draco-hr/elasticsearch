{
  try {
    indicesService.processPendingDeletes(indexSettings.getIndex(),indexSettings,new TimeValue(30,TimeUnit.MINUTES));
    transportService.sendRequest(clusterState.nodes().getMasterNode(),INDEX_STORE_DELETED_ACTION_NAME,new NodeIndexStoreDeletedMessage(index,nodeId),EmptyTransportResponseHandler.INSTANCE_SAME);
  }
 catch (  LockObtainFailedException exc) {
    logger.warn("[{}] failed to lock all shards for index - timed out after 30 seconds",index);
  }
catch (  InterruptedException e) {
    logger.warn("[{}] failed to lock all shards for index - interrupted",index);
  }
}

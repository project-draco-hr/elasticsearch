{
  if (indices.get(indexMetaData.index()) == indexMetaData) {
    return this;
  }
  if (incrementVersion) {
    indexMetaData=IndexMetaData.newIndexMetaDataBuilder(indexMetaData).version(indexMetaData.version() + 1).build();
  }
  indices.put(indexMetaData.index(),indexMetaData);
  return this;
}

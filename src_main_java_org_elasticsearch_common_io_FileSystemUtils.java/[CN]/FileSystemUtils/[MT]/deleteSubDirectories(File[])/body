{
  boolean deleted=true;
  for (  File root : roots) {
    if (root.isDirectory()) {
      File[] files=root.listFiles(new FileFilter(){
        @Override public boolean accept(        File pathname){
          return pathname.isDirectory();
        }
      }
);
      deleted&=deleteRecursively(files,true);
    }
  }
  return deleted;
}

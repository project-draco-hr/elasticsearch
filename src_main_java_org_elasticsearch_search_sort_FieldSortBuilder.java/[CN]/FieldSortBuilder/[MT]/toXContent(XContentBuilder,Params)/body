{
  builder.startObject(fieldName);
  if (order != null) {
    builder.field("order",order.toString());
  }
  if (missing != null) {
    builder.field("missing",missing);
  }
  if (ignoreUnampped != null) {
    builder.field("ignore_unmapped",ignoreUnampped);
  }
  if (sortMode != null) {
    builder.field("sort_mode",sortMode);
  }
  builder.endObject();
  return builder;
}

{
  Objects.requireNonNull(suggestMode,"suggestMode must not be null");
switch (suggestMode) {
case MISSING:
    return SuggestMode.SUGGEST_WHEN_NOT_IN_INDEX;
case POPULAR:
  return SuggestMode.SUGGEST_MORE_POPULAR;
case ALWAYS:
return SuggestMode.SUGGEST_ALWAYS;
default :
throw new IllegalArgumentException("Unknown suggestMode [" + suggestMode + "]");
}
}

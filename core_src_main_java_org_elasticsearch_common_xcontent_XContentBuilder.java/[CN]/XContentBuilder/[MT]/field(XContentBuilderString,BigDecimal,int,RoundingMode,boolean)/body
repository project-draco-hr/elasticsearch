{
  field(name);
  if (toDouble) {
    try {
      generator.writeNumber(value.setScale(scale,rounding).doubleValue());
    }
 catch (    ArithmeticException e) {
      generator.writeString(value.toEngineeringString());
    }
  }
 else {
    generator.writeString(value.toEngineeringString());
  }
  return this;
}

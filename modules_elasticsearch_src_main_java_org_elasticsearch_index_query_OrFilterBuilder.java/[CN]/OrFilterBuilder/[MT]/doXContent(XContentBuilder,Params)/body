{
  builder.startObject(OrFilterParser.NAME);
  builder.startArray("filters");
  for (  FilterBuilder filter : filters) {
    filter.toXContent(builder,params);
  }
  builder.endArray();
  if (cache != null) {
    builder.field("_cache",cache);
  }
  if (filterName != null) {
    builder.field("_name",filterName);
  }
  builder.endObject();
}

{
  assertThat(Operator.fromString("and"),equalTo(Operator.AND));
  assertThat(Operator.fromString("AND"),equalTo(Operator.AND));
  assertThat(Operator.fromString("AnD"),equalTo(Operator.AND));
  assertThat(Operator.fromString("or"),equalTo(Operator.OR));
  assertThat(Operator.fromString("OR"),equalTo(Operator.OR));
  assertThat(Operator.fromString("Or"),equalTo(Operator.OR));
}

{
  super(names,precisionStep,boost,fieldType,docValues,ignoreMalformed,new NamedAnalyzer("_ip/" + precisionStep,new NumericIpAnalyzer(precisionStep)),new NamedAnalyzer("_ip/max",new NumericIpAnalyzer(Integer.MAX_VALUE)),postingsProvider,docValuesProvider,similarity,normsLoading,fieldDataSettings,indexSettings);
  this.nullValue=nullValue;
}

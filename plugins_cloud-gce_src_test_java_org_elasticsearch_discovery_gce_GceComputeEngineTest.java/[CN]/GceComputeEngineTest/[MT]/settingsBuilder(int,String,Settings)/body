{
  Settings.Builder builder=Settings.settingsBuilder().put("discovery.type","gce").put("node.mode","network").put("discovery.zen.join.timeout","100ms").put("discovery.zen.ping.timeout","10ms").put("discovery.initial_state_timeout","300ms").put("transport.tcp.port",getPort(nodeOrdinal)).put("http.enabled",false).extendArray("plugin.types",CloudGcePlugin.class.getName(),mockClass).put(settings).put(super.nodeSettings(nodeOrdinal));
  return builder.build();
}

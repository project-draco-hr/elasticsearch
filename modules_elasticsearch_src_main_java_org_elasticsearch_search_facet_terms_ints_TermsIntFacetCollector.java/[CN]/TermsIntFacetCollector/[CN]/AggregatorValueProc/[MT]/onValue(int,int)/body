{
  if (script != null) {
    scriptParams.put("term",value);
    Object scriptValue=script.execute(docId,scriptParams);
    if (scriptValue == null) {
      return;
    }
    if (scriptValue instanceof Boolean) {
      if (!((Boolean)scriptValue)) {
        return;
      }
    }
 else {
      value=((Number)scriptValue).intValue();
    }
  }
  super.onValue(docId,value);
}

{
  if (script != null) {
    script.setNextDocId(docId);
    script.setNextVar("term",value);
    Object scriptValue=script.run();
    if (scriptValue == null) {
      return;
    }
    if (scriptValue instanceof Boolean) {
      if (!((Boolean)scriptValue)) {
        return;
      }
    }
 else {
      value=((Number)scriptValue).intValue();
    }
  }
  super.onValue(docId,value);
}

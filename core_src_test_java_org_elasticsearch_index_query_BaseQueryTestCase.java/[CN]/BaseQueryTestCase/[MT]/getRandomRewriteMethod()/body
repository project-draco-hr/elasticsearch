{
  String rewrite;
  if (randomBoolean()) {
    rewrite=randomFrom(new ParseField[]{QueryParsers.CONSTANT_SCORE,QueryParsers.SCORING_BOOLEAN,QueryParsers.CONSTANT_SCORE_BOOLEAN}).getPreferredName();
  }
 else {
    rewrite=randomFrom(new ParseField[]{QueryParsers.TOP_TERMS,QueryParsers.TOP_TERMS_BOOST,QueryParsers.TOP_TERMS_BLENDED_FREQS}).getPreferredName() + "1";
  }
  return rewrite;
}

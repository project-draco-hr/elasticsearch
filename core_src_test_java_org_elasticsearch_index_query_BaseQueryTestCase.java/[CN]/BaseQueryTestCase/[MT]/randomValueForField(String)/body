{
  Object value;
switch (fieldName) {
case BOOLEAN_FIELD_NAME:
    value=randomBoolean();
  break;
case INT_FIELD_NAME:
value=randomInt();
break;
case DOUBLE_FIELD_NAME:
value=randomDouble();
break;
case STRING_FIELD_NAME:
value=randomAsciiOfLengthBetween(1,10);
break;
default :
value=randomAsciiOfLengthBetween(1,10);
}
return value;
}

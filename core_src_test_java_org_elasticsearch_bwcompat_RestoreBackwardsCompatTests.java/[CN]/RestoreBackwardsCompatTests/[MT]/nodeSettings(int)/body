{
  if (randomBoolean()) {
    return settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put("path.repo",reposRoot()).build();
  }
 else {
    try {
      URI repoJarPatternUri=new URI("jar:" + reposRoot().toUri().toString() + "*.zip!/repo/");
      return settingsBuilder().put(super.nodeSettings(nodeOrdinal)).putArray("repositories.url.allowed_urls",repoJarPatternUri.toString()).build();
    }
 catch (    URISyntaxException ex) {
      throw new IllegalArgumentException(ex);
    }
  }
}

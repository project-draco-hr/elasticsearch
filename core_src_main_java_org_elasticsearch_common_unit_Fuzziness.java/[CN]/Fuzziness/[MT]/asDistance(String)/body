{
  if (fuzziness instanceof String) {
    if (this == AUTO) {
      final int len=termLen(text);
      if (len <= 2) {
        return 0;
      }
 else       if (len > 5) {
        return 2;
      }
 else {
        return 1;
      }
    }
  }
  return FuzzyQuery.floatToEdits(asFloat(),termLen(text));
}

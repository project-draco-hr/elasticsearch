{
  super.writeTo(out);
  out.writeUTF(cause);
  out.writeUTF(index);
  writeSettingsToStream(settings,out);
  timeout.writeTo(out);
  out.writeVInt(mappings.size());
  for (  Map.Entry<String,String> entry : mappings.entrySet()) {
    out.writeUTF(entry.getKey());
    out.writeUTF(entry.getValue());
  }
}

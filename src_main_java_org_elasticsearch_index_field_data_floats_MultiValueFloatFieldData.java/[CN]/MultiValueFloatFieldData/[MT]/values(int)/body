{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] == 0) {
      break;
    }
    length++;
  }
  if (length == 0) {
    return EMPTY_FLOAT_ARRAY;
  }
  float[] floats;
  if (length < VALUE_CACHE_SIZE) {
    floats=valuesCache.get().get()[length];
  }
 else {
    floats=new float[length];
  }
  for (int i=0; i < length; i++) {
    floats[i]=values[ordinals[i][docId]];
  }
  return floats;
}

{
  int length=0;
  for (  int[] ordinal : ordinals) {
    if (ordinal[docId] != 0) {
      length++;
    }
  }
  if (length == 0) {
    return EMPTY_FLOAT_ARRAY;
  }
  float[] floats;
  if (length < VALUE_CACHE_SIZE) {
    floats=valuesCache.get().get()[length];
  }
 else {
    floats=new float[length];
  }
  int i=0;
  for (  int[] ordinal : ordinals) {
    int loc=ordinal[docId];
    if (loc != 0) {
      floats[i++]=values[loc];
    }
  }
  return floats;
}

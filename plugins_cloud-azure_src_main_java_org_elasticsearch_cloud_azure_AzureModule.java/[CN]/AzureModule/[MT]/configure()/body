{
  logger.debug("starting azure services");
  if (isDiscoveryReady(settings,logger)) {
    logger.debug("starting azure discovery service");
    bind(AzureComputeService.class).to(settings.getAsClass(Management.API_IMPLEMENTATION,AzureComputeServiceImpl.class)).asEagerSingleton();
  }
  if (isSnapshotReady(settings,logger)) {
    logger.debug("starting azure repository service");
    bind(AzureStorageService.class).to(settings.getAsClass(Storage.API_IMPLEMENTATION,AzureStorageServiceImpl.class)).asEagerSingleton();
  }
}

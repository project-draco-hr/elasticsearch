{
  SearchResponse response=client().prepareSearch("idx").setTypes("type").addAggregation(cardinality("cardinality").precisionThreshold(precisionThreshold).field(multiNumericField(false)).script(new Script("_value"))).execute().actionGet();
  assertSearchResponse(response);
  Cardinality count=response.getAggregations().get("cardinality");
  assertThat(count,notNullValue());
  assertThat(count.getName(),equalTo("cardinality"));
  assertCount(count,numDocs * 2);
}

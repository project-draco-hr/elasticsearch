{
  try {
    logger.trace("committing writer with translog id [{}] ",translogId);
    indexWriter.setCommitData(Collections.singletonMap(Translog.TRANSLOG_ID_KEY,Long.toString(translogId)));
    writer.commit();
  }
 catch (  Throwable ex) {
    failEngine("lucene commit failed",ex);
    throw ex;
  }
}

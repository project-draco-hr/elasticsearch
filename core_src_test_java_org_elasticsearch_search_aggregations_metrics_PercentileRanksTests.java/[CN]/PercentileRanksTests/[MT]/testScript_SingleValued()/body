{
  final double[] pcts=randomPercents(minValue,maxValue);
  SearchResponse searchResponse=client().prepareSearch("idx").setQuery(matchAllQuery()).addAggregation(randomCompression(percentileRanks("percentile_ranks")).script(new Script("doc['value'].value")).percentiles(pcts)).execute().actionGet();
  assertThat(searchResponse.getHits().getTotalHits(),equalTo(10l));
  final PercentileRanks percentiles=searchResponse.getAggregations().get("percentile_ranks");
  assertConsistent(pcts,percentiles,minValue,maxValue);
}

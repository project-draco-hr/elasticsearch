  }
  client.admin().indices().prepareCreate("test").execute().actionGet();
  client.admin().cluster().prepareHealth().setWaitForGreenStatus().execute().actionGet();
  client.prepareIndex("test","type1").setSource(jsonBuilder().startObject().field("field","xxx").field("num",100.0).startArray("multi_num").value(1.0).value(2.0f).endArray().endObject()).execute().actionGet();
  client.prepareIndex("test","type1").setSource(jsonBuilder().startObject().field("field","xxx").field("num",200.0).startArray("multi_num").value(2.0).value(3.0f).endArray().endObject()).execute().actionGet();

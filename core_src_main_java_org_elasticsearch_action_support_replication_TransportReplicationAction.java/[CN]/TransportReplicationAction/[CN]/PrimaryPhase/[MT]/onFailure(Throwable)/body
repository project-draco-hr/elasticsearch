{
  if (ExceptionsHelper.status(e) == RestStatus.CONFLICT) {
    if (logger.isTraceEnabled()) {
      logger.trace("failed to execute [{}] on [{}]",e,request,shardId);
    }
  }
 else {
    if (logger.isDebugEnabled()) {
      logger.debug("failed to execute [{}] on [{}]",e,request,shardId);
    }
  }
  finishAsFailed(e);
}

{
  if (state != null) {
    restoreState(state);
    posIncAttr.setPositionIncrement(0);
    keywordAttribute.setKeyword(false);
    state=null;
    return true;
  }
  if (input.incrementToken()) {
    state=captureState();
    keywordAttribute.setKeyword(true);
    return true;
  }
  return false;
}

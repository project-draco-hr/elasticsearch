{
  if (transport.lifecycleState() == Lifecycle.State.STARTED) {
    try {
      transportChannel.sendResponse(e);
    }
 catch (    Throwable e1) {
      logger.warn("Failed to send error message back to client for action [" + reg.getAction() + "]",e1);
      logger.warn("Actual Exception",e);
    }
  }
}

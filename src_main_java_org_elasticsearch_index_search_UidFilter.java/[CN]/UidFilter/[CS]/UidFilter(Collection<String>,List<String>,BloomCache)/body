{
  this.bloomCache=bloomCache;
  this.uids=new Term[types.size() * ids.size()];
  int i=0;
  for (  String type : types) {
    for (    String id : ids) {
      uids[i++]=UidFieldMapper.TERM_FACTORY.createTerm(Uid.createUid(type,id));
    }
  }
  if (this.uids.length > 1) {
    Arrays.sort(this.uids);
  }
}

{
  if (settings == null) {
    return defaultValue;
  }
  Object value=settings.get(name);
  if (value == null) {
    return defaultValue;
  }
 else   if (value instanceof Boolean) {
    return (Boolean)value;
  }
  throw new ParseException("Parameter [" + name + "] must be a boolean, type `"+ value.getClass().getSimpleName()+ "` provided instead",0);
}

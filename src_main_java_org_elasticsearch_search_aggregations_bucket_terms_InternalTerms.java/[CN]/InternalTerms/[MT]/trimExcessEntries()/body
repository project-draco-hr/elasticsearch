{
  if (requiredSize >= buckets.size()) {
    return;
  }
  if (buckets instanceof List) {
    buckets=((List)buckets).subList(0,requiredSize);
    return;
  }
  int i=0;
  for (Iterator<Bucket> iter=buckets.iterator(); iter.hasNext(); ) {
    iter.next();
    if (i++ >= requiredSize) {
      iter.remove();
    }
  }
}

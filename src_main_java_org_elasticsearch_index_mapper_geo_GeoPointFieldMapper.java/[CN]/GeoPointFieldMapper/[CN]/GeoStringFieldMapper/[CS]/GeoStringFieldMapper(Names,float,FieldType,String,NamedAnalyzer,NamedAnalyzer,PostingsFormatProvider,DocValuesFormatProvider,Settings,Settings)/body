{
  super(names,boost,fieldType,nullValue,indexAnalyzer,searchAnalyzer,searchAnalyzer,Defaults.POSITION_OFFSET_GAP,Defaults.IGNORE_ABOVE,postingsProvider,docValuesProvider,null,fieldDataSettings,indexSettings);
  if (hasDocValues()) {
    throw new MapperParsingException("Field [" + names.fullName() + "] cannot have doc values");
  }
}

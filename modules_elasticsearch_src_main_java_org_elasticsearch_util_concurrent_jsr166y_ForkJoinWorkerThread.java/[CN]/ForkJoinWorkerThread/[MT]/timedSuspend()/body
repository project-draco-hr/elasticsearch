{
  long nanos=SPARE_KEEPALIVE_NANOS;
  long startTime=System.nanoTime();
  while ((runState & SUSPENDED) != 0) {
    ++parkCount;
    if ((nanos-=(System.nanoTime() - startTime)) > 0)     LockSupport.parkNanos(this,nanos);
 else {
      int s=runState;
      if (UNSAFE.compareAndSwapInt(this,runStateOffset,s,(s & ~SUSPENDED) | (TRIMMED | TERMINATING)))       return false;
    }
  }
  return true;
}

{
  try {
    if (aggregations == null) {
      aggregations=new ArrayList<>();
    }
    XContentBuilder builder=XContentFactory.jsonBuilder();
    builder.startObject();
    aggregation.toXContent(builder,EMPTY_PARAMS);
    builder.endObject();
    aggregations.add(builder.bytes());
    return this;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}

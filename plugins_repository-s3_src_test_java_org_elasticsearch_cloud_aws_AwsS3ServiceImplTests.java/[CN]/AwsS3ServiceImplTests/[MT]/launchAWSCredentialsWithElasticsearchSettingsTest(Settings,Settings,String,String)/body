{
  String key=S3Repository.getValue(singleRepositorySettings,settings,S3Repository.Repository.KEY_SETTING,S3Repository.Repositories.KEY_SETTING);
  String secret=S3Repository.getValue(singleRepositorySettings,settings,S3Repository.Repository.SECRET_SETTING,S3Repository.Repositories.SECRET_SETTING);
  AWSCredentials credentials=InternalAwsS3Service.buildCredentials(logger,key,secret).getCredentials();
  assertThat(credentials.getAWSAccessKeyId(),is(expectedKey));
  assertThat(credentials.getAWSSecretKey(),is(expectedSecret));
}

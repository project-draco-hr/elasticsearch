{
switch (value) {
case "_index":
    return INDEX;
case "_type":
  return TYPE;
case "_id":
return ID;
case "_routing":
return ROUTING;
case "_parent":
return PARENT;
case "_timestamp":
return TIMESTAMP;
case "_ttl":
return TTL;
default :
throw new IllegalArgumentException("no valid metadata field name [" + value + "]");
}
}

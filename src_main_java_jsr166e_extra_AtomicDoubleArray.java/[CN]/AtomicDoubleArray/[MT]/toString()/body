{
  int iMax=array.length - 1;
  if (iMax == -1)   return "[]";
  StringBuilder b=new StringBuilder((17 + 2) * (iMax + 1));
  b.append('[');
  for (int i=0; ; i++) {
    b.append(longBitsToDouble(getRaw(byteOffset(i))));
    if (i == iMax)     return b.append(']').toString();
    b.append(',').append(' ');
  }
}

{
  try {
    mainWithoutErrorHandling(args,terminal);
  }
 catch (  OptionException e) {
    printHelp(terminal);
    terminal.println(Terminal.Verbosity.SILENT,"ERROR: " + e.getMessage());
    return ExitCodes.USAGE;
  }
catch (  UserError e) {
    terminal.println(Terminal.Verbosity.SILENT,"ERROR: " + e.getMessage());
    return e.exitCode;
  }
  return ExitCodes.OK;
}

{
  Table table=new Table();
  table.startHeaders();
  table.addCell("time(ms)");
  table.addCell("timestamp");
  table.addCell("count");
  table.endHeaders();
  long time=System.currentTimeMillis();
  table.startRow();
  table.addCell(time);
  table.addCell(dateFormat.print(time));
  table.addCell(response.getCount());
  table.endRow();
  return table;
}

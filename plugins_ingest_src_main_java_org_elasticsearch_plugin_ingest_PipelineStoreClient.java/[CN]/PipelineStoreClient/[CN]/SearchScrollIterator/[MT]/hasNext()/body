{
  if (currentIndex < currentHits.length) {
    return true;
  }
 else {
    if (searchResponse == null) {
      return false;
    }
    searchResponse=client.prepareSearchScroll(searchResponse.getScrollId()).setScroll(scrollTimeout).get();
    if (searchResponse.getHits().getHits().length == 0) {
      searchResponse=null;
      return false;
    }
 else {
      currentHits=searchResponse.getHits().getHits();
      currentIndex=0;
      return true;
    }
  }
}

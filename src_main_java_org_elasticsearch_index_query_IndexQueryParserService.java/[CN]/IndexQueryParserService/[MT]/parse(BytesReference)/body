{
  XContentParser parser=null;
  try {
    parser=XContentFactory.xContent(source).createParser(source);
    return parse(cache.get(),parser);
  }
 catch (  QueryParsingException e) {
    throw e;
  }
catch (  Exception e) {
    throw new QueryParsingException(index,"Failed to parse",e);
  }
 finally {
    if (parser != null) {
      parser.close();
    }
  }
}

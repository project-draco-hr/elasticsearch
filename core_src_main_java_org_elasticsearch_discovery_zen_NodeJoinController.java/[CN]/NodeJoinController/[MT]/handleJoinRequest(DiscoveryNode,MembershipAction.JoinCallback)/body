{
  if (electionContext != null) {
    electionContext.addIncomingJoin(node,callback);
    checkPendingJoinsAndElectIfNeeded();
  }
 else {
    clusterService.submitStateUpdateTask("zen-disco-join(node " + node + "])",node,ClusterStateTaskConfig.build(Priority.URGENT),joinTaskExecutor,new JoinTaskListener(callback,logger));
  }
}

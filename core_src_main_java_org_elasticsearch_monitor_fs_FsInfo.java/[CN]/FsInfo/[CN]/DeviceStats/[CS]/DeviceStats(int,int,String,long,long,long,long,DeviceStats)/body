{
  this(majorDeviceNumber,minorDeviceNumber,deviceName,currentReadsCompleted,previousDeviceStats != null ? previousDeviceStats.currentReadsCompleted : -1,currentSectorsWritten,previousDeviceStats != null ? previousDeviceStats.currentSectorsWritten : -1,currentSectorsRead,previousDeviceStats != null ? previousDeviceStats.currentSectorsRead : -1,currentWritesCompleted,previousDeviceStats != null ? previousDeviceStats.currentWritesCompleted : -1);
}

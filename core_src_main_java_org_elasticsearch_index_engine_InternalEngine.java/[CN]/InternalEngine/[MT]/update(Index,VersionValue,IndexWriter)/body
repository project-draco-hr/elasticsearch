{
  boolean created;
  if (versionValue != null) {
    created=versionValue.delete();
  }
 else {
    created=false;
  }
  if (index.docs().size() > 1) {
    indexWriter.updateDocuments(index.uid(),index.docs());
  }
 else {
    indexWriter.updateDocument(index.uid(),index.docs().get(0));
  }
  return created;
}

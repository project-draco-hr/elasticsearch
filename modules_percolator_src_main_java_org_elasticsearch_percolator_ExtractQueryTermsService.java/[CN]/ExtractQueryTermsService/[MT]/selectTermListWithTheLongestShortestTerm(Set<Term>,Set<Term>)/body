{
  if (terms1 == null) {
    return terms2;
  }
 else   if (terms2 == null) {
    return terms1;
  }
 else {
    int terms1ShortestTerm=minTermLength(terms1);
    int terms2ShortestTerm=minTermLength(terms2);
    if (terms1ShortestTerm >= terms2ShortestTerm) {
      return terms1;
    }
 else {
      return terms2;
    }
  }
}

{
  if (path != null) {
    setPathLevel();
    try {
      innerFilter=parseContext.parseInnerQueryBuilder().toFilter(this.shardContext);
    }
  finally {
      resetPathLevel();
    }
    filterParsed=true;
  }
 else {
    source=XContentFactory.smileBuilder().copyCurrentStructure(parseContext.parser()).bytes();
  }
  filterFound=true;
}

{
  blobStore.executorService().execute(new Runnable(){
    @Override public void run(){
      RandomAccessFile raf=null;
      try {
        listener.withStream(out);
        out.flush();
        fsDataStream.flush();
        fsDataStream.sync();
        listener.onCompleted();
      }
 catch (      IOException e) {
        listener.onFailure(e);
      }
    }
  }
);
}

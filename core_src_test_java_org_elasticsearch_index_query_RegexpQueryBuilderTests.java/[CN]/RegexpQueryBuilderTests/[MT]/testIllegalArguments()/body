{
  try {
    if (randomBoolean()) {
      new RegexpQueryBuilder(null,"text");
    }
 else {
      new RegexpQueryBuilder("","text");
    }
    fail("cannot be null or empty");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new RegexpQueryBuilder("field",null);
    fail("cannot be null or empty");
  }
 catch (  IllegalArgumentException e) {
  }
}

{
  client().admin().indices().prepareCreate("test").addMapping("type","{\"type\":{\"properties\":{\"body\":{\"type\":\"string\", \"norms\": { \"enabled\": false }}}}}").execute().actionGet();
  PutMappingResponse putMappingResponse=client().admin().indices().preparePutMapping("test").setType("type").setSource("{\"type\":{\"properties\":{\"body\":{\"type\":\"string\", \"norms\": { \"enabled\": true }}}}}").execute().actionGet();
}

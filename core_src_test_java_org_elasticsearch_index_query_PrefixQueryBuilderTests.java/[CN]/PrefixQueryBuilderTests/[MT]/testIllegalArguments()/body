{
  try {
    if (randomBoolean()) {
      new PrefixQueryBuilder(null,"text");
    }
 else {
      new PrefixQueryBuilder("","text");
    }
    fail("cannot be null or empty");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new PrefixQueryBuilder("field",null);
    fail("cannot be null or empty");
  }
 catch (  IllegalArgumentException e) {
  }
}

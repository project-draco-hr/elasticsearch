{
  SearchResponse response=client().prepareSearch("idx").setTypes("type").addAggregation(terms("terms").field(MULTI_VALUED_FIELD_NAME).collectMode(randomFrom(SubAggCollectionMode.values())).script("floor(_value / 1000 + 1)")).execute().actionGet();
  assertSearchResponse(response);
  Terms terms=response.getAggregations().get("terms");
  assertThat(terms,notNullValue());
  assertThat(terms.getName(),equalTo("terms"));
  assertThat(terms.getBuckets().size(),equalTo(1));
  Terms.Bucket bucket=terms.getBucketByKey("1.0");
  assertThat(bucket,notNullValue());
  assertThat(key(bucket),equalTo("1.0"));
  assertThat(bucket.getKeyAsNumber().intValue(),equalTo(1));
  assertThat(bucket.getDocCount(),equalTo(5l));
}

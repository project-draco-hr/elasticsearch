{
  builder.startObject();
  if (factories != null) {
    for (    AggregatorFactory subAgg : factories) {
      subAgg.toXContent(builder,params);
    }
  }
  if (pipelineAggregatorFactories != null) {
    for (    PipelineAggregatorFactory subAgg : pipelineAggregatorFactories) {
      subAgg.toXContent(builder,params);
    }
  }
  builder.endObject();
  return builder;
}

{
  Class<?> c=findLoadedClass(name);
  if (c == null) {
    if (system != null) {
      try {
        c=system.loadClass(name);
      }
 catch (      ClassNotFoundException ignored) {
      }
    }
    if (c == null) {
      try {
        c=findClass(name);
      }
 catch (      ClassNotFoundException e) {
        c=super.loadClass(name,resolve);
      }
    }
  }
  if (resolve) {
    resolveClass(c);
  }
  return c;
}

{
  if (source == null) {
    return null;
  }
  if (sourceAsMap != null) {
    return sourceAsMap;
  }
  try {
    sourceAsMap=defaultObjectMapper().readValue(source,0,source.length,Map.class);
    return sourceAsMap;
  }
 catch (  Exception e) {
    throw new ElasticSearchParseException("Failed to parse source to map",e);
  }
}

{
  rwl.readLock().lock();
  try {
    flush(new Flush().type(Flush.Type.COMMIT).waitIfOngoing(true));
    if (indexWriter == null) {
      throw new EngineClosedException(shardId,failedEngine);
    }
    return deletionPolicy.snapshot();
  }
 catch (  IOException e) {
    throw new SnapshotFailedEngineException(shardId,e);
  }
 finally {
    rwl.readLock().unlock();
  }
}

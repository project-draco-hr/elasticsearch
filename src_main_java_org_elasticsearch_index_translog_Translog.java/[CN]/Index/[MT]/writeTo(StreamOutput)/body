{
  out.writeVInt(5);
  out.writeUTF(id);
  out.writeUTF(type);
  out.writeBytesReference(source,true);
  if (routing == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(routing);
  }
  if (parent == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(parent);
  }
  out.writeLong(version);
  out.writeLong(timestamp);
  out.writeLong(ttl);
}

{
  out.writeVInt(2);
  out.writeBytesReference(source);
  out.writeVInt(types.length);
  for (  String type : types) {
    out.writeUTF(type);
  }
  if (filteringAliases != null) {
    out.writeVInt(filteringAliases.length);
    for (    String alias : filteringAliases) {
      out.writeUTF(alias);
    }
  }
 else {
    out.writeVInt(0);
  }
}

{
  return ImmutableSettings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put("indices.cache.filter.clean_interval","1ms").put(IndicesQueryCache.INDICES_CACHE_QUERY_CLEAN_INTERVAL,"1ms").put(AutoFilterCachingPolicy.AGGRESSIVE_CACHING_SETTINGS).put(FilterCacheModule.FilterCacheSettings.FILTER_CACHE_TYPE,WeightedFilterCache.class).build();
}

{
  if (store) {
    execute.visitInvokeDynamicInsn(name,"(Ljava/lang/Object;Ljava/lang/Object;)V",WriterConstants.DEF_BOOTSTRAP_HANDLE,new Object[]{DynamicCallSite.STORE});
  }
 else {
    execute.visitInvokeDynamicInsn(name,"(Ljava/lang/Object;)Ljava/lang/Object;",WriterConstants.DEF_BOOTSTRAP_HANDLE,new Object[]{DynamicCallSite.LOAD});
  }
}

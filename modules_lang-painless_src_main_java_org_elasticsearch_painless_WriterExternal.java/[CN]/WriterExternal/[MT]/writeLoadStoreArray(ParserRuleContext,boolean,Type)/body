{
  if (type.sort == Sort.VOID) {
    throw new IllegalStateException(WriterUtility.error(source) + "Cannot load/store void type.");
  }
  if (type.sort == Sort.DEF) {
    if (store) {
      execute.visitInvokeDynamicInsn("arrayStore",WriterConstants.DEF_DYNAMIC_ARRAY_STORE_DESC,WriterConstants.DEF_BOOTSTRAP_HANDLE,new Object[]{DynamicCallSite.ARRAY_STORE});
    }
 else {
      execute.visitInvokeDynamicInsn("arrayLoad",WriterConstants.DEF_DYNAMIC_ARRAY_LOAD_DESC,WriterConstants.DEF_BOOTSTRAP_HANDLE,new Object[]{DynamicCallSite.ARRAY_LOAD});
    }
  }
 else {
    if (store) {
      execute.arrayStore(type.type);
    }
 else {
      execute.arrayLoad(type.type);
    }
  }
}

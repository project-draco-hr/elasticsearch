{
  if (wasEvicted) {
    evictionsMetric.inc();
  }
  if (sizeInBytes != -1) {
    breakerService.getBreaker().addWithoutBreaking(-sizeInBytes);
    totalMetric.dec(sizeInBytes);
    String keyFieldName=fieldNames.indexName();
    CounterMetric total=perFieldTotals.get(keyFieldName);
    if (total != null) {
      total.dec(sizeInBytes);
    }
  }
}

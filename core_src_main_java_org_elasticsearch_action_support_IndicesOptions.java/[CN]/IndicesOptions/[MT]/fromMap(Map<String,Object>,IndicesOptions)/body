{
  return fromParameters(map.containsKey("expand_wildcards") ? map.get("expand_wildcards") : map.get("expandWildcards"),map.containsKey("ignore_unavailable") ? map.get("ignore_unavailable") : map.get("ignoreUnavailable"),map.containsKey("allow_no_indices") ? map.get("allow_no_indices") : map.get("allowNoIndices"),defaultSettings);
}

{
  super.writeTo(out);
  if (indices == null) {
    out.writeVInt(0);
  }
 else {
    out.writeVInt(indices.length);
    for (    String index : indices) {
      out.writeUTF(index);
    }
  }
  if (mappingType == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(mappingType);
  }
  out.writeUTF(mappingSource);
  timeout.writeTo(out);
  out.writeBoolean(ignoreConflicts);
}

{
  return new EngineConfig(shardId,threadPool,indexSettings,warmer,store,deletionPolicy,indexSettings.getMergePolicy(),mapperService.indexAnalyzer(),similarityService.similarity(mapperService),codecService,shardEventListener,translogRecoveryPerformer,indexCache.query(),cachingPolicy,translogConfig,indexSettings.getSettings().getAsTime(IndexingMemoryController.SHARD_INACTIVE_TIME_SETTING,IndexingMemoryController.SHARD_DEFAULT_INACTIVE_TIME));
}

{
  long start=System.currentTimeMillis();
  CRC32 crc=new CRC32();
  byte[] data=new byte[BATCH_SIZE];
  long iter=dataSize / BATCH_SIZE;
  for (long i=0; i < iter; i++) {
    crc.update(data);
  }
  crc.getValue();
  System.out.println("CRC took " + new TimeValue(System.currentTimeMillis() - start));
}

{
  checkNotNull(appendable);
  checkNotNull(delimiter);
  if (tokens.hasNext()) {
    try {
      appendOneToken(appendable,tokens.next());
      while (tokens.hasNext()) {
        appendable.append(delimiter);
        appendOneToken(appendable,tokens.next());
      }
    }
 catch (    IOException e) {
      throw new JoinException(e);
    }
  }
  return appendable;
}

{
  ExtendedStatsBucketPipelineAggregator.ExtendedStatsBucketPipelineAggregatorBuilder factory=new ExtendedStatsBucketPipelineAggregator.ExtendedStatsBucketPipelineAggregatorBuilder(name,bucketsPath);
  if (randomBoolean()) {
    factory.sigma(randomDoubleBetween(0.0,10.0,false));
  }
  return factory;
}

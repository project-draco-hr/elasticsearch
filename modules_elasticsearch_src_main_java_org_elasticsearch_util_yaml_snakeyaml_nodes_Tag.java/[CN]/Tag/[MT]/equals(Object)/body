{
  if (obj == null) {
    return false;
  }
  if (obj instanceof Tag) {
    return value.equals(((Tag)obj).getValue());
  }
 else   if (obj instanceof String) {
    if (value.equals(obj.toString())) {
      System.err.println("Comparing Tag and String is deprecated.");
      return true;
    }
  }
  return false;
}

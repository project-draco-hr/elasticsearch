{
  if (settings == null) {
    return defaultValue;
  }
  Object value=settings.get(name);
  if (value == null) {
    return defaultValue;
  }
 else   if (value instanceof Integer) {
    return (Integer)value;
  }
  throw new SearchParseException(context,"Parameter [" + name + "] must be an integer, type `"+ value.getClass().getSimpleName()+ "` provided instead",null);
}

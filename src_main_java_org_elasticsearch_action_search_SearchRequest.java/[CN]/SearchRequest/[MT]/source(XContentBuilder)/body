{
  try {
    this.source=builder.underlyingBytes();
    this.sourceOffset=0;
    this.sourceLength=builder.underlyingBytesLength();
    this.sourceUnsafe=false;
    return this;
  }
 catch (  IOException e) {
    throw new ElasticSearchGenerationException("Failed to generate [" + builder + "]",e);
  }
}

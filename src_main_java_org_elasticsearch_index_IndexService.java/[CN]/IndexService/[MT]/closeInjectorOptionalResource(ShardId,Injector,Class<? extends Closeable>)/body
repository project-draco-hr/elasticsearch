{
  try {
    final Closeable instance=shardInjector.getInstance(toClose);
    if (instance == null) {
      return false;
    }
    IOUtils.close(instance);
  }
 catch (  Throwable t) {
    logger.debug("{} failed to close {}",t,shardId,Strings.toUnderscoreCase(toClose.getSimpleName()));
  }
  return true;
}

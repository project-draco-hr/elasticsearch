{
  return Objects.equals(query,that.query) && Objects.equals(type,that.type) && Objects.equals(scoreMode,that.scoreMode)&& Objects.equals(minChildren,that.minChildren)&& Objects.equals(maxChildren,that.maxChildren)&& Objects.equals(innerHitBuilder,that.innerHitBuilder)&& Objects.equals(ignoreUnmapped,that.ignoreUnmapped);
}

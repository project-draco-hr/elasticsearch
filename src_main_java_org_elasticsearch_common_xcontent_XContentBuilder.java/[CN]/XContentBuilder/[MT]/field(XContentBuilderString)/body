{
  if (fieldCaseConversion == FieldCaseConversion.UNDERSCORE) {
    generator.writeFieldName(name.underscore());
  }
 else   if (fieldCaseConversion == FieldCaseConversion.CAMELCASE) {
    generator.writeFieldName(name.camelCase());
  }
 else {
    generator.writeFieldName(name.underscore());
  }
  return this;
}

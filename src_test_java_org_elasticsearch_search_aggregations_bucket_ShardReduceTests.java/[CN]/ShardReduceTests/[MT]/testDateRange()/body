{
  SearchResponse response=client().prepareSearch("idx").setQuery(QueryBuilders.matchAllQuery()).addAggregation(dateRange("range").field("date").addRange("r1","2014-01-01","2014-01-10").subAggregation(dateHistogram("histo").field("date").interval(DateHistogram.Interval.DAY).minDocCount(0))).execute().actionGet();
  assertSearchResponse(response);
  DateRange range=response.getAggregations().get("range");
  DateHistogram histo=range.getBucketByKey("r1").getAggregations().get("histo");
  assertThat(histo.getBuckets().size(),equalTo(4));
}

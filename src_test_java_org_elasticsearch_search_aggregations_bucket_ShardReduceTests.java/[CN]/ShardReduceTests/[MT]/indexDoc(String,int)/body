{
  return client().prepareIndex("idx","type").setSource(jsonBuilder().startObject().field("value",value).field("ip","10.0.0." + value).field("location",GeoHashUtils.encode(52,5,12)).field("date",date).field("term-l",1).field("term-d",1.5).field("term-s","term").startObject("nested").field("date",date).endObject().endObject());

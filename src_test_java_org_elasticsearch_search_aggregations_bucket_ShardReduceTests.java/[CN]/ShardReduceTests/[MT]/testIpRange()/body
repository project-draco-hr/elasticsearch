{
  SearchResponse response=client().prepareSearch("idx").setQuery(QueryBuilders.matchAllQuery()).addAggregation(ipRange("range").field("ip").addRange("r1","10.0.0.1","10.0.0.10").subAggregation(dateHistogram("histo").field("date").interval(DateHistogram.Interval.DAY).minDocCount(0))).execute().actionGet();
  assertSearchResponse(response);
  IPv4Range range=response.getAggregations().get("range");
  DateHistogram histo=range.getByKey("r1").getAggregations().get("histo");
  assertThat(histo.buckets().size(),equalTo(4));
}

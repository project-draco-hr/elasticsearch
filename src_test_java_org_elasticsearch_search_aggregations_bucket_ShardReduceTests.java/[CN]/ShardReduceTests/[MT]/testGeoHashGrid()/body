{
  SearchResponse response=client().prepareSearch("idx").setQuery(QueryBuilders.matchAllQuery()).addAggregation(geohashGrid("grid").field("location").subAggregation(dateHistogram("histo").field("date").interval(DateHistogramInterval.DAY).minDocCount(0))).execute().actionGet();
  assertSearchResponse(response);
  GeoHashGrid grid=response.getAggregations().get("grid");
  Histogram histo=grid.getBuckets().iterator().next().getAggregations().get("histo");
  assertThat(histo.getBuckets().size(),equalTo(4));
}

{
  SearchResponse response=client().prepareSearch("idx").setQuery(QueryBuilders.matchAllQuery()).addAggregation(range("range").field("value").addRange("r1",0,10).subAggregation(dateHistogram("histo").field("date").interval(DateHistogram.Interval.DAY).minDocCount(0))).execute().actionGet();
  assertSearchResponse(response);
  Range range=response.getAggregations().get("range");
  DateHistogram histo=range.getBucketByKey("r1").getAggregations().get("histo");
  assertThat(histo.getBuckets().size(),equalTo(4));
}

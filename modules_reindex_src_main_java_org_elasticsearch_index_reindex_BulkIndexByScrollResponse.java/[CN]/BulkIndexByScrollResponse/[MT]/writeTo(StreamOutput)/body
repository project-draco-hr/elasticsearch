{
  super.writeTo(out);
  took.writeTo(out);
  status.writeTo(out);
  out.writeVInt(indexingFailures.size());
  for (  Failure failure : indexingFailures) {
    failure.writeTo(out);
  }
  out.writeVInt(searchFailures.size());
  for (  ShardSearchFailure failure : searchFailures) {
    failure.writeTo(out);
  }
}

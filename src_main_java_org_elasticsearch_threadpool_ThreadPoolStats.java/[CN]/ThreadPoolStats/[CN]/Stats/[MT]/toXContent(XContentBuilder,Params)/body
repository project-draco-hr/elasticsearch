{
  builder.startObject(name,XContentBuilder.FieldCaseConversion.NONE);
  if (threads != -1) {
    builder.field(Fields.THREADS,threads);
  }
  if (queue != -1) {
    builder.field(Fields.QUEUE,queue);
  }
  if (active != -1) {
    builder.field(Fields.ACTIVE,active);
  }
  builder.endObject();
  return builder;
}

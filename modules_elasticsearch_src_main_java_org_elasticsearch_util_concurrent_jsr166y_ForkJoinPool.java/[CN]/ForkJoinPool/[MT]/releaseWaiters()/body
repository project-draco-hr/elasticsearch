{
  long top;
  int id;
  while ((id=(int)((top=eventWaiters) & WAITER_INDEX_MASK)) > 0 && (int)(top >>> EVENT_COUNT_SHIFT) != eventCount) {
    ForkJoinWorkerThread[] ws=workers;
    ForkJoinWorkerThread w;
    if (ws.length >= id && (w=ws[id - 1]) != null && UNSAFE.compareAndSwapLong(this,eventWaitersOffset,top,w.nextWaiter))     LockSupport.unpark(w);
  }
}

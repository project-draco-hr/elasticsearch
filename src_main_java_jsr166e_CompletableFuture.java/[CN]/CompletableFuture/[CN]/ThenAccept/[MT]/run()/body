{
  final CompletableFuture<? extends T> a;
  final Action<? super T> fn;
  final CompletableFuture<Void> dst;
  Object r;
  T t;
  Throwable ex;
  if ((dst=this.dst) != null && (fn=this.fn) != null && (a=this.src) != null && (r=a.result) != null && compareAndSet(0,1)) {
    if (r instanceof AltResult) {
      ex=((AltResult)r).ex;
      t=null;
    }
 else {
      ex=null;
      @SuppressWarnings("unchecked") T tr=(T)r;
      t=tr;
    }
    Executor e=executor;
    if (ex == null) {
      try {
        if (e != null)         e.execute(new AsyncAccept<>(t,fn,dst));
 else         fn.accept(t);
      }
 catch (      Throwable rex) {
        ex=rex;
      }
    }
    if (e == null || ex != null)     dst.internalComplete(null,ex);
  }
}

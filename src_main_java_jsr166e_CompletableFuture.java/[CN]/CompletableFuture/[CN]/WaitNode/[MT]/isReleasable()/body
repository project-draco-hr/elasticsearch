{
  if (thread == null)   return true;
  if (Thread.interrupted()) {
    int i=interruptControl;
    interruptControl=-1;
    if (i > 0)     return true;
  }
  if (deadline != 0L && (nanos <= 0L || (nanos=deadline - System.nanoTime()) <= 0L)) {
    thread=null;
    return true;
  }
  return false;
}

{
  Object r;
  Throwable ex;
  if ((r=result) == null)   r=waitingGet(false);
  if (!(r instanceof AltResult)) {
    @SuppressWarnings("unchecked") T tr=(T)r;
    return tr;
  }
  if ((ex=((AltResult)r).ex) == null)   return null;
  if (ex instanceof CancellationException)   throw (CancellationException)ex;
  if (ex instanceof CompletionException)   throw (CompletionException)ex;
  throw new CompletionException(ex);
}

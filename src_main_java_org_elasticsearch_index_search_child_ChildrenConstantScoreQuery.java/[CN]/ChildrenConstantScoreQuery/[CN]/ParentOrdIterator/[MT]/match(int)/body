{
  if (parentWeight.remaining == 0) {
    try {
      advance(DocIdSetIterator.NO_MORE_DOCS);
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
    return false;
  }
  long parentOrd=ordinals.getOrd(doc);
  if (parentOrd >= 0) {
    boolean match=parentOrds.get(parentOrd);
    if (match) {
      parentWeight.remaining--;
    }
    return match;
  }
  return false;
}

{
  if (remaining == 0) {
    try {
      advance(DocIdSetIterator.NO_MORE_DOCS);
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
    return false;
  }
  values.setDocument(doc);
  BytesRef parentId=values.nextValue();
  int hash=values.currentValueHash();
  boolean match=parentIds.find(parentId,hash) >= 0;
  if (match) {
    remaining--;
  }
  return match;
}

{
  if (this == obj) {
    return true;
  }
  if (super.equals(obj) == false) {
    return false;
  }
  ChildrenConstantScoreQuery that=(ChildrenConstantScoreQuery)obj;
  if (!childQuery.equals(that.childQuery)) {
    return false;
  }

{
  int first=si.read();
  if (first == -1) {
    return null;
  }
  int second=si.read();
  if (second == -1) {
    return null;
  }
  if (first == 0x00 && second == 0x00) {
    return XContentType.XSON;
  }
  if (first == '{' || second == '{') {
    return XContentType.JSON;
  }
  for (int i=2; i < GUESS_HEADER_LENGTH; i++) {
    int val=si.read();
    if (val == -1) {
      return null;
    }
    if (val == '{') {
      return XContentType.JSON;
    }
  }
  return null;
}

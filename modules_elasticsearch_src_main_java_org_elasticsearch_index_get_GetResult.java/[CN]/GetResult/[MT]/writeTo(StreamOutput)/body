{
  out.writeUTF(index);
  out.writeUTF(type);
  out.writeUTF(id);
  out.writeLong(version);
  out.writeBoolean(exists);
  if (exists) {
    if (source == null) {
      out.writeBoolean(false);
    }
 else {
      out.writeBoolean(true);
      source.writeTo(out);
    }
    if (fields == null) {
      out.writeVInt(0);
    }
 else {
      out.writeVInt(fields.size());
      for (      GetField field : fields.values()) {
        field.writeTo(out);
      }
    }
  }
}

{
  super(new Names(name,indexName,indexName,name),precisionStep,boost,fieldType,docValues,Defaults.IGNORE_MALFORMED,NumericFloatAnalyzer.buildNamedAnalyzer(precisionStep),NumericFloatAnalyzer.buildNamedAnalyzer(Integer.MAX_VALUE),postingsProvider,docValuesProvider,null,fieldDataSettings,indexSettings);

{
  super(new Names(name,indexName,indexName,name),precisionStep,boost,fieldType,docValues,Defaults.IGNORE_MALFORMED,Defaults.COERCE,NumericFloatAnalyzer.buildNamedAnalyzer(precisionStep),NumericFloatAnalyzer.buildNamedAnalyzer(Integer.MAX_VALUE),postingsProvider,docValuesProvider,null,null,fieldDataSettings,indexSettings,MultiFields.empty(),null);
  this.nullValue=nullValue;
}

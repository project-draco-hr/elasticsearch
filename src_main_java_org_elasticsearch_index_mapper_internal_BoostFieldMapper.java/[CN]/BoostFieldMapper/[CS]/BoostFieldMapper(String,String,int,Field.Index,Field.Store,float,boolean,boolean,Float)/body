{
  super(new Names(name,indexName,indexName,name),precisionStep,null,index,store,boost,omitNorms,omitTermFreqAndPositions,false,new NamedAnalyzer("_float/" + precisionStep,new NumericFloatAnalyzer(precisionStep)),new NamedAnalyzer("_float/max",new NumericFloatAnalyzer(Integer.MAX_VALUE)));
  this.nullValue=nullValue;
}

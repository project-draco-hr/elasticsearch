{
  super(new Names(name,indexName,indexName,name),precisionStep,boost,fieldType,Defaults.IGNORE_MALFORMED,new NamedAnalyzer("_float/" + precisionStep,new NumericFloatAnalyzer(precisionStep)),new NamedAnalyzer("_float/max",new NumericFloatAnalyzer(Integer.MAX_VALUE)),provider,null,fieldDataSettings);
  this.nullValue=nullValue;
}

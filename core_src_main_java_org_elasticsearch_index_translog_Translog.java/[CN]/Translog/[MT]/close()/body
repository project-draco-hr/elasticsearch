{
  if (closed.compareAndSet(false,true)) {
    try (ReleasableLock lock=writeLock.acquire()){
      try {
        current.sync();
      }
  finally {
        closeFilesIfNoPendingViews();
      }
    }
  finally {
      FutureUtils.cancel(syncScheduler);
      logger.debug("translog closed");
    }
  }
}

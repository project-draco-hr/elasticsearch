{
  final List<TranslogReader> toClose=new ArrayList<>();
  try {
synchronized (this) {
      if (closed == false) {
        try {
          if (onClose != null) {
            onClose.handle(this);
          }
        }
  finally {
          closed=true;
          toClose.addAll(orderedTranslogs);
          orderedTranslogs.clear();
        }
      }
    }
  }
  finally {
    try {
      IOUtils.close(toClose);
    }
 catch (    Exception e) {
      throw new ElasticsearchException("failed to close view",e);
    }
  }
}

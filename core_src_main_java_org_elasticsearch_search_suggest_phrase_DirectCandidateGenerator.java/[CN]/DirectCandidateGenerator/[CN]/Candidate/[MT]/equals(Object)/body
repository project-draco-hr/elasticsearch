{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Candidate other=(Candidate)obj;
  if (term == null) {
    if (other.term != null)     return false;
  }
 else   if (!term.equals(other.term))   return false;
  return true;
}

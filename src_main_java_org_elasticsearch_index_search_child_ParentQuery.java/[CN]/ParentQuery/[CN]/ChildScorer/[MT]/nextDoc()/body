{
  while (true) {
    currentChildDoc=childrenIterator.nextDoc();
    if (currentChildDoc == DocIdSetIterator.NO_MORE_DOCS) {
      return currentChildDoc;
    }
    BytesReference uid=typeCache.parentIdByDoc(currentChildDoc);
    if (uid == null) {
      continue;
    }
    currentScore=uidToScore.get(uid);
    if (currentScore != 0) {
      return currentChildDoc;
    }
  }
}

{
  super.closeInternal();
  rwl.writeLock().lock();
  try {
    if (isReferencedTranslogFile(file()) == false) {
      logger.trace("delete translog file - not referenced and not current anymore {}",file());
      IOUtils.deleteFilesIgnoringExceptions(file());
    }
  }
  finally {
    rwl.writeLock().unlock();
  }
}

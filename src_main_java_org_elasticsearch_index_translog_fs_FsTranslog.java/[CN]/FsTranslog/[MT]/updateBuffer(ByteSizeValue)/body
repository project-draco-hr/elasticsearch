{
  this.bufferSize=bufferSize.bytesAsInt();
  rwl.writeLock().lock();
  try {
    FsTranslogFile current1=this.current;
    if (current1 != null) {
      current1.updateBufferSize(this.bufferSize);
    }
    current1=this.trans;
    if (current1 != null) {
      current1.updateBufferSize(this.bufferSize);
    }
  }
  finally {
    rwl.writeLock().unlock();
  }
}

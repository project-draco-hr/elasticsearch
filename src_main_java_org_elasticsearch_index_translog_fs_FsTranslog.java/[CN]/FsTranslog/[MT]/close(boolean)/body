{
  if (indexSettingsService != null) {
    indexSettingsService.removeListener(applySettings);
  }
  rwl.writeLock().lock();
  try {
    FsTranslogFile current1=this.current;
    if (current1 != null) {
      current1.close(delete);
    }
    current1=this.trans;
    if (current1 != null) {
      current1.close(delete);
    }
  }
  finally {
    rwl.writeLock().unlock();
  }
}

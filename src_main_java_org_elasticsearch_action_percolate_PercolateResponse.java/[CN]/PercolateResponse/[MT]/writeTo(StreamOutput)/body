{
  super.writeTo(out);
  out.writeVLong(tookInMillis);
  out.writeVLong(count);
  if (matches == null) {
    out.writeVInt(-1);
  }
 else {
    out.writeVInt(matches.length);
    for (    Match match : matches) {
      match.writeTo(out);
    }
  }
  out.writeOptionalStreamable(facets);
  out.writeOptionalStreamable(aggregations);
}

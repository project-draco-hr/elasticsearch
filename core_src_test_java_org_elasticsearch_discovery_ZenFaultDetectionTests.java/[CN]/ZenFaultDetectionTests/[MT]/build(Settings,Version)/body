{
  NamedWriteableRegistry namedWriteableRegistry=new NamedWriteableRegistry();
  MockTransportService transportService=new MockTransportService(Settings.builder().put(TransportService.TRACE_LOG_EXCLUDE_SETTING.getKey(),singleton(TransportLivenessAction.NAME)).build(),new LocalTransport(settings,threadPool,version,namedWriteableRegistry,circuitBreakerService),threadPool,ClusterName.DEFAULT);
  transportService.start();
  transportService.acceptIncomingRequests();
  return transportService;
}

{
  try {
    Path pluginDir=PathUtils.get(SitePluginTests.class.getResource("/org/elasticsearch/plugins").toURI());
    return settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put("path.plugins",pluginDir.toAbsolutePath()).put("force.http.enabled",true).build();
  }
 catch (  URISyntaxException ex) {
    throw new RuntimeException(ex);
  }
}

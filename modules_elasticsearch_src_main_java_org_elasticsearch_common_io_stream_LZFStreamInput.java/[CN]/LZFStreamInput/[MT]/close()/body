{
  if (cached) {
    reset();
    return;
  }
  bufferPosition=bufferLength=0;
  byte[] buf=_inputBuffer;
  if (buf != null) {
    _inputBuffer=null;
    _recycler.releaseInputBuffer(buf);
  }
  buf=_decodedBytes;
  if (buf != null) {
    _decodedBytes=null;
    _recycler.releaseDecodeBuffer(buf);
  }
  if (!inputStreamClosed) {
    inputStreamClosed=true;
    inputStream.close();
  }
}

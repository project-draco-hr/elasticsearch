{
  if (queryName == null) {
    builder.field(NAME);
    queryBuilder.toXContent(builder,params);
  }
 else {
    builder.startObject(FQUERY_NAME);
    builder.field("query");
    queryBuilder.toXContent(builder,params);
    if (queryName != null) {
      builder.field("_name",queryName);
    }
    builder.endObject();
  }
}

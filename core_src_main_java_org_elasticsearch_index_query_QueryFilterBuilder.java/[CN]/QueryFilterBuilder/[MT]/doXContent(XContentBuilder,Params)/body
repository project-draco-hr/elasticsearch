{
  if (queryName == null) {
    builder.field(QueryFilterParser.NAME);
    queryBuilder.toXContent(builder,params);
  }
 else {
    builder.startObject(FQueryFilterParser.NAME);
    builder.field("query");
    queryBuilder.toXContent(builder,params);
    if (queryName != null) {
      builder.field("_name",queryName);
    }
    builder.endObject();
  }
}

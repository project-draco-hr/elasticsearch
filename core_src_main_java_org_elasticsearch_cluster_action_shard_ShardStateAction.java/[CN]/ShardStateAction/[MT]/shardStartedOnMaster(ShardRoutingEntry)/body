{
  logger.debug("received shard started for {}",shardRoutingEntry);
  clusterService.submitStateUpdateTask("shard-started (" + shardRoutingEntry.shardRouting + "), reason ["+ shardRoutingEntry.message+ "]",shardRoutingEntry,ClusterStateTaskConfig.build(Priority.URGENT),shardStartedClusterStateHandler,shardStartedClusterStateHandler);
}

{
  logger.warn("{} received shard failed for {}",shardRoutingEntry.failure,shardRoutingEntry.shardRouting.shardId(),shardRoutingEntry);
  clusterService.submitStateUpdateTask("shard-failed (" + shardRoutingEntry.shardRouting + "), message ["+ shardRoutingEntry.message+ "]",shardRoutingEntry,ClusterStateTaskConfig.build(Priority.HIGH),shardFailedClusterStateHandler,shardFailedClusterStateHandler);
}

{
  builder.startObject(TermsQueryParser.NAME);
  builder.field(name,values);
  if (execution != null) {
    builder.field("execution",execution);
  }
  if (boost != -1) {
    builder.field("boost",boost);
  }
  if (queryName != null) {
    builder.field("_name",queryName);
  }
  builder.endObject();
}

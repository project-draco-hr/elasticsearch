{
  SoftReference<Deque<int[]>> ref=intArray.get();
  Deque<int[]> deque=ref == null ? null : ref.get();
  if (deque == null) {
    deque=new ArrayDeque<int[]>();
    intArray.set(new SoftReference<Deque<int[]>>(deque));
  }
  if (deque.isEmpty()) {
    return new int[size];
  }
  int[] ints=deque.pollFirst();
  if (ints.length < size) {
    return new int[size];
  }
  Arrays.fill(ints,sentinal);
  return ints;
}

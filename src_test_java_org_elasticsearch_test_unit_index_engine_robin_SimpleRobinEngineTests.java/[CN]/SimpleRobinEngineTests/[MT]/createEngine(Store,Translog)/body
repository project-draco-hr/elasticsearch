{
  return new RobinEngine(shardId,EMPTY_SETTINGS,new ThreadPool(),new IndexSettingsService(shardId.index(),EMPTY_SETTINGS),null,store,createSnapshotDeletionPolicy(),translog,createMergePolicy(),createMergeScheduler(),new AnalysisService(shardId.index()),new SimilarityService(shardId.index()),new NoneBloomCache(shardId.index()));
}

{
  if (isNormalized()) {
    return this;
  }
  double delta=0;
  if (lng < 0) {
    delta=360;
  }
 else   if (lng >= 0) {
    delta=-360;
  }
  double newLng=lng;
  while (newLng <= -180 || newLng >= 180) {
    newLng+=delta;
  }
  LatLng ret=new LatLng(lat,newLng);
  ret.normalized=true;
  return ret;
}

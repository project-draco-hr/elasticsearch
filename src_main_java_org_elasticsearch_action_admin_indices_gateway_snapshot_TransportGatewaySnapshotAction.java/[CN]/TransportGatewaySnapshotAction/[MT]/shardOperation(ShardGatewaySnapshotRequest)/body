{
  IndexShardGatewayService shardGatewayService=indicesService.indexServiceSafe(request.getIndex()).shardInjectorSafe(request.getShardId()).getInstance(IndexShardGatewayService.class);
  shardGatewayService.snapshot("api");
  return new ShardGatewaySnapshotResponse(request.getIndex(),request.getShardId());
}

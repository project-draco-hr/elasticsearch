{
  if (!enabled) {
    return null;
  }
  byte[] data=context.source();
  if (compress != null && compress) {
    if (compressThreshold == -1 || data.length > compressThreshold) {
      data=LZFEncoder.encodeWithCache(data,data.length);
    }
  }
  return new Field(names.indexName(),data,store);
}

{
  byte[] value=field.getBinaryValue();
  if (value == null) {
    return value;
  }
  if (LZF.isCompressed(value)) {
    try {
      return LZFDecoder.decode(value);
    }
 catch (    IOException e) {
      throw new ElasticSearchParseException("failed to decompress source",e);
    }
  }
  return value;
}

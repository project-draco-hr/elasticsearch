{
switch (flag) {
case Docs:
    builder.setDocs(set);
  break;
case FieldData:
builder.setFieldData(set);
break;
case QueryCache:
builder.setQueryCache(set);
break;
case Flush:
builder.setFlush(set);
break;
case Get:
builder.setGet(set);
break;
case Indexing:
builder.setIndexing(set);
break;
case Merge:
builder.setMerge(set);
break;
case Refresh:
builder.setRefresh(set);
break;
case Search:
builder.setSearch(set);
break;
case Store:
builder.setStore(set);
break;
case Warmer:
builder.setWarmer(set);
break;
case Completion:
builder.setCompletion(set);
break;
case Segments:
builder.setSegments(set);
break;
case Translog:
builder.setTranslog(set);
break;
case Suggest:
break;
case RequestCache:
builder.setRequestCache(set);
break;
case Recovery:
builder.setRecovery(set);
break;
default :
fail("new flag? " + flag);
break;
}
}

{
  return Settings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put(IndicesRequestCache.INDICES_CACHE_REQUEST_CLEAN_INTERVAL.getKey(),"1ms").put(IndexModule.INDEX_QUERY_CACHE_EVERYTHING_SETTING.getKey(),true).put(IndexModule.INDEX_QUERY_CACHE_TYPE_SETTING.getKey(),IndexModule.INDEX_QUERY_CACHE).build();
}

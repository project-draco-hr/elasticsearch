{
  return Settings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put(IndicesService.INDICES_CACHE_CLEAN_INTERVAL_SETTING.getKey(),"1ms").put(IndexModule.INDEX_QUERY_CACHE_EVERYTHING_SETTING.getKey(),true).put(IndexModule.INDEX_QUERY_CACHE_TYPE_SETTING.getKey(),IndexModule.INDEX_QUERY_CACHE).build();
}

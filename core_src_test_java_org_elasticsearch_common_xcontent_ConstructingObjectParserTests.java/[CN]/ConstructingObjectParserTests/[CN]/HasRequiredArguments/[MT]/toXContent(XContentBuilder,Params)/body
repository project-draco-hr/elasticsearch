{
  builder.startObject();
  builder.field("animal",animal);
  builder.field("vegetable",vegetable);
  if (mineral != 0) {
    builder.field("mineral",mineral);
  }
  if (fruit != 0) {
    builder.field("fruit",fruit);
  }
  if (a != null) {
    builder.field("a",a);
  }
  if (b != null) {
    builder.field("b",b);
  }
  if (c != null) {
    builder.field("c",c);
  }
  if (d) {
    builder.field("d",d);
  }
  builder.endObject();
  return builder;
}

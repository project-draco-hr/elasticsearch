{
  for (int s=0; s < randomIntBetween(2,10); s++) {
    if (randomBoolean()) {
      request.addSuggestion(new PhraseSuggestionBuilder("s" + s).field("f").text("test" + i + " test"+ (i - 1)));
    }
 else {
      request.addSuggestion(new TermSuggestionBuilder("s" + s).field("f").text("test" + i));
    }
  }
  return request;
}

{
  long longFlags=0;
  for (  Flag flag : flags) {
    longFlags|=(1 << flag.ordinal());
  }
  out.writeLong(longFlags);
  if (types == null) {
    out.writeVInt(0);
  }
 else {
    out.writeVInt(types.length);
    for (    String type : types) {
      out.writeString(type);
    }
  }
  if (groups == null) {
    out.writeVInt(0);
  }
 else {
    out.writeVInt(groups.length);
    for (    String group : groups) {
      out.writeString(group);
    }
  }
}

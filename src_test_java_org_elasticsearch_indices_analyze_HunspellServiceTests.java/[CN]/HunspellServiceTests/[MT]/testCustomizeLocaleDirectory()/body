{
  Settings settings=ImmutableSettings.settingsBuilder().put("indices.analysis.hunspell.dictionary.location",getResource("/indices/analyze/conf_dir/hunspell")).build();
  cluster().startNode(settings);
  HunspellDictionary dictionary=cluster().getInstance(HunspellService.class).getDictionary("en_US");
  assertThat(dictionary,notNullValue());
}

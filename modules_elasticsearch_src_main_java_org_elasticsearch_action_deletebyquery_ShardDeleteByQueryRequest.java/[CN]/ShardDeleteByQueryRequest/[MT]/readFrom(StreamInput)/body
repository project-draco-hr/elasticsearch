{
  super.readFrom(in);
  querySource=new byte[in.readVInt()];
  in.readFully(querySource);
  if (in.readBoolean()) {
    queryParserName=in.readUTF();
  }
  shardId=in.readVInt();
  int typesSize=in.readVInt();
  if (typesSize > 0) {
    types=new String[typesSize];
    for (int i=0; i < typesSize; i++) {
      types[i]=in.readUTF();
    }
  }
}

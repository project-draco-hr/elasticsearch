{
  super.writeTo(out);
  out.writeUTF(querySource);
  if (queryParserName == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(queryParserName);
  }
  out.writeInt(shardId);
  out.writeInt(types.length);
  for (  String type : types) {
    out.writeUTF(type);
  }
}

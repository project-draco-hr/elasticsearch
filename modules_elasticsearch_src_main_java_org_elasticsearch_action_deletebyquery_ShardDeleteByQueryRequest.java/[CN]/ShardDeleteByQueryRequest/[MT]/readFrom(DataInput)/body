{
  super.readFrom(in);
  querySource=in.readUTF();
  if (in.readBoolean()) {
    queryParserName=in.readUTF();
  }
  shardId=in.readInt();
  int typesSize=in.readInt();
  if (typesSize > 0) {
    types=new String[typesSize];
    for (int i=0; i < typesSize; i++) {
      types[i]=in.readUTF();
    }
  }
}

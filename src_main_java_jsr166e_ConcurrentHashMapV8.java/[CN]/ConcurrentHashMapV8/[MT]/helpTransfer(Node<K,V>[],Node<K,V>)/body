{
  Node<K,V>[] nextTab;
  int sc;
  if ((f instanceof ForwardingNode) && (nextTab=((ForwardingNode<K,V>)f).nextTable) != null) {
    if (nextTab == nextTable && tab == table && transferIndex > transferOrigin && (sc=sizeCtl) < -1 && U.compareAndSwapInt(this,SIZECTL,sc,sc - 1))     transfer(tab,nextTab);
    return nextTab;
  }
  return table;
}

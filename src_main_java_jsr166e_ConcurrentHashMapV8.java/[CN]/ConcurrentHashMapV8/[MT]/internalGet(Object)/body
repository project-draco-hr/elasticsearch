{
  int h=spread(k.hashCode());
  retry:   for (Node<V>[] tab=table; tab != null; ) {
    Node<V> e;
    Object ek;
    V ev;
    int eh;
    for (e=tabAt(tab,(tab.length - 1) & h); e != null; e=e.next) {
      if ((eh=e.hash) < 0) {
        if ((ek=e.key) instanceof TreeBin)         return ((TreeBin<V>)ek).getValue(h,k);
 else {
          tab=(Node<V>[])ek;
          continue retry;
        }
      }
 else       if (eh == h && (ev=e.val) != null && ((ek=e.key) == k || k.equals(ek)))       return ev;
    }
    break;
  }
  return null;
}

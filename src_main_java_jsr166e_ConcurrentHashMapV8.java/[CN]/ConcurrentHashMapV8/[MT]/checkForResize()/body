{
  Node[] tab;
  int n, sc;
  while ((tab=table) != null && (n=tab.length) < MAXIMUM_CAPACITY && (sc=sizeCtl) >= 0 && counter.sum() >= (long)sc && UNSAFE.compareAndSwapInt(this,sizeCtlOffset,sc,-1)) {
    try {
      if (tab == table) {
        table=rebuild(tab);
        sc=(n << 1) - (n >>> 1);
      }
    }
  finally {
      sizeCtl=sc;
    }
  }
}

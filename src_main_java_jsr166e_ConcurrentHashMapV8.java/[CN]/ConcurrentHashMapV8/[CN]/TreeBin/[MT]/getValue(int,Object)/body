{
  Node<V> r=null;
  int c=getState();
  for (Node<V> e=first; e != null; e=e.next) {
    if (c <= 0 && compareAndSetState(c,c - 1)) {
      try {
        r=getTreeNode(h,k,root);
      }
  finally {
        releaseShared(0);
      }
      break;
    }
 else     if (e.hash == h && k.equals(e.key)) {
      r=e;
      break;
    }
 else     c=getState();
  }
  return r == null ? null : r.val;
}

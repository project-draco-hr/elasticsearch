{
  boolean xor=false;
  final long start=System.nanoTime();
  for (int iter=0; iter < iters; ++iter) {
    for (int i=0; i < refs.length; ++i) {
      for (int j=i + 1; j < refs.length; ++j) {
        xor^=comparator.compare(refs[i],refs[j]);
      }
    }
  }
  DUMMY=xor;
  return System.nanoTime() - start;
}

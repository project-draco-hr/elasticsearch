{
  this.delegate=delegate;
  this.name=name;
  if (computeChecksum) {
    if ("segments.gen".equals(name)) {
      this.digest=null;
    }
 else     if (name.startsWith("segments")) {
      this.digest=null;
    }
 else {
      this.digest=new CRC32();
    }
  }
 else {
    this.digest=null;
  }
}

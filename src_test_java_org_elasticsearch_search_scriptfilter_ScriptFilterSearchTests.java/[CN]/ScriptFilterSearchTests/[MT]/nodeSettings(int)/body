{
  return ImmutableSettings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put(GroovyScriptEngineService.GROOVY_SCRIPT_SANDBOX_ENABLED,false).put(FilterCacheModule.FilterCacheSettings.FILTER_CACHE_TYPE,WeightedFilterCache.class).put(FilterCacheSettings.FILTER_CACHE_EVERYTHING,true).build();
}

{
  out.writeByte(actionType.value());
  out.writeUTF(index);
  out.writeUTF(alias);
  if (filter == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(filter);
  }
  if (indexRouting == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(indexRouting);
  }
  if (searchRouting == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeUTF(searchRouting);
  }
}

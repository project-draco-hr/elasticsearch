{
  if (keyed) {
    builder.startObject(CommonFields.BUCKETS);
  }
 else {
    builder.startArray(CommonFields.BUCKETS);
  }
  for (  Bucket bucket : buckets) {
    bucket.toXContent(builder,params,keyed);
  }
  if (keyed) {
    builder.endObject();
  }
 else {
    builder.endArray();
  }
  return builder;
}

{
  Settings settings=Settings.EMPTY;
  NetworkHandle handleA=startServices("TS_A",settings,Version.CURRENT,new ClusterName("a"));
  NetworkHandle handleB=startServices("TS_B",settings,VersionUtils.randomVersionBetween(random(),Version.CURRENT.minimumCompatibilityVersion(),Version.CURRENT),new ClusterName("b"));
  DiscoveryNode connectedNode=handleA.transportService.connectToNodeLightAndHandshake(new DiscoveryNode("",handleB.discoveryNode.getAddress(),emptyMap(),emptySet(),Version.CURRENT.minimumCompatibilityVersion()),timeout,false);
  assertNotNull(connectedNode);
  assertEquals(connectedNode.getName(),"TS_B");
  assertEquals(connectedNode.getVersion(),handleB.discoveryNode.getVersion());
}

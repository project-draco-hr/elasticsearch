{
  Settings settings=Settings.EMPTY;
  ClusterName test=new ClusterName("test");
  NetworkHandle handleA=startServices("TS_A",settings,Version.CURRENT,test);
  NetworkHandle handleB=startServices("TS_B",settings,VersionUtils.randomVersionBetween(random(),Version.CURRENT.minimumCompatibilityVersion(),Version.CURRENT),test);
  DiscoveryNode connectedNode=handleA.transportService.connectToNodeLightAndHandshake(new DiscoveryNode("",handleB.discoveryNode.getAddress(),emptyMap(),emptySet(),Version.CURRENT.minimumCompatibilityVersion()),timeout);
  assertNotNull(connectedNode);
  assertEquals(connectedNode.getName(),"TS_B");
  assertEquals(connectedNode.getVersion(),handleB.discoveryNode.getVersion());
}

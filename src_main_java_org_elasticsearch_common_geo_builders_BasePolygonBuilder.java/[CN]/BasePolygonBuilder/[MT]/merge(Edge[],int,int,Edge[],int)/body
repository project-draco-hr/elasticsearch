{
  for (int i=0; i < length; i+=2) {
    Edge e1=intersections[offset + i + 0];
    Edge e2=intersections[offset + i + 1];
    if (e2.component > 0) {
      numHoles--;
      holes[e2.component - 1]=holes[numHoles];
      holes[numHoles]=null;
    }
    if (e1.intersect != Edge.maxCoordinate() && e2.intersect != Edge.maxCoordinate()) {
      connect(e1,e2);
    }
  }
  return numHoles;
}

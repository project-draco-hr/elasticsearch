{
  client.performRequestAsync("DELETE",scrollPath(),emptyMap(),scrollEntity(scrollId),new ResponseListener(){
    @Override public void onSuccess(    org.elasticsearch.client.Response response){
      logger.debug("Successfully cleared [{}]",scrollId);
    }
    @Override public void onFailure(    Exception t){
      logger.warn(new ParameterizedMessage("Failed to clear scroll [{}]",scrollId),t);
    }
  }
);
}

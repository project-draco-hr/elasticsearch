{
  try {
    if (randomBoolean()) {
      new CommonTermsQueryBuilder(null,"text");
    }
 else {
      new CommonTermsQueryBuilder("","text");
    }
    fail("must be non null");
  }
 catch (  IllegalArgumentException e) {
  }
  try {
    new CommonTermsQueryBuilder("fieldName",null);
    fail("must be non null");
  }
 catch (  IllegalArgumentException e) {
  }
}

  this.searchScrollAction=(TransportSearchScrollAction)actions.get(TransportActions.SEARCH_SCROLL);
  this.moreLikeThisAction=(TransportMoreLikeThisAction)actions.get(TransportActions.MORE_LIKE_THIS);

{
  builder.append(",failures=[");
  Iterator<Failure> failures=getIndexingFailures().iterator();
  int written=0;
  while (failures.hasNext() && written < 3) {
    Failure failure=failures.next();
    builder.append(failure.getMessage());
    if (written != 0) {
      builder.append(", ");
    }
    written++;
  }
  builder.append(']');
}

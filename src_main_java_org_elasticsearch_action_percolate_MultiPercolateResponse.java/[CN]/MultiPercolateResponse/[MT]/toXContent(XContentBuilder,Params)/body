{
  builder.startObject();
  builder.startArray(Fields.RESPONSES);
  for (  MultiPercolateResponse.Item item : items) {
    if (item.isFailure()) {
      builder.startObject();
      builder.field(Fields.ERROR,item.getErrorMessage());
      builder.endObject();
    }
 else {
      item.getResponse().toXContent(builder,params);
    }
  }
  builder.endArray();
  builder.endObject();
  return builder;
}

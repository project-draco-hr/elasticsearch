{
  try {
    final Reader reader=Analysis.getReaderFromFile(env,settings,USER_DICT_OPTION);
    if (reader == null) {
      return null;
    }
 else {
      try {
        return new UserDictionary(reader);
      }
  finally {
        reader.close();
      }
    }
  }
 catch (  IOException e) {
    throw new ElasticSearchException("failed to load kuromoji user dictionary",e);
  }
}

{
  bind(Integer.class).annotatedWith(MergeFactor.class).toInstance(settings.getAsInt("index.merge.policy.mergeFactor",LogMergePolicy.DEFAULT_MERGE_FACTOR));
  bind(MergePolicyProvider.class).to(settings.getAsClass("index.merge.policy.type",LogByteSizeMergePolicyProvider.class)).asEagerSingleton();
}

{
  String sValue=get(settings);
  if (sValue == null) {
    return defaultValue;
  }
  try {
    return Integer.parseInt(sValue);
  }
 catch (  NumberFormatException e) {
    throw new SettingsException("Failed to parse int setting [" + Arrays.toString(settings) + "] with value ["+ sValue+ "]",e);
  }
}

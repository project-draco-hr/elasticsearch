{
  try {
    source=sourceBuilder.unsafeBytes();
    sourceOffset=0;
    sourceLength=sourceBuilder.unsafeBytesLength();
    sourceUnsafe=true;
    this.sourceFromBuilder=true;
  }
 catch (  IOException e) {
    throw new ElasticSearchGenerationException("Failed to generate [" + sourceBuilder + "]",e);
  }
  return this;
}

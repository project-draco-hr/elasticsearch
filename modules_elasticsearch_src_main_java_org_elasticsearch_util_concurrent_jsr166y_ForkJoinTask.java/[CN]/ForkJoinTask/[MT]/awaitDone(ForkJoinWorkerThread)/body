{
  if (status >= 0) {
    w.pool.preJoin(this);
    while (status >= 0) {
      try {
synchronized (this) {
          if (status >= 0)           wait();
 else {
            notifyAll();
            break;
          }
        }
      }
 catch (      InterruptedException ie) {
        cancelIfTerminating();
      }
    }
  }
}

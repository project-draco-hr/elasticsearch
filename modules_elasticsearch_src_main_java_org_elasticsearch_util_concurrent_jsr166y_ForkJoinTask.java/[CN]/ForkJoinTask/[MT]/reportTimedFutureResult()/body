{
  if (Thread.interrupted())   throw new InterruptedException();
  Throwable ex;
  int s=status & COMPLETION_MASK;
  if (s == NORMAL)   return getRawResult();
 else   if (s == CANCELLED)   throw new CancellationException();
 else   if (s == EXCEPTIONAL && (ex=exceptionMap.get(this)) != null)   throw new ExecutionException(ex);
 else   throw new TimeoutException();
}

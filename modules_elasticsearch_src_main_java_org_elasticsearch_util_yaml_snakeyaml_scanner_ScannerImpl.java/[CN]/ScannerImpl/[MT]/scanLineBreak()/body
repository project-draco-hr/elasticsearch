{
  char ch=reader.peek();
  if ("\r\n\u0085".indexOf(ch) != -1) {
    if ("\r\n".equals(reader.prefix(2))) {
      reader.forward(2);
    }
 else {
      reader.forward();
    }
    return "\n";
  }
 else   if ("\u2028\u2029".indexOf(ch) != -1) {
    reader.forward();
    return String.valueOf(ch);
  }
  return "";
}

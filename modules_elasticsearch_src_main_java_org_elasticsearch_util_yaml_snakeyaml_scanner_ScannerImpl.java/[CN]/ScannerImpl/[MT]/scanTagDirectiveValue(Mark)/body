{
  while (reader.peek() == ' ') {
    reader.forward();
  }
  String handle=scanTagDirectiveHandle(startMark);
  while (reader.peek() == ' ') {
    reader.forward();
  }
  String prefix=scanTagDirectivePrefix(startMark);
  List<String> result=new ArrayList<String>(2);
  result.add(handle);
  result.add(prefix);
  return result;
}

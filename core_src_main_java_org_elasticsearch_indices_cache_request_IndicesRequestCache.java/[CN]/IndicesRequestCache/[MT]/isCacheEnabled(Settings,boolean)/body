{
  Boolean enable=settings.getAsBoolean(INDEX_CACHE_REQUEST_ENABLED,null);
  if (enable == null) {
    enable=settings.getAsBoolean(DEPRECATED_INDEX_CACHE_REQUEST_ENABLED,null);
    if (enable != null) {
      deprecationLogger.deprecated("The [" + DEPRECATED_INDEX_CACHE_REQUEST_ENABLED + "] settings is now deprecated, use ["+ INDEX_CACHE_REQUEST_ENABLED+ "] instead");
    }
  }
  if (enable == null) {
    enable=defaultEnable;
  }
  return enable;
}

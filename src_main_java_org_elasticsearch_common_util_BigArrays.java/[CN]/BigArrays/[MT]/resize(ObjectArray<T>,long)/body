{
  if (array instanceof BigObjectArray) {
    ((BigObjectArray<?>)array).resize(size);
    return array;
  }
 else {
    final ObjectArray<T> newArray=newObjectArray(size);
    for (long i=0, end=Math.min(size,array.size()); i < end; ++i) {
      newArray.set(i,array.get(i));
    }
    array.close();
    return newArray;
  }
}

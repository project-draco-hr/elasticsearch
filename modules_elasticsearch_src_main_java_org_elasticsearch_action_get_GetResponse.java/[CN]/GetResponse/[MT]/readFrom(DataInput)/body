{
  index=in.readUTF();
  type=in.readUTF();
  id=in.readUTF();
  exists=in.readBoolean();
  if (exists) {
    int size=in.readInt();
    if (size > 0) {
      source=new byte[size];
      in.readFully(source);
    }
    size=in.readInt();
    if (size > 0) {
      fields=newHashMapWithExpectedSize(size);
      for (int i=0; i < size; i++) {
        GetField field=readGetField(in);
        fields.put(field.name(),field);
      }
    }
  }
}

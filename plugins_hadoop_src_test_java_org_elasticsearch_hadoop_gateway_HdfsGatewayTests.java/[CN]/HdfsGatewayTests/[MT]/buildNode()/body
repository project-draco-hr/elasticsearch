{
  Settings settings=settingsBuilder().put("cluster.name","test-cluster-" + NetworkUtils.getLocalAddress().getHostName()).put("gateway.type","hdfs").put("gateway.hdfs.uri","file:///").put("gateway.hdfs.path","data/hdfs/gateway").build();
  return nodeBuilder().settings(settingsBuilder().put(settings).put("node.name","node1")).build();
}

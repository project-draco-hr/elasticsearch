{
  long sizeInBytes=0;
  for (  ConcurrentMap<Filter,DocSet> map : cache.values()) {
    for (    DocSet docSet : map.values()) {
      sizeInBytes+=docSet.sizeInBytes();
    }
  }
  for (  ConcurrentMap<Filter,DocSet> map : weakCache.values()) {
    for (    DocSet docSet : map.values()) {
      sizeInBytes+=docSet.sizeInBytes();
    }
  }
  return sizeInBytes;
}

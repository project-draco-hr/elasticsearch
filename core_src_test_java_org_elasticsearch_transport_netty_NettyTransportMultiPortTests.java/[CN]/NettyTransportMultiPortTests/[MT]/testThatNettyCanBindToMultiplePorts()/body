{
  Settings settings=settingsBuilder().put("network.host",host).put("transport.tcp.port",22).put("transport.profiles.default.port",0).put("transport.profiles.client1.port",0).build();
  ThreadPool threadPool=new ThreadPool("tst");
  try (NettyTransport transport=startNettyTransport(settings,threadPool)){
    assertEquals(1,transport.profileBoundAddresses().size());
    assertEquals(1,transport.boundAddress().boundAddresses().length);
  }
  finally {
    terminate(threadPool);
  }
}

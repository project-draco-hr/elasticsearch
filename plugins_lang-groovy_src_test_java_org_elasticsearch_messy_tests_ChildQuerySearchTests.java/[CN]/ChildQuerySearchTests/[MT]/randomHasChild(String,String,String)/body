{
  if (randomBoolean()) {
    if (randomBoolean()) {
      return constantScoreQuery(hasChildQuery(type,termQuery(field,value)));
    }
 else {
      return boolQuery().must(matchAllQuery()).filter(hasChildQuery(type,termQuery(field,value)));
    }
  }
 else {
    return hasChildQuery(type,termQuery(field,value));
  }
}

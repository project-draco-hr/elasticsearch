{
  if (name == null) {
    builder.startObject();
  }
 else {
    builder.startObject(name);
  }
  if (globalText != null) {
    builder.field("text",globalText);
  }
  for (  SuggestionBuilder<?> suggestion : suggestions) {
    builder=suggestion.toXContent(builder,params);
  }
  builder.endObject();
  return builder;
}

{
  if (Thread.interrupted()) {
    wasInterrupted=true;
    if (interruptible)     return true;
  }
  if (!timed)   LockSupport.park(this);
 else {
    long waitTime=nanos - (System.nanoTime() - startTime);
    if (waitTime <= 0)     return true;
    LockSupport.parkNanos(this,waitTime);
  }
  return isReleasable();
}

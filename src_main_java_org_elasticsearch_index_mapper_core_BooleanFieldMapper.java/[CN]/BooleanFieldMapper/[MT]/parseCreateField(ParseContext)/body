{
  if (!indexed() && !stored()) {
    return null;
  }
  XContentParser.Token token=context.parser().currentToken();
  String value=null;
  if (token == XContentParser.Token.VALUE_NULL) {
    if (nullValue != null) {
      value=nullValue ? "T" : "F";
    }
  }
 else {
    value=context.parser().booleanValue() ? "T" : "F";
  }
  if (value == null) {
    return null;
  }
  return new Field(names.indexName(),value,fieldType);
}

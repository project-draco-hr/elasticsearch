{
  if (array instanceof Map) {
    return ((Map)array).get(index);
  }
 else   if (array.getClass().isArray()) {
    try {
      return Array.get(array,(int)index);
    }
 catch (    final Throwable throwable) {
      throw new IllegalArgumentException("Error loading value from " + "array class [" + array.getClass().getCanonicalName() + "].",throwable);
    }
  }
 else   if (array instanceof List) {
    return ((List)array).get((int)index);
  }
 else {
    throw new IllegalArgumentException("Attempting to address a non-array type " + "[" + array.getClass().getCanonicalName() + "] as an array.");
  }
}

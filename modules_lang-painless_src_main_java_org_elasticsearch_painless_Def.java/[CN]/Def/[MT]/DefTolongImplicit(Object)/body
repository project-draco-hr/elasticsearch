{
  if (value instanceof Byte) {
    return (byte)value;
  }
 else   if (value instanceof Short) {
    return (short)value;
  }
 else   if (value instanceof Character) {
    return (char)value;
  }
 else   if (value instanceof Integer) {
    return (int)value;
  }
 else {
    return (long)value;
  }
}

{
switch (s.toLowerCase(Locale.ROOT)) {
case "true":
case "all":
    return ONLY_DISK_ALLOWED;
case "false":
case "none":
  return EVERYTHING_ALLOWED;
case "sandbox":
case "sandboxed":
return SANDBOXED_ONLY;
default :
throw new ElasticsearchIllegalArgumentException("Unrecognized script allowance setting: [" + s + "]");
}
}

{
  int scriptTypeVal=in.readVInt();
switch (scriptTypeVal) {
case INDEXED_VAL:
    return INDEXED;
case INLINE_VAL:
  return INLINE;
case FILE_VAL:
return FILE;
default :
throw new ElasticsearchIllegalArgumentException("Unexpected value read for ScriptType got [" + scriptTypeVal + "] expected one of ["+ INLINE_VAL+ ","+ INDEXED_VAL+ ","+ FILE_VAL+ "]");
}
}

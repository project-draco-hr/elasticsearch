{
  int scriptTypeVal=in.readVInt();
  for (  ScriptType type : values()) {
    if (type.val == scriptTypeVal) {
      return type;
    }
  }
  throw new IllegalArgumentException("Unexpected value read for ScriptType got [" + scriptTypeVal + "] expected one of ["+ INLINE.val+ ","+ FILE.val+ ","+ INDEXED.val+ "]");
}

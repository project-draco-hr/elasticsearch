{
  if (scriptType != null) {
switch (scriptType) {
case INDEXED:
      out.writeVInt(INDEXED_VAL);
    return;
case INLINE:
  out.writeVInt(INLINE_VAL);
return;
case FILE:
out.writeVInt(FILE_VAL);
return;
default :
throw new ElasticsearchIllegalStateException("Unknown ScriptType " + scriptType);
}
}
 else {
out.writeVInt(INLINE_VAL);
}
}

{
  if (request.getContent().hasArray()) {
    return request.getContent().array();
  }
  if (cachedData != null) {
    return cachedData;
  }
  cachedData=new byte[request.getContent().readableBytes()];
  request.getContent().getBytes(request.getContent().readerIndex(),cachedData);
  return cachedData;
}

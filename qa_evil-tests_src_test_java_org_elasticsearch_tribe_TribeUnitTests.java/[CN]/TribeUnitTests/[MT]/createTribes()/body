{
  Settings baseSettings=Settings.builder().put("http.enabled",false).put("node.mode",NODE_MODE).put("path.home",createTempDir()).build();
  tribe1=new TribeClientNode(Settings.builder().put(baseSettings).put("cluster.name","tribe1").put("name","tribe1_node").put(DiscoveryService.SETTING_DISCOVERY_SEED,random().nextLong()).build()).start();
  tribe2=new TribeClientNode(Settings.builder().put(baseSettings).put("cluster.name","tribe2").put("name","tribe2_node").put(DiscoveryService.SETTING_DISCOVERY_SEED,random().nextLong()).build()).start();
}

{
  int pointer=lookup[docId];
  if (pointer == 0) {
    return 0;
  }
 else   if (pointer > 0) {
    return pointer;
  }
 else {
    pointer=-pointer;
    int allOrdsIndex=pointer >> parent.storageShift;
    int ordsIndex=(pointer & ((1 << parent.storageShift) - 1));
    return ordinals[allOrdsIndex][ordsIndex];
  }
}

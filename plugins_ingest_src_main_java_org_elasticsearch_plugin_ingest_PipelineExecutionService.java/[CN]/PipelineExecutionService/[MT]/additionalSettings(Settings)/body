{
  Settings settings=nodeSettings.getAsSettings("threadpool." + THREAD_POOL_NAME);
  if (!settings.names().isEmpty()) {
    return Settings.EMPTY;
  }
  int availableProcessors=EsExecutors.boundedNumberOfProcessors(nodeSettings);
  return Settings.builder().put("threadpool." + THREAD_POOL_NAME + ".type","fixed").put("threadpool." + THREAD_POOL_NAME + ".size",availableProcessors).put("threadpool." + THREAD_POOL_NAME + ".queue_size",200).build();
}

{
  FsStats.Path[] paths=new FsStats.Path[1];
  FsStats.Path path=new FsStats.Path("/path.data",null,usage.getTotalBytes(),usage.getFreeBytes(),usage.getFreeBytes());
  paths[0]=path;
  FsStats fsStats=new FsStats(System.currentTimeMillis(),paths);
  return new NodeStats(new DiscoveryNode(nodeName,null,Version.V_2_0_0),System.currentTimeMillis(),null,null,null,null,null,fsStats,null,null,null);
}

{
  FsInfo.Path[] paths=new FsInfo.Path[1];
  FsInfo.Path path=new FsInfo.Path("/path.data",null,usage.getTotalBytes(),usage.getFreeBytes(),usage.getFreeBytes());
  paths[0]=path;
  FsInfo fsInfo=new FsInfo(System.currentTimeMillis(),paths);
  return new NodeStats(new DiscoveryNode(nodeName,null,Version.V_2_0_0),System.currentTimeMillis(),null,null,null,null,null,fsInfo,null,null,null);
}

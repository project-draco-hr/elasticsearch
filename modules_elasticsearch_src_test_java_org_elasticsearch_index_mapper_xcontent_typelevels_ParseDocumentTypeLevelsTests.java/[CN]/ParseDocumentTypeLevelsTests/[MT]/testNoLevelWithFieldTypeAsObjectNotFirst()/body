{
  String defaultMapping=XContentFactory.jsonBuilder().startObject().startObject("type").endObject().endObject().string();
  XContentDocumentMapper defaultMapper=XContentMapperTests.newParser().parse(defaultMapping);
  ParsedDocument doc=defaultMapper.parse("type","1",XContentFactory.jsonBuilder().startObject().field("test1","value1").startObject("type").field("type_field","type_value").endObject().field("test2","value2").startObject("inner").field("inner_field","inner_value").endObject().endObject().copiedBytes());
  assertThat(doc.doc().get("type.type_field"),equalTo("type_value"));
  assertThat(doc.doc().get("test1"),equalTo("value1"));
  assertThat(doc.doc().get("test2"),equalTo("value2"));
  assertThat(doc.doc().get("inner.inner_field"),equalTo("inner_value"));
}

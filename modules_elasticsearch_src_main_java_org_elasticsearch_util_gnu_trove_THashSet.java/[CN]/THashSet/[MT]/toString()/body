{
  final StringBuilder buf=new StringBuilder("{");
  forEach(new TObjectProcedure(){
    private boolean first=true;
    public boolean execute(    Object value){
      if (first)       first=false;
 else       buf.append(",");
      buf.append(value);
      return true;
    }
  }
);
  buf.append("}");
  return buf.toString();
}

{
  if (filter == null || filter.isEmpty()) {
    this.filter=null;
    return this;
  }
  try {
    this.filter=new CompressedString(XContentFactory.jsonBuilder().map(filter).string());
    return this;
  }
 catch (  IOException e) {
    throw new ElasticSearchGenerationException("Failed to build json for alias request",e);
  }
}

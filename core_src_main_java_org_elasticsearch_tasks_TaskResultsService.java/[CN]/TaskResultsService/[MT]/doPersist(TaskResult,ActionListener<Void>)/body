{
  client.prepareIndex(TASK_RESULT_INDEX,TASK_RESULT_TYPE,taskResult.getTaskId().toString()).setSource(taskResult.getResult()).execute(new ActionListener<IndexResponse>(){
    @Override public void onResponse(    IndexResponse indexResponse){
      listener.onResponse(null);
    }
    @Override public void onFailure(    Throwable e){
      listener.onFailure(e);
    }
  }
);
}

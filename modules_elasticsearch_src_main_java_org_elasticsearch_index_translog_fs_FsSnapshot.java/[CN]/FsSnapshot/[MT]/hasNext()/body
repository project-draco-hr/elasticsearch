{
  try {
    if (position > length) {
      return false;
    }
    int opSize=dis.readInt();
    position+=4;
    if ((position + opSize) > length) {
      return false;
    }
    position+=opSize;
    byte[] data=new byte[opSize];
    dis.readFully(data);
    lastOperationRead=TranslogStreams.readTranslogOperation(new BytesStreamInput(data));
    return true;
  }
 catch (  Exception e) {
    return false;
  }
}

{
  super.innerToXContent(builder,params);
  if (payload != null && payload.length() > 0) {
    XContentType contentType=XContentFactory.xContentType(payload);
    if (contentType == null) {
      builder.field("payload",payload.toUtf8());
    }
 else {
      builder.rawField("payload",payload);
    }
  }
  return builder;
}

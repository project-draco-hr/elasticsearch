{
  if (name == null) {
    for (    Suggestion<?> suggestion : suggestions) {
      suggestion.toXContent(builder,params);
    }
  }
 else {
    builder.startObject(name);
    for (    Suggestion<?> suggestion : suggestions) {
      suggestion.toXContent(builder,params);
    }
    builder.endObject();
  }
  return builder;
}

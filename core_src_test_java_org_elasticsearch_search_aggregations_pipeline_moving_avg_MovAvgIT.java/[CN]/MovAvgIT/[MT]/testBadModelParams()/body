{
  try {
    SearchResponse response=client().prepareSearch("idx").setTypes("type").addAggregation(histogram("histo").field(INTERVAL_FIELD).interval(interval).extendedBounds(0L,(long)(interval * (numBuckets - 1))).subAggregation(metric).subAggregation(movingAvg("movavg_counts").window(10).modelBuilder(randomModelBuilder(100)).gapPolicy(gapPolicy).setBucketsPaths("_count"))).execute().actionGet();

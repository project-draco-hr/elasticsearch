{
  try {
    client().prepareSearch("idx").setTypes("type").addAggregation(histogram("histo").field(INTERVAL_FIELD).interval(interval).extendedBounds(new ExtendedBounds(0L,(long)(interval * (numBuckets - 1)))).subAggregation(randomMetric("the_metric",VALUE_FIELD)).subAggregation(movingAvg("movavg_counts","_count").window(-10).modelBuilder(new SimpleModel.SimpleModelBuilder()).gapPolicy(gapPolicy))).execute().actionGet();
    fail("MovingAvg should not accept a window that is negative");
  }
 catch (  SearchPhaseExecutionException exception) {
  }
}

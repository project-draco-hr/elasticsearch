{
  builder.startObject(NAME);
  if (types != null) {
    if (types.length == 1) {
      builder.field("type",types[0]);
    }
 else {
      builder.array("types",types);
    }
  }
  builder.startArray("values");
  for (  String value : ids) {
    builder.value(value);
  }
  builder.endArray();
  if (boost != 1.0f) {
    builder.field("boost",boost);
  }
  if (queryName != null) {
    builder.field("_name",queryName);
  }
  builder.endObject();
}

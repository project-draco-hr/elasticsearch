{
  builder.startObject(NAME);
  builder.array(IdsQueryParser.TYPE_FIELD.getPreferredName(),types);
  builder.startArray(IdsQueryParser.VALUES_FIELD.getPreferredName());
  for (  String value : ids) {
    builder.value(value);
  }
  builder.endArray();
  printBoostAndQueryName(builder);
  builder.endObject();
}

{
  if (current == _tail) {
    addLast(newElement);
  }
 else   if (current == null) {
    addFirst(newElement);
  }
 else {
    TLinkable n=current.getNext();
    newElement.setPrevious(current);
    newElement.setNext(n);
    current.setNext(newElement);
    n.setPrevious(newElement);
    _size++;
  }
}

{
  if (current == _head) {
    addFirst(newElement);
  }
 else   if (current == null) {
    addLast(newElement);
  }
 else {
    TLinkable p=current.getPrevious();
    newElement.setNext(current);
    p.setNext(newElement);
    newElement.setPrevious(p);
    current.setPrevious(newElement);
    _size++;
  }
}

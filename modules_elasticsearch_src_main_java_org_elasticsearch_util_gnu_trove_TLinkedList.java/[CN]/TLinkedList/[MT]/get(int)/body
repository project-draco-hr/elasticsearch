{
  if (index < 0 || index >= _size) {
    throw new IndexOutOfBoundsException("Index: " + index + ", Size: "+ _size);
  }
  if (index > (_size >> 1)) {
    int position=_size - 1;
    T node=_tail;
    while (position > index) {
      node=(T)node.getPrevious();
      position--;
    }
    return node;
  }
 else {
    int position=0;
    T node=_head;
    while (position < index) {
      node=(T)node.getNext();
      position++;
    }
    return node;
  }
}

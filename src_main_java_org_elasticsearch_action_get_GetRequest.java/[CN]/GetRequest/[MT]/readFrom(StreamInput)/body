{
  super.readFrom(in);
  type=in.readString();
  id=in.readString();
  routing=in.readOptionalString();
  preference=in.readOptionalString();
  refresh=in.readBoolean();
  int size=in.readInt();
  if (size >= 0) {
    fields=new String[size];
    for (int i=0; i < size; i++) {
      fields[i]=in.readString();
    }
  }
  byte realtime=in.readByte();
  if (realtime == 0) {
    this.realtime=false;
  }
 else   if (realtime == 1) {
    this.realtime=true;
  }
}

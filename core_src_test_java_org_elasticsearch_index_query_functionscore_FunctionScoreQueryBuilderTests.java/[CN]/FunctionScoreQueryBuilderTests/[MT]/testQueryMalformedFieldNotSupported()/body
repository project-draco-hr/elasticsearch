{
  String json="{\n" + "  \"function_score\" : {\n" + "    \"not_supported\" : \"value\"\n"+ "  }\n"+ "}";
  try {
    parseQuery(json);
    fail("parse should have failed");
  }
 catch (  ParsingException e) {
    assertThat(e.getMessage(),containsString("field [not_supported] is not supported"));
  }
}

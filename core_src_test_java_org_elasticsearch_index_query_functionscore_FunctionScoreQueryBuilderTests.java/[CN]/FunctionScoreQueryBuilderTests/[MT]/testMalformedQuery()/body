{
  String json="{\n" + "    \"function_score\":{\n" + "        \"query\":{\n"+ "            \"bool\":{\n"+ "                \"must\":{\"match\":{\"field\":\"value\"}}"+ "             },\n"+ "            \"ignored_field_name\": {\n"+ "                {\"match\":{\"field\":\"value\"}}\n"+ "            }\n"+ "            }\n"+ "        }\n"+ "    }\n"+ "}";
  expectParsingException(json,"[query] is already defined.");
}

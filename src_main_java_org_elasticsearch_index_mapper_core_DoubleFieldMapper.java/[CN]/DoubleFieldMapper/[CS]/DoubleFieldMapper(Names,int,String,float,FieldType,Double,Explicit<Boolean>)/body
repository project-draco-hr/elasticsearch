{
  super(names,precisionStep,fuzzyFactor,boost,fieldType,ignoreMalformed,new NamedAnalyzer("_double/" + precisionStep,new NumericDoubleAnalyzer(precisionStep)),new NamedAnalyzer("_double/max",new NumericDoubleAnalyzer(Integer.MAX_VALUE)));
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();

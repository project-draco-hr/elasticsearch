{
  Engine.Searcher searcher=null;
  try {
    searcher=engine.searcher();
    return new DocsStats(searcher.reader().numDocs(),searcher.reader().numDeletedDocs());
  }
 catch (  Exception e) {
    return new DocsStats();
  }
 finally {
    if (searcher != null) {
      searcher.release();
    }
  }
}

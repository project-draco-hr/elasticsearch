{
  if (e < 0) {
    throw MathRuntimeException.createIllegalArgumentException("cannot raise an integral value to a negative power ({0}^{1})",k,e);
  }
  int result=1;
  int k2p=k;
  while (e != 0) {
    if ((e & 0x1) != 0) {
      result*=k2p;
    }
    k2p*=k2p;
    e=e >> 1;
  }
  return result;
}

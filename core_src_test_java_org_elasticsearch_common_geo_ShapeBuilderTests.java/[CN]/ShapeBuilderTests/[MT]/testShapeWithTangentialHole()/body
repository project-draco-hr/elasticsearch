{
  PolygonBuilder builder=ShapeBuilder.newPolygon().point(179,10).point(168,15).point(164,0).point(166,-15).point(179,-10).point(179,10);
  builder.hole().point(-177,10).point(-178,-10).point(-180,-5).point(-180,5).point(-177,10);
  Shape shape=builder.close().build();
  assertMultiPolygon(shape);
}

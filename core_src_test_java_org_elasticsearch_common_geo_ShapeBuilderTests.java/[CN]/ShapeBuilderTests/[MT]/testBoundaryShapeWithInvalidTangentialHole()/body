{
  PolygonBuilder builder=ShapeBuilder.newPolygon().point(-177,10).point(176,15).point(172,0).point(176,-15).point(-177,-10).point(-177,10);
  builder.hole().point(-177,10).point(172,0).point(180,-5).point(176,-10).point(-177,10);
  Shape shape=builder.close().build();
  assertMultiPolygon(shape);
}

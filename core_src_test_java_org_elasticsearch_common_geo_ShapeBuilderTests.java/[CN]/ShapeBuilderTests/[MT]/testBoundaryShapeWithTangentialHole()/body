{
  PolygonBuilder builder=ShapeBuilders.newPolygon().point(-177,10).point(176,15).point(172,0).point(176,-15).point(-177,-10).point(-177,10);
  builder.hole(new LineStringBuilder().point(-177,10).point(-178,-10).point(-180,-5).point(-180,5).point(-177,10));
  builder.hole(new LineStringBuilder().point(172,0).point(176,10).point(176,-5).point(172,0));
  Shape shape=builder.close().build();
  assertMultiPolygon(shape);
}

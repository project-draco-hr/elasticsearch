{
  PolygonBuilder builder=ShapeBuilders.newPolygon(new PointListBuilder().point(180,0).point(176,4).point(176,4).point(-176,4).point(180,0).list());
  try {
    builder.close().build();
    fail("Expected InvalidShapeException");
  }
 catch (  InvalidShapeException e) {
    assertThat(e.getMessage(),containsString("duplicate consecutive coordinates at: ("));
  }
}

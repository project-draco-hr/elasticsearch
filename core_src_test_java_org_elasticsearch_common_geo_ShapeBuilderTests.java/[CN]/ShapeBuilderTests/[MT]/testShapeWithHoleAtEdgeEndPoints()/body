{
  PolygonBuilder builder=ShapeBuilders.newPolygon().point(-4,2).point(4,2).point(6,0).point(4,-2).point(-4,-2).point(-6,0).point(-4,2);
  builder.hole(new LineStringBuilder().point(4,1).point(4,-1).point(-4,-1).point(-4,1).point(4,1));
  Shape shape=builder.close().build();
  assertPolygon(shape);
}

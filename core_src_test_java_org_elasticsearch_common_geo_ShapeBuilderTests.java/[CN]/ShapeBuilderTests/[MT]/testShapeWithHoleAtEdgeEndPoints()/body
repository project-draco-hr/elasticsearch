{
  PolygonBuilder builder=ShapeBuilders.newPolygon(new PointListBuilder().point(-4,2).point(4,2).point(6,0).point(4,-2).point(-4,-2).point(-6,0).point(-4,2).list());
  builder.hole(new LineStringBuilder(new PointListBuilder().point(4,1).point(4,-1).point(-4,-1).point(-4,1).point(4,1).list()));
  Shape shape=builder.close().build();
  assertPolygon(shape);
}

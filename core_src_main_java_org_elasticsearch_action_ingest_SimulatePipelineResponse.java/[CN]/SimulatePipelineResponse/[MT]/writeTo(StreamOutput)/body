{
  super.writeTo(out);
  out.writeBoolean(isError());
  if (isError()) {
    error.writeTo(out);
  }
 else {
    out.writeString(pipelineId);
    out.writeBoolean(verbose);
    out.writeVInt(results.size());
    for (    SimulateDocumentResult response : results) {
      response.writeTo(out);
    }
  }
}

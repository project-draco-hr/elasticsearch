{
  Settings settings=ImmutableSettings.settingsBuilder().put("path.conf",this.getResource("config").getPath()).put("name",getClass().getName()).put(IndexMetaData.SETTING_VERSION_CREATED,Version.CURRENT).build();
  Index index=new Index("test");
  injector=new ModulesBuilder().add(new EnvironmentModule(new Environment(settings)),new SettingsModule(settings),new CodecModule(settings),new ThreadPoolModule(settings),new IndicesQueriesModule(),new ScriptModule(settings),new IndexSettingsModule(index,settings),new IndexCacheModule(settings),new AnalysisModule(settings),new IndexEngineModule(settings),new SimilarityModule(settings),new IndexNameModule(index),new IndexQueryParserModule(settings),new FunctionScoreModule(),new AbstractModule(){
    @Override protected void configure(){
      bind(ClusterService.class).toProvider(Providers.of((ClusterService)null));
      bind(CircuitBreakerService.class).to(NoneCircuitBreakerService.class);
    }
  }
).createInjector();
  IndexQueryParserService queryParserService=injector.getInstance(IndexQueryParserService.class);
  context=new QueryParseContext(index,queryParserService);
}

{
  Settings settings=ImmutableSettings.Builder.EMPTY_SETTINGS;
  Index index=new Index("test");
  injector=new ModulesBuilder().add(new SettingsModule(settings),new CacheRecyclerModule(settings),new CodecModule(settings),new ThreadPoolModule(settings),new IndicesQueriesModule(),new ScriptModule(settings),new IndexSettingsModule(index,settings),new IndexCacheModule(settings),new AnalysisModule(settings),new IndexEngineModule(settings),new SimilarityModule(settings),new IndexNameModule(index),new IndexQueryParserModule(settings),new FunctionScoreModule(),new AbstractModule(){
    @Override protected void configure(){
      bind(ClusterService.class).toProvider(Providers.of((ClusterService)null));
      bind(CircuitBreakerService.class).to(DummyCircuitBreakerService.class);
    }
  }
).createInjector();
  IndexQueryParserService queryParserService=injector.getInstance(IndexQueryParserService.class);
  context=new QueryParseContext(index,queryParserService);
}

{
  IndexQueryParserService queryParser=queryParser();
  String query=copyToStringFromClasspath("/org/elasticsearch/index/query/fquery-filter.json");
  ParsedQuery parsedQuery=queryParser.parse(query);
  assertThat(parsedQuery.namedFilters().containsKey("test"),equalTo(true));
  Query expected=Queries.filtered(new TermQuery(new Term("name.first","shay")),new ConstantScoreQuery(new TermQuery(new Term("name.last","banon"))));
  assertEquals(expected,parsedQuery.query());
}

{
  IndexQueryParserService queryParser=queryParser();
  String query=copyToStringFromClasspath("/org/elasticsearch/index/query/not-filter.json");
  Query parsedQuery=queryParser.parse(query).query();
  Query expected=Queries.filtered(new TermQuery(new Term("name.first","shay")),Queries.not(new TermQuery(new Term("name.first","shay1"))));
  assertEquals(expected,parsedQuery);
}

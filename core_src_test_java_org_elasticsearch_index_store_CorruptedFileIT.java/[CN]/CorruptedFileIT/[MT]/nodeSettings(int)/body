{
  return Settings.builder().put(super.nodeSettings(nodeOrdinal)).extendArray("plugin.types",MockTransportService.Plugin.class.getName()).put(RecoverySettings.INDICES_RECOVERY_CONCURRENT_STREAMS,10).put(RecoverySettings.INDICES_RECOVERY_CONCURRENT_SMALL_FILE_STREAMS,10).put(ThrottlingAllocationDecider.CLUSTER_ROUTING_ALLOCATION_NODE_CONCURRENT_RECOVERIES,5).build();
}

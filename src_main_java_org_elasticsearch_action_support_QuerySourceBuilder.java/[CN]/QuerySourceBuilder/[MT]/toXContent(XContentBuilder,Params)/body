{
  builder.startObject();
  if (queryBuilder != null) {
    builder.field("query");
    queryBuilder.toXContent(builder,params);
  }
  if (queryBinary != null) {
    if (XContentFactory.xContentType(queryBinary) == builder.contentType()) {
      builder.rawField("query",queryBinary);
    }
 else {
      builder.field("query_binary",queryBinary);
    }
  }
  builder.endObject();
  return builder;
}

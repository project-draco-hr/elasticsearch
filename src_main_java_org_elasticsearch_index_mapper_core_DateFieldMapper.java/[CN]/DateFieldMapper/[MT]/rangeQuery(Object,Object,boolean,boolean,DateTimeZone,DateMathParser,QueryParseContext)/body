{
  return NumericRangeQuery.newLongRange(names.indexName(),precisionStep,lowerTerm == null ? null : parseToMilliseconds(lowerTerm,context,false,timeZone,forcedDateParser == null ? dateMathParser : forcedDateParser),upperTerm == null ? null : parseToMilliseconds(upperTerm,context,includeUpper,timeZone,forcedDateParser == null ? dateMathParser : forcedDateParser),includeLower,includeUpper);
}

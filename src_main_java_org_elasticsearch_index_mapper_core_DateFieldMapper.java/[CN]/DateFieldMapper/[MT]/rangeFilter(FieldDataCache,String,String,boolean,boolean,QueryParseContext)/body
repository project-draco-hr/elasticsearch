{
  long now=context == null ? System.currentTimeMillis() : context.nowInMillis();
  return NumericRangeFieldDataFilter.newLongRange(fieldDataCache,names.indexName(),lowerTerm == null ? null : dateMathParser.parse(lowerTerm,now),upperTerm == null ? null : (includeUpper && parseUpperInclusive) ? dateMathParser.parseUpperInclusive(upperTerm,now) : dateMathParser.parse(upperTerm,now),includeLower,includeUpper);
}

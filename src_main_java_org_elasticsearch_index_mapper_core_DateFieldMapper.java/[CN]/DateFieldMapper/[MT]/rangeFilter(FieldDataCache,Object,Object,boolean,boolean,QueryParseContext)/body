{
  long now=context == null ? System.currentTimeMillis() : context.nowInMillis();
  return NumericRangeFieldDataFilter.newLongRange(fieldDataCache,names.indexName(),lowerTerm == null ? null : dateMathParser.parse(convertToString(lowerTerm),now),upperTerm == null ? null : (includeUpper && parseUpperInclusive) ? dateMathParser.parseUpperInclusive(convertToString(upperTerm),now) : dateMathParser.parse(convertToString(upperTerm),now),includeLower,includeUpper);
}

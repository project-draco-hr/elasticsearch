{
  long now=context == null ? System.currentTimeMillis() : context.nowInMillis();
  return NumericRangeFilter.newLongRange(names.indexName(),precisionStep,lowerTerm == null ? null : dateMathParser.parse(lowerTerm,now),upperTerm == null ? null : (includeUpper && parseUpperInclusive) ? dateMathParser.parseUpperInclusive(upperTerm,now) : dateMathParser.parse(upperTerm,now),includeLower,includeUpper);
}

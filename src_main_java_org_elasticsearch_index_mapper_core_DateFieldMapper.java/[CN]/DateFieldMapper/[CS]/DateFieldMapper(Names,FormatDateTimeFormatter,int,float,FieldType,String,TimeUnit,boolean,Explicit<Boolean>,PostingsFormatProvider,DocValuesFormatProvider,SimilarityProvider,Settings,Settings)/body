{
  super(names,precisionStep,boost,fieldType,ignoreMalformed,new NamedAnalyzer("_date/" + precisionStep,new NumericDateAnalyzer(precisionStep,dateTimeFormatter.parser())),new NamedAnalyzer("_date/max",new NumericDateAnalyzer(Integer.MAX_VALUE,dateTimeFormatter.parser())),postingsProvider,docValuesProvider,similarity,fieldDataSettings,indexSettings);
  this.dateTimeFormatter=dateTimeFormatter;
  this.nullValue=nullValue;
  this.timeUnit=timeUnit;
  this.roundCeil=roundCeil;
  this.dateMathParser=new DateMathParser(dateTimeFormatter,timeUnit);
}

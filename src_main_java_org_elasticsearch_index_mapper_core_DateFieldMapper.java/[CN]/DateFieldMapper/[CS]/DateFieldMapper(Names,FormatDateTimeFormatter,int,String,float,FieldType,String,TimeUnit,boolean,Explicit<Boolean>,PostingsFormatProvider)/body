{
  super(names,precisionStep,fuzzyFactor,boost,fieldType,ignoreMalformed,new NamedAnalyzer("_date/" + precisionStep,new NumericDateAnalyzer(precisionStep,dateTimeFormatter.parser())),new NamedAnalyzer("_date/max",new NumericDateAnalyzer(Integer.MAX_VALUE,dateTimeFormatter.parser())),provider);
  this.dateTimeFormatter=dateTimeFormatter;
  this.nullValue=nullValue;
  this.timeUnit=timeUnit;
  this.parseUpperInclusive=parseUpperInclusive;
  this.dateMathParser=new DateMathParser(dateTimeFormatter,timeUnit);
}

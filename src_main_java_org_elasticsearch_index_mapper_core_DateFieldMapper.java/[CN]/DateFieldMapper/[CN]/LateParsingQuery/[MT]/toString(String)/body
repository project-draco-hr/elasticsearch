{
  final StringBuilder sb=new StringBuilder();
  return sb.append(names.indexName()).append(':').append(includeLower ? '[' : '{').append((lowerTerm == null) ? "*" : lowerTerm.toString()).append(" TO ").append((upperTerm == null) ? "*" : upperTerm.toString()).append(includeUpper ? ']' : '}').append(ToStringUtils.boost(getBoost())).toString();
}

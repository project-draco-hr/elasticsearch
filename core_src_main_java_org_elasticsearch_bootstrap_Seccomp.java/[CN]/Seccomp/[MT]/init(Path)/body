{
  if (Constants.LINUX) {
    return linuxImpl();
  }
 else   if (Constants.MAC_OS_X) {
    macImpl(tmpFile);
    return 1;
  }
 else   if (Constants.SUN_OS) {
    solarisImpl();
    return 1;
  }
 else {
    throw new UnsupportedOperationException("syscall filtering not supported for OS: '" + Constants.OS_NAME + "'");
  }
}

{
  double sumOfSqrs=0;
  for (  InternalAggregation aggregation : aggregations) {
    InternalExtendedStats stats=(InternalExtendedStats)aggregation;
    sumOfSqrs+=stats.getSumOfSquares();
  }
  final InternalStats stats=super.doReduce(aggregations,reduceContext);
  return new InternalExtendedStats(name,stats.getCount(),stats.getSum(),stats.getMin(),stats.getMax(),sumOfSqrs,sigma,valueFormatter,reducers(),getMetaData());
}

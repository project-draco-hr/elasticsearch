{
  return XContentFactory.jsonBuilder().startObject().startObject("doc").startArray("dynamic_templates").startObject().startObject("template_raw").field("match","*_raw").field("match_mapping_type","string").startObject("mapping").field("type","string").field("index","not_analyzed").endObject().endObject().endObject().startObject().startObject("template_all").field("match","*").field("match_mapping_type","string").startObject("mapping").field("type","string").field("copy_to","{name}_raw").endObject().endObject().endObject().endArray();
}

{
  String mapping=jsonBuilder().startObject().startObject("type").field("dynamic","strict").startObject("properties").startObject("field1").field("type","text").endObject().endObject().endObject().endObject().string();
  DocumentMapper defaultMapper=createIndex("test").mapperService().documentMapperParser().parse("type",new CompressedXContent(mapping));
  try {
    defaultMapper.parse("test","type","1",jsonBuilder().startObject().field("field1","value1").field("field2","value2").bytes());
    fail();
  }
 catch (  StrictDynamicMappingException e) {
  }
  try {
    defaultMapper.parse("test","type","1",XContentFactory.jsonBuilder().startObject().field("field1","value1").field("field2",(String)null).bytes());
    fail();
  }
 catch (  StrictDynamicMappingException e) {
  }
}

{
  IndexService indexService=createIndex("test");
  DocumentMapperParser parser=indexService.mapperService().documentMapperParser();
  String mapping=XContentFactory.jsonBuilder().startObject().startObject("type").startObject("properties").startObject("foo").field("type","string").endObject().endObject().endObject().string();
  DocumentMapper mapper=parser.parse("type",new CompressedXContent(mapping));
  Mapper update=parse(mapper,parser,XContentFactory.jsonBuilder().startObject().field("foo","bar").endObject());
  assertNull(update);
}

{
  this.name=StringHelper.intern(name);
  fieldsData=value;
  isIndexed=indexed;
  isTokenized=indexed;
  omitTermFreqAndPositions=true;
  omitNorms=true;
  if (value != null) {
    isStored=true;
    isBinary=true;
    binaryLength=value.length;
    binaryOffset=0;
  }
  setStoreTermVector(Field.TermVector.NO);
}

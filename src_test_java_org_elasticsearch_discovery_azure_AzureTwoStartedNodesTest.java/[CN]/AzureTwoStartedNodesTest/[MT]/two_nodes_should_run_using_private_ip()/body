{
  ImmutableSettings.Builder settings=ImmutableSettings.settingsBuilder().put("cloud.azure.service_name","dummy").put("cloud.azure.host_type","private_ip").put(super.settingsBuilder());
  logger.info("--> start first node");
  internalCluster().startNode(settings);
  assertThat(client().admin().cluster().prepareState().setMasterNodeTimeout("1s").execute().actionGet().getState().nodes().masterNodeId(),notNullValue());
  logger.info("--> start another node");
  internalCluster().startNode(settings);
  assertThat(client().admin().cluster().prepareState().setMasterNodeTimeout("1s").execute().actionGet().getState().nodes().masterNodeId(),notNullValue());
  checkNumberOfNodes(2);
}

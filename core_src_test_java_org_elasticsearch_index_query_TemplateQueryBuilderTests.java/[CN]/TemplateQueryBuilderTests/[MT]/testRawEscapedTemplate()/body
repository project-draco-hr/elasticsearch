{
  String expectedTemplateString="{\"match_{{template}}\": {}}\"";
  String query="{\"template\": {\"query\": \"{\\\"match_{{template}}\\\": {}}\\\"\",\"params\" : {\"template\" : \"all\"}}}";
  Map<String,Object> params=new HashMap<>();
  params.put("template","all");
  QueryBuilder expectedBuilder=new TemplateQueryBuilder(new Template(expectedTemplateString,ScriptType.INLINE,null,null,params));
  assertParsedQuery(query,expectedBuilder);
}

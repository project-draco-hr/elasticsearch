{
switch (field) {
case STATUS:
    if (value == null) {
      unsetStatus();
    }
 else {
      setStatus((Status)value);
    }
  break;
case HEADERS:
if (value == null) {
  unsetHeaders();
}
 else {
  setHeaders((Map<String,String>)value);
}
break;
case BODY:
if (value == null) {
unsetBody();
}
 else {
setBody((ByteBuffer)value);
}
break;
}
}

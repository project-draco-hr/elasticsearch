{
  if (freq == 0) {
    return;
  }
  if (startOffsets.ints.length < freq) {
    startOffsets.grow(freq);
    endOffsets.grow(freq);
    positions.grow(freq);
    payloadsLengths.grow(freq);
    payloadsStarts.grow(freq);
  }
  payloads.offset=0;
  payloadsLengths.offset=0;
  payloadsStarts.offset=0;
  payloads.grow(freq * 8);
}

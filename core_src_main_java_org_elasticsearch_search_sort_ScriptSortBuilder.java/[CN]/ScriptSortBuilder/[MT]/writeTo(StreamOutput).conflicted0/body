{
  script.writeTo(out);
  type.writeTo(out);
  order.writeTo(out);
  out.writeBoolean(sortMode != null);
  if (sortMode != null) {
    sortMode.writeTo(out);
  }
  out.writeOptionalString(nestedPath);
  boolean hasNestedFilter=nestedFilter != null;
  out.writeBoolean(hasNestedFilter);
  if (hasNestedFilter) {
    out.writeQuery(nestedFilter);
  }
}

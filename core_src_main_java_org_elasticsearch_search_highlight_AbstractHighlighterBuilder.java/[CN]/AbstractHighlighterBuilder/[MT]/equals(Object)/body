{
  if (this == obj) {
    return true;
  }
  if (obj == null || getClass() != obj.getClass()) {
    return false;
  }
  @SuppressWarnings("unchecked") HB other=(HB)obj;
  return Arrays.equals(preTags,other.preTags) && Arrays.equals(postTags,other.postTags) && Objects.equals(fragmentSize,other.fragmentSize)&& Objects.equals(numOfFragments,other.numOfFragments)&& Objects.equals(highlighterType,other.highlighterType)&& Objects.equals(fragmenter,other.fragmenter)&& Objects.equals(highlightQuery,other.highlightQuery)&& Objects.equals(order,other.order)&& Objects.equals(highlightFilter,other.highlightFilter)&& Objects.equals(forceSource,other.forceSource)&& Objects.equals(boundaryMaxScan,other.boundaryMaxScan)&& Arrays.equals(boundaryChars,other.boundaryChars)&& Objects.equals(noMatchSize,other.noMatchSize)&& Objects.equals(phraseLimit,other.phraseLimit)&& Objects.equals(options,other.options)&& Objects.equals(requireFieldMatch,other.requireFieldMatch)&& doEquals(other);
}

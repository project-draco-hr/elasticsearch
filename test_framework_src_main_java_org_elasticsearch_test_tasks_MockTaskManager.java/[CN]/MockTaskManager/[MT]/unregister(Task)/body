{
  Task removedTask=super.unregister(task);
  if (removedTask != null) {
    for (    MockTaskManagerListener listener : listeners) {
      try {
        listener.onTaskUnregistered(task);
      }
 catch (      Exception e) {
        logger.warn(new ParameterizedMessage("failed to notify task manager listener about unregistering the task with id {}",task.getId()),e);
      }
    }
  }
 else {
    logger.warn("trying to remove the same with id {} twice",task.getId());
  }
  return removedTask;
}

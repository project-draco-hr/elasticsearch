{
  if (file.isFile()) {
    String groupName=file.getParentFile().getName().replace('.','_');
    Set<File> filesSet=files.get(groupName);
    if (filesSet == null) {
      filesSet=Sets.newHashSet();
      files.put(groupName,filesSet);
    }
    filesSet.add(file);
  }
 else   if (file.isDirectory()) {
    walkDir(file,fileSuffix,files);
  }
}

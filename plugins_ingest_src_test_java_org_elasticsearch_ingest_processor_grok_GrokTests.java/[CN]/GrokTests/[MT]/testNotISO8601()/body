{
  Grok grok=new Grok(basePatterns,"^%{TIMESTAMP_ISO8601}$");
  List<String> timeMessages=Arrays.asList("2001-13-01T00:00:00","2001-00-01T00:00:00","2001-01-00T00:00:00","2001-01-32T00:00:00","2001-01-aT00:00:00","2001-01-1aT00:00:00","2001-01-01Ta0:00:00","2001-01-01T0:00:00","2001-01-01T25:00:00","2001-01-01T01:60:00","2001-01-01T00:aa:00","2001-01-01T00:00:aa","2001-01-01T00:00:-1","2001-01-01T00:00:61","2001-01-01T00:00:00A","2001-01-01T00:00:00+","2001-01-01T00:00:00+25","2001-01-01T00:00:00+2500","2001-01-01T00:00:00+25:00","2001-01-01T00:00:00-25","2001-01-01T00:00:00-2500","2001-01-01T00:00:00-00:61");
  for (  String msg : timeMessages) {
    assertThat(grok.match(msg),is(false));
  }
}

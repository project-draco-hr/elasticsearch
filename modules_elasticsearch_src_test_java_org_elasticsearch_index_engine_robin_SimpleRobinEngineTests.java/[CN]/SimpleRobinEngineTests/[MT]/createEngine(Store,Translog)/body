{
  return new RobinEngine(shardId,EMPTY_SETTINGS,new IndexSettingsService(shardId.index(),EMPTY_SETTINGS),store,createSnapshotDeletionPolicy(),translog,createMergePolicy(),createMergeScheduler(),new AnalysisService(shardId.index()),new SimilarityService(shardId.index()),new NonBloomCache(shardId.index()));
}

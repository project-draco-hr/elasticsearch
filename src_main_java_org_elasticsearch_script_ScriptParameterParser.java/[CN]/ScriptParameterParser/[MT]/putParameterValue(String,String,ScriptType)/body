{
  if (parameterValues.get(coreParameterName) == null) {
    parameterValues.put(coreParameterName,new ScriptParameterValue(script,scriptType));
  }
 else {
    throw new ScriptParseException("Only one of [" + coreParameterName + ", "+ coreParameterName+ FILE_SUFFIX+ ", "+ coreParameterName+ INDEXED_SUFFIX+ "] is allowed.");
  }
}

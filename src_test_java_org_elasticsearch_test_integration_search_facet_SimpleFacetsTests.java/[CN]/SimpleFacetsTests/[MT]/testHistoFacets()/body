    assertThat(facet.getEntries().get(0).getTotal(),equalTo(82d));
    assertThat(facet.getEntries().get(0).getMean(),equalTo(20.5d));
    assertThat(facet.getEntries().get(1).getKey(),equalTo(1100l));

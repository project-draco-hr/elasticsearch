{
  if (update != null) {
    doUpdate(data);
  }
  if (rename != null) {
    doRename(data);
  }
  if (convert != null) {
    doConvert(data);
  }
  if (split != null) {
    doSplit(data);
  }
  if (gsub != null) {
    doGsub(data);
  }
  if (join != null) {
    doJoin(data);
  }
  if (remove != null) {
    doRemove(data);
  }
  if (trim != null) {
    doTrim(data);
  }
  if (uppercase != null) {
    doUppercase(data);
  }
  if (lowercase != null) {
    doLowercase(data);
  }
}

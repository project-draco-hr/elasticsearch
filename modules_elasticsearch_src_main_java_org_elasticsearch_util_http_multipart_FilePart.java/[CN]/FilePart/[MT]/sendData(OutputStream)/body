{
  LOG.trace("enter sendData(OutputStream out)");
  if (lengthOfData() == 0) {
    LOG.debug("No data to send.");
    return;
  }
  byte[] tmp=new byte[4096];
  InputStream instream=source.createInputStream();
  try {
    int len;
    while ((len=instream.read(tmp)) >= 0) {
      out.write(tmp,0,len);
    }
  }
  finally {
    instream.close();
  }
}

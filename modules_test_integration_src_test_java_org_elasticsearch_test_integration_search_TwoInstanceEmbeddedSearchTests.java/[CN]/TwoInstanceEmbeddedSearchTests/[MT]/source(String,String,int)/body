{
  StringBuilder multi=new StringBuilder().append(nameValue);
  for (int i=0; i < age; i++) {
    multi.append(" ").append(nameValue);
  }
  return "{ type1 : { \"id\" : \"" + id + "\", \"nid\" : "+ id+ ", \"name\" : \""+ (nameValue + id)+ "\", age : "+ age+ ", multi : \""+ multi.toString()+ "\", _boost : "+ (age * 10)+ " } }";
}

{
  final Iterator<Variable> itr=variables.iterator();
  while (itr.hasNext()) {
    final Variable variable=itr.next();
    if (variable.name.equals(name)) {
      return variable;
    }
  }
  if (location != null) {
    throw new IllegalArgumentException("Error " + location + ": Variable ["+ name+ "] not defined.");
  }
  return null;
}

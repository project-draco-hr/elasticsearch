{
  if (buffer.capacity() == smallBufferSizeInBytes) {
    boolean success=smallCache.offer(buffer);
    if (!success) {
      super.release(buffer);
    }
  }
 else   if (buffer.capacity() == largeBufferSizeInBytes) {
    boolean success=largeCache.offer(buffer);
    if (!success) {
      super.release(buffer);
    }
  }
}

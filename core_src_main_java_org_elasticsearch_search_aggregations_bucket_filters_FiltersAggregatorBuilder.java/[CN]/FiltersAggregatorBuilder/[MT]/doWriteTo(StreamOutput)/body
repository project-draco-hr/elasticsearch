{
  out.writeBoolean(keyed);
  out.writeVInt(filters.size());
  if (keyed) {
    for (    KeyedFilter keyedFilter : filters) {
      keyedFilter.writeTo(out);
    }
  }
 else {
    for (    KeyedFilter keyedFilter : filters) {
      out.writeQuery(keyedFilter.filter());
    }
  }
  out.writeBoolean(otherBucket);
  out.writeString(otherBucketKey);
}

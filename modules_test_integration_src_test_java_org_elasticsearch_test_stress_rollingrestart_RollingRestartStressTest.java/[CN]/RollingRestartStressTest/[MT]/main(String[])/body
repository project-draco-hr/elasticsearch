{
  System.setProperty("es.logger.prefix","");
  Settings settings=ImmutableSettings.settingsBuilder().put("index.shard.check_index",true).put("gateway.type","none").build();
  RollingRestartStressTest test=new RollingRestartStressTest().settings(settings).numberOfNodes(4).initialNumberOfDocs(1000).textTokens(150).numberOfFields(10).cleanNodeData(true).indexers(5).indexerThrottle(TimeValue.timeValueMillis(50)).period(TimeValue.timeValueMinutes(10));
  test.run();
}

{
  super.readFrom(in);
  if (in.readBoolean()) {
    index=in.readUTF();
  }
  text=in.readUTF();
  if (in.readBoolean()) {
    analyzer=in.readUTF();
  }
  if (in.readBoolean()) {
    tokenizer=in.readUTF();
  }
  int size=in.readVInt();
  if (size > 0) {
    tokenFilters=new String[size];
    for (int i=0; i < size; i++) {
      tokenFilters[i]=in.readUTF();
    }
  }
  if (in.readBoolean()) {
    field=in.readUTF();
  }
}

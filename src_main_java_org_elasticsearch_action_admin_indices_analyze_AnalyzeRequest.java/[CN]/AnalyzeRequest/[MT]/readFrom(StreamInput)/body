{
  super.readFrom(in);
  index=in.readOptionalUTF();
  text=in.readUTF();
  analyzer=in.readOptionalUTF();
  tokenizer=in.readOptionalUTF();
  int size=in.readVInt();
  if (size > 0) {
    tokenFilters=new String[size];
    for (int i=0; i < size; i++) {
      tokenFilters[i]=in.readUTF();
    }
  }
  field=in.readOptionalUTF();
}

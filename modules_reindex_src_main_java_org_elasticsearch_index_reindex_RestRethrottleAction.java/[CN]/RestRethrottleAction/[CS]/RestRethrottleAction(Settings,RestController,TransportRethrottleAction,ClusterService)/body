{
  super(settings);
  this.action=action;
  this.clusterService=clusterService;
  controller.registerHandler(POST,"/_update_by_query/{taskId}/_rethrottle",this);
  controller.registerHandler(POST,"/_delete_by_query/{taskId}/_rethrottle",this);

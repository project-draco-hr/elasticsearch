{
  super.writeTo(out);
  out.writeString(index);
  out.writeString(id);
  out.writeString(type);
  out.writeLong(version);
  if (matches == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    out.writeVInt(matches.size());
    for (    String match : matches) {
      out.writeString(match);
    }
  }
}

{
  String mapping=XContentFactory.jsonBuilder().startObject().startObject("type").endObject().endObject().string();
  XContentDocumentMapper docMapper=MapperTests.newParser().parse(mapping);
  ParsedDocument doc=docMapper.parse(SourceToParse.source(XContentFactory.jsonBuilder().startObject().field("field","value").endObject().copiedBytes()).type("type").id("1").routing("routing_value"));
  assertThat(doc.doc().get("_routing"),equalTo("routing_value"));
  assertThat(doc.doc().get("field"),equalTo("value"));
}

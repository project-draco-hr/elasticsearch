{
  if (indexMetaData.getState() == IndexMetaData.State.OPEN && isSupportedVersion(indexMetaData) == false) {
    throw new IllegalStateException("The index [" + indexMetaData.getIndex() + "] was created before v2.0.0.beta1 and wasn't upgraded."+ " This index should be open using a version before "+ Version.CURRENT.minimumCompatibilityVersion()+ " and upgraded using the upgrade API.");
  }
}

{
  builder.startObject();
  if (field != null) {
    builder.field("field",field);
  }
  if (script != null) {
    builder.field("script",script);
  }
  if (lang != null) {
    builder.field("lang",lang);
  }
  if (this.params != null) {
    builder.field("params").map(this.params);
  }
  if (missing != null) {
    builder.field("missing",missing);
  }
  doInternalXContent(builder,params);
  return builder.endObject();
}

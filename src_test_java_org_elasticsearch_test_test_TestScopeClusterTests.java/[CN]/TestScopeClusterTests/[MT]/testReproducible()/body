{
  if (ITER++ == 0) {
    CLUSTER_SEED=cluster().seed();
    for (int i=0; i < SEQUENCE.length; i++) {
      SEQUENCE[i]=randomLong();
    }
  }
 else {
    assertEquals(CLUSTER_SEED,new Long(cluster().seed()));
    for (int i=0; i < SEQUENCE.length; i++) {
      assertThat(SEQUENCE[i],equalTo(randomLong()));
    }
  }
}

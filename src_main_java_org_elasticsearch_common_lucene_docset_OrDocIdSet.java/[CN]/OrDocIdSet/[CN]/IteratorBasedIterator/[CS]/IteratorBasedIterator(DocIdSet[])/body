{
  _curDoc=-1;
  _heap=new Item[sets.length];
  _size=0;
  long cost=0;
  for (  DocIdSet set : sets) {
    DocIdSetIterator iterator=set.iterator();
    if (iterator != null) {
      _heap[_size++]=new Item(iterator);
      cost+=iterator.cost();
    }
  }
  this.cost=cost;
  if (_size == 0)   _curDoc=DocIdSetIterator.NO_MORE_DOCS;
}

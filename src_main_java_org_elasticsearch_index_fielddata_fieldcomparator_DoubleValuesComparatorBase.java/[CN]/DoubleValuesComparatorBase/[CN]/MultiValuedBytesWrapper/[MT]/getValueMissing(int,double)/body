{
  DoubleValues.Iter iter=delegate.getIter(docId);
  if (!iter.hasNext()) {
    return missing;
  }
  double currentVal=iter.next();
  double relevantVal=currentVal;
  while (true) {
    int cmp=Double.compare(currentVal,relevantVal);
    if (reversed) {
      if (cmp > 0) {
        relevantVal=currentVal;
      }
    }
 else {
      if (cmp < 0) {
        relevantVal=currentVal;
      }
    }
    if (!iter.hasNext()) {
      break;
    }
    currentVal=iter.next();
  }
  return relevantVal;
}

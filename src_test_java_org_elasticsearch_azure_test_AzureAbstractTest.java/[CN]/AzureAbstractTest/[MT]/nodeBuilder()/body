{
  ImmutableSettings.Builder builder=ImmutableSettings.settingsBuilder().put("path.data","./target/es/data").put("path.logs","./target/es/logs").put("path.work","./target/es/work").put("cloud.azure.api.impl",mock).put("cloud.azure.refresh_interval","5s").put("node.name",(nodes.size() + 1) + "#" + mock.getSimpleName());
  Node node=NodeBuilder.nodeBuilder().settings(builder).node();
  nodes.add(node);
}

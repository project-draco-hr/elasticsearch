{
  client().admin().indices().prepareCreate(indexName).setSettings(ImmutableSettings.settingsBuilder().put(IndexEngineModule.EngineSettings.ENGINE_TYPE,InternalEngineWithControllableTimingForTestingModule.class.getName()).put("number_of_shards",1).put("number_of_replicas",0).build()).execute().actionGet();
  internalEngine=InternalEngineWithControllableTimingForTesting.currentTestInstance;
}

{
  length=length < GUESS_HEADER_LENGTH ? length : GUESS_HEADER_LENGTH;
  if (length > 1 && data[offset] == 0x00 && data[offset + 1] == 0x00) {
    return XContentType.XSON;
  }
  for (int i=offset; i < length; i++) {
    if (data[i] == '{') {
      return XContentType.JSON;
    }
  }
  return null;
}

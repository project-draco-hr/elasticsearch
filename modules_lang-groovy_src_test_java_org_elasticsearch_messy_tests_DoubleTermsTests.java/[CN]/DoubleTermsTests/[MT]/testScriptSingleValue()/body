{
  SearchResponse response=client().prepareSearch("idx").setTypes("type").addAggregation(terms("terms").collectMode(randomFrom(SubAggCollectionMode.values())).script(new Script("doc['" + MULTI_VALUED_FIELD_NAME + "'].value"))).execute().actionGet();
  assertSearchResponse(response);
  Terms terms=response.getAggregations().get("terms");
  assertThat(terms,notNullValue());
  assertThat(terms.getName(),equalTo("terms"));
  assertThat(terms.getBuckets().size(),equalTo(5));
  for (int i=0; i < 5; i++) {
    Terms.Bucket bucket=terms.getBucketByKey("" + (double)i);
    assertThat(bucket,notNullValue());
    assertThat(key(bucket),equalTo("" + (double)i));
    assertThat(bucket.getKeyAsNumber().intValue(),equalTo(i));
    assertThat(bucket.getDocCount(),equalTo(1l));
  }
}

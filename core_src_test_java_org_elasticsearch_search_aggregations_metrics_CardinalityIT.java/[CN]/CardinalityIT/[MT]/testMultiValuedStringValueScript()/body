{
  SearchResponse response=client().prepareSearch("idx").setTypes("type").addAggregation(cardinality("cardinality").precisionThreshold(precisionThreshold).field("str_values").script(new Script("_value",ScriptType.INLINE,CustomScriptPlugin.NAME,emptyMap()))).execute().actionGet();
  assertSearchResponse(response);
  Cardinality count=response.getAggregations().get("cardinality");
  assertThat(count,notNullValue());
  assertThat(count.getName(),equalTo("cardinality"));
  assertCount(count,numDocs * 2);
}

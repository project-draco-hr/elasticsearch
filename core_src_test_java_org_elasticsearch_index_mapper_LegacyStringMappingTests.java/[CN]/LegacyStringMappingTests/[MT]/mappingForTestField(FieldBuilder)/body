{
  XContentBuilder b=JsonXContent.contentBuilder();
  b.startObject();
{
    b.startObject("test_type");
{
      b.startObject("properties");
{
        b.startObject("test_field");
{
          fieldBuilder.populateMappingForField(b);
        }
        b.endObject();
      }
      b.endObject();
    }
    b.endObject();
  }
  return b.endObject();
}

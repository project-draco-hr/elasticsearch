{
  if (hasMapper()) {
    Analyzer analyzer=mapper().searchQuoteAnalyzer();
    if (analyzer != null) {
      return analyzer;
    }
  }
  if (docMapper != null && docMapper.searchQuotedAnalyzer() != null) {
    return docMapper.searchQuotedAnalyzer();
  }
  return mapperService.searchQuoteAnalyzer();
}

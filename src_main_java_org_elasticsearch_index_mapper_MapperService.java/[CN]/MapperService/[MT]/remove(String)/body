{
synchronized (mutex) {
    DocumentMapper docMapper=mappers.get(type);
    if (docMapper == null) {
      return;
    }
    docMapper.close();
    mappers=newMapBuilder(mappers).remove(type).map();
    removeObjectAndFieldMappers(docMapper);
    for (    DocumentTypeListener typeListener : typeListeners) {
      typeListener.removed(type);
    }
  }
}

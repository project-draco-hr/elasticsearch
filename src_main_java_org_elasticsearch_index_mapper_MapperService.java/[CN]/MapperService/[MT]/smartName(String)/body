{
  FieldMappers fieldMappers=fullName(smartName);
  if (fieldMappers != null) {
    return new SmartNameFieldMappers(this,fieldMappers,null,false);
  }
  fieldMappers=indexName(smartName);
  if (fieldMappers != null) {
    return new SmartNameFieldMappers(this,fieldMappers,null,false);
  }
  fieldMappers=name(smartName);
  if (fieldMappers != null) {
    return new SmartNameFieldMappers(this,fieldMappers,null,false);
  }
  return null;
}

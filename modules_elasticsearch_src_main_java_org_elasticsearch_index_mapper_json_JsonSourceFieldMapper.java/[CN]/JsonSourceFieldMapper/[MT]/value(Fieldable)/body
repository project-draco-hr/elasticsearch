{
  if (field.stringValue() != null) {
    return field.stringValue();
  }
  byte[] compressed=field.getBinaryValue();
  if (compressed == null) {
    return null;
  }
  try {
    return compressor.decompressString(compressed);
  }
 catch (  IOException e) {
    throw new MapperCompressionException("Failed to decompress data",e);
  }
}

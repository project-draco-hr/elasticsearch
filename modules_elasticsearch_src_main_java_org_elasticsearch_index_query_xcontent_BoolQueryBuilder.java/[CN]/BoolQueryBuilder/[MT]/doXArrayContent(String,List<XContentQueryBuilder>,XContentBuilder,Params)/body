{
  if (clauses.isEmpty()) {
    return;
  }
  if (clauses.size() == 1) {
    builder.field(field);
    clauses.get(0).toXContent(builder,params);
  }
 else {
    builder.startArray(field);
    for (    XContentQueryBuilder clause : clauses) {
      clause.toXContent(builder,params);
    }
    builder.endArray();
  }
}

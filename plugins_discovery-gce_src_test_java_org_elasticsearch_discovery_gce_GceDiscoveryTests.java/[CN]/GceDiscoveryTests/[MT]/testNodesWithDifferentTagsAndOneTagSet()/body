{
  Settings nodeSettings=Settings.builder().put(GceInstancesService.PROJECT_SETTING.getKey(),projectName).put(GceInstancesService.ZONE_SETTING.getKey(),"europe-west1-b").putArray(GceUnicastHostsProvider.TAGS_SETTING.getKey(),"elasticsearch").build();
  mock=new GceInstancesServiceMock(nodeSettings);
  List<DiscoveryNode> discoveryNodes=buildDynamicNodes(mock,nodeSettings);
  assertThat(discoveryNodes,hasSize(1));
  assertThat(discoveryNodes.get(0).getId(),is("#cloud-test2-0"));
}

{
  NoneCircuitBreakerService circuitBreakerService=new NoneCircuitBreakerService();
  return new MapperService(index,indexSettings,new Environment(),newAnalysisService(),new IndexFieldDataService(index,ImmutableSettings.Builder.EMPTY_SETTINGS,new IndicesFieldDataCache(ImmutableSettings.Builder.EMPTY_SETTINGS,new IndicesFieldDataCacheListener(circuitBreakerService),new ThreadPool("testing-only")),circuitBreakerService),new PostingsFormatService(index),new DocValuesFormatService(index),newSimilarityLookupService(),null);

{
  String query;
  if (params.size() == 0) {
    query=null;
  }
 else {
    query=Joiner.on('&').withKeyValueSeparator("=").join(params);
  }
  try {
    return new URI(protocol,null,host,port,path,query,null);
  }
 catch (  URISyntaxException e) {
    throw new IllegalArgumentException(e);
  }
}

{
  if (List.class.isAssignableFrom(property.getPropertyType()) || Set.class.isAssignableFrom(property.getPropertyType()) || Map.class.isAssignableFrom(property.getPropertyType())) {
    if (property.getReadMethod().getGenericReturnType() instanceof ParameterizedType) {
      ParameterizedType grt=(ParameterizedType)property.getReadMethod().getGenericReturnType();
      return grt.getActualTypeArguments();
    }
 else {
      return null;
    }
  }
 else {
    return null;
  }
}

{
  if (s.indexOf("\\") >= 0) {
    StringBuilder sb=new StringBuilder();
    for (int i=0; i < s.length(); i++) {
      char ch=s.charAt(i);
      if (ch == '\\' && i < s.length() - 1) {
        sb.append(s.charAt(++i));
      }
 else {
        sb.append(ch);
      }
    }
    return sb.toString();
  }
  return s;
}

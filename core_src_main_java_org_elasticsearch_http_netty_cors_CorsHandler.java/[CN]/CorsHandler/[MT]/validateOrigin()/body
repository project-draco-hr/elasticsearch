{
  if (config.isAnyOriginSupported()) {
    return true;
  }
  final String origin=request.headers().get(ORIGIN);
  if (Strings.isNullOrEmpty(origin)) {
    return true;
  }
  if ("null".equals(origin) && config.isNullOriginAllowed()) {
    return true;
  }
  return config.isOriginAllowed(origin);
}

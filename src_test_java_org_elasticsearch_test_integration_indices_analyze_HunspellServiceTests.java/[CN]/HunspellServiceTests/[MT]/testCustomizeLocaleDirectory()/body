{
  Settings settings=ImmutableSettings.settingsBuilder().put("indices.analysis.hunspell.dictionary.location",getClass().getResource("/indices/analyze/conf_dir/hunspell").getFile()).build();
  Node node=startNode("node1",settings);
  HunspellDictionary dictionary=((InternalNode)node).injector().getInstance(HunspellService.class).getDictionary("en_US");
  assertThat(dictionary,notNullValue());
}

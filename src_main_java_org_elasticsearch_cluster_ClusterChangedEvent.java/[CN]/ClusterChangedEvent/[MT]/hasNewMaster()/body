{
  String oldMaster=previousState().getNodes().masterNodeId();
  String newMaster=state().getNodes().masterNodeId();
  if (oldMaster == null && newMaster == null) {
    return false;
  }
  if (oldMaster == null && newMaster != null) {
    return true;
  }
  return oldMaster.equals(newMaster) == false;
}

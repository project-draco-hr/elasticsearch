{
  builder.startObject(HasChildQueryParser.NAME);
  builder.field("query");
  queryBuilder.toXContent(builder,params);
  builder.field("type",childType);
  if (scope != null) {
    builder.field("scope",scope);
  }
  if (boost != 1.0f) {
    builder.field("boost",boost);
  }
  builder.endObject();
}

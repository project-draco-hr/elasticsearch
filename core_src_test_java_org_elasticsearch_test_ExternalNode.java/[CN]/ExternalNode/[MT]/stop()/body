{
  if (running()) {
    try {
      if (this.client != null) {
        client.close();
      }
    }
  finally {
      process.destroy();
      try {
        process.waitFor();
      }
 catch (      InterruptedException e) {
        Thread.interrupted();
      }
      process=null;
      nodeInfo=null;
    }
  }
}

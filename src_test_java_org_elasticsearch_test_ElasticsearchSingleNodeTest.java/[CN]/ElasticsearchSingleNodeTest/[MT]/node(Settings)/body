{
  return NodeBuilder.nodeBuilder().local(true).data(true).settings(ImmutableSettings.builder().put(ClusterName.SETTING,ElasticsearchSingleNodeTest.class.getName()).put("node.name",ElasticsearchSingleNodeTest.class.getName()).put(IndexMetaData.SETTING_NUMBER_OF_SHARDS,1).put(IndexMetaData.SETTING_NUMBER_OF_REPLICAS,0).put(settings).put(EsExecutors.PROCESSORS,1).put("http.enabled",false).put("index.store.type","ram").put("gateway.type","none")).build().start();
}

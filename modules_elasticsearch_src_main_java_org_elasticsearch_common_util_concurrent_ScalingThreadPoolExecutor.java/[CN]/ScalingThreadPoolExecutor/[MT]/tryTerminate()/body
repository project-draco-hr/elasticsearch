{
  if (poolSize == 0) {
    int state=runState;
    if (state < STOP && queueSize.get() > 0) {
      state=RUNNING;
      Thread t=addThread();
      if (t != null)       t.start();
    }
    if (state == STOP || state == SHUTDOWN) {
      runState=TERMINATED;
      termination.signalAll();
    }
  }
}

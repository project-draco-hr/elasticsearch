{
  logger.debug("starting azure services");
  if (isDiscoveryReady(settings,logger)) {
    logger.debug("starting azure discovery service");
    bind(AzureComputeService.class).to(settings.getAsClass("cloud.azure.api.impl",AzureComputeServiceImpl.class)).asEagerSingleton();
  }
  if (isSnapshotReady(settings,logger)) {
    logger.debug("starting azure repository service");
    bind(AzureStorageService.class).to(settings.getAsClass("repositories.azure.api.impl",AzureStorageServiceImpl.class)).asEagerSingleton();
  }
}

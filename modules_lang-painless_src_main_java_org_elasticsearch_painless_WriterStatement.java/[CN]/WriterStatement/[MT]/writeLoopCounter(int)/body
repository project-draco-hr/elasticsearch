{
  final Label end=new Label();
  execute.iinc(metadata.loopCounterSlot,-count);
  execute.visitVarInsn(Opcodes.ILOAD,metadata.loopCounterSlot);
  execute.push(0);
  execute.ifICmp(GeneratorAdapter.GT,end);
  execute.throwException(PAINLESS_ERROR_TYPE,"The maximum number of statements that can be executed in a loop has been reached.");
  execute.mark(end);
}

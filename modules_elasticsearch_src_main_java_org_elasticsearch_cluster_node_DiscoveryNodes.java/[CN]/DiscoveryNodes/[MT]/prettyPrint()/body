{
  StringBuilder sb=new StringBuilder();
  sb.append("nodes: \n");
  for (  DiscoveryNode node : this) {
    sb.append("   ").append(node);
    if (node == localNode()) {
      sb.append(", local");
    }
    if (node == masterNode()) {
      sb.append(", master");
    }
    sb.append("\n");
  }
  return sb.toString();
}

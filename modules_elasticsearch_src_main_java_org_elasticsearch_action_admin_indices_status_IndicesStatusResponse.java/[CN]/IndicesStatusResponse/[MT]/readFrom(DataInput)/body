{
  super.readFrom(in);
  shards=new ShardStatus[in.readInt()];
  for (int i=0; i < shards.length; i++) {
    shards[i]=readIndexShardStatus(in);
  }
  indicesSettings=newHashMap();
  int size=in.readInt();
  for (int i=0; i < size; i++) {
    indicesSettings.put(in.readUTF(),readSettingsFromStream(in));
  }
}

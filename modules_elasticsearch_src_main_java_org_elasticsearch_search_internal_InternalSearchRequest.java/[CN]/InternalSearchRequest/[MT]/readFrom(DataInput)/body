{
  index=in.readUTF();
  shardId=in.readInt();
  if (in.readBoolean()) {
    scroll=readScroll(in);
  }
  from=in.readInt();
  size=in.readInt();
  if (in.readBoolean()) {
    timeout=readTimeValue(in);
  }
  source=new byte[in.readInt()];
  in.readFully(source);
  int typesSize=in.readInt();
  if (typesSize > 0) {
    types=new String[typesSize];
    for (int i=0; i < typesSize; i++) {
      types[i]=in.readUTF();
    }
  }
}

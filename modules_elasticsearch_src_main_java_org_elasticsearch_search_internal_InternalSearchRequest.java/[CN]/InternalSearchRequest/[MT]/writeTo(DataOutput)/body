{
  out.writeUTF(index);
  out.writeInt(shardId);
  if (scroll == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    scroll.writeTo(out);
  }
  if (timeout == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    timeout.writeTo(out);
  }
  if (source == null) {
    out.writeInt(0);
  }
 else {
    out.writeInt(source.length);
    out.write(source);
  }
  if (extraSource == null) {
    out.writeInt(0);
  }
 else {
    out.writeInt(extraSource.length);
    out.write(extraSource);
  }
  out.writeInt(types.length);
  for (  String type : types) {
    out.writeUTF(type);
  }
}

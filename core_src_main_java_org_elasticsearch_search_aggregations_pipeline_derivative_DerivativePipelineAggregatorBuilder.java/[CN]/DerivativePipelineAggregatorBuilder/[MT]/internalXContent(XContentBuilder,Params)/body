{
  if (format != null) {
    builder.field(FORMAT_FIELD.getPreferredName(),format);
  }
  if (gapPolicy != null) {
    builder.field(GAP_POLICY_FIELD.getPreferredName(),gapPolicy.getName());
  }
  if (units != null) {
    builder.field(FORMAT_FIELD.getPreferredName(),units);
  }
  return builder;
}

{
  super(names,precisionStep,fuzzyFactor,index,store,boost,omitNorms,omitTermFreqAndPositions,new NamedAnalyzer("_long/" + precisionStep,new NumericLongAnalyzer(precisionStep)),new NamedAnalyzer("_long/max",new NumericLongAnalyzer(Integer.MAX_VALUE)));
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();
}

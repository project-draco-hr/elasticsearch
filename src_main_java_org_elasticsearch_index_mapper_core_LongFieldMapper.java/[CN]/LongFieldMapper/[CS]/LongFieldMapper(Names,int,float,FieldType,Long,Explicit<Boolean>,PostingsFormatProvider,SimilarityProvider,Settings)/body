{
  super(names,precisionStep,boost,fieldType,ignoreMalformed,new NamedAnalyzer("_long/" + precisionStep,new NumericLongAnalyzer(precisionStep)),new NamedAnalyzer("_long/max",new NumericLongAnalyzer(Integer.MAX_VALUE)),provider,similarity,fieldDataSettings);
  this.nullValue=nullValue;
  this.nullValueAsString=nullValue == null ? null : nullValue.toString();
}

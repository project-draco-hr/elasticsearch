{
  boolean exceptionThrown=false;
  try {
    String[] fieldNames={"someField","anotherField"};
    run(addMapping(prepareCreate("test"),"type1",new Object[]{fieldNames[0],"type","string","term_vector","with_positions_offsets"},new Object[]{fieldNames[1],"type","string","term_vector","with_positions"}));
  }
 catch (  JsonGenerationException e) {
    exceptionThrown=true;
  }
  assertThat(exceptionThrown,equalTo(false));
}

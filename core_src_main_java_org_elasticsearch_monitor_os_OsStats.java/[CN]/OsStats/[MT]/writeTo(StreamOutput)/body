{
  out.writeVLong(timestamp);
  out.writeOptionalStreamable(cpu);
  if (mem == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    mem.writeTo(out);
  }
  if (swap == null) {
    out.writeBoolean(false);
  }
 else {
    out.writeBoolean(true);
    swap.writeTo(out);
  }
}

{
  builder=builder.startObject().field(NAME.getPreferredName(),toString()).field(REASON.getPreferredName(),reason).field(TIME.getPreferredName(),String.format(Locale.US,"%.10gms",(double)(getTime() / 1000000.0)));
  if (!children.isEmpty()) {
    builder=builder.startArray(CHILDREN.getPreferredName());
    for (    CollectorResult child : children) {
      builder=child.toXContent(builder,params);
    }
    builder=builder.endArray();
  }
  builder=builder.endObject();
  return builder;
}

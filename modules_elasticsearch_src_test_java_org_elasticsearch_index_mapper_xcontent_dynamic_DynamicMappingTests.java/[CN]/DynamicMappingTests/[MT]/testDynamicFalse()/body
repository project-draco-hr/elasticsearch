{
  String mapping=XContentFactory.jsonBuilder().startObject().startObject("type").field("dynamic","false").startObject("properties").startObject("field1").field("type","string").endObject().endObject().endObject().endObject().string();
  XContentDocumentMapper defaultMapper=MapperTests.newParser().parse(mapping);
  ParsedDocument doc=defaultMapper.parse("type","1",XContentFactory.jsonBuilder().startObject().field("field1","value1").field("field2","value2").copiedBytes());
  assertThat(doc.doc().get("field1"),equalTo("value1"));
  assertThat(doc.doc().get("field2"),nullValue());
}

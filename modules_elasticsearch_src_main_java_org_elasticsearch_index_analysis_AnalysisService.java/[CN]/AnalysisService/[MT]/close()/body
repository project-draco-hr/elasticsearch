{
  for (  NamedAnalyzer analyzer : analyzers.values()) {
    if (analyzer.scope() == AnalyzerScope.INDEX) {
      try {
        analyzer.close();
      }
 catch (      NullPointerException e) {
      }
catch (      Exception e) {
        logger.debug("failed to close analyzer " + analyzer);
      }
    }
  }
}

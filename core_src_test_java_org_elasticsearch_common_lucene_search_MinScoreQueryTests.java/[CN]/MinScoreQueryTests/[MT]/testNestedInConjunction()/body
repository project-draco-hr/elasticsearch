{
  Term t1=randomTerm();
  Term t2=randomTerm();
  Query tq1=new TermQuery(t1);
  Query tq2=new TermQuery(t2);
  float minScore=randomMinScore(tq1);
  BooleanQuery bq1=new BooleanQuery.Builder().add(new MinScoreQuery(tq1,minScore),Occur.MUST).add(tq2,Occur.FILTER).build();
  BooleanQuery bq2=new BooleanQuery.Builder().add(tq1,Occur.MUST).add(tq2,Occur.FILTER).build();
  assertMinScoreEquivalence(bq2,bq1,minScore);
}

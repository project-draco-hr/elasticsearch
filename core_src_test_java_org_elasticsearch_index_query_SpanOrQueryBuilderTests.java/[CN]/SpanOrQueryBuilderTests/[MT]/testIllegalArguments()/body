    SpanOrQueryBuilder spanOrBuilder=new SpanOrQueryBuilder(SpanTermQueryBuilder.PROTOTYPE);
    spanOrBuilder.clause(null);
    fail("cannot be null");
  }
 catch (  IllegalArgumentException e) {
  }
}

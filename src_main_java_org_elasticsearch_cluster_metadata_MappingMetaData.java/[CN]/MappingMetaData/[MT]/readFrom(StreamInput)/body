{
  String type=in.readString();
  CompressedString source=CompressedString.readCompressedString(in);
  Id id=new Id(in.readBoolean() ? in.readString() : null);
  Routing routing=new Routing(in.readBoolean(),in.readBoolean() ? in.readString() : null);
  Timestamp timestamp=new Timestamp(in.readBoolean(),in.readBoolean() ? in.readString() : null,in.readString(),in.getVersion().onOrAfter(Version.V_1_4_0_Beta1) ? (in.readBoolean() ? in.readString() : null) : TimestampFieldMapper.Defaults.DEFAULT_TIMESTAMP);
  final boolean hasParentField=in.readBoolean();
  return new MappingMetaData(type,source,id,routing,timestamp,hasParentField);
}

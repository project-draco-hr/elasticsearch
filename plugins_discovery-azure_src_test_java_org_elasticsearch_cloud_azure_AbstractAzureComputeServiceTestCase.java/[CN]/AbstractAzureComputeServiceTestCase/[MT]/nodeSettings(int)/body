{
  Settings.Builder builder=Settings.settingsBuilder().put(super.nodeSettings(nodeOrdinal)).put("discovery.type","azure").put("node.mode","network");
  builder.put(Management.SUBSCRIPTION_ID,"fake").put(Discovery.REFRESH_SETTING.getKey(),"5s").put(Management.KEYSTORE_PATH,"dummy").put(Management.KEYSTORE_PASSWORD,"dummy").put(Management.SERVICE_NAME,"dummy");
  return builder.build();
}

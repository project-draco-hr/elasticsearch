{
  Settings.Builder builder=Settings.settingsBuilder().extendArray("plugin.types",CloudAzurePlugin.class.getName(),TestPlugin.class.getName()).put(Storage.API_IMPLEMENTATION,mock).put(Storage.CONTAINER,"snapshots");

{
  super(new Names(name,name,name,name),1.0f,fieldType,null,indexAnalyzer,searchAnalyzer,postingsProvider,docValuesProvider,similarity,normsLoading,fieldDataSettings,indexSettings);
  if (hasDocValues()) {
    throw new MapperParsingException("Field [" + names.fullName() + "] is always tokenized and cannot have doc values");
  }
  this.enabledState=enabled;
  this.autoBoost=autoBoost;
}

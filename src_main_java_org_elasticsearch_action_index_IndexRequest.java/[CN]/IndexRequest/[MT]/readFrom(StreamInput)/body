{
  super.readFrom(in);
  type=in.readUTF();
  if (in.readBoolean()) {
    id=in.readUTF();
  }
  if (in.readBoolean()) {
    routing=in.readUTF();
  }
  if (in.readBoolean()) {
    parent=in.readUTF();
  }
  if (in.readBoolean()) {
    timestamp=in.readUTF();
  }
  ttl=in.readLong();
  sourceUnsafe=false;
  sourceOffset=0;
  sourceLength=in.readVInt();
  source=new byte[sourceLength];
  in.readFully(source);
  opType=OpType.fromId(in.readByte());
  refresh=in.readBoolean();
  version=in.readLong();
  if (in.readBoolean()) {
    percolate=in.readUTF();
  }
  versionType=VersionType.fromValue(in.readByte());
}

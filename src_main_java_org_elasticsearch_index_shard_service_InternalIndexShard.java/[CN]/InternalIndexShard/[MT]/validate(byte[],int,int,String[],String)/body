{
  readAllowed();
  if (querySourceLength == 0) {
    return true;
  }
 else {
    try {
      queryParserService.parse(querySource,querySourceOffset,querySourceLength);
    }
 catch (    QueryParsingException e) {
      return false;
    }
catch (    AssertionError e) {
      return false;
    }
  }
  return true;
}

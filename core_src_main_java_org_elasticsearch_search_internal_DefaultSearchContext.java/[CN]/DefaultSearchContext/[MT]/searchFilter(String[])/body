{
  Query filter=mapperService().searchFilter(types);
  if (filter == null && aliasFilter == null) {
    return null;
  }
  BooleanQuery bq=new BooleanQuery();
  if (filter != null) {
    bq.add(filter,Occur.MUST);
  }
  if (aliasFilter != null) {
    bq.add(aliasFilter,Occur.MUST);
  }
  return new ConstantScoreQuery(bq);
}

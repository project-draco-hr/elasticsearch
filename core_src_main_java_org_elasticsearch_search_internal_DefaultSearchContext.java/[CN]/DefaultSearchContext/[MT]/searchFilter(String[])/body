{
  Query typesFilter=createSearchFilter(types,aliasFilter,mapperService().hasNested());
  if (sliceFilter == null) {
    return typesFilter;
  }
  if (typesFilter == null) {
    return sliceFilter;
  }
  return new BooleanQuery.Builder().add(typesFilter,Occur.FILTER).add(sliceFilter,Occur.FILTER).build();
}

{
  if (!ctx.decltype().LBRACE().isEmpty()) {
    Location location=location(ctx);
    String arrayType=ctx.decltype().getText();
    SReturn code=new SReturn(location,new EChain(location,new LNewArray(location,arrayType,Arrays.asList(new EChain(location,new LVariable(location,"size"))))));
    String name="lambda$" + synthetic.size();
    synthetic.add(new SFunction(new FunctionReserved(),location,arrayType,name,Arrays.asList("int"),Arrays.asList("size"),Arrays.asList(code),true));
    return new EFunctionRef(location(ctx),"this",name);
  }
  return new EFunctionRef(location(ctx),ctx.decltype().getText(),ctx.NEW().getText());
}

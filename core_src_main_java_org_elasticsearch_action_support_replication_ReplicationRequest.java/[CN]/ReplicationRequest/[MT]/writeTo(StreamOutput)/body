{
  super.writeTo(out);
  if (shardId != null) {
    out.writeBoolean(true);
    shardId.writeTo(out);
  }
 else {
    out.writeBoolean(false);
  }
  out.writeByte(consistencyLevel.id());
  timeout.writeTo(out);
  out.writeString(index);
  out.writeVLong(routedBasedOnClusterVersion);
  out.writeVLong(primaryTerm);
}

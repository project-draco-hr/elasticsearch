{
  super.readFrom(in);
  if (in.readBoolean()) {
    shardId=ShardId.readShardId(in);
  }
 else {
    shardId=null;
  }
  activeShardCount=ActiveShardCount.readFrom(in);
  timeout=new TimeValue(in);
  index=in.readString();
  routedBasedOnClusterVersion=in.readVLong();
  primaryTerm=in.readVLong();
}

{
  builder.startArray(Fields.RESPONSES);
  for (  MultiPercolateResponse.Item item : items) {
    builder.startObject();
    if (item.isFailure()) {
      ElasticsearchException.renderThrowable(builder,params,item.getFailure());
    }
 else {
      item.getResponse().toXContent(builder,params);
    }
    builder.endObject();
  }
  builder.endArray();
  return builder;
}

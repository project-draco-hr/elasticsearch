{
  try {
    final Path[] locations=translog.locations();
    translog.close();
    if (translog.currentId() > 1) {
      assertFileDeleted(translog,translog.currentId() - 1);
    }
    assertFileIsPresent(translog,translog.currentId());
    IOUtils.rm(locations);
  }
  finally {
    super.tearDown();
  }
}

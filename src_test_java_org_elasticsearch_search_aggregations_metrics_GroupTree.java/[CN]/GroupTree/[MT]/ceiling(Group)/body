{
  if (size == 0) {
    return null;
  }
 else   if (size == 1) {
    return base.compareTo(leaf) <= 0 ? leaf : null;
  }
 else {
    if (base.compareTo(leaf) < 0) {
      Group r=left.ceiling(base);
      if (r == null) {
        r=right.first();
      }
      return r;
    }
 else {
      return right.ceiling(base);
    }
  }
}

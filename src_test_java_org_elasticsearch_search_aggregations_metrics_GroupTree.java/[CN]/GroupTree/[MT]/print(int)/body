{
  for (int i=0; i < depth; i++) {
    System.out.printf("| ");
  }
  int imbalance=Math.abs((left != null ? left.depth : 1) - (right != null ? right.depth : 1));
  System.out.printf("%s%s, %d, %d, %d\n",(imbalance > 1 ? "* " : "") + (right != null && leaf.compareTo(right.first()) != 0 ? "+ " : ""),leaf,size,count,this.depth);
  if (left != null) {
    left.print(depth + 1);
    right.print(depth + 1);
  }
}

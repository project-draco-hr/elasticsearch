{
  if (indexReader.leaves().size() <= 1) {
    return this;
  }
  try {
    return cache.load(indexReader,this);
  }
 catch (  Throwable e) {
    if (e instanceof ElasticsearchException) {
      throw (ElasticsearchException)e;
    }
 else {
      throw new ElasticsearchException(e.getMessage(),e);
    }
  }
}

{
  if (!event.metaDataChanged()) {
    return;
  }
  executor.execute(new Runnable(){
    @Override public void run(){
      logger.debug("writing to gateway");
      try {
        gateway.write(event.state().metaData());
      }
 catch (      Exception e) {
        logger.error("failed to write to gateway",e);
      }
    }
  }
);
}

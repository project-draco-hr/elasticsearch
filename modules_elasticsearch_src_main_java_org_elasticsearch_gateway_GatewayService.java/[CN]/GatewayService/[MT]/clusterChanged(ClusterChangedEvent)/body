{
  if (lifecycle.started() && event.localNodeMaster()) {
    if (event.firstMaster() && firstMasterRead.compareAndSet(false,true)) {
      executor.execute(new Runnable(){
        @Override public void run(){
          readFromGateway(null);
        }
      }
);
    }
 else {
      writeToGateway(event);
    }
  }
}
